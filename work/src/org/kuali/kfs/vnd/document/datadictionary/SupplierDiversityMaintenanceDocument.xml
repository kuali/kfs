<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="SupplierDiversityMaintenanceDocument" parent="SupplierDiversityMaintenanceDocument-parentBean" />

  <bean id="SupplierDiversityMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.vendor.bo.SupplierDiversity" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="SupplierDiversityMaintenanceDocument-EditSupplierDiversity" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>vendorSupplierDiversityCode</value>
      </list>
    </property>
    <property name="label" value="Supplier Diversity" />
    <property name="shortLabel" value="Supplier Diversity" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-VN"
            p:parameterName="SUPPLIER_DIVERSITY"
            p:parameterDetailType="SupplierDiversity" />
    </property>
    <property name="documentTypeName" value="SupplierDiversityMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="PMSD" />
    <property name="workflowProperties" >
      <ref bean="SupplierDiversityMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>PA_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="SupplierDiversityMaintenanceDocument-EditSupplierDiversity" parent="SupplierDiversityMaintenanceDocument-EditSupplierDiversity-parentBean" />

  <bean id="SupplierDiversityMaintenanceDocument-EditSupplierDiversity-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Supplier Diversity" />
    <property name="title" value="Edit Supplier Diversity" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="vendorSupplierDiversityCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="vendorSupplierDiversityDescription"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="active" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="SupplierDiversityMaintenanceDocument-workflowProperties" parent="SupplierDiversityMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="SupplierDiversityMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
