<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="OwnershipCategory" parent="OwnershipCategory-parentBean" />

  <bean id="OwnershipCategory-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.vendor.bo.OwnershipCategory" />
    <property name="inquiryDefinition" >
      <ref bean="OwnershipCategory-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="OwnershipCategory-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="vendorOwnershipCategoryCode" />
    <property name="objectLabel" value="Ownership Type Category" />
    <property name="attributes" >
      <list>
        <ref bean="OwnershipCategory-vendorOwnershipCategoryCode" />
        <ref bean="OwnershipCategory-vendorOwnershipCategoryDescription" />
        <ref bean="OwnershipCategory-active" />
        <ref bean="OwnershipCategory-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="OwnershipCategory-vendorOwnershipCategoryCode" parent="OwnershipCategory-vendorOwnershipCategoryCode-parentBean" />

  <bean id="OwnershipCategory-vendorOwnershipCategoryCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="vendorOwnershipCategoryCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Ownership Type Category Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="6" />
    </property>
  </bean>
  <bean id="OwnershipCategory-vendorOwnershipCategoryDescription" parent="OwnershipCategory-vendorOwnershipCategoryDescription-parentBean" />

  <bean id="OwnershipCategory-vendorOwnershipCategoryDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="vendorOwnershipCategoryDescription" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Ownership Type Category Description" />
    <property name="shortLabel" value="Description" />
    <property name="maxLength" value="45" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="47" />
    </property>
  </bean>
  <bean id="OwnershipCategory-active" parent="OwnershipCategory-active-parentBean" />

  <bean id="OwnershipCategory-active-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
    <property name="name" value="active" />
  </bean>
  <bean id="OwnershipCategory-versionNumber" parent="OwnershipCategory-versionNumber-parentBean" />

  <bean id="OwnershipCategory-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="OwnershipCategory-inquiryDefinition" parent="OwnershipCategory-inquiryDefinition-parentBean" />

  <bean id="OwnershipCategory-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Ownership Type Category Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="vendorOwnershipCategoryDescription" />
              <bean parent="FieldDefinition"
                    p:attributeName="vendorOwnershipCategoryCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="active" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="OwnershipCategory-lookupDefinition" parent="OwnershipCategory-lookupDefinition-parentBean" />

  <bean id="OwnershipCategory-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Ownership Type Category Lookup" />
    <property name="instructions" value="Lookup an Owner Category" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="vendorOwnershipCategoryCode" />
        <bean parent="FieldDefinition"
              p:attributeName="vendorOwnershipCategoryDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="vendorOwnershipCategoryCode" />
        <bean parent="FieldDefinition"
              p:attributeName="vendorOwnershipCategoryDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
  </bean>
</beans>
