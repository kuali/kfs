<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.kra.routingform.bo.RoutingFormInstitutionCostShare">
        <customAttributeEnabled>false</customAttributeEnabled>

        <inquiry>
            <title>Routing Form Institution Cost Share Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="accountNumber" />
                <field attributeName="chartOfAccountsCode" />
                <field attributeName="organizationCode" />
                <field attributeName="routingFormCostShareAmount" />
                <field attributeName="routingFormCostShareDescription" />
                <field attributeName="routingFormCostShareSequenceNumber" />
                <field attributeName="documentNumber" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Routing Form Institution Cost Share Lookup</title>

            <instructions>Look up a Routing Form Institution Cost Share</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="routingFormCostShareSequenceNumber" />
                    <sortAttribute attributeName="documentNumber" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="accountNumber" required="false" />
                <lookupField attributeName="chartOfAccountsCode" required="false" />
                <lookupField attributeName="organizationCode" required="false" />
                <lookupField attributeName="routingFormCostShareAmount" required="false" />
                <lookupField attributeName="routingFormCostShareDescription" required="false" />
                <lookupField attributeName="routingFormCostShareSequenceNumber" required="false" />
                <lookupField attributeName="documentNumber" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="accountNumber" />
                <field attributeName="chartOfAccountsCode" />
                <field attributeName="organizationCode" />
                <field attributeName="routingFormCostShareAmount" />
                <field attributeName="routingFormCostShareDescription" />
                <field attributeName="routingFormCostShareSequenceNumber" />
                <field attributeName="documentNumber" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="accountNumber" forceUppercase="true">
                <label>Account Number</label>
                <shortLabel>Account</shortLabel>

                <maxLength>7</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="9" />
                </control>

                <summary>accountNumber</summary>
                <description>accountNumber description...</description>
			</attribute>

            <attribute name="chartOfAccountsCode" forceUppercase="true">
                <label>Chart Of Accounts Code</label>
                <shortLabel>Chart</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <select valuesFinderClass="org.kuali.core.lookup.keyvalues.SimpleChartValuesFinder" />
                </control>

                <summary>chartOfAccountsCode</summary>
                <description>chartOfAccountsCode description...</description>
			</attribute>

            <attribute name="organizationCode" forceUppercase="true">
                <label>Organization Code</label>
                <shortLabel>Org</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="6" />
                </control>

                <summary>organizationCode</summary>
                <description>organizationCode description...</description>
			</attribute>

            <attribute name="routingFormCostShareAmount" forceUppercase="true">
                <label>Routing Form Cost Share Amount</label>
                <shortLabel>Amount</shortLabel>

                <maxLength>19</maxLength>

                <validationPattern>
                    <fixedPoint allowNegative="true" precision="19" scale="2" />
                </validationPattern>

                <control>
                    <currency size="10" formattedMaxLength="26" />
                </control>

                <summary>routingFormCostShareAmount</summary>
                <description>routingFormCostShareAmount description...</description>
			</attribute>

            <attribute name="routingFormCostShareDescription" forceUppercase="true">
                <label>Routing Form Cost Share Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>250</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="50"/>
                </control>

                <summary>routingFormCostShareDescription</summary>
                <description>routingFormCostShareDescription description...</description>
			</attribute>

            <attribute name="routingFormCostShareSequenceNumber" forceUppercase="true">
                <label>Routing Form Cost Share Sequence Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <hidden />
                </control>

                <summary>routingFormCostShareSequenceNumber</summary>
                <description>routingFormCostShareSequenceNumber description...</description>
			</attribute>

            <attribute name="documentNumber" forceUppercase="true">
                <label>Document Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>14</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <hidden />
                </control>

                <summary>documentNumber</summary>
                <description>documentNumber description...</description>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
