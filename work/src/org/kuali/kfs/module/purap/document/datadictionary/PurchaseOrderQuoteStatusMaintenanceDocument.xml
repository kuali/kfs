<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="PurchaseOrderQuoteStatusMaintenanceDocument" parent="PurchaseOrderQuoteStatusMaintenanceDocument-parentBean" />

  <bean id="PurchaseOrderQuoteStatusMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.purap.bo.PurchaseOrderQuoteStatus" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="PurchaseOrderQuoteStatusMaintenanceDocument-EditPurchaseOrderQuote" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>statusCode</value>
      </list>
    </property>
    <property name="label" value="Purchase Order Quote Status" />
    <property name="shortLabel" value="Purchase Order Quote Status" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-PA"
            p:parameterName="PURCHASE_ORDER_QUOTE_STATUS"
            p:parameterDetailType="PurchaseOrderQuoteStatus" />
    </property>
    <property name="documentTypeName" value="PurchaseOrderQuoteStatusMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="PMQS" />
    <property name="workflowProperties" >
      <ref bean="PurchaseOrderQuoteStatusMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>PA_PUR_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="PurchaseOrderQuoteStatusMaintenanceDocument-EditPurchaseOrderQuote" parent="PurchaseOrderQuoteStatusMaintenanceDocument-EditPurchaseOrderQuote-parentBean" />

  <bean id="PurchaseOrderQuoteStatusMaintenanceDocument-EditPurchaseOrderQuote-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Purchase Order Quote" />
    <property name="title" value="Edit Purchase Order Quote" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="statusCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="statusDescription"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="active" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="PurchaseOrderQuoteStatusMaintenanceDocument-workflowProperties" parent="PurchaseOrderQuoteStatusMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="PurchaseOrderQuoteStatusMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
