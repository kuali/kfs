<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.financial.bo.ProcurementCardVendor">


        <inquiry>
            <title>Procurement Card Transaction Vendor Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="documentNumber" />
                <field attributeName="financialDocumentTransactionLineNumber" />
                <field attributeName="vendorName" />
                <field attributeName="transactionMerchantCategoryCode" />
                <field attributeName="vendorCityName" />
                <field attributeName="vendorLine1Address" />
                <field attributeName="vendorLine2Address" />
                <field attributeName="vendorOrderNumber" />
                <field attributeName="vendorStateCode" />
                <field attributeName="vendorZipCode" />
                <field attributeName="versionNumber" />
                <field attributeName="visaVendorIdentifier" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <attributes>
            <attributeReference name="documentNumber" sourceClassName="org.kuali.core.bo.DocumentHeader" sourceAttributeName="documentNumber" />

            <attributeReference name="financialDocumentTransactionLineNumber" sourceClassName="org.kuali.module.financial.bo.ProcurementCardTransactionDetail" sourceAttributeName="financialDocumentTransactionLineNumber" />

            <attribute name="transactionMerchantCategoryCode" forceUppercase="true">
                <label>Transaction Merchant Category Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>4</maxLength>

                <control>
                    <text size="6" />
                </control>

                <summary><![CDATA[&nbsp;]]></summary>
                <description>The system code for the category of the transaction merchant.</description>
            </attribute>

            <attribute name="vendorCityName" forceUppercase="true">
                <label>Vendor City Name</label>
                <shortLabel>Name</shortLabel>

                <maxLength>45</maxLength>

                <control>
                    <text size="47" />
                </control>

                <summary><![CDATA[&nbsp;]]></summary>
                <description>The city in the address record of the transaction vendor.</description>
            </attribute>

            <attribute name="vendorLine1Address" forceUppercase="true">
                <label>Vendor Line1 Address</label>
                <shortLabel>Address</shortLabel>

                <maxLength>45</maxLength>

                <control>
                    <text size="47" />
                </control>

                <summary><![CDATA[&nbsp;]]></summary>
                <description>The first line in the adddres record of the transaction vendor.</description>
            </attribute>

            <attribute name="vendorLine2Address" forceUppercase="true">
                <label>Vendor Line2 Address</label>
                <shortLabel>Address</shortLabel>

                <maxLength>45</maxLength>

                <control>
                    <text size="47" />
                </control>

                <summary><![CDATA[&nbsp;]]></summary>
                <description>The second line in the adddres record of the transaction vendor.</description>
            </attribute>

            <attribute name="vendorName" forceUppercase="true">
                <label>Vendor Name</label>
                <shortLabel>Name</shortLabel>

                <maxLength>45</maxLength>

                <control>
                    <text size="47" />
                </control>

                <summary><![CDATA[&nbsp;]]></summary>
                <description>The name of the transaction vendor.</description>
            </attribute>

            <attribute name="vendorOrderNumber" forceUppercase="true">
                <label>Vendor Order Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>12</maxLength>

                <control>
                    <text size="14" />
                </control>

                <summary><![CDATA[&nbsp;]]></summary>
                <description>The order number assigned by the transaction vendor.</description>
            </attribute>

            <attribute name="vendorStateCode" forceUppercase="true">
                <label>Vendor State Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>2</maxLength>

                <control>
                    <text size="4" />
                </control>

                <summary><![CDATA[&nbsp;]]></summary>
                <description>The state code in the address record of the transaction vendor. </description>
            </attribute>

            <attribute name="vendorZipCode" forceUppercase="true">
                <label>Vendor Postal (ZIP) Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>20</maxLength>

                <control>
                    <text size="22" />
                </control>

                <summary><![CDATA[&nbsp;]]></summary>
                <description>The postal code in the address record of the transaction vendor.</description>
            </attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

            <attribute name="visaVendorIdentifier" forceUppercase="true">
                <label>Visa Vendor Identifier</label>
                <shortLabel>Identifier</shortLabel>

                <maxLength>16</maxLength>

                <control>
                    <text size="18" />
                </control>

                <summary><![CDATA[&nbsp;]]></summary>
                <description>The visa identifier of the transaction vendor.</description>
            </attribute>

        </attributes>
    </businessObject>
</dictionaryEntry>
