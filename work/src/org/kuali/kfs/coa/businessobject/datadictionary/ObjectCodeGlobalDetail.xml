<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="ObjectCodeGlobalDetail" parent="ObjectCodeGlobalDetail-parentBean" />

  <bean id="ObjectCodeGlobalDetail-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.chart.bo.ObjectCodeGlobalDetail" />
    <property name="objectLabel" value="Object Code Change Detail" />
    <property name="attributes" >
      <list>
        <ref bean="ObjectCodeGlobalDetail-universityFiscalYear" />
        <ref bean="ObjectCodeGlobalDetail-chartOfAccountsCode" />
        <ref bean="ObjectCodeGlobalDetail-versionNumber" />
        <ref bean="ObjectCodeGlobalDetail-newCollectionRecord" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="ObjectCodeGlobalDetail-universityFiscalYear" parent="ObjectCodeGlobalDetail-universityFiscalYear-parentBean" />

  <bean id="ObjectCodeGlobalDetail-universityFiscalYear-parentBean" abstract="true" parent="AttributeReferenceDummy-genericFiscalYear">
    <property name="name" value="universityFiscalYear" />
    <property name="label" value="Fiscal Year" />
    <property name="shortLabel" value="FY" />
    <property name="required" value="false" />
  </bean>
  <bean id="ObjectCodeGlobalDetail-chartOfAccountsCode" parent="ObjectCodeGlobalDetail-chartOfAccountsCode-parentBean" />

  <bean id="ObjectCodeGlobalDetail-chartOfAccountsCode-parentBean" abstract="true" parent="Chart-chartOfAccountsCode">
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.module.chart.lookup.keyvalues.ChartValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
  </bean>
  <bean id="ObjectCodeGlobalDetail-versionNumber" parent="ObjectCodeGlobalDetail-versionNumber-parentBean" />

  <bean id="ObjectCodeGlobalDetail-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>
  <bean id="ObjectCodeGlobalDetail-newCollectionRecord" parent="ObjectCodeGlobalDetail-newCollectionRecord-parentBean" />

  <bean id="ObjectCodeGlobalDetail-newCollectionRecord-parentBean" abstract="true" parent="AttributeReferenceDummy-newCollectionRecord">
  </bean>
</beans>
