<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xmlns:dd="http://rice.kuali.org/dd" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd         http://rice.kuali.org/dd         http://rice.kuali.org/dd/dd.xsd">

  <bean id="PurchaseOrderQuoteStatusMaintenanceDocument" parent="PurchaseOrderQuoteStatusMaintenanceDocument-parentBean"/>

  <bean id="PurchaseOrderQuoteStatusMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.purap.businessobject.PurchaseOrderQuoteStatus"/>
    <property name="maintainableClass" value="org.kuali.rice.kns.maintenance.KualiMaintainableImpl"/>
    <property name="maintainableSections">
      <list>
        <ref bean="PurchaseOrderQuoteStatusMaintenanceDocument-EditPurchaseOrderQuote"/>
      </list>
    </property>
    <property name="lockingKeys">
      <list>
        <value>statusCode</value>
      </list>
    </property>
    <property name="label" value="Purchase Order Quote Status"/>
    <property name="shortLabel" value="Purchase Order Quote Status"/>
    <property name="helpDefinition">
      <bean parent="HelpDefinition" p:parameterClass="org.kuali.kfs.module.purap.businessobject.PurchaseOrderQuoteStatus" p:parameterName="PURCHASE_ORDER_QUOTE_STATUS"/>
    </property>
    <property name="documentTypeName" value="PMQS"/>
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase"/>
    <property name="workflowAttributes">
    	<ref bean="PurchaseOrderQuoteStatusMaintenanceDocument-workflowAttributes"/>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->

  <bean id="PurchaseOrderQuoteStatusMaintenanceDocument-EditPurchaseOrderQuote" parent="PurchaseOrderQuoteStatusMaintenanceDocument-EditPurchaseOrderQuote-parentBean"/>

  <bean id="PurchaseOrderQuoteStatusMaintenanceDocument-EditPurchaseOrderQuote-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Purchase Order Quote"/>
    <property name="title" value="Edit Purchase Order Quote"/>
    <property name="maintainableItems">
      <list>
        <bean parent="MaintainableFieldDefinition" p:name="statusCode" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="statusDescription" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="active" p:defaultValue="true"/>
      </list>
    </property>
  </bean>
  
  <!-- workflow attributes -->
  <bean id="PurchaseOrderQuoteStatusMaintenanceDocument-workflowAttributes" parent="PurchaseOrderQuoteStatusMaintenanceDocument-workflowAttributes-parentBean"/>
  
  <bean id="PurchaseOrderQuoteStatusMaintenanceDocument-workflowAttributes-parentBean" class="org.kuali.rice.kns.datadictionary.WorkflowAttributes" abstract="true"/>
</beans>