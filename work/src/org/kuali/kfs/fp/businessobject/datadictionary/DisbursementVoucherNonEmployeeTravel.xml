                <summary>
                    <![CDATA[&nbsp;]]>
                </summary>
                <description>A free-form text field that specifies the actual dollar amount for mileage involved in the Non-Employee Travel</description>
            </attribute>

            <attribute name="totalTravelAmount">
                <label>Grand Total</label>
                <shortLabel>Total</shortLabel>

                <maxLength>22</maxLength>

                <required>false</required>

                <control>
