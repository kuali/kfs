<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../core/datadictionary/dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.chart.bo.A21SubAccount">
        <customAttributeEnabled>true</customAttributeEnabled>

        <!--
            This object has no Inquiry or Lookups intentionally.  It is never dealt with on its 
            own, rather it is always accessed as part of SubAccount, so does not need an 
            Inquiry or Lookup.
        -->

        <titleAttribute>A21SubAccountNumber</titleAttribute>

        <attributes>
            <attributeReference name="chartOfAccountsCode" sourceClassName="org.kuali.module.chart.bo.Chart" sourceAttributeName="chartOfAccountsCode">
                <control>
                    <select valuesFinderClass="org.kuali.core.lookup.keyvalues.ChartValuesFinder" />
                </control>
            </attributeReference>

            <attributeReference name="accountNumber" sourceClassName="org.kuali.module.chart.bo.Account" sourceAttributeName="accountNumber" />

            <attributeReference name="subAccountNumber" sourceClassName="org.kuali.module.chart.bo.SubAccount" sourceAttributeName="subAccountNumber" />

            <attribute name="subAccountTypeCode" forceUppercase="true">
                <label>Sub-Account Type Code</label>
                <shortLabel>Type Code</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern>
                    <alphaNumeric />
                </validationPattern>

                <control>
                    <text size="2" />
                </control>

                <summary>Sub-Account Type Code</summary>
                <description>Sub-Account Type Code should be EX or CS indicating Indirect Cost Recovery or Cost Sharing respectively.</description>
            </attribute>

            <attributeReference name="indirectCostRecoveryTypeCode" sourceClassName="org.kuali.module.chart.bo.codes.ICRTypeCode" sourceAttributeName="code" />

            <attribute name="financialIcrSeriesIdentifier" forceUppercase="true">
                <label>Series Id</label>
                <shortLabel>Series Id</shortLabel>

                <maxLength>3</maxLength>
                <validationPattern>
                    <alphaNumeric />
                </validationPattern>

                <control>
                    <text size="3" />
                </control>

                <summary>Series Id</summary>
                <description>Series Id</description>
            </attribute>

            <attributeReference name="indirectCostRecoveryChartOfAccountsCode" sourceClassName="org.kuali.module.chart.bo.Chart" sourceAttributeName="chartOfAccountsCode">
                <label>ICR Chart</label>
                <shortLabel>ICR Chart</shortLabel>
                <control>
                    <select valuesFinderClass="org.kuali.core.lookup.keyvalues.ChartValuesFinder" />
                </control>

                <summary>ICR Chart</summary>
                <description>ICR Chart...</description>
            </attributeReference>

            <attributeReference name="indirectCostRecoveryAccountNumber" sourceClassName="org.kuali.module.chart.bo.Account" sourceAttributeName="accountNumber">
                <label>ICR Account</label>
                <shortLabel>ICR Account</shortLabel>
                <summary>ICR Account</summary>
                <description>ICR Account...</description>
            </attributeReference>

            <attributeReference name="offCampusCode" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="activeIndicator">
                <label>Off Campus Indicator</label>
                <shortLabel>Off Campus</shortLabel>
                <summary>Off Campus Indicator</summary>
                <description>Off Campus Indicator</description>
            </attributeReference>

            <attributeReference name="costShareChartOfAccountCode" sourceClassName="org.kuali.module.chart.bo.Chart" sourceAttributeName="chartOfAccountsCode">
                <label>Cost Sharing Chart of Accounts Code</label>
                <shortLabel>Cost Sharting Chart Code</shortLabel>
                <control>
                    <select valuesFinderClass="org.kuali.core.lookup.keyvalues.ChartValuesFinder" />
                </control>
                <summary>Cost Sharing Chart of Accounts Code</summary>
                <description>Cost Sharing Chart of Accounts Code</description>
            </attributeReference>


            <attributeReference name="costShareSourceAccountNumber" sourceClassName="org.kuali.module.chart.bo.Account" sourceAttributeName="accountNumber">
                <label>Cost Sharing Account Number</label>
                <shortLabel>Cost Sharing Account</shortLabel>
                <summary>Cost Sharing Account Number</summary>
                <description>Cost Sharing Account Number</description>
            </attributeReference>


            <attributeReference name="costShareSourceSubAccountNumber" sourceClassName="org.kuali.module.chart.bo.SubAccount" sourceAttributeName="subAccountNumber">
                <label>Cost Sharing Sub-Account Number</label>
                <shortLabel>Cost Sharing Sub-Account</shortLabel>
                <summary>Cost Sharing Sub-Account Number</summary>
                <description>Cost Sharing Sub-Account Number</description>
            </attributeReference>


            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
        
        <objectLabel>A21 Sub-Account</objectLabel>
    </businessObject>
</dictionaryEntry>
