<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9 http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">	
	<changeSet context="bootstrap,demo" author="kfs" id="KFS_LOAD_KRNS_NMSPC_T">
		<loadData tableName="KRNS_NMSPC_T" file="bootstrap/krns_nmspc_t.csv">
			<column header="NMSPC_CD" type="string" />
			<column header="NM" type="string" />
			<column header="ACTV_IND" type="string" />
			<column header="APPL_NMSPC_CD" type="string" />
		</loadData>
		<rollback>
			<delete tableName="KRNS_NMSPC_T">
				<where>APPL_NMSPC_CD = 'KFS'</where>
			</delete>
		</rollback>
    </changeSet>
	<changeSet context="bootstrap,demo" author="kfs" id="KFS_LOAD_KRNS_PARM_DTL_TYP_T">
		<loadData tableName="KRNS_PARM_DTL_TYP_T" file="bootstrap/krns_parm_dtl_typ_t.csv">
			<column header="NMSPC_CD" type="string" />
			<column header="OBJ_ID" type="string" />
			<column header="PARM_DTL_TYP_CD" type="string" />
			<column header="NM" type="string" />
			<column header="ACTV_IND" type="string" />
		</loadData>
		<rollback>
			<delete tableName="KRNS_PARM_DTL_TYP_T">
				<where>NMSPC_CD LIKE 'KFS%'</where>
			</delete>
		</rollback>
    </changeSet>

	<changeSet context="bootstrap,demo" author="kfs" id="KFS_LOAD_KRNS_PARM_T_oracle" dbms="oracle">
		<loadData tableName="KRNS_PARM_T" file="bootstrap/krns_parm_t.csv">
			<column header="NMSPC_CD" type="string" />
			<column header="OBJ_ID" type="string" />
			<column header="PARM_DTL_TYP_CD" type="string" />
			<column header="PARM_NM" type="string" />
			<column header="PARM_TYP_CD" type="string" />
			<column header="TXT" type="string" />
			<column header="PARM_DESC_TXT" type="string" />
			<column header="CONS_CD" type="string" />
			<column header="APPL_NMSPC_CD" type="string" />
		</loadData>
		<rollback>
			<delete tableName="KRNS_PARM_T">
				<where>APPL_NMSPC_CD = 'KFS'</where>
			</delete>
		</rollback>
    </changeSet>

	<changeSet context="bootstrap,demo" author="kfs" id="KFS_LOAD_KRNS_PARM_T_mysql" dbms="mysql">
		<sql>
		   SET sql_mode='NO_BACKSLASH_ESCAPES'
		</sql>
		<loadData tableName="KRNS_PARM_T" file="bootstrap/krns_parm_t.csv">
			<column header="NMSPC_CD" type="string" />
			<column header="OBJ_ID" type="string" />
			<column header="PARM_DTL_TYP_CD" type="string" />
			<column header="PARM_NM" type="string" />
			<column header="PARM_TYP_CD" type="string" />
			<column header="TXT" type="string" />
			<column header="PARM_DESC_TXT" type="string" />
			<column header="CONS_CD" type="string" />
			<column header="APPL_NMSPC_CD" type="string" />
		</loadData>
		<rollback>
			<delete tableName="KRNS_PARM_T">
				<where>APPL_NMSPC_CD = 'KFS'</where>
			</delete>
		</rollback>
    </changeSet>

	<changeSet context="bootstrap,demo" author="kfs" id="KFS_LOAD_KR_COUNTY_T">
		<loadData tableName="KR_COUNTY_T" file="bootstrap/kr_county_t.csv">
			<column header="COUNTY_CD" type="string" />
			<column header="STATE_CD" type="string" />
			<column header="POSTAL_CNTRY_CD" type="string" />
			<column header="OBJ_ID" type="string" />
			<column header="COUNTY_NM" type="string" />
			<column header="ACTV_IND" type="string" />
		</loadData>
		<rollback>
			<delete tableName="KR_COUNTY_T" />
		</rollback>
    </changeSet>
	<changeSet context="bootstrap,demo" author="kfs" id="KFS_LOAD_KR_POSTAL_CODE_T">
		<loadData tableName="KR_POSTAL_CODE_T" file="bootstrap/kr_postal_code_t.csv" />
		<rollback>
			<delete tableName="KR_POSTAL_CODE_T" />
		</rollback>
    </changeSet>
    
  <changeSet context="bootstrap,demo" author="kfs" id="KFS_LOAD_KR_STATE_T">
    <loadData tableName="KR_STATE_T" file="bootstrap/kr_state_t.csv" />
    <rollback>
      <delete tableName="KR_STATE_T" >
        <where>POSTAL_CNTRY_CD in ('AU', 'CA', 'MX')</where>
      </delete>
    </rollback>
  </changeSet>

</databaseChangeLog>   
      