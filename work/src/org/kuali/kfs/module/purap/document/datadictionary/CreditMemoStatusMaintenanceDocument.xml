<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xmlns:dd="http://rice.kuali.org/dd" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd         http://rice.kuali.org/dd         http://rice.kuali.org/dd/dd.xsd">

  <bean id="CreditMemoStatusMaintenanceDocument" parent="CreditMemoStatusMaintenanceDocument-parentBean"/>

  <bean id="CreditMemoStatusMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.purap.businessobject.CreditMemoStatus"/>
    <property name="maintainableClass" value="org.kuali.kfs.sys.document.FinancialSystemMaintainable"/>
    <property name="maintainableSections">
      <list>
        <ref bean="CreditMemoStatusMaintenanceDocument-EditCreditMemoStatus"/>
      </list>
    </property>
    <property name="allowsNewOrCopy" value="false"/>
    <property name="lockingKeys">
      <list>
        <value>statusCode</value>
      </list>
    </property>
    
    
    
    <property name="documentTypeName" value="PMCM"/>
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase"/>
    <property name="workflowAttributes">
    	<ref bean="CreditMemoStatusMaintenanceDocument-workflowAttributes"/>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->

  <bean id="CreditMemoStatusMaintenanceDocument-EditCreditMemoStatus" parent="CreditMemoStatusMaintenanceDocument-EditCreditMemoStatus-parentBean"/>

  <bean id="CreditMemoStatusMaintenanceDocument-EditCreditMemoStatus-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Credit Memo Status"/>
    <property name="title" value="Edit Credit Memo Status"/>
    <property name="maintainableItems">
      <list>
        <bean parent="MaintainableFieldDefinition" p:name="statusCode" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="statusDescription" p:required="true"/>
      </list>
    </property>
  </bean>
  
  <!-- workflow attributes -->
  <bean id="CreditMemoStatusMaintenanceDocument-workflowAttributes" parent="CreditMemoStatusMaintenanceDocument-workflowAttributes-parentBean"/>
  
  <bean id="CreditMemoStatusMaintenanceDocument-workflowAttributes-parentBean" class="org.kuali.rice.kns.datadictionary.WorkflowAttributes" abstract="true"/>
</beans>