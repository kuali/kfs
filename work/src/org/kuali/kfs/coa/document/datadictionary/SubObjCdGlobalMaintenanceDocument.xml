<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="SubObjCdGlobalMaintenanceDocument" parent="SubObjCdGlobalMaintenanceDocument-parentBean" />

  <bean id="SubObjCdGlobalMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.chart.bo.SubObjCdGlobal" />
    <property name="maintainableClass" value="org.kuali.module.chart.maintenance.SubObjCdGlobalMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="SubObjCdGlobalMaintenanceDocument-EditGlobalSubObjectCode" />
        <ref bean="SubObjCdGlobalMaintenanceDocument-EditObjectCodes" />
        <ref bean="SubObjCdGlobalMaintenanceDocument-EditAccounts" />
      </list>
    </property>
    <property name="defaultExistenceChecks" >
      <list>
        <bean parent="ReferenceDefinition"
              p:attributeName="chartOfAccounts"
              p:attributeToHighlightOnFail="chartOfAccountsCode" />
        <bean parent="ReferenceDefinition"
              p:attributeName="universityFiscal"
              p:attributeToHighlightOnFail="universityFiscalYear" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>documentNumber</value>
      </list>
    </property>
    <property name="label" value="Sub-Object Code Global" />
    <property name="shortLabel" value="Sub-Object Code Global" />
    <property name="summary" value="Global Sub Object Code Maintenance Document" />
    <property name="description" value="Global Sub Object Code Maintenance Document" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-CA"
            p:parameterName="SUB_OBJECT_CODE_GLOBAL"
            p:parameterDetailType="SubObjCdGlobal" />
    </property>
    <property name="documentTypeName" value="SubObjCdGlobalMaintenanceDocument" />
    <property name="businessRulesClass" value="org.kuali.module.chart.rules.SubObjCdGlobalRule" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="GSOB" />
    <property name="workflowProperties" >
      <ref bean="SubObjCdGlobalMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>kualiUniversalGroup</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="SubObjCdGlobalMaintenanceDocument-EditGlobalSubObjectCode" parent="SubObjCdGlobalMaintenanceDocument-EditGlobalSubObjectCode-parentBean" />

  <bean id="SubObjCdGlobalMaintenanceDocument-EditGlobalSubObjectCode-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Global Sub Object Code" />
    <property name="title" value="Edit Global Sub Object Code" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="universityFiscalYear"
              p:required="true"
              p:defaultValueFinderClass="org.kuali.kfs.lookup.valuefinder.FiscalYearFinder" />
        <bean parent="MaintainableFieldDefinition"
              p:name="chartOfAccountsCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialSubObjectCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialSubObjectCodeName"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialSubObjectCodeShortName"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialSubObjectActiveIndicator"
              p:defaultValue="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>
  <bean id="SubObjCdGlobalMaintenanceDocument-EditObjectCodes" parent="SubObjCdGlobalMaintenanceDocument-EditObjectCodes-parentBean" />

  <bean id="SubObjCdGlobalMaintenanceDocument-EditObjectCodes-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Object Codes" />
    <property name="title" value="Edit Object Codes" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableCollectionDefinition">
          <property name="name" value="subObjCdGlobalDetails" />
          <property name="businessObjectClass" value="org.kuali.module.chart.bo.SubObjCdGlobalDetail" />
          <property name="sourceClassName" value="org.kuali.module.chart.bo.ObjectCode" />
          <property name="sourceAttributeName" value="VALUE DOESN'T MATTER" />
          <property name="summaryTitle" value="Object Code" />
          <property name="summaryFields" >
            <list>
              <bean parent="MaintainableFieldDefinition"
                    p:name="financialObjectCode" />
            </list>
          </property>
          <property name="maintainableFields" >
            <list>
              <bean parent="MaintainableFieldDefinition"
                    p:name="universityFiscalYear"
                    p:required="true"
                    p:defaultValueFinderClass="org.kuali.kfs.lookup.valuefinder.FiscalYearFinder"
                    p:readOnlyAfterAdd="true"
                    p:template="universityFiscalYear" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="chartOfAccountsCode"
                    p:readOnlyAfterAdd="true"
                    p:template="chartOfAccountsCode" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="financialObjectCode"
                    p:required="true"
                    p:template="financialObjectCode" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="newCollectionRecord" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="versionNumber" />
            </list>
          </property>
          <property name="duplicateIdentificationFields">
          	<list>
          		<bean parent="MaintainableFieldDefinition" p:name="chartOfAccountsCode" />
          		<bean parent="MaintainableFieldDefinition" p:name="financialObjectCode" />
          	</list>
          </property>
        </bean>
      </list>
    </property>
  </bean>
  <bean id="SubObjCdGlobalMaintenanceDocument-EditAccounts" parent="SubObjCdGlobalMaintenanceDocument-EditAccounts-parentBean" />

  <bean id="SubObjCdGlobalMaintenanceDocument-EditAccounts-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Accounts" />
    <property name="title" value="Edit Accounts" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableCollectionDefinition">
          <property name="name" value="accountGlobalDetails" />
          <property name="businessObjectClass" value="org.kuali.module.chart.bo.AccountGlobalDetail" />
          <property name="sourceClassName" value="org.kuali.module.chart.bo.Account" />
          <property name="sourceAttributeName" value="VALUE DOESN'T MATTER" />
          <property name="summaryTitle" value="Account" />
          <property name="summaryFields" >
            <list>
              <bean parent="MaintainableFieldDefinition"
                    p:name="accountNumber" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="account.accountName" />
            </list>
          </property>
          <property name="maintainableFields" >
            <list>
              <bean parent="MaintainableFieldDefinition"
                    p:name="chartOfAccountsCode"
                    p:readOnlyAfterAdd="true"
                    p:template="chartOfAccountsCode" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="accountNumber"
                    p:required="true"
                    p:template="accountNumber" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="account.accountName"
                    p:readOnly="true"
                    p:template="accountName" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="newCollectionRecord" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="versionNumber" />
            </list>
          </property>
          <property name="duplicateIdentificationFields">
          	<list>
          		<bean parent="MaintainableFieldDefinition" p:name="chartOfAccountsCode" />
          		<bean parent="MaintainableFieldDefinition" p:name="accountNumber" />
          	</list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="SubObjCdGlobalMaintenanceDocument-workflowProperties" parent="SubObjCdGlobalMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="SubObjCdGlobalMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
    <property name="workflowPropertyGroups" >
      <list>
        <bean parent="WorkflowPropertyGroup">
          <property name="workflowProperties" >
            <list>
              <bean parent="WorkflowProperty"
                    p:path="newMaintainableObject.businessObject.accountGlobalDetails" />
              <bean parent="WorkflowProperty"
                    p:path="newMaintainableObject.businessObject.subObjCdGlobalDetails" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>
</beans>
