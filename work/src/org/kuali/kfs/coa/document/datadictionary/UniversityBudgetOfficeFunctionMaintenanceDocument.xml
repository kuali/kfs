<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="UniversityBudgetOfficeFunctionMaintenanceDocument" parent="UniversityBudgetOfficeFunctionMaintenanceDocument-parentBean" />

  <bean id="UniversityBudgetOfficeFunctionMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.chart.bo.UniversityBudgetOfficeFunction" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="UniversityBudgetOfficeFunctionMaintenanceDocument-EditUniversityBudgetOfficeFunction" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>financialUniversityBudgetOfficeFunctionCode</value>
      </list>
    </property>
    <property name="label" value="University Budget Office Function" />
    <property name="shortLabel" value="University Budget Office Function" />
    <property name="summary" value="University Budget Office Function Maintenance Document" />
    <property name="description" value="Document used to create or update University Budget Office Function Maintenance Document objects" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-CA"
            p:parameterName="UNIVERSITY_BUDGET_OFFICE_FUNCTION"
            p:parameterDetailType="UniversityBudgetOfficeFunction" />
    </property>
    <property name="documentTypeName" value="UniversityBudgetOfficeFunctionMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="UFUN" />
    <property name="workflowProperties" >
      <ref bean="UniversityBudgetOfficeFunctionMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>SY_MAINTAINENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="UniversityBudgetOfficeFunctionMaintenanceDocument-EditUniversityBudgetOfficeFunction" parent="UniversityBudgetOfficeFunctionMaintenanceDocument-EditUniversityBudgetOfficeFunction-parentBean" />

  <bean id="UniversityBudgetOfficeFunctionMaintenanceDocument-EditUniversityBudgetOfficeFunction-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit University Budget Office Function" />
    <property name="title" value="Edit University Budget Office Function" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="financialUniversityBudgetOfficeFunctionCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialUniversityBudgetOfficeFunctionName"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="UniversityBudgetOfficeFunctionMaintenanceDocument-workflowProperties" parent="UniversityBudgetOfficeFunctionMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="UniversityBudgetOfficeFunctionMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
