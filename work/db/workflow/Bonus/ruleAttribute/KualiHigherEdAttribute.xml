<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="ns:workflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
  <ruleAttributes xmlns="ns:workflow/RuleAttribute" xsi:schemaLocation="ns:workflow/RuleAttribute resource:RuleAttribute">
    <ruleAttribute>
      <name>KualiHigherEdAttribute</name>
      <className>org.kuali.workflow.attribute.KualiXmlRuleAttributeImpl</className>
      <label>Higher Education</label>
      <description>Higher Education Function Code Both Accounting Lines</description>
      <type>RuleXmlAttribute</type>
      <routingConfig>
        <fieldDef name="financialHigherEdFunctionCd" title="kuali_dd_label(Account)" workflowType="ALL">
          <display>
            <type>text</type>
          </display>
          <validation required="true" />
          <quickfinder appliesTo="financialHigherEdFunctionCd" draw="true" service="workflow-org.kuali.module.chart.bo.HigherEdFunction-Lookupable(null)" />
          <fieldEvaluation>
            <xpathexpression>wf:xstreamsafe('//org.kuali.core.bo.SourceAccountingLine/account/financialHigherEdFunctionCd') = wf:ruledata('financialHigherEdFunctionCd') or
                                wf:xstreamsafe('//org.kuali.core.bo.TargetAccountingLine/account/financialHigherEdFunctionCd') = wf:ruledata('financialHigherEdFunctionCd')</xpathexpression>
          </fieldEvaluation>
        </fieldDef>
      </routingConfig>
    </ruleAttribute>
  </ruleAttributes>
</data>
