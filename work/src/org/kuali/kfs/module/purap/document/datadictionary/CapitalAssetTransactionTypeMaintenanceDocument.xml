<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="CapitalAssetTransactionTypeMaintenanceDocument" parent="CapitalAssetTransactionTypeMaintenanceDocument-parentBean" />

  <bean id="CapitalAssetTransactionTypeMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.purap.bo.CapitalAssetTransactionType" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="CapitalAssetTransactionTypeMaintenanceDocument-EditCapitalAssetTransactionType" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>capitalAssetTransactionTypeCode</value>
      </list>
    </property>
    <property name="label" value="Capital Asset Transaction Type" />
    <property name="shortLabel" value="Capital Asset Transaction Type" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-PA"
            p:parameterName="CAPITAL_ASSET_TRANSACTION_TYPE"
            p:parameterDetailType="CapitalAssetTransactionType" />
    </property>
    <property name="documentTypeName" value="CapitalAssetTransactionTypeMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="PMTT" />
    <property name="workflowProperties" >
      <ref bean="CapitalAssetTransactionTypeMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>PA_PUR_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="CapitalAssetTransactionTypeMaintenanceDocument-EditCapitalAssetTransactionType" parent="CapitalAssetTransactionTypeMaintenanceDocument-EditCapitalAssetTransactionType-parentBean" />

  <bean id="CapitalAssetTransactionTypeMaintenanceDocument-EditCapitalAssetTransactionType-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Capital Asset Transaction Type" />
    <property name="title" value="Edit Capital Asset Transaction Type" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="capitalAssetTransactionTypeCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="capitalAssetTransactionTypeDescription"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="capitalAssetTransactionTypeServiceIndicator" />
        <bean parent="MaintainableFieldDefinition"
              p:name="active" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="CapitalAssetTransactionTypeMaintenanceDocument-workflowProperties" parent="CapitalAssetTransactionTypeMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="CapitalAssetTransactionTypeMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
