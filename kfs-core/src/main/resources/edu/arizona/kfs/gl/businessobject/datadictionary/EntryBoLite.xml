<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

<!--     <bean id="EntryBo" parent="Entry-parentBean"> -->
    <bean id="EntryBo" parent="BusinessObjectEntry">
        <property name="businessObjectClass" value="edu.arizona.kfs.gl.businessobject.EntryLiteBo" />
        <property name="inquiryDefinition">
            <ref bean="EntryLiteBo-inquiryDefinition"/>
        </property>
        <property name="lookupDefinition">
            <ref bean="EntryLiteBo-lookupDefinition"/>
        </property>
        <property name="objectLabel" value="EntryBo"/>
        <property name="attributes">
            <list>
                <ref bean="EntryLiteBo-gecDocumentNumber" />
                <ref bean="EntryLiteBo-entryId" />
                <ref bean="EntryLiteBo-universityFiscalYear"/>
                <ref bean="EntryLiteBo-chartOfAccountsCode"/>
                <ref bean="EntryLiteBo-accountNumber"/>
                <ref bean="EntryLiteBo-subAccountNumber"/>
                <ref bean="EntryLiteBo-financialObjectCode"/>
                <ref bean="EntryLiteBo-financialSubObjectCode"/>
                <ref bean="EntryLiteBo-financialObjectTypeCode"/>
                <ref bean="EntryLiteBo-financialBalanceTypeCode"/>
                <ref bean="EntryLiteBo-universityFiscalPeriodCode"/>
                <ref bean="EntryLiteBo-financialDocumentTypeCode"/>
                <ref bean="EntryLiteBo-financialSystemOriginationCode"/>
                <ref bean="EntryLiteBo-documentNumber"/>
                <ref bean="EntryLiteBo-transactionLedgerEntrySequenceNumber"/>
                <ref bean="EntryLiteBo-transactionLedgerEntryDescription"/>
                <ref bean="EntryLiteBo-transactionLedgerEntryAmount"/>
                <ref bean="EntryLiteBo-transactionDebitCreditCode"/>
                <ref bean="EntryLiteBo-transactionDate"/>
                <ref bean="EntryLiteBo-organizationDocumentNumber"/>
                <ref bean="EntryLiteBo-projectCode"/>
                <ref bean="EntryLiteBo-organizationReferenceId"/>
                <ref bean="EntryLiteBo-referenceFinancialDocumentTypeCode"/>
                <ref bean="EntryLiteBo-referenceFinancialSystemOriginationCode"/>
                <ref bean="EntryLiteBo-referenceFinancialDocumentNumber"/>
                <ref bean="EntryLiteBo-financialDocumentReversalDate"/>
                <ref bean="EntryLiteBo-transactionEncumbranceUpdateCode"/>
                <ref bean="EntryLiteBo-transactionPostingDate"/>
                <ref bean="EntryLiteBo-transactionDateTimeStamp"/>
            </list>
        </property>
    </bean>

    <!-- Attribute Definitions -->

    <bean id="EntryLiteBo-gecDocumentNumber" parent="DocumentHeader-documentNumber">
        <property name="name" value="gecDocumentNumber" />
        <property name="label" value="GEC Document Number" />
        <property name="shortLabel" value="GEC Doc Num" />
    </bean>

    <bean id="EntryLiteBo-entryId" parent="AttributeReferenceDummy-genericBigText">
        <property name="name" value="entryId" />
        <property name="label" value="Entry Id" />
        <property name="shortLabel" value="Entry Id" />
    </bean>

    <bean id="EntryLiteBo-universityFiscalYear" parent="Entry-universityFiscalYear-parentBean" />

    <bean id="EntryLiteBo-chartOfAccountsCode" parent="Entry-chartOfAccountsCode-parentBean" />

    <bean id="EntryLiteBo-accountNumber" parent="Entry-accountNumber-parentBean" />

    <bean id="EntryLiteBo-subAccountNumber" parent="Entry-subAccountNumber-parentBean" />

    <bean id="EntryLiteBo-financialObjectCode" parent="Entry-financialObjectCode-parentBean" />

    <bean id="EntryLiteBo-financialSubObjectCode" parent="Entry-financialSubObjectCode-parentBean" />

    <bean id="EntryLiteBo-financialObjectTypeCode" parent="Entry-financialObjectTypeCode-parentBean" />

    <bean id="EntryLiteBo-financialBalanceTypeCode" parent="Entry-financialBalanceTypeCode-parentBean" />

    <bean id="EntryLiteBo-universityFiscalPeriodCode" parent="Entry-universityFiscalPeriodCode-parentBean" />

    <bean id="EntryLiteBo-financialDocumentTypeCode" parent="Entry-financialDocumentTypeCode-parentBean" />

    <bean id="EntryLiteBo-financialSystemOriginationCode" parent="Entry-financialSystemOriginationCode-parentBean" />

    <bean id="EntryLiteBo-documentNumber" parent="Entry-documentNumber-parentBean" />

    <bean id="EntryLiteBo-transactionLedgerEntrySequenceNumber" parent="Entry-transactionLedgerEntrySequenceNumber-parentBean" />

    <bean id="EntryLiteBo-transactionLedgerEntryDescription" parent="Entry-transactionLedgerEntryDescription-parentBean" />

    <bean id="EntryLiteBo-transactionLedgerEntryAmount" parent="Entry-transactionLedgerEntryAmount-parentBean" />

    <bean id="EntryLiteBo-transactionDebitCreditCode" parent="Entry-transactionDebitCreditCode-parentBean" />

    <bean id="EntryLiteBo-transactionDate" parent="Entry-transactionDate-parentBean" />

    <bean id="EntryLiteBo-organizationDocumentNumber" parent="Entry-organizationDocumentNumber-parentBean" />

    <bean id="EntryLiteBo-projectCode" parent="Entry-projectCode-parentBean" />

    <bean id="EntryLiteBo-organizationReferenceId" parent="Entry-organizationReferenceId-parentBean" />

    <bean id="EntryLiteBo-referenceFinancialDocumentTypeCode" parent="Entry-referenceFinancialDocumentTypeCode-parentBean" />

    <bean id="EntryLiteBo-referenceFinancialSystemOriginationCode" parent="Entry-referenceFinancialSystemOriginationCode-parentBean" />

    <bean id="EntryLiteBo-referenceFinancialDocumentNumber" parent="Entry-referenceFinancialDocumentNumber-parentBean" />

    <bean id="EntryLiteBo-financialDocumentReversalDate" parent="Entry-financialDocumentReversalDate-parentBean" />

    <bean id="EntryLiteBo-transactionEncumbranceUpdateCode" parent="Entry-transactionEncumbranceUpdateCode-parentBean" />

    <bean id="EntryLiteBo-transactionPostingDate" parent="Entry-transactionPostingDate-parentBean" />

    <bean id="EntryLiteBo-transactionDateTimeStamp" parent="Entry-transactionDateTimeStamp-parentBean" />

<!-- Business Object Inquiry Definition -->

    <bean id="EntryLiteBo-inquiryDefinition" parent="InquiryDefinition">
        <property name="title" value="General Ledger Entry for GEC" />
        <property name="inquirySections">
            <list>
                <ref bean="EntryLiteBo-inquirySectionDefinition" />
            </list>
        </property>
    </bean>

    <bean id="EntryLiteBo-inquirySectionDefinition" parent="InquirySectionDefinition">
        <property name="title" value="" />
        <property name="numberOfColumns" value="1" />
        <property name="inquiryFields">
            <list>
                <bean parent="FieldDefinition" p:attributeName="universityFiscalYear" />
                <bean parent="FieldDefinition" p:attributeName="chartOfAccountsCode" />
                <bean parent="FieldDefinition" p:attributeName="accountNumber" />
                <bean parent="FieldDefinition" p:attributeName="subAccountNumber" />
                <bean parent="FieldDefinition" p:attributeName="financialObjectCode" />
                <bean parent="FieldDefinition" p:attributeName="financialSubObjectCode" />
                <bean parent="FieldDefinition" p:attributeName="financialBalanceTypeCode" />
                <bean parent="FieldDefinition" p:attributeName="financialObjectTypeCode" />
                <bean parent="FieldDefinition" p:attributeName="universityFiscalPeriodCode" />
                <bean parent="FieldDefinition" p:attributeName="financialSystemOriginationCode" />
                <bean parent="FieldDefinition" p:attributeName="financialDocumentTypeCode" />
                <bean parent="FieldDefinition" p:attributeName="documentNumber" />
                <bean parent="FieldDefinition" p:attributeName="transactionLedgerEntrySequenceNumber" />
                <bean parent="FieldDefinition" p:attributeName="transactionLedgerEntryDescription" />
                <bean parent="FieldDefinition" p:attributeName="transactionLedgerEntryAmount" />
                <bean parent="FieldDefinition" p:attributeName="transactionDebitCreditCode" />
                <bean parent="FieldDefinition" p:attributeName="transactionDate" />
                <bean parent="FieldDefinition" p:attributeName="organizationDocumentNumber" />
                <bean parent="FieldDefinition" p:attributeName="projectCode" />
                <bean parent="FieldDefinition" p:attributeName="organizationReferenceId" />
                <bean parent="FieldDefinition" p:attributeName="referenceFinancialDocumentTypeCode" />
                <bean parent="FieldDefinition" p:attributeName="referenceFinancialSystemOriginationCode" />
                <bean parent="FieldDefinition" p:attributeName="referenceFinancialDocumentNumber" />
                <bean parent="FieldDefinition" p:attributeName="financialDocumentReversalDate" />
                <bean parent="FieldDefinition" p:attributeName="transactionEncumbranceUpdateCode" />
                <bean parent="FieldDefinition" p:attributeName="transactionPostingDate" />
                <bean parent="FieldDefinition" p:attributeName="transactionDateTimeStamp" p:noInquiry="true" />
            </list>
        </property>
    </bean>

<!-- Business Object Lookup Definition -->

    <bean id="EntryLiteBo-lookupDefinition" parent="LookupDefinition">
        <property name="title" value="General Ledger Entry Lookup for GEC"/>
        <property name="lookupableID" value="glEntryLookupable"/>
        <property name="numOfColumns" value="2"/>
        
        <property name="defaultSort">
            <bean parent="SortDefinition">
                <property name="attributeNames">
                    <list>
                        <value>financialObjectCode</value>
                        <value>financialSubObjectCode</value>
                        <value>financialBalanceTypeCode</value>
                        <value>financialObjectTypeCode</value>
                    </list>
                </property>
            </bean>
        </property>
        <property name="lookupFields">
            <list>
                <bean parent="FieldDefinition" p:attributeName="universityFiscalYear" p:treatWildcardsAndOperatorsAsLiteral="true" p:defaultValueFinderClass="org.kuali.kfs.sys.businessobject.defaultvalue.FiscalYearFinder"/>
                <bean parent="FieldDefinition" p:attributeName="financialDocumentTypeCode" p:treatWildcardsAndOperatorsAsLiteral="true" />
                <bean parent="FieldDefinition" p:attributeName="chartOfAccountsCode" p:treatWildcardsAndOperatorsAsLiteral="true" />
                <bean parent="FieldDefinition" p:attributeName="financialSystemOriginationCode" p:treatWildcardsAndOperatorsAsLiteral="true" />
                <bean parent="FieldDefinition" p:attributeName="accountNumber" p:treatWildcardsAndOperatorsAsLiteral="false" />
                <bean parent="FieldDefinition" p:attributeName="documentNumber" p:treatWildcardsAndOperatorsAsLiteral="true" />
                <bean parent="FieldDefinition" p:attributeName="subAccountNumber" p:forceLookup="true"/>
                <bean parent="FieldDefinition" p:attributeName="organizationDocumentNumber" p:treatWildcardsAndOperatorsAsLiteral="true" />
                <bean parent="FieldDefinition" p:attributeName="financialObjectCode" p:treatWildcardsAndOperatorsAsLiteral="false" />
                <bean parent="FieldDefinition" p:attributeName="projectCode" p:treatWildcardsAndOperatorsAsLiteral="true" />
                <bean parent="FieldDefinition" p:attributeName="financialSubObjectCode" p:treatWildcardsAndOperatorsAsLiteral="false" />
                <bean parent="FieldDefinition" p:attributeName="organizationReferenceId" p:treatWildcardsAndOperatorsAsLiteral="true" />
                <bean parent="FieldDefinition" p:attributeName="financialBalanceTypeCode" p:treatWildcardsAndOperatorsAsLiteral="true" p:defaultValueFinderClass="org.kuali.kfs.gl.businessobject.options.ActualBalanceTypeOptionFinder"/>
                <bean parent="FieldDefinition" p:attributeName="referenceFinancialDocumentTypeCode" p:treatWildcardsAndOperatorsAsLiteral="true" />
                <bean parent="FieldDefinition" p:attributeName="financialObjectTypeCode" p:treatWildcardsAndOperatorsAsLiteral="true" />
                <bean parent="FieldDefinition" p:attributeName="referenceFinancialSystemOriginationCode" p:treatWildcardsAndOperatorsAsLiteral="true" />
                <bean parent="FieldDefinition" p:attributeName="universityFiscalPeriodCode"  p:treatWildcardsAndOperatorsAsLiteral="false" p:defaultValueFinderClass="org.kuali.kfs.gl.businessobject.options.PeriodCodeOptionFinder"/>
                <bean parent="FieldDefinition" p:attributeName="referenceFinancialDocumentNumber" p:treatWildcardsAndOperatorsAsLiteral="true" />
            </list>
        </property>
        <property name="resultFields">
            <list>
                <bean parent="FieldDefinition" p:attributeName="gecDocumentNumber" />
                <bean parent="FieldDefinition" p:attributeName="universityFiscalYear" />
                <bean parent="FieldDefinition" p:attributeName="chartOfAccountsCode" />
                <bean parent="FieldDefinition" p:attributeName="accountNumber" />
                <bean parent="FieldDefinition" p:attributeName="subAccountNumber" />
                <bean parent="FieldDefinition" p:attributeName="financialObjectCode" />
                <bean parent="FieldDefinition" p:attributeName="financialSubObjectCode" />
                <bean parent="FieldDefinition" p:attributeName="financialBalanceTypeCode" />
                <bean parent="FieldDefinition" p:attributeName="financialObjectTypeCode" />
                <bean parent="FieldDefinition" p:attributeName="universityFiscalPeriodCode" />
                <bean parent="FieldDefinition" p:attributeName="financialDocumentTypeCode" />
                <bean parent="FieldDefinition" p:attributeName="financialSystemOriginationCode" />
                <bean parent="FieldDefinition" p:attributeName="documentNumber" />
                <bean parent="FieldDefinition" p:attributeName="transactionLedgerEntryDescription" />
                <bean parent="FieldDefinition" p:attributeName="transactionLedgerEntryAmount" />
                <bean parent="FieldDefinition" p:attributeName="transactionDebitCreditCode" />
                <bean parent="FieldDefinition" p:attributeName="transactionDate" />
                <bean parent="FieldDefinition" p:attributeName="organizationDocumentNumber" />
                <bean parent="FieldDefinition" p:attributeName="projectCode" />
                <bean parent="FieldDefinition" p:attributeName="organizationReferenceId" />
                <bean parent="FieldDefinition" p:attributeName="referenceFinancialDocumentTypeCode" />
                <bean parent="FieldDefinition" p:attributeName="referenceFinancialSystemOriginationCode" />
                <bean parent="FieldDefinition" p:attributeName="referenceFinancialDocumentNumber" />
            </list>
        </property>
    </bean>

</beans>