<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="ns:workflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
  <ruleTemplates xmlns="ns:workflow/RuleTemplate" xsi:schemaLocation="ns:workflow/RuleTemplate resource:RuleTemplate">
    <ruleTemplate>
      <name>KualiPurchaseOrderTransmissionTemplate</name>
      <description>Kuali Purchase Order Document Transmission</description>
      <attributes>
        <attribute>
          <name>KualiPurchaseOrderTransmissionMethodAttribute</name>
          <required>false</required>
        </attribute>
        <attribute>
          <name>WorkflowRoutedByUserRoleAttribute</name>
          <required>false</required>
        </attribute>
        <attribute>
          <name>KualiPurApSourceDocumentRoutedUserRoleAttribute</name>
          <required>false</required>
        </attribute>
      </attributes>
      <ruleDefaults>
        <description>Rule for routing Purchase Order documents for transmission by transmission method.</description>
        <fromDate>07/01/2006</fromDate>
        <toDate>01/01/2100</toDate>
        <ignorePrevious>true</ignorePrevious>
        <active>true</active>
        <defaultActionRequested>F</defaultActionRequested>
        <supportsComplete>true</supportsComplete>
        <supportsApprove>true</supportsApprove>
        <supportsAcknowledge>true</supportsAcknowledge>
        <supportsFYI>true</supportsFYI>
      </ruleDefaults>
    </ruleTemplate>
  </ruleTemplates>
</data>
