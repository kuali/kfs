<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="SubFundGroupTypeMaintenanceDocument" parent="SubFundGroupTypeMaintenanceDocument-parentBean" />

  <bean id="SubFundGroupTypeMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.chart.bo.SubFundGroupType" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="SubFundGroupTypeMaintenanceDocument-EditSub-FundGroupType" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>subFundGroupTypeCode</value>
      </list>
    </property>
    <property name="label" value="Sub-Fund Group Type" />
    <property name="shortLabel" value="Sub-Fund Group Type" />
    <property name="summary" value="Sub-Fund Group Type Maintenance Document" />
    <property name="description" value="Document used to create or update Sub Fund Group Type objects" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-CA"
            p:parameterName="SUB_FUND_GROUP_TYPE"
            p:parameterDetailType="SubFundGroupType" />
    </property>
    <property name="documentTypeName" value="SubFundGroupTypeMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="SFGT" />
    <property name="workflowProperties" >
      <ref bean="SubFundGroupTypeMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>SY_MAINTAINENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="SubFundGroupTypeMaintenanceDocument-EditSub-FundGroupType" parent="SubFundGroupTypeMaintenanceDocument-EditSub-FundGroupType-parentBean" />

  <bean id="SubFundGroupTypeMaintenanceDocument-EditSub-FundGroupType-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Sub-Fund Group Type" />
    <property name="title" value="Edit Sub-Fund Group Type" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="subFundGroupTypeCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="subFundGroupTypeDescription"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="subFundGroupTypeActiveIndicator" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="SubFundGroupTypeMaintenanceDocument-workflowProperties" parent="SubFundGroupTypeMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="SubFundGroupTypeMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
