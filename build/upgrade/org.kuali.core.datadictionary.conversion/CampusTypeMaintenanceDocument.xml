<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "https://test.kuali.org/dtds/dataDictionary-1_0.dtd">

<dictionaryEntry>
    <maintenanceDocument>
        <businessObjectClass>org.kuali.core.bo.CampusType</businessObjectClass>
        <maintainableClass>org.kuali.core.maintenance.KualiMaintainableImpl</maintainableClass>

        <documentAuthorizerClass>org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase</documentAuthorizerClass>
        <authorizations>
            <authorization action="initiate">
                <workgroups>
                    <workgroup>KUALI_ROLE_MAINTENANCE</workgroup>
                </workgroups>
            </authorization>
        </authorizations>

        <documentTypeName>KualiCampusTypeMaintenanceDocument</documentTypeName>
        <documentTypeCode>CTYP</documentTypeCode>
        <label>Campus Type</label>
        <summary>Campus Type Maintenance Document</summary>
        <description>Maintenance document which allows you to create new Campus Types, or modify existing Campus Types.</description>
        <help parameterNamespace="KR-NS" parameterDetailType="CampusType" parameterName="CAMPUS_TYPE_CODE" />

        <lockingKeys>
            <field attributeName="campusTypeCode" />
        </lockingKeys>

        <maintainableSections>
            <maintainableSection title="Edit Campus Type">
                <maintainableItems>
                    <maintainableField name="campusTypeCode" required="true" />
                    <maintainableField name="campusTypeName" required="true" />
                    <maintainableField name="dataObjectMaintenanceCodeActiveIndicator" />
                    <maintainableField name="versionNumber" />
                </maintainableItems>
            </maintainableSection>
        </maintainableSections>
    </maintenanceDocument>
</dictionaryEntry>
