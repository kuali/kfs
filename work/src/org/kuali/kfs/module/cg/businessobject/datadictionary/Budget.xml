<!-- Business Object Inquiry Definition -->


  <bean id="Budget-inquiryDefinition" parent="Budget-inquiryDefinition-parentBean" />

  <bean id="Budget-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Budget Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="documentNumber" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="Budget-lookupDefinition" parent="Budget-lookupDefinition-parentBean" />

  <bean id="Budget-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Budget Lookup" />
    <property name="instructions" value="Look up a Budget" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>documentNumber</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="budgetName" />
        <bean parent="FieldDefinition"
              p:attributeName="budgetProjectDirectorUniversalIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="universalUser.personUserIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="universalUser.personName" />
        <bean parent="FieldDefinition"
              p:attributeName="budgetAgency.fullName" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="budgetName" />
        <bean parent="FieldDefinition"
              p:attributeName="budgetParentTrackNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="electronicResearchAdministrationGrantNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="budgetTypeCodeText" />
        <bean parent="FieldDefinition"
              p:attributeName="projectDirector.universalUser.personUserIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="projectDirector.universalUser.personName"
              p:forceInquiry="true" />
        <bean parent="FieldDefinition"
              p:attributeName="budgetAgency.agencyNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="budgetAgency.fullName" />
      </list>
    </property>
  </bean>
</beans>
