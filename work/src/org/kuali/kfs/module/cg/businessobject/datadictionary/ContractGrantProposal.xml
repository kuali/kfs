<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="ContractGrantProposal" parent="ContractGrantProposal-parentBean" />

  <bean id="ContractGrantProposal-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.cg.businessobject.ContractGrantProposal" />
    <property name="inquiryDefinition" >
      <ref bean="ContractGrantProposal-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="ContractGrantProposal-lookupDefinition" />
    </property>
    <property name="objectLabel" value="Contract Grant Proposal" />
    <property name="attributes" >
      <list>
        <ref bean="ContractGrantProposal-proposalCreateDate" />
        <ref bean="ContractGrantProposal-proposalNumber" />
        <ref bean="ContractGrantProposal-documentNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="ContractGrantProposal-proposalCreateDate" parent="ContractGrantProposal-proposalCreateDate-parentBean" />

  <bean id="ContractGrantProposal-proposalCreateDate-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="proposalCreateDate" />
    <property name="label" value="Proposal Create Date" />
    <property name="shortLabel" value="Date" />
    <property name="maxLength" value="7" />
    <property name="validationPattern" >
      <bean parent="DateValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="9" />
    </property>
    
    
  </bean>
  <bean id="ContractGrantProposal-proposalNumber" parent="ContractGrantProposal-proposalNumber-parentBean" />

  <bean id="ContractGrantProposal-proposalNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="proposalNumber" />
    <property name="label" value="Institution Proposal No." />
    <property name="shortLabel" value="Proposal No." />
    <property name="maxLength" value="12" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="14" />
    </property>
    
    
  </bean>
  <bean id="ContractGrantProposal-documentNumber" parent="ContractGrantProposal-documentNumber-parentBean" />

  <bean id="ContractGrantProposal-documentNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="documentNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Document Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="14" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="16" />
    </property>
    
    
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="ContractGrantProposal-inquiryDefinition" parent="ContractGrantProposal-inquiryDefinition-parentBean" />

  <bean id="ContractGrantProposal-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Contract Grant Proposal Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="proposalCreateDate" />
              <bean parent="FieldDefinition"
                    p:attributeName="proposalNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="documentNumber" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="ContractGrantProposal-lookupDefinition" parent="ContractGrantProposal-lookupDefinition-parentBean" />

  <bean id="ContractGrantProposal-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Contract Grant Proposal Lookup" />
    <property name="instructions" value="Look up a Contract Grant Proposal" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="proposalCreateDate" />
        <bean parent="FieldDefinition"
              p:attributeName="proposalNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="proposalCreateDate" />
        <bean parent="FieldDefinition"
              p:attributeName="proposalNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
      </list>
    </property>
  </bean>
</beans>
