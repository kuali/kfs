<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="NonInvoiced" parent="NonInvoiced-parentBean" />

  <bean id="NonInvoiced-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.ar.bo.NonInvoiced" />
    <property name="inquiryDefinition" >
      <ref bean="NonInvoiced-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="NonInvoiced-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="NonInvoiced-accountNumber" />
        <ref bean="NonInvoiced-chartOfAccountsCode" />
        <ref bean="NonInvoiced-documentNumber" />
        <ref bean="NonInvoiced-financialDocumentLineAmount" />
        <ref bean="NonInvoiced-financialDocumentLineNumber" />
        <ref bean="NonInvoiced-financialDocumentOverrideCode" />
        <ref bean="NonInvoiced-financialDocumentPostingYear" />
        <ref bean="NonInvoiced-financialObjectCode" />
        <ref bean="NonInvoiced-financialSubObjectCode" />
        <ref bean="NonInvoiced-projectCode" />
        <ref bean="NonInvoiced-subAccountNumber" />
        <ref bean="NonInvoiced-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="NonInvoiced-accountNumber" parent="NonInvoiced-accountNumber-parentBean" />

  <bean id="NonInvoiced-accountNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="accountNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Account Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="7" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="9" />
    </property>
    <property name="summary" value="accountNumber" />
    <property name="description" value="accountNumber description..." />
  </bean>
  <bean id="NonInvoiced-chartOfAccountsCode" parent="NonInvoiced-chartOfAccountsCode-parentBean" />

  <bean id="NonInvoiced-chartOfAccountsCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="chartOfAccountsCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Chart Of Accounts Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    <property name="summary" value="chartOfAccountsCode" />
    <property name="description" value="chartOfAccountsCode description..." />
  </bean>
  <bean id="NonInvoiced-documentNumber" parent="NonInvoiced-documentNumber-parentBean" />

  <bean id="NonInvoiced-documentNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="documentNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Document Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="14" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="16" />
    </property>
    <property name="summary" value="documentNumber" />
    <property name="description" value="documentNumber description..." />
  </bean>
  <bean id="NonInvoiced-financialDocumentLineAmount" parent="NonInvoiced-financialDocumentLineAmount-parentBean" />

  <bean id="NonInvoiced-financialDocumentLineAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialDocumentLineAmount" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Financial Document Line Amount" />
    <property name="shortLabel" value="Amount" />
    <property name="maxLength" value="19" />
    <property name="validationPattern" >
      <bean parent="FloatingPointValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="21" />
    </property>
    <property name="summary" value="financialDocumentLineAmount" />
    <property name="description" value="financialDocumentLineAmount description..." />
  </bean>
  <bean id="NonInvoiced-financialDocumentLineNumber" parent="NonInvoiced-financialDocumentLineNumber-parentBean" />

  <bean id="NonInvoiced-financialDocumentLineNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialDocumentLineNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Financial Document Line Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="7" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="9" />
    </property>
    <property name="summary" value="financialDocumentLineNumber" />
    <property name="description" value="financialDocumentLineNumber description..." />
  </bean>
  <bean id="NonInvoiced-financialDocumentOverrideCode" parent="NonInvoiced-financialDocumentOverrideCode-parentBean" />

  <bean id="NonInvoiced-financialDocumentOverrideCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialDocumentOverrideCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Financial Document Override Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="1" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="3" />
    </property>
    <property name="summary" value="financialDocumentOverrideCode" />
    <property name="description" value="financialDocumentOverrideCode description..." />
  </bean>
  <bean id="NonInvoiced-financialDocumentPostingYear" parent="NonInvoiced-financialDocumentPostingYear-parentBean" />

  <bean id="NonInvoiced-financialDocumentPostingYear-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialDocumentPostingYear" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Financial Document Posting Year" />
    <property name="shortLabel" value="Year" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="6" />
    </property>
    <property name="summary" value="financialDocumentPostingYear" />
    <property name="description" value="financialDocumentPostingYear description..." />
  </bean>
  <bean id="NonInvoiced-financialObjectCode" parent="NonInvoiced-financialObjectCode-parentBean" />

  <bean id="NonInvoiced-financialObjectCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialObjectCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Financial Object Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="6" />
    </property>
    <property name="summary" value="financialObjectCode" />
    <property name="description" value="financialObjectCode description..." />
  </bean>
  <bean id="NonInvoiced-financialSubObjectCode" parent="NonInvoiced-financialSubObjectCode-parentBean" />

  <bean id="NonInvoiced-financialSubObjectCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialSubObjectCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Financial Sub Object Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="3" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="5" />
    </property>
    <property name="summary" value="financialSubObjectCode" />
    <property name="description" value="financialSubObjectCode description..." />
  </bean>
  <bean id="NonInvoiced-projectCode" parent="NonInvoiced-projectCode-parentBean" />

  <bean id="NonInvoiced-projectCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="projectCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Project Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
    <property name="summary" value="projectCode" />
    <property name="description" value="projectCode description..." />
  </bean>
  <bean id="NonInvoiced-subAccountNumber" parent="NonInvoiced-subAccountNumber-parentBean" />

  <bean id="NonInvoiced-subAccountNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="subAccountNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Sub Account Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="5" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="7" />
    </property>
    <property name="summary" value="subAccountNumber" />
    <property name="description" value="subAccountNumber description..." />
  </bean>
  <bean id="NonInvoiced-versionNumber" parent="NonInvoiced-versionNumber-parentBean" />

  <bean id="NonInvoiced-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="NonInvoiced-inquiryDefinition" parent="NonInvoiced-inquiryDefinition-parentBean" />

  <bean id="NonInvoiced-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Non-Invoiced Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="accountNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="chartOfAccountsCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="documentNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialDocumentLineAmount" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialDocumentLineNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialDocumentOverrideCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialDocumentPostingYear" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialObjectCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialSubObjectCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="projectCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="subAccountNumber" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="NonInvoiced-lookupDefinition" parent="NonInvoiced-lookupDefinition-parentBean" />

  <bean id="NonInvoiced-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Non-Invoiced Lookup" />
    <property name="instructions" value="Look up a Non-Invoiced" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="accountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentLineAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentLineNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentOverrideCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentPostingYear" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialSubObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="projectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="subAccountNumber" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="accountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentLineAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentLineNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentOverrideCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentPostingYear" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialSubObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="projectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="subAccountNumber" />
      </list>
    </property>
  </bean>
</beans>
