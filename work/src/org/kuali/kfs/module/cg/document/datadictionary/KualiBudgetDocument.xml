        	<headerNavigationTab>
        		<navigateTo>personnel</navigateTo>
        		<displayName>Personnel</displayName>
        		<pageHelp parameterNamespace="KFS-RA" parameterDetailType="Budget" parameterName="BUDGET_PERSONNEL_PAGE"/>
