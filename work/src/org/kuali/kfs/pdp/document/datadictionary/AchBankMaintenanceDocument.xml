<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../../core/datadictionary/dataDictionary-1_0.dtd">

<dictionaryEntry>
    <maintenanceDocument>
        <businessObjectClass>org.kuali.module.pdp.bo.AchBank</businessObjectClass>
        <maintainableClass>org.kuali.core.maintenance.KualiMaintainableImpl</maintainableClass>
        <businessRulesClass>org.kuali.module.pdp.rules.AchBankRule</businessRulesClass>

        <documentAuthorizerClass>org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase</documentAuthorizerClass>
        <authorizations>
            <authorization action="initiate">
                <workgroups>
                    <workgroup>PD_VIEW_ACH</workgroup>
                </workgroups>
            </authorization>
        </authorizations>

        <documentTypeName>AchBankMaintenanceDocument</documentTypeName>
        <documentTypeCode>ABNK</documentTypeCode>

        <label>ACH Bank</label>
        <shortLabel>AchBankMaintDoc</shortLabel>

        <summary>Ach bank maintenance document</summary>
        <description>Document used to create or update Ach Bank objects</description>
        <help parameterNamespace="KFS-PD" parameterDetailType="AchBank" parameterName="BANK" />

        <lockingKeys>
            <field attributeName="bankRoutingNumber" />
        </lockingKeys>
        
        <defaultExistenceChecks>
            <reference attributeName="bankState" attributeToHighlightOnFail="bankStateCode" />        
        </defaultExistenceChecks>

        <maintainableSections>
            <maintainableSection title="Edit ACH Bank">
                <maintainableItems>
                    <maintainableField name="bankRoutingNumber" required="true" />
                    <maintainableField name="bankOfficeCode" required="true" />
                    <maintainableField name="bankServiceNumber" required="true" />
                    <maintainableField name="bankTypeCode" required="true" />
               		<maintainableField name="bankNewRoutingNumber" required="true" />
                	<maintainableField name="bankName" required="true" />
                	<maintainableField name="bankStreetAddress" required="true" />
                	<maintainableField name="bankCityName" required="true" />
                	<maintainableField name="bankStateCode" required="true" />
                	<maintainableField name="bankZipCode" required="true" />
 					<maintainableField name="bankZipExtensionCode" required="true" />
 					<maintainableField name="bankPhoneAreaCode" required="true" />
 					<maintainableField name="bankPhonePrefixNumber" required="true" />
 					<maintainableField name="bankPhoneSuffixNumber" required="true" />
 					<maintainableField name="bankInstitutionStatusCode" required="false" />
 					<maintainableField name="bankDataViewCode" required="false" />
                    <maintainableField name="versionNumber" />
                </maintainableItems>
            </maintainableSection>
        </maintainableSections>
        
        <workflowProperties/>
    </maintenanceDocument>
</dictionaryEntry>
