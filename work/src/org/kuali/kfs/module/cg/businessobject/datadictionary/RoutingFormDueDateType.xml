<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.kra.routingform.bo.RoutingFormDueDateType">


        <inquiry>
            <title>Routing Form Due Date Type Inquiry</title>
             
            <inquirySections>
                <inquirySection title="Header Text">
                    <inquiryFields>
                        <field attributeName="documentNumber" />
                        <field attributeName="dueDateTypeCode" />
                    </inquiryFields>
                </inquirySection>
            </inquirySections>  
        </inquiry>

        <lookup>
            <title>Routing Form Due Date Type Lookup</title>

            <instructions>Lookup a Routing Form Due Date Type</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="dueDateTypeCode" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="documentNumber" required="false" />
                <lookupField attributeName="dueDateTypeCode" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="documentNumber" />
                <field attributeName="dueDateTypeCode" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="documentNumber" forceUppercase="true">
                <label>Document Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>14</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="16" />
                </control>

                <summary>documentNumber</summary>
                <description>documentNumber description...</description>
			</attribute>

            <attribute name="dueDateTypeCode" forceUppercase="true">
                <label>Due Date Type Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="4" />
                </control>

                <summary>dueDateTypeCode</summary>
                <description>dueDateTypeCode description...</description>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
