<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../core/datadictionary/dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.kra.budget.bo.BudgetUser">

        <customAttributeEnabled>true</customAttributeEnabled>

        <inquiry>
            <title>Personnel Inquiry</title>

            <inquiryFields>
                <field attributeName="user.personUniversalIdentifier" />
                <field attributeName="user.personUserIdentifier" />
                <field attributeName="user.personName" />
                <field attributeName="user.deptid" />
                <field attributeName="user.employeeStatusCode" />
                <field attributeName="user.employeeTypeCode" />
            </inquiryFields>
        </inquiry>



        <attributes>

            <attribute name="documentHeaderId" forceUppercase="false">
                <label>Document Header Id</label>
                <shortLabel>Document Id</shortLabel>

                <maxLength>14</maxLength>

                <control>
                    <text size="16" />
                </control>

                <summary>The document's displayable identifier.</summary>
                <description>The document id is generated by the workflow environment and is unique to each installation of Kuali...</description>
            </attribute>

            <attribute name="budgetUserSequenceNumber" forceUppercase="false">
                <label>Budget Period Sequence Number</label>
                <shortLabel>Budget Pd. Seq. No.</shortLabel>

                <maxLength>2</maxLength>

                <control>
                    <text size="4" />
                </control>

                <summary>The user sequence number.</summary>
                <description>Sequence number of the user within a budget.</description>
            </attribute>

            <attribute name="fiscalCampusCode" forceUppercase="false">
                <label>Fiscal Campus Code</label>
                <shortLabel>Fiscal Campus Code</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern>
                    <anyCharacter />
                </validationPattern>

                <control>
                    <text size="2" />
                </control>

                <summary>Fiscal Campus Code</summary>
                <description>Fiscal Campus Code</description>
            </attribute>

            <attribute name="primaryDepartmentCode" forceUppercase="false">
                <label>Primary Department Code</label>
                <shortLabel>Pri. Dept. Code</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern>
                    <anyCharacter />
                </validationPattern>

                <control>
                    <text size="4" />
                </control>

                <summary>Primary Department Code</summary>
                <description>Primary Department Code</description>
            </attribute>

            <attribute name="personNamePrefixText" forceUppercase="false">
                <label>Prefix</label>
                <shortLabel>Pre</shortLabel>

                <maxLength>15</maxLength>
                <validationPattern>
                    <anyCharacter />
                </validationPattern>

                <control>
                    <text size="15" />
                </control>

                <summary>Prefix</summary>
                <description>Prefix</description>
            </attribute>

            <attribute name="personNameSuffixText" forceUppercase="false">
                <label>Suffix</label>
                <shortLabel>Suff</shortLabel>

                <maxLength>15</maxLength>
                <validationPattern>
                    <anyCharacter />
                </validationPattern>

                <control>
                    <text size="15" />
                </control>

                <summary>Suffix</summary>
                <description>Suffix</description>
            </attribute>

            <attribute name="personSeniorKeyIndicator" forceUppercase="false">
                <label>Senior Key Personnel</label>
                <shortLabel>Sr. Person</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <checkbox />
                </control>

                <summary>Senior Key Personnel</summary>
                <description>Senior Key Personnel</description>
                <formatterClass>org.kuali.core.web.format.BooleanFormatter</formatterClass>

            </attribute>

            <attribute name="personProjectDirectorIndicator" forceUppercase="false">
                <label>Project Directory</label>
                <shortLabel>PD</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <checkbox />
                </control>

                <summary>Project Directory</summary>
                <description>Project Directory</description>
                <formatterClass>org.kuali.core.web.format.BooleanFormatter</formatterClass>
            </attribute>

            <attribute name="personSecretarialClericalIndicator" forceUppercase="false">
                <label>Secretarial/Clerical</label>
                <shortLabel>Sec/Cl</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <checkbox />
                </control>

                <summary>Secretarial/Clerical</summary>
                <description>Secretarial/Clerical</description>
                <formatterClass>org.kuali.core.web.format.BooleanFormatter</formatterClass>

            </attribute>

            <attribute name="personPostDoctoralIndicator" forceUppercase="false">
                <label>Post Doctoral</label>
                <shortLabel>Post Doc</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <checkbox />
                </control>

                <summary>Post Doctoral</summary>
                <description>Post Doctoral</description>
                <formatterClass>org.kuali.core.web.format.BooleanFormatter</formatterClass>

            </attribute>

            <attribute name="objectId" forceUppercase="true">
                <label>Object Id</label>
                <shortLabel>GUID</shortLabel>

                <maxLength>36</maxLength>

                <validationPattern>
                    <anyCharacter />
                </validationPattern>

                <control>
                    <text size="36" />
                </control>

                <summary>objectId summary...</summary>
                <description>objectId description...</description>
            </attribute>

            <attribute name="versionNumber" forceUppercase="true">
                <label>Version Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>10</maxLength>

                <validationPattern>
                    <numeric />
                </validationPattern>

                <control>
                    <text size="-1" />
                </control>

                <summary>versionNumber summary...</summary>
                <description>versionNumber description...</description>
            </attribute>


            <attributeReference name="baseSalary" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericAmount" />

            <attribute name="role" forceUppercase="false">
                <label>Role</label>
                <shortLabel>Role</shortLabel>

                <maxLength>20</maxLength>

                <control>
                    <text size="20" />
                </control>

                <summary>Role</summary>
                <description>Role</description>
            </attribute>

            <attribute name="personSalaryJustificationText">
                <label>Justification for rate change</label>
                <shortLabel>Justification</shortLabel>

                <maxLength>250</maxLength>

                <required>false</required>

                <control>
                    <textarea rows="3" cols="30" />
                </control>

                <summary>Justification for Rate Change</summary>
                <description>A place to describe why a change was made to the base salary of the Personnel record.</description>
            </attribute>

            <attribute name="appointmentTypeCode" forceUppercase="false">
                <!-- This attribute does not correspond to a field in the database.
                    It exists solely to provide a drop down box in the user interface.
                    We regretfully acknowledge the violation of the tenets of MVC 
                    architecture.  This is a case of expediency trumping proper design.
                -->
                <label>Appointment Type Code</label>
                <shortLabel>Appt Type Code</shortLabel>

                <maxLength>10</maxLength>

                <control>
                    <select valuesFinderClass="org.kuali.module.kra.budget.lookup.keyvalues.BudgetAppointmentTypeValuesFinder" />
                </control>

                <summary>Appointment Type Code</summary>
                <description>Appointment Type Code</description>
            </attribute>

            <attribute name="personHourlyNumber" forceUppercase="false">
                <label>Number of Hourlies</label>
                <shortLabel># Hourlies</shortLabel>

                <maxLength>2</maxLength>

                <control>
                    <text size="4" />
                </control>

                <summary>Number of Hourlies</summary>
                <description>The total number of hourly employees that this record represents.</description>
            </attribute>

            <attributeReference name="user.personUniversalIdentifier" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericSystemId">
                <label>University ID</label>
                <shortLabel>Univ ID</shortLabel>
                <summary>University ID</summary>
                <description>University ID</description>
            </attributeReference>

            <attributeReference name="user.personUserIdentifier" sourceClassName="org.kuali.core.bo.user.UniversalUser" sourceAttributeName="personUserIdentifier">
                <label>User ID</label>
                <shortLabel>User ID</shortLabel>
                <summary>User ID</summary>
                <description>User ID</description>
            </attributeReference>

            <attributeReference name="user.personName" sourceClassName="org.kuali.core.bo.user.UniversalUser" sourceAttributeName="personName">
                <label>Project Director Name</label>
                <shortLabel>Project Director Name</shortLabel>
                <summary>Project Director Name</summary>
                <description>Project Director Name</description>
            </attributeReference>

            <attributeReference name="user.deptid" sourceClassName="org.kuali.core.bo.user.UniversalUser" sourceAttributeName="deptid">
                <label>Dept ID</label>
                <shortLabel>Dept ID</shortLabel>
                <summary>Dept ID</summary>
                <description>Dept ID</description>
            </attributeReference>

            <attributeReference name="user.employeeStatusCode" sourceClassName="org.kuali.core.bo.user.UniversalUser" sourceAttributeName="employeeStatusCode">
                <label>Employee Status</label>
                <shortLabel>Employee Status</shortLabel>
                <summary>Employee Statuse</summary>
                <description>Employee Status</description>
            </attributeReference>

            <attributeReference name="user.employeeTypeCode" sourceClassName="org.kuali.core.bo.user.UniversalUser" sourceAttributeName="employeeTypeCode">
                <label>Employee Type</label>
                <shortLabel>Employee Typee</shortLabel>
                <summary>Employee Type</summary>
                <description>Employee Type</description>
            </attributeReference>
        </attributes>
    </businessObject>

</dictionaryEntry>