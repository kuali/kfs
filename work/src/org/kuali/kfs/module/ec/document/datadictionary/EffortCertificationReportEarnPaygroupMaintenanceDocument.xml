<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="EffortCertificationReportEarnPaygroupMaintenanceDocument" parent="EffortCertificationReportEarnPaygroupMaintenanceDocument-parentBean" />

  <bean id="EffortCertificationReportEarnPaygroupMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.effort.bo.EffortCertificationReportEarnPaygroup" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="EffortCertificationReportEarnPaygroupMaintenanceDocument-EditReportEarnPaygroup" />
      </list>
    </property>
    <property name="defaultExistenceChecks" >
      <list>
        <bean parent="ReferenceDefinition"
              p:attributeName="options"
              p:attributeToHighlightOnFail="universityFiscalYear" />
        <bean parent="ReferenceDefinition"
              p:attributeName="effortCertificationReportType"
              p:activeIndicatorAttributeName="active"
              p:attributeToHighlightOnFail="effortCertificationReportTypeCode" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>universityFiscalYear</value>
        <value>effortCertificationReportTypeCode</value>
        <value>earnCode</value>
        <value>payGroup</value>
      </list>
    </property>
    <property name="label" value="Report Earn Paygroup Document" />
    <property name="shortLabel" value="Effort Certification Report Earn Pay Document" />
    <property name="summary" value="Report Earn Pay Document" />
    <property name="description" value="Document used to create or update Effort Certification Report Earn Pay Document" />
    <property name="documentTypeName" value="EffortCertificationReportEarnPaygroupMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="ARPG" />
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>LD_EFFORT_ADMINISTRATORS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="EffortCertificationReportEarnPaygroupMaintenanceDocument-EditReportEarnPaygroup" parent="EffortCertificationReportEarnPaygroupMaintenanceDocument-EditReportEarnPaygroup-parentBean" />

  <bean id="EffortCertificationReportEarnPaygroupMaintenanceDocument-EditReportEarnPaygroup-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Report Earn Paygroup" />
    <property name="title" value="Edit Report Earn Paygroup" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="universityFiscalYear"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="effortCertificationReportTypeCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="earnCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="payGroup"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="active"
              p:defaultValue="true"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>
</beans>
