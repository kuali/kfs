<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../../core/datadictionary/dataDictionary-1_0.dtd">

<dictionaryEntry>
    <maintenanceDocument>
        <businessObjectClass>org.kuali.module.chart.bo.HigherEdFunction</businessObjectClass>
        <maintainableClass>org.kuali.core.maintenance.KualiMaintainableImpl</maintainableClass>

        <documentAuthorizerClass>org.kuali.core.document.MaintenanceDocumentAuthorizerBase</documentAuthorizerClass>
        <authorizations>
            <authorization action="initiate">
                <workgroups>
                    <workgroup>KUALI_ROLE_MAINTENANCE</workgroup>
                </workgroups>
            </authorization>
        </authorizations>

        <documentTypeName>KualiHigherEdFunctionMaintenanceDocument</documentTypeName>
        <documentTypeCode>HEFN</documentTypeCode>

        <label>Higher Education Function Maintenance Document</label>
        <shortLabel>HigherEdFunctionMaintDoc</shortLabel>

        <summary>Higher Education Function maintenance document</summary>
        <description>Document used to create or update Higher Education Function objects</description>
        <help securityGroupName="ChartMaintenanceEDoc" parameterName="CHART_HELP_HIGHER_EDUCATION_FUNCTION" />
        
        <lockingKeys>
            <field attributeName="financialHigherEdFunctionCd" />
        </lockingKeys>

        <defaultExistenceChecks>
            <reference attributeName="universityBudgetOfficeFunction" attributeToHighlightOnFail="finUnivBdgtOfficeFunctionCd" />
            <reference attributeName="aicpaFunction" attributeToHighlightOnFail="finAicpaFunctionCode" />
            <reference attributeName="federalFunction" attributeToHighlightOnFail="financialFederalFunctionCode" />
        </defaultExistenceChecks>

        <maintainableSections>
            <maintainableSection title="Edit Higher Education Function">
                <maintainableItems>
                    <maintainableField name="financialHigherEdFunctionCd" required="true" />
                    <maintainableField name="financialHigherEdFunctionNm" required="true" />
                    <maintainableField name="finUnivBdgtOfficeFunctionCd" required="true" />
                    <maintainableField name="finAicpaFunctionCode" required="true" />
                    <maintainableField name="financialFederalFunctionCode" required="true" />
                    <maintainableField name="versionNumber" />
                </maintainableItems>
            </maintainableSection>
        </maintainableSections>
    </maintenanceDocument>
</dictionaryEntry>