<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="BudgetConstructionObjectDump" parent="BudgetConstructionObjectDump-parentBean"/>

  <bean id="BudgetConstructionObjectDump-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.bc.businessobject.BudgetConstructionObjectDump"/>
    <property name="inquiryDefinition">
      <ref bean="BudgetConstructionObjectDump-inquiryDefinition"/>
    </property>
    <property name="lookupDefinition">
      <ref bean="BudgetConstructionObjectDump-lookupDefinition"/>
    </property>
    <property name="objectLabel" value="Budget Construction Object Dump"/>
    <property name="attributes">
      <list>
        <ref bean="BudgetConstructionObjectDump-accountNumber"/>
        <ref bean="BudgetConstructionObjectDump-chartOfAccountsCode"/>
        <ref bean="BudgetConstructionObjectDump-financialObjectCode"/>
        <ref bean="BudgetConstructionObjectDump-organizationChartOfAccountsCode"/>
        <ref bean="BudgetConstructionObjectDump-organizationCode"/>
        <ref bean="BudgetConstructionObjectDump-principalId"/>
        <ref bean="BudgetConstructionObjectDump-subAccountNumber"/>
        <ref bean="BudgetConstructionObjectDump-subFundGroupCode"/>
        <ref bean="BudgetConstructionObjectDump-universityFiscalYear"/>
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="BudgetConstructionObjectDump-accountNumber" parent="BudgetConstructionObjectDump-accountNumber-parentBean"/>

  <bean id="BudgetConstructionObjectDump-accountNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="accountNumber"/>
    <property name="forceUppercase" value="true"/>
    <property name="label" value="Account Number"/>
    <property name="shortLabel" value="Number"/>
    <property name="maxLength" value="7"/>
    <property name="validationPattern">
      <bean parent="AnyCharacterValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="9"/>
    </property>
    
    
  </bean>
  <bean id="BudgetConstructionObjectDump-chartOfAccountsCode" parent="BudgetConstructionObjectDump-chartOfAccountsCode-parentBean"/>

  <bean id="BudgetConstructionObjectDump-chartOfAccountsCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="chartOfAccountsCode"/>
    <property name="forceUppercase" value="true"/>
    <property name="label" value="Chart Of Accounts Code"/>
    <property name="shortLabel" value="Code"/>
    <property name="maxLength" value="2"/>
    <property name="validationPattern">
      <bean parent="AnyCharacterValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="4"/>
    </property>
    
    
  </bean>
  <bean id="BudgetConstructionObjectDump-financialObjectCode" parent="BudgetConstructionObjectDump-financialObjectCode-parentBean"/>

  <bean id="BudgetConstructionObjectDump-financialObjectCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialObjectCode"/>
    <property name="forceUppercase" value="true"/>
    <property name="label" value="Financial Object Code"/>
    <property name="shortLabel" value="Code"/>
    <property name="maxLength" value="4"/>
    <property name="validationPattern">
      <bean parent="AnyCharacterValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="6"/>
    </property>
    
    
  </bean>
  <bean id="BudgetConstructionObjectDump-organizationChartOfAccountsCode" parent="BudgetConstructionObjectDump-organizationChartOfAccountsCode-parentBean"/>

  <bean id="BudgetConstructionObjectDump-organizationChartOfAccountsCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="organizationChartOfAccountsCode"/>
    <property name="forceUppercase" value="true"/>
    <property name="label" value="Organization Chart Of Accounts Code"/>
    <property name="shortLabel" value="Code"/>
    <property name="maxLength" value="2"/>
    <property name="validationPattern">
      <bean parent="AnyCharacterValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="4"/>
    </property>
    
    
  </bean>
  <bean id="BudgetConstructionObjectDump-organizationCode" parent="BudgetConstructionObjectDump-organizationCode-parentBean"/>

  <bean id="BudgetConstructionObjectDump-organizationCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="organizationCode"/>
    <property name="forceUppercase" value="true"/>
    <property name="label" value="Organization Code"/>
    <property name="shortLabel" value="Code"/>
    <property name="maxLength" value="4"/>
    <property name="validationPattern">
      <bean parent="AnyCharacterValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="6"/>
    </property>
    
    
  </bean>
  <bean id="BudgetConstructionObjectDump-principalId" parent="BudgetConstructionObjectDump-principalId-parentBean"/>

  <bean id="BudgetConstructionObjectDump-principalId-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="principalId"/>
    <property name="forceUppercase" value="true"/>
    <property name="label" value="Person Universal Identifier"/>
    <property name="shortLabel" value="Identifier"/>
    <property name="maxLength" value="10"/>
    <property name="validationPattern">
      <bean parent="NumericValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="12"/>
    </property>
    
    
  </bean>
  <bean id="BudgetConstructionObjectDump-subAccountNumber" parent="BudgetConstructionObjectDump-subAccountNumber-parentBean"/>

  <bean id="BudgetConstructionObjectDump-subAccountNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="subAccountNumber"/>
    <property name="forceUppercase" value="true"/>
    <property name="label" value="Sub Account Number"/>
    <property name="shortLabel" value="Number"/>
    <property name="maxLength" value="5"/>
    <property name="validationPattern">
      <bean parent="AnyCharacterValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="7"/>
    </property>
    
    
  </bean>
  <bean id="BudgetConstructionObjectDump-subFundGroupCode" parent="BudgetConstructionObjectDump-subFundGroupCode-parentBean"/>

  <bean id="BudgetConstructionObjectDump-subFundGroupCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="subFundGroupCode"/>
    <property name="forceUppercase" value="true"/>
    <property name="label" value="Sub Fund Group Code"/>
    <property name="shortLabel" value="Code"/>
    <property name="maxLength" value="6"/>
    <property name="validationPattern">
      <bean parent="AnyCharacterValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="8"/>
    </property>
    
    
  </bean>
  <bean id="BudgetConstructionObjectDump-universityFiscalYear" parent="BudgetConstructionObjectDump-universityFiscalYear-parentBean"/>

  <bean id="BudgetConstructionObjectDump-universityFiscalYear-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="universityFiscalYear"/>
    <property name="forceUppercase" value="true"/>
    <property name="label" value="University Fiscal Year"/>
    <property name="shortLabel" value="Year"/>
    <property name="maxLength" value="4"/>
    <property name="validationPattern">
      <bean parent="NumericValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="6"/>
    </property>
    
    
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="BudgetConstructionObjectDump-inquiryDefinition" parent="BudgetConstructionObjectDump-inquiryDefinition-parentBean"/>

  <bean id="BudgetConstructionObjectDump-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Budget Construction Object Dump Inquiry"/>
    <property name="inquirySections">
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value=""/>
          <property name="numberOfColumns" value="1"/>
          <property name="inquiryFields">
            <list>
              <bean parent="FieldDefinition" p:attributeName="accountNumber"/>
              <bean parent="FieldDefinition" p:attributeName="chartOfAccountsCode"/>
              <bean parent="FieldDefinition" p:attributeName="financialObjectCode"/>
              <bean parent="FieldDefinition" p:attributeName="organizationChartOfAccountsCode"/>
              <bean parent="FieldDefinition" p:attributeName="organizationCode"/>
              <bean parent="FieldDefinition" p:attributeName="principalId"/>
              <bean parent="FieldDefinition" p:attributeName="subAccountNumber"/>
              <bean parent="FieldDefinition" p:attributeName="subFundGroupCode"/>
              <bean parent="FieldDefinition" p:attributeName="universityFiscalYear"/>
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="BudgetConstructionObjectDump-lookupDefinition" parent="BudgetConstructionObjectDump-lookupDefinition-parentBean"/>

  <bean id="BudgetConstructionObjectDump-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Budget Construction Object Dump Lookup"/>
    
    <property name="defaultSort">
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="accountNumber"/>
        <bean parent="FieldDefinition" p:attributeName="chartOfAccountsCode"/>
        <bean parent="FieldDefinition" p:attributeName="financialObjectCode"/>
        <bean parent="FieldDefinition" p:attributeName="organizationChartOfAccountsCode"/>
        <bean parent="FieldDefinition" p:attributeName="organizationCode"/>
        <bean parent="FieldDefinition" p:attributeName="principalId"/>
        <bean parent="FieldDefinition" p:attributeName="subAccountNumber"/>
        <bean parent="FieldDefinition" p:attributeName="subFundGroupCode"/>
        <bean parent="FieldDefinition" p:attributeName="universityFiscalYear"/>
      </list>
    </property>
    <property name="resultFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="accountNumber"/>
        <bean parent="FieldDefinition" p:attributeName="chartOfAccountsCode"/>
        <bean parent="FieldDefinition" p:attributeName="financialObjectCode"/>
        <bean parent="FieldDefinition" p:attributeName="organizationChartOfAccountsCode"/>
        <bean parent="FieldDefinition" p:attributeName="organizationCode"/>
        <bean parent="FieldDefinition" p:attributeName="principalId"/>
        <bean parent="FieldDefinition" p:attributeName="subAccountNumber"/>
        <bean parent="FieldDefinition" p:attributeName="subFundGroupCode"/>
        <bean parent="FieldDefinition" p:attributeName="universityFiscalYear"/>
      </list>
    </property>
  </bean>
</beans>