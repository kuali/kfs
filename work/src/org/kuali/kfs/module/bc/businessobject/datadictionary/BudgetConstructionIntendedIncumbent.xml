<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.budget.bo.BudgetConstructionIntendedIncumbent">


        <inquiry>
            <title>Budget Construction Intended Incumbent Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="emplid" />
                <field attributeName="grade" />
                <field attributeName="iuClassificationLevel" />
                <field attributeName="personName" />
                <field attributeName="salaryAdministrationPlan" />
                <field attributeName="setidSalary" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Budget Construction Intended Incumbent Lookup</title>

            <instructions>Look up a Budget Construction Intended Incumbent</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="emplid" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="emplid" required="false" />
                <lookupField attributeName="personName" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="emplid" />
                <field attributeName="grade" />
                <field attributeName="iuClassificationLevel" />
                <field attributeName="personName" />
                <field attributeName="salaryAdministrationPlan" />
                <field attributeName="setidSalary" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="emplid" forceUppercase="true">
                <label>Employee Id</label>
                <shortLabel>Emplid</shortLabel>

                <maxLength>11</maxLength>
                <validationPattern><anyCharacter /></validationPattern>
                <required>true</required>

                <control>
                    <text size="13" />
                </control>

                <summary>Employee Id</summary>
                <description>Identifies employee</description>
			</attribute>

            <attribute name="grade" forceUppercase="true">
                <label>Salary Grade</label>
                <shortLabel>Sal Grade</shortLabel>

                <maxLength>3</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="5" />
                </control>

                <summary>Salary Grade</summary>
                <description>Salary Grade for the employee.</description>
			</attribute>

            <attribute name="iuClassificationLevel" forceUppercase="true">
                <label>Iu Classification Level</label>
                <shortLabel>Cls</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="4" />
                </control>

                <summary>Classification Level</summary>
                <description>Classification Level of the employee.</description>
			</attribute>

            <attribute name="personName" forceUppercase="true">
                <label>Person Name</label>
                <shortLabel>Name</shortLabel>

                <maxLength>50</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>

                <control>
                    <text size="52" />
                </control>

                <summary>Person Name</summary>
                <description>Last and first name of the employee.</description>
			</attribute>

            <attribute name="salaryAdministrationPlan" forceUppercase="true">
                <label>Salary Administration Plan</label>
                <shortLabel>Plan</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="6" />
                </control>

                <summary>Salary Administration Plan</summary>
                <description>Salary Administration Plan for the employee.</description>
			</attribute>

            <attribute name="setidSalary" forceUppercase="true">
                <label>Salary Setid</label>
                <shortLabel>Sal Setid</shortLabel>

                <maxLength>5</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="7" />
                </control>

                <summary>Salary Setid</summary>
                <description>Salary admin plan set identifier for the employee.</description>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>