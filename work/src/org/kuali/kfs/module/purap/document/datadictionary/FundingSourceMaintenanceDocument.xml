<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xmlns:dd="http://rice.kuali.org/dd" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd         http://rice.kuali.org/dd         http://rice.kuali.org/dd/dd.xsd">

  <bean id="FundingSourceMaintenanceDocument" parent="FundingSourceMaintenanceDocument-parentBean"/>

  <bean id="FundingSourceMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.purap.businessobject.FundingSource"/>
    <property name="businessRulesClass" value="org.kuali.kfs.module.purap.document.validation.impl.FundingSourceRule"/>
    <property name="maintainableClass" value="org.kuali.rice.kns.maintenance.KualiMaintainableImpl"/>
    <property name="maintainableSections">
      <list>
        <ref bean="FundingSourceMaintenanceDocument-EditFundingSource"/>
      </list>
    </property>
    <property name="lockingKeys">
      <list>
        <value>fundingSourceCode</value>
      </list>
    </property>
    
    
    
    <property name="documentTypeName" value="PMFS"/>
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase"/>
    <property name="workflowAttributes">
    	<ref bean="FundingSourceMaintenanceDocument-workflowAttributes"/>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->

  <bean id="FundingSourceMaintenanceDocument-EditFundingSource" parent="FundingSourceMaintenanceDocument-EditFundingSource-parentBean"/>

  <bean id="FundingSourceMaintenanceDocument-EditFundingSource-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Funding Source"/>
    <property name="title" value="Edit Funding Source"/>
    <property name="maintainableItems">
      <list>
        <bean parent="MaintainableFieldDefinition" p:name="fundingSourceCode" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="fundingSourceDescription" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="active" p:defaultValue="true"/>
      </list>
    </property>
  </bean>
  
  <!-- workflow attributes -->
  <bean id="FundingSourceMaintenanceDocument-workflowAttributes" parent="FundingSourceMaintenanceDocument-workflowAttributes-parentBean"/>
  
  <bean id="FundingSourceMaintenanceDocument-workflowAttributes-parentBean" class="org.kuali.rice.kns.datadictionary.WorkflowAttributes" abstract="true"/>
</beans>