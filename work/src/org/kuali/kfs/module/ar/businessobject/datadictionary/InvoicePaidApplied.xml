<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="InvoicePaidApplied" parent="InvoicePaidApplied-parentBean" />

  <bean id="InvoicePaidApplied-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.ar.bo.InvoicePaidApplied" />
    <property name="inquiryDefinition" >
      <ref bean="InvoicePaidApplied-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="InvoicePaidApplied-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="InvoicePaidApplied-documentNumber" />
        <ref bean="InvoicePaidApplied-financialDocumentReferenceInvoiceNumber" />
        <ref bean="InvoicePaidApplied-invoiceItemAppliedAmount" />
        <ref bean="InvoicePaidApplied-invoiceItemNumber" />
        <ref bean="InvoicePaidApplied-paidAppliedItemNumber" />
        <ref bean="InvoicePaidApplied-universityFiscalPeriodCode" />
        <ref bean="InvoicePaidApplied-universityFiscalYear" />
        <ref bean="InvoicePaidApplied-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="InvoicePaidApplied-documentNumber" parent="InvoicePaidApplied-documentNumber-parentBean" />

  <bean id="InvoicePaidApplied-documentNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="documentNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Document Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="14" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="16" />
    </property>
    <property name="summary" value="documentNumber" />
    <property name="description" value="documentNumber description..." />
  </bean>
  <bean id="InvoicePaidApplied-financialDocumentReferenceInvoiceNumber" parent="InvoicePaidApplied-financialDocumentReferenceInvoiceNumber-parentBean" />

  <bean id="InvoicePaidApplied-financialDocumentReferenceInvoiceNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialDocumentReferenceInvoiceNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Financial Document Reference Invoice Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="14" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="16" />
    </property>
    <property name="summary" value="financialDocumentReferenceInvoiceNumber" />
    <property name="description" value="financialDocumentReferenceInvoiceNumber description..." />
  </bean>
  <bean id="InvoicePaidApplied-invoiceItemAppliedAmount" parent="InvoicePaidApplied-invoiceItemAppliedAmount-parentBean" />

  <bean id="InvoicePaidApplied-invoiceItemAppliedAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="invoiceItemAppliedAmount" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Invoice Item Applied Amount" />
    <property name="shortLabel" value="Amount" />
    <property name="maxLength" value="19" />
    <property name="validationPattern" >
      <bean parent="FloatingPointValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="21" />
    </property>
    <property name="summary" value="invoiceItemAppliedAmount" />
    <property name="description" value="invoiceItemAppliedAmount description..." />
  </bean>
  <bean id="InvoicePaidApplied-invoiceItemNumber" parent="InvoicePaidApplied-invoiceItemNumber-parentBean" />

  <bean id="InvoicePaidApplied-invoiceItemNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="invoiceItemNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Invoice Item Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="7" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="9" />
    </property>
    <property name="summary" value="invoiceItemNumber" />
    <property name="description" value="invoiceItemNumber description..." />
  </bean>
  <bean id="InvoicePaidApplied-paidAppliedItemNumber" parent="InvoicePaidApplied-paidAppliedItemNumber-parentBean" />

  <bean id="InvoicePaidApplied-paidAppliedItemNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="paidAppliedItemNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Paid Applied Item Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="7" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="9" />
    </property>
    <property name="summary" value="paidAppliedItemNumber" />
    <property name="description" value="paidAppliedItemNumber description..." />
  </bean>
  <bean id="InvoicePaidApplied-universityFiscalPeriodCode" parent="InvoicePaidApplied-universityFiscalPeriodCode-parentBean" />

  <bean id="InvoicePaidApplied-universityFiscalPeriodCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="universityFiscalPeriodCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="University Fiscal Period Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    <property name="summary" value="universityFiscalPeriodCode" />
    <property name="description" value="universityFiscalPeriodCode description..." />
  </bean>
  <bean id="InvoicePaidApplied-universityFiscalYear" parent="InvoicePaidApplied-universityFiscalYear-parentBean" />

  <bean id="InvoicePaidApplied-universityFiscalYear-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="universityFiscalYear" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="University Fiscal Year" />
    <property name="shortLabel" value="Year" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="6" />
    </property>
    <property name="summary" value="universityFiscalYear" />
    <property name="description" value="universityFiscalYear description..." />
  </bean>
  <bean id="InvoicePaidApplied-versionNumber" parent="InvoicePaidApplied-versionNumber-parentBean" />

  <bean id="InvoicePaidApplied-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="InvoicePaidApplied-inquiryDefinition" parent="InvoicePaidApplied-inquiryDefinition-parentBean" />

  <bean id="InvoicePaidApplied-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Invoice Paid Applied Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="documentNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialDocumentReferenceInvoiceNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="invoiceItemAppliedAmount" />
              <bean parent="FieldDefinition"
                    p:attributeName="invoiceItemNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="paidAppliedItemNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="universityFiscalPeriodCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="universityFiscalYear" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="InvoicePaidApplied-lookupDefinition" parent="InvoicePaidApplied-lookupDefinition-parentBean" />

  <bean id="InvoicePaidApplied-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Invoice Paid Applied Lookup" />
    <property name="instructions" value="Look up an Invoice Paid Applied" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentReferenceInvoiceNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="invoiceItemAppliedAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="invoiceItemNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="paidAppliedItemNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalPeriodCode" />
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalYear" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentReferenceInvoiceNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="invoiceItemAppliedAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="invoiceItemNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="paidAppliedItemNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalPeriodCode" />
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalYear" />
      </list>
    </property>
  </bean>
</beans>
