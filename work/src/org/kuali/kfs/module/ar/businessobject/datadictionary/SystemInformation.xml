<!-- Business Object Inquiry Definition -->

  <bean id="SystemInformation-inquiryDefinition" parent="SystemInformation-inquiryDefinition-parentBean" />

  <bean id="SystemInformation-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="System Information Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="System Information" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="universityFiscalYear" />
              <bean parent="FieldDefinition"
                    p:attributeName="universityFederalEmployerIdentificationNumber" />                    
              <bean parent="FieldDefinition"
                    p:attributeName="processingChartOfAccountCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="processingOrganization.codeAndDescription" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialDocumentInitiatorIdentifier" />                    
              <bean parent="FieldDefinition"
                    p:attributeName="lockboxNumber" />        	  
              <bean parent="FieldDefinition"
                    p:attributeName="active" />
             </list>
          </property>
        </bean>                     
		<bean parent="InquirySectionDefinition">
			<property name="title" value="Object Codes" />
			<property name="numberOfColumns" value="1" />
			<property name="inquiryFields">
			<list>                
              <bean parent="FieldDefinition"
                    p:attributeName="creditCardObjectCode" />   
              <bean parent="FieldDefinition"
                    p:attributeName="discountObjectCode" />
             </list>
          </property>
        </bean>                      
		<bean parent="InquirySectionDefinition">
			<property name="title" value="University Clearing Account" />
			<property name="numberOfColumns" value="1" />
			<property name="inquiryFields">
			<list>                                              
              <bean parent="FieldDefinition"
                    p:attributeName="universityClearingChartOfAccountsCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="universityClearingAccountNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="universityClearingSubAccountNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="universityClearingObjectCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="universityClearingSubObjectCode" />
             </list>
          </property>
        </bean> 
		<bean parent="InquirySectionDefinition">
			<property name="title" value="Wire Account" />
			<property name="numberOfColumns" value="1" />
			<property name="inquiryFields">
			<list>                                              
              <bean parent="FieldDefinition"
                    p:attributeName="wireChartOfAccountsCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="wireAccountNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="wireSubAccountNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="wireObjectCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="wireSubObjectCode" />
            </list>
          </property>
        </bean>     
		<bean parent="InquirySectionDefinition">
			<property name="title" value="Remit To Information" />
			<property name="numberOfColumns" value="1" />
			<property name="inquiryFields">
			<list>                                              
              <bean parent="FieldDefinition"
                    p:attributeName="organizationCheckPayableToName" />
              <bean parent="FieldDefinition"
                    p:attributeName="organizationRemitToAddressName" />
              <bean parent="FieldDefinition"
                    p:attributeName="organizationRemitToLine1StreetAddress" />
              <bean parent="FieldDefinition"
                    p:attributeName="organizationRemitToLine2StreetAddress" />
              <bean parent="FieldDefinition"
                    p:attributeName="organizationRemitToCityName" />
              <bean parent="FieldDefinition"
                    p:attributeName="organizationRemitToStateCode" />	
              <bean parent="FieldDefinition"
                    p:attributeName="organizationRemitToZipCode" />					
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="SystemInformation-lookupDefinition" parent="SystemInformation-lookupDefinition-parentBean" />

  <bean id="SystemInformation-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="System Information Lookup" />
    <property name="instructions" value="Look up System Information" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalYear" />
        <bean parent="FieldDefinition"
              p:attributeName="processingChartOfAccountCode" />
        <bean parent="FieldDefinition"
              p:attributeName="processingOrganizationCode" />
        <bean parent="FieldDefinition"
              p:attributeName="lockboxNumber" /> 	  
        <bean parent="FieldDefinition"
              p:attributeName="active" p:defaultValue="Y" /> 	  
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="systemInformationViewer" />
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalYear" />
        <bean parent="FieldDefinition"
              p:attributeName="processingChartOfAccountCode" />
        <bean parent="FieldDefinition"
              p:attributeName="processingOrganizationCode" />
        <bean parent="FieldDefinition"
              p:attributeName="lockboxNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="active" /> 	  
      </list>
    </property>
  </bean>
</beans>

