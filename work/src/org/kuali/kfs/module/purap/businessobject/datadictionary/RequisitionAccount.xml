  </bean>
  <bean id="RequisitionAccount-referenceTypeCode" parent="RequisitionAccount-referenceTypeCode-parentBean" />

  <bean id="RequisitionAccount-referenceTypeCode-parentBean" abstract="true" parent="DocumentType-financialDocumentTypeCode">
    <property name="name" value="referenceTypeCode" />
    <property name="label" value="Reference Type Code" />
    <property name="shortLabel" value="Ref Type Code" />
    <property name="required" value="false" />
    <property name="summary" value="&amp;nbsp;" />
    <property name="description" value="The system code for the type of the system from which the original encumbering transaction came, selected from a pulldown list." />
  </bean>
  <bean id="RequisitionAccount-projectCode" parent="RequisitionAccount-projectCode-parentBean" />

  <bean id="RequisitionAccount-projectCode-parentBean" abstract="true" parent="ProjectCode-code">
    <property name="name" value="projectCode" />
    <property name="required" value="false" />
    <property name="summary" value="&amp;nbsp;" />
    <property name="description" value="The title of the project." />
  </bean>
  <bean id="RequisitionAccount-financialSubObjectCode" parent="RequisitionAccount-financialSubObjectCode-parentBean" />

  <bean id="RequisitionAccount-financialSubObjectCode-parentBean" abstract="true" parent="SubObjCd-financialSubObjectCode">
    <property name="required" value="false" />
