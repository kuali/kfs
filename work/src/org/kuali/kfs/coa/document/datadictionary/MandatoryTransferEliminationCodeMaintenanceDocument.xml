<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="MandatoryTransferEliminationCodeMaintenanceDocument" parent="MandatoryTransferEliminationCodeMaintenanceDocument-parentBean" />

  <bean id="MandatoryTransferEliminationCodeMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.chart.bo.codes.MandatoryTransferEliminationCode" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="MandatoryTransferEliminationCodeMaintenanceDocument-EditMandatoryTransferEliminationCode" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>code</value>
      </list>
    </property>
    <property name="label" value="Mandatory Transfer Elimination" />
    <property name="shortLabel" value="Mandatory Transfer Elimination" />
    <property name="summary" value="Mandatory Transfer EliminationCode Maintenance Document" />
    <property name="description" value="Maintenance document which allows you to create new Mandatory Transfer Elimination Codes, or modify existing Mandatory Transfer Elimination Codes." />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-CA"
            p:parameterName="MANDATORY_TRANSFER_ELIMINATION"
            p:parameterDetailType="MandatoryTransferEliminationCode" />
    </property>
    <property name="documentTypeName" value="MandatoryTransferEliminationCodeMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="MTE" />
    <property name="workflowProperties" >
      <ref bean="MandatoryTransferEliminationCodeMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>SY_MAINTAINENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="MandatoryTransferEliminationCodeMaintenanceDocument-EditMandatoryTransferEliminationCode" parent="MandatoryTransferEliminationCodeMaintenanceDocument-EditMandatoryTransferEliminationCode-parentBean" />

  <bean id="MandatoryTransferEliminationCodeMaintenanceDocument-EditMandatoryTransferEliminationCode-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Mandatory Transfer Elimination Code" />
    <property name="title" value="Edit Mandatory Transfer Elimination Code" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="code"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="name"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="MandatoryTransferEliminationCodeMaintenanceDocument-workflowProperties" parent="MandatoryTransferEliminationCodeMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="MandatoryTransferEliminationCodeMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
