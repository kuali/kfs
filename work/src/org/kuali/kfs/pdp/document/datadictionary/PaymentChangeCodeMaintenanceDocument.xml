<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xmlns:dd="http://rice.kuali.org/dd" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd         http://rice.kuali.org/dd         http://rice.kuali.org/dd/dd.xsd">

  <bean id="PaymentChangeCodeMaintenanceDocument" parent="PaymentChangeCodeMaintenanceDocument-parentBean"/>

  <bean id="PaymentChangeCodeMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.pdp.businessobject.PaymentChangeCode"/>
    <property name="maintainableClass" value="org.kuali.kfs.sys.document.FinancialSystemMaintainable"/>
    <property name="maintainableSections">
      <list>
        <ref bean="PaymentChangeCodeMaintenanceDocument-EditPaymentChangeCode"/>
      </list>
    </property>
    
    <property name="lockingKeys">
      <list>
        <value>code</value>
      </list>
    </property>
    
    
    
    
    <property name="documentTypeName" value="PMTC"/>
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase"/>
    <property name="allowsNewOrCopy" value="false"/>
    <property name="workflowAttributes">
    	<ref bean="PaymentChangeCodeMaintenanceDocument-workflowAttributes"/>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="PaymentChangeCodeMaintenanceDocument-EditPaymentChangeCode" parent="PaymentChangeCodeMaintenanceDocument-EditPaymentChangeCode-parentBean"/>

  <bean id="PaymentChangeCodeMaintenanceDocument-EditPaymentChangeCode-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Payment Change Code"/>
    <property name="title" value="Edit Payment Change Code"/>
    <property name="maintainableItems">
      <list>
        <bean parent="MaintainableFieldDefinition" p:name="code" p:unconditionallyReadOnly="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="name" p:required="true"/>
      </list>
    </property>
  </bean>
  
  <!-- workflow attributes -->
  <bean id="PaymentChangeCodeMaintenanceDocument-workflowAttributes" parent="PaymentChangeCodeMaintenanceDocument-workflowAttributes-parentBean"/>
  
  <bean id="PaymentChangeCodeMaintenanceDocument-workflowAttributes-parentBean" class="org.kuali.rice.kns.datadictionary.WorkflowAttributes" abstract="true"/>
</beans>