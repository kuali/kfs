  <bean id="BudgetConstructionActiveJobMove-emplid" parent="BudgetConstructionActiveJobMove-emplid-parentBean"/>

  <bean id="BudgetConstructionActiveJobMove-emplid-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="emplid"/>
    <property name="forceUppercase" value="true"/>
    <property name="label" value="Emplid"/>
    <property name="shortLabel" value="Emplid"/>
    <property name="maxLength" value="11"/>
    <property name="validationPattern">
      <bean parent="AnyCharacterValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="13"/>
    </property>
    
    
  </bean>
  <bean id="BudgetConstructionActiveJobMove-employeeRecord" parent="BudgetConstructionActiveJobMove-employeeRecord-parentBean"/>

  <bean id="BudgetConstructionActiveJobMove-employeeRecord-parentBean" abstract="true" parent="AttributeDefinition">
