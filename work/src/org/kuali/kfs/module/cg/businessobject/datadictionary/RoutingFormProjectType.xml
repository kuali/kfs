<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.kra.routingform.bo.RoutingFormProjectType">
        <customAttributeEnabled>false</customAttributeEnabled>

        <inquiry>
            <title>Routing Form Project Type Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="projectTypeCode" />
                <field attributeName="projectTypeDescription" />
                <field attributeName="documentNumber" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Routing Form Project Type Lookup</title>

            <instructions>Look up a Routing Form Project Type</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="projectTypeCode" />
                    <sortAttribute attributeName="documentNumber" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="projectTypeCode" required="false" />
                <lookupField attributeName="projectTypeDescription" required="false" />
                <lookupField attributeName="documentNumber" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="projectTypeCode" />
                <field attributeName="projectTypeDescription" />
                <field attributeName="documentNumber" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="projectTypeCode" forceUppercase="true">
                <label>Project Type Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>70</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="72" />
                </control>

                <summary>projectTypeCode</summary>
                <description>projectTypeCode description...</description>
			</attribute>

            <attribute name="projectTypeDescription" forceUppercase="true">
                <label>Project Type Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>75</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="77" />
                </control>

                <summary>projectTypeDescription</summary>
                <description>projectTypeDescription description...</description>
			</attribute>

            <attribute name="documentNumber" forceUppercase="true">
                <label>Document Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>14</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="16" />
                </control>

                <summary>documentNumber</summary>
                <description>documentNumber description...</description>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
