<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.purap.bo.CapitalAssetTransactionType">
        <customAttributeEnabled>false</customAttributeEnabled>

        <inquiry>
            <title>Capital Asset Transaction Type Inquiry</title>

            <inquiryFields>
                <field attributeName="capitalAssetTransactionTypeCode" />
                <field attributeName="capitalAssetTransactionTypeDescription" />
                <field attributeName="capitalAssetTransactionTypeServiceIndicator" />
                <field attributeName="dataObjectMaintenanceCodeActiveIndicator" />
            </inquiryFields>
        </inquiry>

        <lookup>
            <title>Capital Asset Transaction Type Lookup</title>

			<menubar><![CDATA[<a href="maintenance.do?methodToCall=start&businessObjectClassName=org.kuali.module.purap.bo.CapitalAssetTransactionType">Create New</a>]]></menubar>

            <instructions>Lookup a Capital Asset Transaction Type</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="capitalAssetTransactionTypeDescription" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="capitalAssetTransactionTypeCode" required="false" />
                <lookupField attributeName="capitalAssetTransactionTypeDescription" required="false" />
                <lookupField attributeName="capitalAssetTransactionTypeServiceIndicator" required="false" />
                <lookupField attributeName="dataObjectMaintenanceCodeActiveIndicator" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="capitalAssetTransactionTypeCode" />
                <field attributeName="capitalAssetTransactionTypeDescription" />
                <field attributeName="capitalAssetTransactionTypeServiceIndicator" />
                <field attributeName="dataObjectMaintenanceCodeActiveIndicator" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="capitalAssetTransactionTypeCode" forceUppercase="true">
                <label>CA Transaction Type Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="6" />
                </control>

                <summary>capitalAssetTransactionTypeCode</summary>
                <description>capitalAssetTransactionTypeCode description...</description>
			</attribute>

            <attribute name="capitalAssetTransactionTypeDescription" forceUppercase="false">
                <label>CA Transaction Type Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>45</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true" /></validationPattern>

                <control>
                    <text size="47" />
                </control>

                <summary>capitalAssetTransactionTypeDescription</summary>
                <description>capitalAssetTransactionTypeDescription description...</description>
			</attribute>

            <attribute name="capitalAssetTransactionTypeServiceIndicator" forceUppercase="false">
                <label>CA Transaction Type Service Indicator</label>
                <shortLabel>Indicator</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <checkbox />
                </control>

                <summary>capitalAssetTransactionTypeServiceIndicator</summary>
                <description>capitalAssetTransactionTypeServiceIndicator description...</description>
			</attribute>

            <attributeReference name="dataObjectMaintenanceCodeActiveIndicator" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="activeIndicator" />

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
