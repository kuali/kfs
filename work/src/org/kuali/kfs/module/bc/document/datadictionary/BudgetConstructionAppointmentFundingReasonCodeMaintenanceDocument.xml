<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="AppointmentFundingReasonCodeMaintenanceDocument" parent="AppointmentFundingReasonCodeMaintenanceDocument-parentBean" />

  <bean id="AppointmentFundingReasonCodeMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.budget.bo.BudgetConstructionAppointmentFundingReasonCode" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="AppointmentFundingReasonCodeMaintenanceDocument-EditBCAppointmentFundingReasonCode" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>appointmentFundingReasonCode</value>
      </list>
    </property>
    <property name="label" value="Appointment Funding Reason Code" />
    <property name="shortLabel" value="BC - Appt Funding Reason Code Maint Doc" />
    <property name="summary" value="Budget Construction Appointment Funding Reason Code Maintenance Document" />
    <property name="description" value="Document used to create or update Budget Construction Appointment Funding Reason Code Maintenance Document objects" />
    <property name="documentTypeName" value="AppointmentFundingReasonCodeMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="BCRC" />
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>SY_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="AppointmentFundingReasonCodeMaintenanceDocument-EditBCAppointmentFundingReasonCode" parent="AppointmentFundingReasonCodeMaintenanceDocument-EditBCAppointmentFundingReasonCode-parentBean" />

  <bean id="AppointmentFundingReasonCodeMaintenanceDocument-EditBCAppointmentFundingReasonCode-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit BC Appointment Funding Reason Code" />
    <property name="title" value="Edit BC Appointment Funding Reason Code" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="appointmentFundingReasonCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="appointmentFundingReasonDescription"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="rowActiveIndicator"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>
</beans>
