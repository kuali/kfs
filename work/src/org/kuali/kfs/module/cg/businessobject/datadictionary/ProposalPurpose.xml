<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.cg.bo.ProposalPurpose">
        <customAttributeEnabled>false</customAttributeEnabled>

        <inquiry>
            <title>Proposal Purpose Inquiry</title>

            <inquiryFields>
                <field attributeName="proposalPurposeCode" />
                <field attributeName="proposalPurposeDescription" />
                <field attributeName="rowActiveIndicator" />
            </inquiryFields>
        </inquiry>

        <lookup>
            <title>Proposal Purpose Lookup</title>

			<menubar><![CDATA[<a href="maintenance.do?methodToCall=start&businessObjectClassName=org.kuali.module.cg.bo.ProposalPurpose">Create New</a>]]></menubar>

            <instructions>Look up a Proposal Purpose</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="proposalPurposeCode" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="proposalPurposeCode" required="false" />
                <lookupField attributeName="proposalPurposeDescription" required="false" />
                <lookupField attributeName="rowActiveIndicator" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="proposalPurposeCode" />
                <field attributeName="proposalPurposeDescription" />
                <field attributeName="rowActiveIndicator" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="proposalPurposeCode" forceUppercase="true">
                <label>Proposal Purpose Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>1</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="3" />
                </control>

                <summary>proposalPurposeCode</summary>
                <description>proposalPurposeCode description...</description>
			</attribute>

            <attribute name="proposalPurposeDescription" forceUppercase="true">
                <label>Proposal Purpose Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>26</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="28" />
                </control>

                <summary>proposalPurposeDescription</summary>
                <description>proposalPurposeDescription description...</description>
			</attribute>

            <attribute name="rowActiveIndicator" forceUppercase="false">
                <label>Active Indicator</label>
                <shortLabel>Active Indicator</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <checkbox />
                </control>

                <summary>Active Indicator</summary>
                <description>Contracts And Grants Proposal Purpose Active Indicator</description>
            </attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
