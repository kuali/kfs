<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="BudgetConstructionDuration" parent="BudgetConstructionDuration-parentBean" />

  <bean id="BudgetConstructionDuration-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.bc.businessobject.BudgetConstructionDuration" />
    <property name="inquiryDefinition" >
      <ref bean="BudgetConstructionDuration-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="BudgetConstructionDuration-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="appointmentDurationCode" />
    <property name="objectLabel" value="Budget Construction Duration" />
    <property name="attributes" >
      <list>
        <ref bean="BudgetConstructionDuration-appointmentDurationCode" />
        <ref bean="BudgetConstructionDuration-appointmentDurationDescription" />
        <ref bean="BudgetConstructionDuration-active" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->



  <bean id="BudgetConstructionDuration-appointmentDurationCode" parent="BudgetConstructionDuration-appointmentDurationCode-parentBean" />

  <bean id="BudgetConstructionDuration-appointmentDurationCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="appointmentDurationCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Appointment Duration Code" />
    <property name="shortLabel" value="Duration Code" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="6" />
    </property>
    
    
  </bean>
  <bean id="BudgetConstructionDuration-appointmentDurationDescription" parent="BudgetConstructionDuration-appointmentDurationDescription-parentBean" />

  <bean id="BudgetConstructionDuration-appointmentDurationDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="appointmentDurationDescription" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Appointment Duration Description" />
    <property name="shortLabel" value="Duration Desc" />
    <property name="maxLength" value="40" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="42" />
    </property>
    
    
  </bean>
  
  <bean id="BudgetConstructionDuration-active" parent="BudgetConstructionDuration-active-parentBean" />
  <bean id="BudgetConstructionDuration-active-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
    <property name="name" value="active" />
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="BudgetConstructionDuration-inquiryDefinition" parent="BudgetConstructionDuration-inquiryDefinition-parentBean" />

  <bean id="BudgetConstructionDuration-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Duration Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="appointmentDurationCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="appointmentDurationDescription" />
              <bean parent="FieldDefinition"
                    p:attributeName="active" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="BudgetConstructionDuration-lookupDefinition" parent="BudgetConstructionDuration-lookupDefinition-parentBean" />

  <bean id="BudgetConstructionDuration-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Appointment Funding Duration Lookup" />
    <property name="instructions" value="Look up a Budget Construction Duration" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="appointmentDurationCode" />
        <bean parent="FieldDefinition"
              p:attributeName="appointmentDurationDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="appointmentDurationCode"
              p:forceInquiry="true" />
        <bean parent="FieldDefinition"
              p:attributeName="appointmentDurationDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
  </bean>
</beans>
