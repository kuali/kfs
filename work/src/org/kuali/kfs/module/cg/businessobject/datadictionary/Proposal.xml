              <bean parent="FieldDefinition"
                    p:attributeName="proposalStatusCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="grantNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="proposalBeginningDate" />
              <bean parent="FieldDefinition"
                    p:attributeName="proposalEndingDate" />
              <bean parent="FieldDefinition"
                    p:attributeName="proposalSubmissionDate" />
              <bean parent="FieldDefinition"
                    p:attributeName="proposalClosingDate" />
              <bean parent="FieldDefinition"
                    p:attributeName="proposalRejectedDate" />
              <bean parent="FieldDefinition"
                    p:attributeName="proposalLastUpdateDate" />
              <bean parent="FieldDefinition"
                    p:attributeName="proposalDirectCostAmount" />
              <bean parent="FieldDefinition"
                    p:attributeName="proposalIndirectCostAmount" />
              <bean parent="FieldDefinition"
                    p:attributeName="proposalTotalAmount" />
              <bean parent="FieldDefinition"
                    p:attributeName="proposalTotalProjectAmount" />
              <bean parent="FieldDefinition"
                    p:attributeName="proposalAwardTypeCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="proposalPurposeCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="cfdaNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="proposalFellowName" />
              <bean parent="FieldDefinition"
                    p:attributeName="proposalDueDate" />
              <bean parent="FieldDefinition"
                    p:attributeName="proposalFederalPassThroughIndicator" />
              <bean parent="FieldDefinition"
                    p:attributeName="federalPassThroughAgencyNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="federalPassThroughAgency.fullName" />
            </list>
          </property>
        </bean>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Organizations" />
          <property name="numberOfColumns" value="1" />
