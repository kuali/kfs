  </bean>
  
  <bean id="SystemInformation-wireChartOfAccountsCode" parent="SystemInformation-wireChartOfAccountsCode-parentBean" />
  <bean id="SystemInformation-wireChartOfAccountsCode-parentBean" abstract="true" parent="Chart-chartOfAccountsCode">
    <property name="name" value="wireChartOfAccountsCode" />
    <property name="label" value="Chart Of Account" />
