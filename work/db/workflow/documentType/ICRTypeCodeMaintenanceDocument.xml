<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="ns:workflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
  <documentTypes xmlns="ns:workflow/DocumentType" xsi:schemaLocation="ns:workflow/DocumentType resource:DocumentType">
    <documentType>
      <name>ICRTypeCodeMaintenanceDocument</name>
      <parent>KualiMaintenanceDocument</parent>
      <label>Indirect Cost Recovery Type</label>
      <postProcessorName>org.kuali.workflow.postprocessor.KualiPostProcessor</postProcessorName>
      <superUserWorkgroupName>SY_FUNCTIONAL_SUPER_USERS</superUserWorkgroupName>
      <blanketApprovePolicy>NONE</blanketApprovePolicy>
      <defaultExceptionWorkgroupName>SY_FUNCTIONAL_SUPER_USERS</defaultExceptionWorkgroupName>
      <docHandler>${application.url}/kr/maintenance.do?methodToCall=docHandler</docHandler>
      <active>true</active>
      <routingVersion>1</routingVersion>
      <routePaths>
        <routePath>
          <start name="Adhoc Routing" nextNode="Central Review" />
          <requests name="Central Review" />
        </routePath>
      </routePaths>
      <routeNodes>
        <start name="Adhoc Routing">
          <activationType>S</activationType>
          <mandatoryRoute>false</mandatoryRoute>
          <finalApproval>false</finalApproval>
        </start>
        <requests name="Central Review">
          <activationType>S</activationType>
          <ruleTemplate>KualiCentralRoutingTemplate</ruleTemplate>
          <mandatoryRoute>false</mandatoryRoute>
          <finalApproval>false</finalApproval>
        </requests>
      </routeNodes>
    </documentType>
  </documentTypes>
</data>
