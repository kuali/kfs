            <attribute name="proposalNumber" forceUppercase="true">
            	<label>Award Proposal Number</label>
            	<shortLabel>Award Prop Num</shortLabel>

                <maxLength>12</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="14" />
                </control>

            	<summary>Award Proposal Number</summary>
            	<description>Award Proposal Number description...</description>
            </attribute>

            <attributeReference name="awardAccounts.accountNumber" sourceClassName="org.kuali.module.cg.bo.AwardAccount" sourceAttributeName="accountNumber">
				<summary>Award Account Number</summary>
            </attributeReference>

            <attributeReference name="awardOrganizations.organizationCode" sourceClassName="org.kuali.module.cg.bo.AwardOrganization" sourceAttributeName="organizationCode">
