<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.cg.bo.ProposalStatus">
        <customAttributeEnabled>false</customAttributeEnabled>

        <inquiry>
            <title>Proposal Status Inquiry</title>

            <inquiryFields>
                <field attributeName="proposalStatusCode" />
                <field attributeName="proposalStatusDescription" />
                <field attributeName="rowActiveIndicator" />
            </inquiryFields>
        </inquiry>

        <lookup>
            <title>Proposal Status Lookup</title>

			<menubar><![CDATA[<a href="maintenance.do?methodToCall=start&businessObjectClassName=org.kuali.module.cg.bo.ProposalStatus">Create New</a>]]></menubar>

            <instructions>Look up a Proposal Status</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="proposalStatusCode" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="proposalStatusCode" required="false" />
                <lookupField attributeName="proposalStatusDescription" required="false" />
                <lookupField attributeName="rowActiveIndicator" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="proposalStatusCode" />
                <field attributeName="proposalStatusDescription" />
                <field attributeName="rowActiveIndicator" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="proposalStatusCode" forceUppercase="true">
                <label>Proposal Status Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="4" />
                </control>

                <summary>proposalStatusCode</summary>
                <description>proposalStatusCode description...</description>
			</attribute>

            <attribute name="proposalStatusDescription" forceUppercase="true">
                <label>Proposal Status Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>15</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="17" />
                </control>

                <summary>proposalStatusDescription</summary>
                <description>proposalStatusDescription description...</description>
			</attribute>

            <attribute name="rowActiveIndicator" forceUppercase="false">
                <label>Active Indicator</label>
                <shortLabel>Active Indicator</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <checkbox />
                </control>

                <summary>Active Indicator</summary>
                <description>Contracts And Grants Proposal Status Active Indicator</description>
            </attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
