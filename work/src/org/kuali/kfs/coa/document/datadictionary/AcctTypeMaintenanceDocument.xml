<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../../core/datadictionary/dataDictionary-1_0.dtd">

<dictionaryEntry>
    <maintenanceDocument>
        <businessObjectClass>org.kuali.module.chart.bo.AcctType</businessObjectClass>
        <maintainableClass>org.kuali.core.maintenance.KualiMaintainableImpl</maintainableClass>

        <documentAuthorizerClass>org.kuali.core.document.MaintenanceDocumentAuthorizerBase</documentAuthorizerClass>
        <authorizations>
            <authorization action="initiate">
                <workgroups>
                    <workgroup>KUALI_ROLE_MAINTENANCE</workgroup>
                </workgroups>
            </authorization>
        </authorizations>

        <documentTypeName>KualiAccountTypeMaintenanceDocument</documentTypeName>
        <documentTypeCode>ATYP</documentTypeCode>
        <label>Account Type Maintenance Document</label>
        <shortLabel>AccountTypeMaintDoc</shortLabel>
        <summary>Account Type Maintenance Document</summary>
        <description>Maintenance document which allows you to create new Account Types, or modify existing Account Types.</description>
        <help securityGroupName="ChartMaintenanceEDoc" parameterName="CHART_HELP_ACCOUNT_TYPE" />

        <lockingKeys>
            <field attributeName="accountTypeCode" />
        </lockingKeys>

        <maintainableSections>
            <maintainableSection title="Edit Account Type">
                <maintainableItems>
                    <maintainableField name="accountTypeCode" required="true" />
                    <maintainableField name="accountTypeName" required="true" />
                    <maintainableField name="versionNumber" />
                </maintainableItems>
            </maintainableSection>
        </maintainableSections>
    </maintenanceDocument>

</dictionaryEntry>