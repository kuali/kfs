		<property name="name" value="customerFaxNumber" />
		<property name="forceUppercase" value="true" />
		<property name="label" value="Fax Number" />
		<property name="shortLabel" value="Number" />
		<property name="maxLength" value="20" />
		<property name="validationPattern">
			<bean parent="PhoneNumberValidationPattern" />
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="22" />
		</property>
		<property name="summary" value="customerFaxNumber" />
		<property name="description"
			value="customerFaxNumber description..." />
	</bean>
	<bean id="Customer-customerFederalIdentifierNumber"
		parent="Customer-customerFederalIdentifierNumber-parentBean" />

	<bean id="Customer-customerFederalIdentifierNumber-parentBean"
		abstract="true" parent="AttributeDefinition">
		<property name="name" value="customerFederalIdentifierNumber" />
		<property name="forceUppercase" value="true" />
		<property name="label"
			value="Federal Identifier Number" />
		<property name="shortLabel" value="Number" />
		<property name="maxLength" value="9" />
		<property name="validationPattern">
			<bean parent="AnyCharacterValidationPattern" />
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="11" />
		</property>
		<property name="summary"
			value="customerFederalIdentifierNumber" />
		<property name="description"
			value="customerFederalIdentifierNumber description..." />
	</bean>
	<bean id="Customer-customerLastActivityDate"
		parent="Customer-customerLastActivityDate-parentBean" />

	<bean id="Customer-customerLastActivityDate-parentBean"
		abstract="true" parent="GenericAttributes-genericDate">
		<property name="name" value="customerLastActivityDate" />
		<property name="forceUppercase" value="true" />
		<property name="label" value="Last Activity Date" />
		<property name="summary" value="customerLastActivityDate" />
		<property name="description"
			value="customerLastActivityDate description..." />
	</bean>
	<bean id="Customer-customerName"
		parent="Customer-customerName-parentBean" />

	<bean id="Customer-customerName-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="name" value="customerName" />
		<property name="forceUppercase" value="true" />
		<property name="label" value="Customer Name" />
		<property name="shortLabel" value="Name" />
		<property name="maxLength" value="60" />
		<property name="validationPattern">
			<bean parent="AlphaValidationPattern"
				  p:allowWhitespace="true" />
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="62" />
		</property>
		<property name="summary" value="customerName" />
		<property name="description"
			value="customerName description..." />
	</bean>
	<bean id="Customer-customerNumber"
		parent="Customer-customerNumber-parentBean" />

	<bean id="Customer-customerNumber-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="name" value="customerNumber" />
		<property name="forceUppercase" value="true" />
		<property name="label" value="Customer Number" />
		<property name="shortLabel" value="Number" />
		<property name="maxLength" value="9" />
		<property name="validationPattern">
			<bean parent="AnyCharacterValidationPattern" />
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="11" />
		</property>
		<property name="summary" value="customerNumber" />
		<property name="description"
			value="customerNumber description..." />
	</bean>
	<bean id="Customer-customerParentCompanyNumber"
		parent="Customer-customerParentCompanyNumber-parentBean" />

	<bean id="Customer-customerParentCompanyNumber-parentBean"
		abstract="true" parent="AttributeDefinition">
		<property name="name" value="customerParentCompanyNumber" />
		<property name="forceUppercase" value="true" />
		<property name="label" value="Customer Parent Company" />
		<property name="shortLabel" value="Customer Parent" />
		<property name="maxLength" value="9" />
		<property name="validationPattern">
			<bean parent="AnyCharacterValidationPattern" />
		</property>
        <property name="control" >
            <bean parent="SelectControlDefinition"
                  p:valuesFinderClass="org.kuali.module.ar.lookup.keyvalues.CustomerValuesFinder"
                  p:includeKeyInLabel="false" />
        </property>
		<property name="summary" value="customerParentCompanyNumber" />
		<property name="description"
			value="customerParentCompanyNumber description..." />
	</bean>
	<bean id="Customer-customerPhoneNumber"
		parent="Customer-customerPhoneNumber-parentBean" />

	<bean id="Customer-customerPhoneNumber-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="name" value="customerPhoneNumber" />
		<property name="forceUppercase" value="true" />
		<property name="label" value="Phone Number" />
		<property name="shortLabel" value="Phone" />
		<property name="maxLength" value="20" />
		<property name="validationPattern">
			<bean parent="AnyCharacterValidationPattern" />
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="22" />
		</property>
		<property name="summary" value="customerPhoneNumber" />
		<property name="description"
			value="customerPhoneNumber description..." />
	</bean>
	<bean id="Customer-customerRecordAddDate"
		parent="Customer-customerRecordAddDate-parentBean" />

	<bean id="Customer-customerRecordAddDate-parentBean" abstract="true"
		parent="GenericAttributes-genericDate">
		<property name="name" value="customerRecordAddDate" />
		<property name="forceUppercase" value="true" />
		<property name="label" value="Customer Record Add Date" />
		<property name="shortLabel" value="Date" />
		<property name="summary" value="customerRecordAddDate" />
		<property name="description"
			value="customerRecordAddDate description..." />
	</bean>
	<bean id="Customer-customerSocialSecurityNumberIdentifier"
		parent="Customer-customerSocialSecurityNumberIdentifier-parentBean" />

	<bean
		id="Customer-customerSocialSecurityNumberIdentifier-parentBean"
		abstract="true" parent="AttributeDefinition">
		<property name="name"
			value="customerSocialSecurityNumberIdentifier" />
		<property name="forceUppercase" value="true" />
		<property name="label"
			value="Social Security Identifier" />
		<property name="shortLabel" value="Identifier" />
		<property name="maxLength" value="9" />
		<property name="validationPattern">
			<bean parent="AnyCharacterValidationPattern" />
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="11" />
		</property>
		<property name="summary"
			value="customerSocialSecurityNumberIdentifier" />
		<property name="description"
			value="customerSocialSecurityNumberIdentifier description..." />
	</bean>
	<bean id="Customer-customerTaxExemptIndicator"
		parent="Customer-customerTaxExemptIndicator-parentBean" />

	<bean id="Customer-customerTaxExemptIndicator-parentBean"
		abstract="true" parent="AttributeDefinition">
		<property name="name" value="customerTaxExemptIndicator" />
		<property name="forceUppercase" value="true" />
		<property name="label" value="Tax Exempt Indicator" />
		<property name="shortLabel" value="Tax" />
		<property name="maxLength" value="1" />
		<property name="control">
			<bean parent="CheckboxControlDefinition" />
		</property>
		<property name="summary" value="customerActiveIndicator" />
		<property name="description"
			value="customerActiveIndicator description..." />
	</bean>
	<bean id="Customer-customerTypeCode"
		parent="Customer-customerTypeCode-parentBean" />

	<bean id="Customer-customerTypeCode-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="name" value="customerTypeCode" />
		<property name="forceUppercase" value="true" />
		<property name="label" value="Customer Type" />
		<property name="shortLabel" value="Type" />
		<property name="maxLength" value="2" />
		<property name="validationPattern">
			<bean parent="AnyCharacterValidationPattern" />
		</property>
		<property name="control">
			<bean parent="SelectControlDefinition"
				p:valuesFinderClass="org.kuali.module.ar.lookup.keyvalues.CustomerTypeValuesFinder"
