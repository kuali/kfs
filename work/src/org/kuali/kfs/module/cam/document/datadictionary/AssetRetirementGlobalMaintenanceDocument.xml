<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="AssetRetirementGlobalMaintenanceDocument" parent="AssetRetirementGlobalMaintenanceDocument-parentBean" />

  <bean id="AssetRetirementGlobalMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.cams.bo.AssetRetirementGlobal" />
    <property name="maintainableClass" value="org.kuali.module.cams.maintenance.AssetRetirementGlobalMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="AssetRetirementGlobalMaintenanceDocument-RetirementInformation" />
        <ref bean="AssetRetirementGlobalMaintenanceDocument-ExternalTransferorGift" />
        <ref bean="AssetRetirementGlobalMaintenanceDocument-Theft" />
        <ref bean="AssetRetirementGlobalMaintenanceDocument-AuctionorSold" />
        <ref bean="AssetRetirementGlobalMaintenanceDocument-AssetDetailInformation" />
      </list>
    </property>
    <property name="defaultExistenceChecks" >
      <list>
        <bean parent="ReferenceDefinition"
              p:attributeName="asset"
              p:collection="assetRetirementGlobalDetails"
              p:attributeToHighlightOnFail="capitalAssetNumber" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>documentNumber</value>
      </list>
    </property>
    <property name="label" value="Asset Retirement Global" />
    <property name="shortLabel" value="Asset Retirement Global" />
    <property name="summary" value="Retirement Global Asset Maintenance Document" />
    <property name="description" value="Document used to create or update global retirement asset document." />
    <property name="documentTypeName" value="AssetRetirementGlobalMaintenanceDocument" />
    <property name="businessRulesClass" value="org.kuali.module.cams.rules.AssetRetirementGlobalRule" />
    <property name="documentAuthorizerClass" value="org.kuali.module.cams.document.authorization.AssetRetirementAuthorizer" />
    <property name="documentTypeCode" value="AR" />
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>kualiUniversalGroup</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="AssetRetirementGlobalMaintenanceDocument-RetirementInformation" parent="AssetRetirementGlobalMaintenanceDocument-RetirementInformation-parentBean" />

  <bean id="AssetRetirementGlobalMaintenanceDocument-RetirementInformation-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Retirement Information" />
    <property name="title" value="Retirement Information" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableSubSectionHeaderDefinition"
              p:name="Retirement Reason" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
        <bean parent="MaintainableFieldDefinition"
              p:name="retirementReasonCode"
              p:readOnly="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="retirementDate"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="mergedTargetCapitalAssetNumber" />
      </list>
    </property>
  </bean>
  <bean id="AssetRetirementGlobalMaintenanceDocument-ExternalTransferorGift" parent="AssetRetirementGlobalMaintenanceDocument-ExternalTransferorGift-parentBean" />

  <bean id="AssetRetirementGlobalMaintenanceDocument-ExternalTransferorGift-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="ExternalTransferOrGift" />
    <property name="title" value="External Transfer or Gift" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="sharedRetirementInfo.retirementContactName" />
        <bean parent="MaintainableFieldDefinition"
              p:name="sharedRetirementInfo.retirementInstitutionName" />
        <bean parent="MaintainableFieldDefinition"
              p:name="sharedRetirementInfo.retirementStreetAddress" />
        <bean parent="MaintainableFieldDefinition"
              p:name="sharedRetirementInfo.retirementCityName" />
        <bean parent="MaintainableFieldDefinition"
              p:name="sharedRetirementInfo.retirementStateCode" />
        <bean parent="MaintainableFieldDefinition"
              p:name="sharedRetirementInfo.retirementZipCode" />
        <bean parent="MaintainableFieldDefinition"
              p:name="sharedRetirementInfo.retirementCountryCode" />
        <bean parent="MaintainableFieldDefinition"
              p:name="sharedRetirementInfo.retirementPhoneNumber" />
      </list>
    </property>
  </bean>
  <bean id="AssetRetirementGlobalMaintenanceDocument-Theft" parent="AssetRetirementGlobalMaintenanceDocument-Theft-parentBean" />

  <bean id="AssetRetirementGlobalMaintenanceDocument-Theft-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Theft" />
    <property name="title" value="Theft" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="sharedRetirementInfo.paidCaseNumber" />
      </list>
    </property>
  </bean>
  <bean id="AssetRetirementGlobalMaintenanceDocument-AuctionorSold" parent="AssetRetirementGlobalMaintenanceDocument-AuctionorSold-parentBean" />

  <bean id="AssetRetirementGlobalMaintenanceDocument-AuctionorSold-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="AuctionOrSold" />
    <property name="title" value="Auction or Sold" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="sharedRetirementInfo.buyerDescription" />
        <bean parent="MaintainableFieldDefinition"
              p:name="sharedRetirementInfo.salePrice" />
        <bean parent="MaintainableFieldDefinition"
              p:name="sharedRetirementInfo.estimatedSellingPrice" />
        <bean parent="MaintainableFieldDefinition"
              p:name="sharedRetirementInfo.handlingFeeAmount" />
        <bean parent="MaintainableFieldDefinition"
              p:name="sharedRetirementInfo.preventiveMaintenanceAmount" />
        <bean parent="MaintainableFieldDefinition"
              p:name="sharedRetirementInfo.cashReceiptFinancialDocumentNumber" />
        <bean parent="MaintainableFieldDefinition"
              p:name="sharedRetirementInfo.retirementChartOfAccountsCode" />
        <bean parent="MaintainableFieldDefinition"
              p:name="sharedRetirementInfo.retirementAccountNumber" />
      </list>
    </property>
  </bean>
  <bean id="AssetRetirementGlobalMaintenanceDocument-AssetDetailInformation" parent="AssetRetirementGlobalMaintenanceDocument-AssetDetailInformation-parentBean" />

  <bean id="AssetRetirementGlobalMaintenanceDocument-AssetDetailInformation-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Asset Detail Information" />
    <property name="title" value="Asset Detail Information" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableCollectionDefinition">
          <property name="name" value="assetRetirementGlobalDetails" />
          <property name="businessObjectClass" value="org.kuali.module.cams.bo.AssetRetirementGlobalDetail" />
          <property name="summaryTitle" value="Asset Retired" />
          <property name="summaryFields" >
            <list>
              <bean parent="MaintainableFieldDefinition"
                    p:name="capitalAssetNumber" />
            </list>
          </property>
          <property name="duplicateIdentificationFields">
          	<list>
          		<bean parent="MaintainableFieldDefinition" p:name="capitalAssetNumber" />
          	</list>
          </property>
          <property name="maintainableFields" >
            <list>
              <bean parent="MaintainableFieldDefinition"
                    p:name="versionNumber" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="capitalAssetNumber"
                    p:readOnlyAfterAdd="true"
                    p:template="capitalAssetNumber" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="asset.organizationOwnerChartOfAccountsCode"
                    p:readOnly="true"
                    p:template="organizationOwnerChartOfAccountsCode" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="asset.organizationOwnerAccountNumber"
                    p:readOnly="true"
                    p:template="organizationOwnerAccountNumber" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="asset.acquisitionTypeCode"
                    p:readOnly="true"
                    p:template="acquisitionTypeCode" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="asset.inventoryStatusCode"
                    p:readOnly="true"
                    p:template="inventoryStatusCode" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="asset.conditionCode"
                    p:readOnly="true"
                    p:template="conditionCode" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="asset.capitalAssetDescription"
                    p:readOnly="true"
                    p:template="capitalAssetDescription" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="asset.capitalAssetTypeCode"
                    p:readOnly="true"
                    p:template="capitalAssetTypeCode" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="asset.vendorName"
                    p:readOnly="true"
                    p:template="vendorName" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="asset.manufacturerModelNumber"
                    p:readOnly="true"
                    p:template="manufacturerModelNumber" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="asset.serialNumber"
                    p:readOnly="true"
                    p:template="serialNumber" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="asset.campusTagNumber"
                    p:readOnly="true"
                    p:template="campusTagNumber" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="asset.governmentTagNumber"
                    p:readOnly="true"
                    p:template="governmentTagNumber" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="asset.lastInventoryDate"
                    p:readOnly="true"
                    p:template="lastInventoryDate" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="asset.createDate"
                    p:readOnly="true"
                    p:template="createDate" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="asset.capitalAssetInServiceDate"
                    p:readOnly="true"
                    p:template="capitalAssetInServiceDate" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="asset.depreciationDate"
                    p:readOnly="true"
                    p:template="depreciationDate" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="asset.totalCostAmount"
                    p:readOnly="true"
                    p:template="totalCostAmount" />
              <bean parent="MaintainableFieldDefinition"
              		p:name="asset.accumulatedDepreciation"
              		p:readOnly="true" 
              		p:template="accumulatedDepreciation"/>
              <bean parent="MaintainableFieldDefinition"
              		p:name="asset.bookValue"
              		p:readOnly="true" 
              		p:template="bookValue" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="asset.federalContribution"
                    p:readOnly="true"
                    p:template="federalContribution" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="newCollectionRecord" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>
</beans>
