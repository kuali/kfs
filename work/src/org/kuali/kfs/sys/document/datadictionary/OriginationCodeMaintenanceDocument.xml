<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="OriginationCodeMaintenanceDocument" parent="OriginationCodeMaintenanceDocument-parentBean" />

  <bean id="OriginationCodeMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.bo.OriginationCode" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="OriginationCodeMaintenanceDocument-EditOriginationCode" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>financialSystemOriginationCode</value>
      </list>
    </property>
    <property name="label" value="Origination Code" />
    <property name="shortLabel" value="Origination Code" />
    <property name="summary" value="Origination Code maintenance document" />
    <property name="description" value="Document used to create or update Origination Code objects" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-SY"
            p:parameterName="ORIGINATION"
            p:parameterDetailType="OriginationCode" />
    </property>
    <property name="documentTypeName" value="OriginationCodeMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="ORIG" />
    <property name="workflowProperties" >
      <ref bean="OriginationCodeMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>SY_FUNCTIONAL_SUPER_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="OriginationCodeMaintenanceDocument-EditOriginationCode" parent="OriginationCodeMaintenanceDocument-EditOriginationCode-parentBean" />

  <bean id="OriginationCodeMaintenanceDocument-EditOriginationCode-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Origination Code" />
    <property name="title" value="Edit Origination Code" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="financialSystemOriginationCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="nextFinancialDocumentNumber"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialSystemServerName"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialSystemDatabaseName"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialSystemDatabaseDesc" />
        <bean parent="MaintainableFieldDefinition"
              p:name="nextCapitalAssetNumber" />
        <bean parent="MaintainableFieldDefinition"
              p:name="nextNonCapitalAssetNumber" />
        <bean parent="MaintainableFieldDefinition"
              p:name="nextCgProposalNumber" />
        <bean parent="MaintainableFieldDefinition"
              p:name="nextContractsAndGrantsAgencyNumber" />
        <bean parent="MaintainableFieldDefinition"
              p:name="nextContractsAndGrantsSubcontractorNumber" />
        <bean parent="MaintainableFieldDefinition"
              p:name="nextDisbVchrPayeeIdNbr" />
        <bean parent="MaintainableFieldDefinition"
              p:name="nextFdocAttachmentIdentifier" />
        <bean parent="MaintainableFieldDefinition"
              p:name="acctRcvbNextCustomerNumber" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialDocumentNextItemIdentifier" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="OriginationCodeMaintenanceDocument-workflowProperties" parent="OriginationCodeMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="OriginationCodeMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
