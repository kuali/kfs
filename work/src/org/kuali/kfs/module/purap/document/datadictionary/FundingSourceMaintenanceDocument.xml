<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="FundingSourceMaintenanceDocument" parent="FundingSourceMaintenanceDocument-parentBean" />

  <bean id="FundingSourceMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.purap.bo.FundingSource" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="FundingSourceMaintenanceDocument-EditFundingSource" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>fundingSourceCode</value>
      </list>
    </property>
    <property name="label" value="Funding Source" />
    <property name="shortLabel" value="Funding Source" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-PA"
            p:parameterName="FUNDING_SOURCE"
            p:parameterDetailType="FundingSource" />
    </property>
    <property name="documentTypeName" value="FundingSourceMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="PMFS" />
    <property name="workflowProperties" >
      <ref bean="FundingSourceMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>PA_PUR_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="FundingSourceMaintenanceDocument-EditFundingSource" parent="FundingSourceMaintenanceDocument-EditFundingSource-parentBean" />

  <bean id="FundingSourceMaintenanceDocument-EditFundingSource-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Funding Source" />
    <property name="title" value="Edit Funding Source" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="fundingSourceCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="fundingSourceDescription"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="active"
              p:defaultValue="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="FundingSourceMaintenanceDocument-workflowProperties" parent="FundingSourceMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="FundingSourceMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
