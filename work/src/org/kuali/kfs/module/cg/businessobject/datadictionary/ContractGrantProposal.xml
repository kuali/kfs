<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.kra.routingform.bo.ContractGrantProposal">
        <customAttributeEnabled>false</customAttributeEnabled>

        <inquiry>
            <title>Contract Grant Proposal Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="proposalCreateDate" />
                <field attributeName="proposalNumber" />
                <field attributeName="documentNumber" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Contract Grant Proposal Lookup</title>

            <instructions>Look up a Contract Grant Proposal</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="documentNumber" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="proposalCreateDate" required="false" />
                <lookupField attributeName="proposalNumber" required="false" />
                <lookupField attributeName="documentNumber" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="proposalCreateDate" />
                <field attributeName="proposalNumber" />
                <field attributeName="documentNumber" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="proposalCreateDate" forceUppercase="true">
                <label>Proposal Create Date</label>
                <shortLabel>Date</shortLabel>

                <maxLength>7</maxLength>
                <validationPattern><date /></validationPattern>

                <control>
                    <text size="9" />
                </control>

                <summary>proposalCreateDate</summary>
                <description>proposalCreateDate description...</description>
			</attribute>

            <attribute name="proposalNumber" forceUppercase="true">
                <label>Institution Proposal No.</label>
                <shortLabel>Proposal No.</shortLabel>

                <maxLength>12</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="14" />
                </control>

                <summary>proposalNumber</summary>
                <description>proposalNumber description...</description>
			</attribute>

            <attribute name="documentNumber" forceUppercase="true">
                <label>Document Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>14</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="16" />
                </control>

                <summary>documentNumber</summary>
                <description>documentNumber description...</description>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
