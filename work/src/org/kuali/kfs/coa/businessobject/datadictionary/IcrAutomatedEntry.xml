<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="IcrAutomatedEntry" parent="IcrAutomatedEntry-parentBean" />

  <bean id="IcrAutomatedEntry-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.chart.bo.IcrAutomatedEntry" />
    <property name="inquiryDefinition" >
      <ref bean="IcrAutomatedEntry-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="IcrAutomatedEntry-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="awardIndrCostRcvyEntryNbr" />
    <property name="objectLabel" value="Indirect Cost Recovery Automated Entry" />
    <property name="attributes" >
      <list>
        <ref bean="IcrAutomatedEntry-universityFiscalYear" />
        <ref bean="IcrAutomatedEntry-financialIcrSeriesIdentifier" />
        <ref bean="IcrAutomatedEntry-balanceTypeCode" />
        <ref bean="IcrAutomatedEntry-awardIndrCostRcvyEntryNbr" />
        <ref bean="IcrAutomatedEntry-chartOfAccountsCode" />
        <ref bean="IcrAutomatedEntry-accountNumber" />
        <ref bean="IcrAutomatedEntry-subAccountNumber" />
        <ref bean="IcrAutomatedEntry-financialObjectCode" />
        <ref bean="IcrAutomatedEntry-financialSubObjectCode" />
        <ref bean="IcrAutomatedEntry-offsetBalanceSheetObjectCodeNumber" />
        <ref bean="IcrAutomatedEntry-transactionDebitIndicator" />
        <ref bean="IcrAutomatedEntry-awardIndrCostRcvyRatePct" />
        <ref bean="IcrAutomatedEntry-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="IcrAutomatedEntry-universityFiscalYear" parent="IcrAutomatedEntry-universityFiscalYear-parentBean" />

  <bean id="IcrAutomatedEntry-universityFiscalYear-parentBean" abstract="true" parent="GenericAttributes-genericFiscalYear">
    <property name="name" value="universityFiscalYear" />
    <property name="required" value="true" />
  </bean>
  <bean id="IcrAutomatedEntry-financialIcrSeriesIdentifier" parent="IcrAutomatedEntry-financialIcrSeriesIdentifier-parentBean" />

  <bean id="IcrAutomatedEntry-financialIcrSeriesIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialIcrSeriesIdentifier" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Series Id" />
    <property name="shortLabel" value="Series Id" />
    <property name="maxLength" value="3" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="3" />
    </property>
  </bean>
  <bean id="IcrAutomatedEntry-balanceTypeCode" parent="IcrAutomatedEntry-balanceTypeCode-parentBean" />

  <bean id="IcrAutomatedEntry-balanceTypeCode-parentBean" abstract="true" parent="BalanceTyp-code">
    <property name="name" value="balanceTypeCode" />
  </bean>
  <bean id="IcrAutomatedEntry-awardIndrCostRcvyEntryNbr" parent="IcrAutomatedEntry-awardIndrCostRcvyEntryNbr-parentBean" />

  <bean id="IcrAutomatedEntry-awardIndrCostRcvyEntryNbr-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="awardIndrCostRcvyEntryNbr" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="ICR Entry Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="3" />
    </property>
  </bean>
  <bean id="IcrAutomatedEntry-chartOfAccountsCode" parent="IcrAutomatedEntry-chartOfAccountsCode-parentBean" />

  <bean id="IcrAutomatedEntry-chartOfAccountsCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="chartOfAccountsCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Chart Code" />
    <property name="shortLabel" value="Chart" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="2" />
    </property>
  </bean>
  <bean id="IcrAutomatedEntry-accountNumber" parent="IcrAutomatedEntry-accountNumber-parentBean" />

  <bean id="IcrAutomatedEntry-accountNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="accountNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Account" />
    <property name="shortLabel" value="ACCT" />
    <property name="maxLength" value="7" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="7" />
    </property>
  </bean>
  <bean id="IcrAutomatedEntry-subAccountNumber" parent="IcrAutomatedEntry-subAccountNumber-parentBean" />

  <bean id="IcrAutomatedEntry-subAccountNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="subAccountNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Sub-Account" />
    <property name="shortLabel" value="SubAcct" />
    <property name="maxLength" value="5" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="5" />
    </property>
  </bean>
  <bean id="IcrAutomatedEntry-financialObjectCode" parent="IcrAutomatedEntry-financialObjectCode-parentBean" />

  <bean id="IcrAutomatedEntry-financialObjectCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialObjectCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Object Code" />
    <property name="shortLabel" value="Object" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
  </bean>
  <bean id="IcrAutomatedEntry-financialSubObjectCode" parent="IcrAutomatedEntry-financialSubObjectCode-parentBean" />

  <bean id="IcrAutomatedEntry-financialSubObjectCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialSubObjectCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Sub-Object Code" />
    <property name="shortLabel" value="SubObject" />
    <property name="maxLength" value="3" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="3" />
    </property>
  </bean>
  <bean id="IcrAutomatedEntry-offsetBalanceSheetObjectCodeNumber" parent="IcrAutomatedEntry-offsetBalanceSheetObjectCodeNumber-parentBean" />

  <bean id="IcrAutomatedEntry-offsetBalanceSheetObjectCodeNumber-parentBean" abstract="true" parent="ObjectCode-financialObjectCode">
    <property name="name" value="offsetBalanceSheetObjectCodeNumber" />
    <property name="label" value="Offset Object" />
    <property name="shortLabel" value="Offset Object" />
  </bean>
  <bean id="IcrAutomatedEntry-transactionDebitIndicator" parent="IcrAutomatedEntry-transactionDebitIndicator-parentBean" />

  <bean id="IcrAutomatedEntry-transactionDebitIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="transactionDebitIndicator" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Transaction Debit Credit Code" />
    <property name="shortLabel" value="D/C" />
    <property name="maxLength" value="1" />
    <property name="validationPattern" >
      <bean parent="CharsetValidationPattern"
            p:validChars="CD" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.kfs.lookup.keyvalues.DebitCreditValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
  </bean>
  <bean id="IcrAutomatedEntry-awardIndrCostRcvyRatePct" parent="IcrAutomatedEntry-awardIndrCostRcvyRatePct-parentBean" />

  <bean id="IcrAutomatedEntry-awardIndrCostRcvyRatePct-parentBean" abstract="true" parent="GenericAttributes-percent">
    <property name="name" value="awardIndrCostRcvyRatePct" />
    <property name="label" value="ICR Percent" />
    <property name="shortLabel" value="Rate%" />
    <property name="required" value="true" />
  </bean>
  <bean id="IcrAutomatedEntry-versionNumber" parent="IcrAutomatedEntry-versionNumber-parentBean" />

  <bean id="IcrAutomatedEntry-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="IcrAutomatedEntry-inquiryDefinition" parent="IcrAutomatedEntry-inquiryDefinition-parentBean" />

  <bean id="IcrAutomatedEntry-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Indirect Cost Recovery Automated Entry Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="universityFiscalYear" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialIcrSeriesIdentifier" />
              <bean parent="FieldDefinition"
                    p:attributeName="balanceTypeCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="awardIndrCostRcvyEntryNbr" />
              <bean parent="FieldDefinition"
                    p:attributeName="chartOfAccountsCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="accountNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="subAccountNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialObjectCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialSubObjectCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="offsetBalanceSheetObjectCodeNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="transactionDebitIndicator" />
              <bean parent="FieldDefinition"
                    p:attributeName="awardIndrCostRcvyRatePct" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="IcrAutomatedEntry-lookupDefinition" parent="IcrAutomatedEntry-lookupDefinition-parentBean" />

  <bean id="IcrAutomatedEntry-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Indirect Cost Recovery Automated Entry Lookup" />
    <property name="instructions" value="Lookup an Indirect Cost Recovery Automated Entry" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>accountNumber</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalYear" />
        <bean parent="FieldDefinition"
              p:attributeName="financialIcrSeriesIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="balanceTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="accountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="offsetBalanceSheetObjectCodeNumber" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalYear" />
        <bean parent="FieldDefinition"
              p:attributeName="financialIcrSeriesIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="balanceTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="accountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="offsetBalanceSheetObjectCodeNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="financialSubObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="subAccountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="awardIndrCostRcvyEntryNbr" />
        <bean parent="FieldDefinition"
              p:attributeName="transactionDebitIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="awardIndrCostRcvyRatePct" />
      </list>
    </property>
  </bean>
</beans>
