<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="ProposalSubcontractor" parent="ProposalSubcontractor-parentBean" />

  <bean id="ProposalSubcontractor-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.cg.bo.ProposalSubcontractor" />
    <property name="inquiryDefinition" >
      <ref bean="ProposalSubcontractor-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="ProposalSubcontractor-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="ProposalSubcontractor-proposalNumber" />
        <ref bean="ProposalSubcontractor-subcontractorNumber" />
        <ref bean="ProposalSubcontractor-proposalSubcontractorAmount" />
        <ref bean="ProposalSubcontractor-proposalSubcontractorNumber" />
        <ref bean="ProposalSubcontractor-proposalSubcontractorDescription" />
        <ref bean="ProposalSubcontractor-subcontractor.subcontractorName" />
        <ref bean="ProposalSubcontractor-newCollectionRecord" />
        <ref bean="ProposalSubcontractor-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="ProposalSubcontractor-proposalNumber" parent="ProposalSubcontractor-proposalNumber-parentBean" />

  <bean id="ProposalSubcontractor-proposalNumber-parentBean" abstract="true" parent="Proposal-proposalNumber">
  </bean>
  <bean id="ProposalSubcontractor-subcontractorNumber" parent="ProposalSubcontractor-subcontractorNumber-parentBean" />

  <bean id="ProposalSubcontractor-subcontractorNumber-parentBean" abstract="true" parent="Subcontractor-subcontractorNumber">
  </bean>
  <bean id="ProposalSubcontractor-proposalSubcontractorAmount" parent="ProposalSubcontractor-proposalSubcontractorAmount-parentBean" />

  <bean id="ProposalSubcontractor-proposalSubcontractorAmount-parentBean" abstract="true" parent="GenericAttributes-genericAmount">
    <property name="name" value="proposalSubcontractorAmount" />
    <property name="description" value="Proposal Subcontractor Amount" />
    <property name="formatterClass" value="org.kuali.core.web.format.CurrencyFormatter" />
  </bean>
  <bean id="ProposalSubcontractor-proposalSubcontractorNumber" parent="ProposalSubcontractor-proposalSubcontractorNumber-parentBean" />

  <bean id="ProposalSubcontractor-proposalSubcontractorNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="proposalSubcontractorNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Proposal Subcontractor Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
  </bean>
  <bean id="ProposalSubcontractor-proposalSubcontractorDescription" parent="ProposalSubcontractor-proposalSubcontractorDescription-parentBean" />

  <bean id="ProposalSubcontractor-proposalSubcontractorDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="proposalSubcontractorDescription" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Description" />
    <property name="shortLabel" value="Description" />
    <property name="maxLength" value="30" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true"
            p:maxLength="30" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="30" />
    </property>
  </bean>
  <bean id="ProposalSubcontractor-subcontractor.subcontractorName" parent="ProposalSubcontractor-subcontractor.subcontractorName-parentBean" />

  <bean id="ProposalSubcontractor-subcontractor.subcontractorName-parentBean" abstract="true" parent="Subcontractor-subcontractorName">
    <property name="name" value="subcontractor.subcontractorName" />
    <property name="label" value="Name" />
  </bean>
  <bean id="ProposalSubcontractor-newCollectionRecord" parent="ProposalSubcontractor-newCollectionRecord-parentBean" />

  <bean id="ProposalSubcontractor-newCollectionRecord-parentBean" abstract="true" parent="GenericAttributes-newCollectionRecord">
  </bean>
  <bean id="ProposalSubcontractor-versionNumber" parent="ProposalSubcontractor-versionNumber-parentBean" />

  <bean id="ProposalSubcontractor-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="ProposalSubcontractor-inquiryDefinition" parent="ProposalSubcontractor-inquiryDefinition-parentBean" />

  <bean id="ProposalSubcontractor-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Proposal Subcontractor Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Proposal Subcontractor Details" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="proposalNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="proposalSubcontractorAmount" />
              <bean parent="FieldDefinition"
                    p:attributeName="proposalSubcontractorNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="subcontractorNumber" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="ProposalSubcontractor-lookupDefinition" parent="ProposalSubcontractor-lookupDefinition-parentBean" />

  <bean id="ProposalSubcontractor-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Proposal Subcontractor Lookup" />
    <property name="instructions" value="Look up a Proposal Subcontractor" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="proposalNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="proposalSubcontractorAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="proposalSubcontractorNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="subcontractorNumber" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="proposalNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="proposalSubcontractorAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="proposalSubcontractorNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="subcontractorNumber" />
      </list>
    </property>
  </bean>
</beans>
