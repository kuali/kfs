<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="Budget" parent="Budget-parentBean" />

  <bean id="Budget-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.kra.budget.bo.Budget" />
    <property name="inquiryDefinition" >
      <ref bean="Budget-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="Budget-lookupDefinition" />
    </property>
    <property name="objectLabel" value="Budget" />
    <property name="objectDescription" value="Help description for budget parameters - general." />
    <property name="attributes" >
      <list>
        <ref bean="Budget-documentNumber" />
        <ref bean="Budget-budgetParentTrackNumber" />
        <ref bean="Budget-budgetName" />
        <ref bean="Budget-institutionCostShareIndicator" />
        <ref bean="Budget-budgetThirdPartyCostShareIndicator" />
        <ref bean="Budget-electronicResearchAdministrationGrantNumber" />
        <ref bean="Budget-budgetProgramAnnouncementNumber" />
        <ref bean="Budget-budgetProgramAnnouncementName" />
        <ref bean="Budget-budgetTypeCodeText" />
        <ref bean="Budget-budgetPersonnelInflationRate" />
        <ref bean="Budget-budgetNonpersonnelInflationRate" />
        <ref bean="Budget-budgetFringeRateDescription" />
        <ref bean="Budget-routeSheetTrackNumber" />
        <ref bean="Budget-agencyModularIndicator" />
        <ref bean="Budget-budgetAgency" />
        <ref bean="Budget-budgetAgencyNumber" />
        <ref bean="Budget-federalPassThroughAgency" />
        <ref bean="Budget-budgetProjectDirectorUniversalIdentifier" />
        <ref bean="Budget-projectDirector.universalUser.personUserIdentifier" />
        <ref bean="Budget-projectDirector.universalUser.personName" />
        <ref bean="Budget-universalUser.personUserIdentifier" />
        <ref bean="Budget-universalUser.personName" />
        <ref bean="Budget-budgetAgency.agencyNumber" />
        <ref bean="Budget-budgetAgency.fullName" />
      </list>
    </property>
    <property name="collections" >
      <list>
        <bean parent="CollectionDefinition"
              p:name="tasks"
              p:label="Tasks"
              p:shortLabel="Tasks"
              p:summary="Budget Tasks"
              p:description="Tasks that occur as part of the budget" />
        <bean parent="CollectionDefinition"
              p:name="periods"
              p:label="Periods"
              p:shortLabel="Period"
              p:summary="Budget Periods"
              p:description="Periods that occur during of the budget" />
      </list>
    </property>
    <property name="relationships" >
      <list>
        <bean parent="RelationshipDefinition">
          <property name="objectAttributeName" value="universalUser" />
          <property name="primitiveAttributes" >
            <list>
              <bean parent="PrimitiveAttributeDefinition"
                    p:sourceName="budgetProjectDirectorUniversalIdentifier"
                    p:targetName="personUniversalIdentifier" />
            </list>
          </property>
          <property name="supportAttributes" >
            <list>
              <bean parent="SupportAttributeDefinition"
                    p:identifier="true"
                    p:sourceName="universalUser.personUserIdentifier"
                    p:targetName="personUserIdentifier" />
              <bean parent="SupportAttributeDefinition"
                    p:sourceName="universalUser.personName"
                    p:targetName="personName" />
            </list>
          </property>
        </bean>
        <bean parent="RelationshipDefinition">
          <property name="objectAttributeName" value="budgetAgency" />
          <property name="primitiveAttributes" >
            <list>
              <bean parent="PrimitiveAttributeDefinition"
                    p:sourceName="budgetAgencyNumber"
                    p:targetName="agencyNumber" />
            </list>
          </property>
          <property name="supportAttributes" >
            <list>
              <bean parent="SupportAttributeDefinition"
                    p:identifier="true"
                    p:sourceName="budgetAgency.fullName"
                    p:targetName="fullName" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


