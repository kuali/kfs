<!-- Business Object Inquiry Definition -->


  <bean id="OrganizationAccountingDefault-inquiryDefinition" parent="OrganizationAccountingDefault-inquiryDefinition-parentBean" />

  <bean id="OrganizationAccountingDefault-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Organization Accounting Default Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Organization Accounting Defaults" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="universityFiscalYear" />
              <bean parent="FieldDefinition"
                    p:attributeName="chartOfAccountsCode" />    
              <bean parent="FieldDefinition"
                    p:attributeName="organizationCode" />
            </list>
          </property>
        </bean>
		<bean parent="InquirySectionDefinition">
          <property name="title" value="Organization Income Account Defaults" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="defaultInvoiceChartOfAccountsCode" />
			  <bean parent="FieldDefinition"
                    p:attributeName="defaultInvoiceAccountNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="defaultInvoiceSubAccountNumber" />                                        
              <bean parent="FieldDefinition"
                    p:attributeName="defaultInvoiceFinancialObjectCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="defaultInvoiceFinancialSubObjectCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="defaultInvoiceProjectCode" />                    
              <bean parent="FieldDefinition"
                    p:attributeName="defaultInvoiceOrganizationReferenceIdentifier" />
            </list>
          </property>
        </bean>
		<bean parent="InquirySectionDefinition">
          <property name="title" value="Organization Receivable Account Defaults" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="defaultPaymentChartOfAccountsCode" />
 			  <bean parent="FieldDefinition"
                    p:attributeName="defaultPaymentAccountNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="defaultPaymentSubAccountNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="defaultPaymentFinancialObjectCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="defaultPaymentFinancialSubObjectCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="defaultPaymentProjectCode" />                    
              <bean parent="FieldDefinition"
                    p:attributeName="defaultPaymentOrganizationReferenceIdentifier" />
            </list>
          </property>
        </bean>
		<bean parent="InquirySectionDefinition">
          <property name="title" value="Organization Writeoff Account Defaults" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="writeoffChartOfAccountsCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="writeoffAccountNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="writeoffSubAccountNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="writeoffFinancialObjectCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="writeoffFinancialSubObjectCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="writeoffProjectCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="writeoffOrganizationReferenceIdentifier" />
            </list>
          </property>
        </bean>                        
      </list>
    </property>
    <property name="inquirableClass" value="org.kuali.module.ar.maintenance.OrganizationAccountingDefaultInquirable" />
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="OrganizationAccountingDefault-lookupDefinition" parent="OrganizationAccountingDefault-lookupDefinition-parentBean" />

  <bean id="OrganizationAccountingDefault-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Organization Accounting Default Lookup" />
    <property name="instructions" value="Look up an Organization Accounting Default" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalYear" />
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationCode" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="organizationAccountingDefaultViewer" />
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationCode" />
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalYear" />
      </list>
    </property>
  </bean>
</beans>
