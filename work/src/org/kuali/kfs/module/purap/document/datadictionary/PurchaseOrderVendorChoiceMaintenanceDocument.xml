<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="PurchaseOrderVendorChoiceMaintenanceDocument" parent="PurchaseOrderVendorChoiceMaintenanceDocument-parentBean" />

  <bean id="PurchaseOrderVendorChoiceMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.purap.bo.PurchaseOrderVendorChoice" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="PurchaseOrderVendorChoiceMaintenanceDocument-EditPOVendorChoice" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>purchaseOrderVendorChoiceCode</value>
      </list>
    </property>
    <property name="label" value="Purchase Order Vendor Choice" />
    <property name="shortLabel" value="Purchase Order Vendor Choice" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-PA"
            p:parameterName="PURCHASE_ORDER_VENDOR_CHOICE"
            p:parameterDetailType="PurchaseOrderVendorChoice" />
    </property>
    <property name="documentTypeName" value="PurchaseOrderVendorChoiceMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="PMVC" />
    <property name="workflowProperties" >
      <ref bean="PurchaseOrderVendorChoiceMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>PA_PUR_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="PurchaseOrderVendorChoiceMaintenanceDocument-EditPOVendorChoice" parent="PurchaseOrderVendorChoiceMaintenanceDocument-EditPOVendorChoice-parentBean" />

  <bean id="PurchaseOrderVendorChoiceMaintenanceDocument-EditPOVendorChoice-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit PO Vendor Choice" />
    <property name="title" value="Edit PO Vendor Choice" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="purchaseOrderVendorChoiceCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="purchaseOrderVendorChoiceDescription"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="active" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="PurchaseOrderVendorChoiceMaintenanceDocument-workflowProperties" parent="PurchaseOrderVendorChoiceMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="PurchaseOrderVendorChoiceMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
