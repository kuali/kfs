<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../core/datadictionary/dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kfs.bo.Building">


        <inquiry>
            <title>Building Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="campusCode" />
                <field attributeName="buildingCode" />
                <field attributeName="buildingName" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Building Lookup</title>

            <instructions>Lookup a Building</instructions>

            <defaultSort attributeName="buildingCode" sortAscending="true" />

            <lookupFields>
                <lookupField attributeName="campusCode" />
                <lookupField attributeName="buildingCode" />
                <lookupField attributeName="buildingName" />
            </lookupFields>

            <resultFields>
                <field attributeName="campusCode" forceInquiry="true" />
                <field attributeName="buildingCode" />
                <field attributeName="buildingName" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="campusCode" forceUppercase="true">
                <label>Campus Code</label>
                <shortLabel>Campus Code</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern>
                    <alphaNumeric />
                </validationPattern>
                <required>true</required>

                <control>
                    <text size="2" />
                </control>

                <summary>Campus Code</summary>
                <description>The code uniquely identifying a particular campus.</description>
            </attribute>

            <attribute name="buildingCode" forceUppercase="true">
                <label>Building Code</label>
                <shortLabel>Building Code</shortLabel>

                <maxLength>10</maxLength>
                <validationPattern>
                    <alphaNumeric />
                </validationPattern>
                <required>true</required>

                <control>
                    <text size="10" />
                </control>

                <summary>Building Code</summary>
                <description>The code uniquely identifying a particular building.</description>
            </attribute>

            <attribute name="buildingName" forceUppercase="false">
                <label>Building Name</label>
                <shortLabel>Name</shortLabel>

                <maxLength>40</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>
                <required>true</required>

                <control>
                    <text size="40" />
                </control>

                <summary>Building Name</summary>
                <description>Building Name</description>
            </attribute>
            
            <attribute name="buildingStreetAddress">
            	<label>Building Street Address</label>
            	<shortLabel>Bldg. Street Addr.</shortLabel>
            	
            	<maxLength>40</maxLength>
            	<validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>
            	
            	<control>
            		<text size="40"/>
            	</control>
            	
            	<summary>buildingStreetAddress</summary>
            	<description>buildingStreetAddress description...</description>
            </attribute>
            
            <attribute name="buildingAddressCityName">
            	<label>Building Address City Name</label>
            	<shortLabel>Bldg. City</shortLabel>
            	
            	<maxLength>37</maxLength>
            	<validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>
            	
            	<control>
            		<text size="37"/>
            	</control>
            	
            	<summary>buildingAddressCityName</summary>
            	<description>buildingAddressCityName description...</description>
            </attribute>
            
            <attribute name="buildingAddressStateCode" forceUppercase="true">
            	<label>Building Address State Code</label>
            	<shortLabel>Bldg. State</shortLabel>
            	
            	<maxLength>2</maxLength>
            	<validationPattern><anyCharacter exactLength="2" /></validationPattern>
            	
            	<control>
            		<select valuesFinderClass="org.kuali.kfs.lookup.keyvalues.StateValuesFinder"/>
            	</control>
            	
            	<summary>buildingAddressStateCode</summary>
                <description>buildingAddressStateCode description...</description>
            </attribute>
            
            <attribute name="buildingAddressZipCode">
            	<label>Building Address Zip Code</label>
            	<shortLabel>Bldg. Zip</shortLabel>
            	
            	<maxLength>9</maxLength>
            	<validationPattern><zipcode /></validationPattern>
            	
            	<control>
            		<text size="9"/>
            	</control>
            	
            	<summary>buildingAddressZipCode</summary>
                <description>buildingAddressZipCode description...</description>
            </attribute>
            
            <attribute name="alternateBuildingCode">
            	<label>Alternate Building Code</label>
            	<shortLabel>Alt. Bldg. Code</shortLabel>
            	
            	<maxLength>4</maxLength>
            	<validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>
            	
            	<control>
            		<text size="4"/>
            	</control>
            	
            	<summary>alternateBuildingCode</summary>
                <description>alternateBuildingCode description...</description>
            </attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>

