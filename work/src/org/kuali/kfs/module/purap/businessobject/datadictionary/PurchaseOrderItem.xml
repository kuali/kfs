<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../../core/datadictionary/dataDictionary-1_0.dtd" [
<!ENTITY purApItemBaseAttributes SYSTEM "../dictionary/entity/PurApItemBaseAttributes.xml">
<!ENTITY purchasingItemBaseAttributes SYSTEM "../dictionary/entity/PurchasingItemBaseAttributes.xml">
]>

<dictionaryEntry>
	<businessObject businessObjectClass="org.kuali.module.purap.bo.PurchaseOrderItem">


		<inquiry>
			<title>Purchase Order Item Inquiry</title>

			<inquirySections>
				<inquirySection title="Header Text">
					<inquiryFields>
						<field attributeName="capitalAssetTransactionTypeCode" />
						<field attributeName="externalOrganizationB2bProductReferenceNumber" />
						<field attributeName="externalOrganizationB2bProductTypeName" />
						<field attributeName="itemActiveIndicator" />
						<field attributeName="itemAssignedToTradeInIndicator" />
						<field attributeName="itemAuxiliaryPartIdentifier" />
						<field attributeName="itemCapitalAssetNoteText" />
						<field attributeName="itemCatalogNumber" />
						<field attributeName="itemDescription" />
						<field attributeName="itemInvoicedTotalAmount" />
						<field attributeName="itemInvoicedTotalQuantity" />
						<field attributeName="itemLineNumber" />
						<field attributeName="itemQuantity" />
						<field attributeName="itemOutstandingEncumberedQuantity" />
						<field attributeName="itemOutstandingEncumbranceAmount" />
						<field attributeName="itemReceivedTotalQuantity" />
						<field attributeName="itemReturnedTotalQuantity" />
						<field attributeName="itemTypeCode" />
						<field attributeName="itemUnitOfMeasureCode" />
						<field attributeName="itemUnitPrice" />
						<field attributeName="purchaseOrderCommodityCd" />
						<field attributeName="itemIdentifier" />
						<field attributeName="requisitionLineIdentifier" />
					</inquiryFields>
				</inquirySection>
			</inquirySections>
		</inquiry>

		<lookup>
			<title>Purchase Order Item Lookup</title>

			<instructions>Lookup an Purchase Order Item</instructions>

			<defaultSort sortAscending="true">
				<sortAttributes>
					<sortAttribute attributeName="itemDescription" />
				</sortAttributes>
			</defaultSort>

			<lookupFields>
				<lookupField attributeName="capitalAssetTransactionTypeCode" required="false" />
				<lookupField attributeName="externalOrganizationB2bProductReferenceNumber" required="false" />
				<lookupField attributeName="externalOrganizationB2bProductTypeName" required="false" />
				<lookupField attributeName="itemActiveIndicator" required="false" />
				<lookupField attributeName="itemAssignedToTradeInIndicator" required="false" />
				<lookupField attributeName="itemAuxiliaryPartIdentifier" required="false" />
				<lookupField attributeName="itemCapitalAssetNoteText" required="false" />
				<lookupField attributeName="itemCatalogNumber" required="false" />
				<lookupField attributeName="itemDescription" required="false" />
				<lookupField attributeName="itemInvoicedTotalAmount" required="false" />
				<lookupField attributeName="itemInvoicedTotalQuantity" required="false" />
				<lookupField attributeName="itemLineNumber" required="false" />
				<lookupField attributeName="itemQuantity" required="false" />
				<lookupField attributeName="itemOutstandingEncumberedQuantity" required="false" />
				<lookupField attributeName="itemOutstandingEncumbranceAmount" required="false" />
				<lookupField attributeName="itemReceivedTotalQuantity" required="false" />
				<lookupField attributeName="itemReturnedTotalQuantity" required="false" />
				<lookupField attributeName="itemTypeCode" required="false" />
				<lookupField attributeName="itemUnitOfMeasureCode" required="false" />
				<lookupField attributeName="itemUnitPrice" required="false" />
				<lookupField attributeName="purchaseOrderCommodityCd" required="false" />
				<lookupField attributeName="itemIdentifier" required="false" />
				<lookupField attributeName="requisitionLineIdentifier" required="false" />
			</lookupFields>

			<resultFields>
				<field attributeName="capitalAssetTransactionTypeCode" />
				<field attributeName="externalOrganizationB2bProductReferenceNumber" />
				<field attributeName="externalOrganizationB2bProductTypeName" />
				<field attributeName="itemActiveIndicator" />
				<field attributeName="itemAssignedToTradeInIndicator" />
				<field attributeName="itemAuxiliaryPartIdentifier" />
				<field attributeName="itemCapitalAssetNoteText" />
				<field attributeName="itemCatalogNumber" />
				<field attributeName="itemDescription" />
				<field attributeName="itemInvoicedTotalAmount" />
				<field attributeName="itemInvoicedTotalQuantity" />
				<field attributeName="itemLineNumber" />
				<field attributeName="itemQuantity" />
				<field attributeName="itemOutstandingEncumberedQuantity" />
				<field attributeName="itemOutstandingEncumbranceAmount" />
				<field attributeName="itemReceivedTotalQuantity" />
				<field attributeName="itemReturnedTotalQuantity" />
				<field attributeName="itemTypeCode" />
				<field attributeName="itemUnitOfMeasureCode" />
				<field attributeName="itemUnitPrice" />
				<field attributeName="purchaseOrderCommodityCd" />
				<field attributeName="itemIdentifier" />
				<field attributeName="requisitionLineIdentifier" />
			</resultFields>
		</lookup>

		<attributes>

			&purApItemBaseAttributes;
			&purchasingItemBaseAttributes;

			<attribute name="itemActiveIndicator" forceUppercase="true">
				<label>Item Active Indicator</label>
				<shortLabel>Indicator</shortLabel>

				<maxLength>1</maxLength>

				<control>
					<checkbox />
				</control>

				<summary>itemActiveIndicator</summary>
				<description>itemActiveIndicator description...</description>
			</attribute>

			<attributeReference name="itemInvoicedTotalAmount" sourceClassName="org.kuali.module.purap.bo.PurApAttributeReferenceDummy" sourceAttributeName="genericItemAmount">
				<label>Item Invoiced Total Amount</label>
				<shortLabel>Amount</shortLabel>
				<summary>itemInvoicedTotalAmount</summary>
				<description>itemInvoicedTotalAmount description...</description>
			</attributeReference>
			
			<attributeReference name="itemInvoicedTotalQuantity" sourceClassName="org.kuali.module.purap.bo.PurApAttributeReferenceDummy" sourceAttributeName="genericItemQuantity">
				<label>Item Invoiced Total Quantity</label>
				<shortLabel>Quantity</shortLabel>
				<summary>itemInvoicedTotalQuantity</summary>
				<description>itemInvoicedTotalQuantity description...</description>
			</attributeReference>

			<attributeReference name="itemOutstandingEncumberedQuantity" sourceClassName="org.kuali.module.purap.bo.PurApAttributeReferenceDummy" sourceAttributeName="genericItemQuantity">
				<label>Item Outstanding Encumbered Quantity</label>
				<shortLabel>Outstanding Encumbered Quantity</shortLabel>
				<summary>itemOutstandingEncumberedQuantity</summary>
				<description>itemOutstandingEncumberedQuantity description...</description>
			</attributeReference>

			<attributeReference name="itemOutstandingEncumbranceAmount" sourceClassName="org.kuali.module.purap.bo.PurApAttributeReferenceDummy" sourceAttributeName="genericItemAmount">
				<label>Item Outstanding Encumbrance Amount</label>
				<shortLabel>Outstanding Encumbrance Amount</shortLabel>
				<summary>itemOutstandingEncumbranceAmount</summary>
				<description>itemOutstandingEncumbranceAmount description...</description>
			</attributeReference>
			
			<attributeReference name="itemReceivedTotalQuantity" sourceClassName="org.kuali.module.purap.bo.PurApAttributeReferenceDummy" sourceAttributeName="genericItemQuantity">
				<label>Item Received Total Quantity</label>
				<shortLabel>Received Total Quantity</shortLabel>
				<summary>itemReceivedTotalQuantity</summary>
				<description>itemReceivedTotalQuantity description...</description>
			</attributeReference>

			<attributeReference name="itemReturnedTotalQuantity" sourceClassName="org.kuali.module.purap.bo.PurApAttributeReferenceDummy" sourceAttributeName="genericItemQuantity">
				<label>Item Returned Total Quantity</label>
				<shortLabel>Returned Total Quantity</shortLabel>
				<summary>itemReturnedTotalQuantity</summary>
				<description>itemReturnedTotalQuantity description...</description>
			</attributeReference>

			<attribute name="purchaseOrderCommodityCd" forceUppercase="true">
				<label>Purchase Order Commodity Cd</label>
				<shortLabel>Purchase Order Commodity Cd</shortLabel>

				<maxLength>11</maxLength>
				<validationPattern>
					<anyCharacter />
				</validationPattern>

				<control>
					<text size="13" />
				</control>

				<summary>purchaseOrderCommodityCd</summary>
				<description>purchaseOrderCommodityCd description...</description>
			</attribute>

			<attribute name="itemSelectedForRetransmitIndicator" forceUppercase="true">
				<label>Item Selected For Retransmit Indicator</label>
				<shortLabel>Selected For Retransmit</shortLabel>

				<maxLength>1</maxLength>

				<control>
					<checkbox />
				</control>

				<summary>itemSelectedForRetransmitIndicator</summary>
				<description>itemSelectedForRetransmitIndicator description...</description>
			</attribute>

		</attributes>
	</businessObject>
</dictionaryEntry>
