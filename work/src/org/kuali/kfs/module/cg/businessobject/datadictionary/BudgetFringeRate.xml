<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="BudgetFringeRate" parent="BudgetFringeRate-parentBean" />

  <bean id="BudgetFringeRate-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.kra.budget.bo.BudgetFringeRate" />
    <property name="inquiryDefinition" >
      <ref bean="BudgetFringeRate-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="BudgetFringeRate-lookupDefinition" />
    </property>
    <property name="objectLabel" value="Budget Fringe Rates" />
    <property name="objectDescription" value="Fringe rates for the Budget." />
    <property name="attributes" >
      <list>
        <ref bean="BudgetFringeRate-documentNumber" />
        <ref bean="BudgetFringeRate-institutionAppointmentTypeCode" />
        <ref bean="BudgetFringeRate-contractsAndGrantsFringeRateAmount" />
        <ref bean="BudgetFringeRate-institutionCostShareFringeRateAmount" />
        <ref bean="BudgetFringeRate-budgetLastUpdateTimestamp" />
        <ref bean="BudgetFringeRate-objectId" />
        <ref bean="BudgetFringeRate-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="BudgetFringeRate-documentNumber" parent="BudgetFringeRate-documentNumber-parentBean" />

  <bean id="BudgetFringeRate-documentNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="documentNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Budget Number" />
    <property name="shortLabel" value="Budget Number" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="8" />
    </property>
    <property name="summary" value="Budget Number..." />
    <property name="description" value="Budget Number..." />
  </bean>
  <bean id="BudgetFringeRate-institutionAppointmentTypeCode" parent="BudgetFringeRate-institutionAppointmentTypeCode-parentBean" />

  <bean id="BudgetFringeRate-institutionAppointmentTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="institutionAppointmentTypeCode" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Appointment Type Code" />
    <property name="shortLabel" value="Appointment Type Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="2" />
    </property>
    <property name="summary" value="Appointment Type Code" />
    <property name="description" value="Appointment Type Code..." />
  </bean>
  <bean id="BudgetFringeRate-contractsAndGrantsFringeRateAmount" parent="BudgetFringeRate-contractsAndGrantsFringeRateAmount-parentBean" />

  <bean id="BudgetFringeRate-contractsAndGrantsFringeRateAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="contractsAndGrantsFringeRateAmount" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Contract and Grants Fringe Rate Amount" />
    <property name="shortLabel" value="C&amp;G Fringe Rate" />
    <property name="maxLength" value="6" />
    <property name="validationPattern" >
      <bean parent="FloatingPointValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
    <property name="summary" value="Cost Share Fringe Rate Amount" />
    <property name="description" value="Cost Share Fringe Rate Amount..." />
  </bean>
  <bean id="BudgetFringeRate-institutionCostShareFringeRateAmount" parent="BudgetFringeRate-institutionCostShareFringeRateAmount-parentBean" />

  <bean id="BudgetFringeRate-institutionCostShareFringeRateAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="institutionCostShareFringeRateAmount" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Cost Share Fringe Rate Amount" />
    <property name="shortLabel" value="Fringe Rate" />
    <property name="maxLength" value="6" />
    <property name="validationPattern" >
      <bean parent="FloatingPointValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
    <property name="summary" value="Cost Share Fringe Rate Amount" />
    <property name="description" value="Cost Share Fringe Rate Amount..." />
  </bean>
  <bean id="BudgetFringeRate-budgetLastUpdateTimestamp" parent="BudgetFringeRate-budgetLastUpdateTimestamp-parentBean" />

  <bean id="BudgetFringeRate-budgetLastUpdateTimestamp-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="budgetLastUpdateTimestamp" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Budget Last Update Timestamp" />
    <property name="shortLabel" value="Last Update" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="DateValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
    <property name="summary" value="Budget Last Update Timestamp" />
    <property name="description" value="Budget Last Update Timestamp..." />
  </bean>
  <bean id="BudgetFringeRate-objectId" parent="BudgetFringeRate-objectId-parentBean" />

  <bean id="BudgetFringeRate-objectId-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="objectId" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Object Id" />
    <property name="shortLabel" value="GUID" />
    <property name="maxLength" value="36" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="36" />
    </property>
    <property name="summary" value="objectId summary..." />
    <property name="description" value="objectId description..." />
  </bean>
  <bean id="BudgetFringeRate-versionNumber" parent="BudgetFringeRate-versionNumber-parentBean" />

  <bean id="BudgetFringeRate-versionNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="versionNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Version Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="-1" />
    </property>
    <property name="summary" value="versionNumber summary..." />
    <property name="description" value="versionNumber description..." />
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="BudgetFringeRate-inquiryDefinition" parent="BudgetFringeRate-inquiryDefinition-parentBean" />

  <bean id="BudgetFringeRate-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Budget Fringe Rate Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="documentNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="institutionAppointmentTypeCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="contractsAndGrantsFringeRateAmount" />
              <bean parent="FieldDefinition"
                    p:attributeName="institutionCostShareFringeRateAmount" />
              <bean parent="FieldDefinition"
                    p:attributeName="budgetLastUpdateTimestamp" />
              <bean parent="FieldDefinition"
                    p:attributeName="objectId" />
              <bean parent="FieldDefinition"
                    p:attributeName="versionNumber" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="BudgetFringeRate-lookupDefinition" parent="BudgetFringeRate-lookupDefinition-parentBean" />

  <bean id="BudgetFringeRate-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Budget Fringe Rate Lookup" />
    <property name="instructions" value="Lookup a Budget Fringe Rate" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>documentNumber</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="institutionAppointmentTypeCode" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="institutionAppointmentTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="contractsAndGrantsFringeRateAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="institutionCostShareFringeRateAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="budgetLastUpdateTimestamp" />
        <bean parent="FieldDefinition"
              p:attributeName="objectId" />
        <bean parent="FieldDefinition"
              p:attributeName="versionNumber" />
      </list>
    </property>
  </bean>
</beans>
