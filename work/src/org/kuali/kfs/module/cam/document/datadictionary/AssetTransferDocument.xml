  <bean id="AssetTransferDocument-organizationText" parent="AssetTransferDocument-organizationText-parentBean" />

  <bean id="AssetTransferDocument-organizationText-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="organizationText" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Organization Text" />
    <property name="shortLabel" value="Text" />
    <property name="maxLength" value="255" />
