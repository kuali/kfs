<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="AchBankMaintenanceDocument" parent="AchBankMaintenanceDocument-parentBean" />

  <bean id="AchBankMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.pdp.bo.AchBank" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="AchBankMaintenanceDocument-EditACHBank" />
      </list>
    </property>
    <property name="defaultExistenceChecks" >
      <list>
        <bean parent="ReferenceDefinition"
              p:attributeName="bankState"
              p:attributeToHighlightOnFail="bankStateCode" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>bankRoutingNumber</value>
      </list>
    </property>
    <property name="label" value="ACH Bank" />
    <property name="shortLabel" value="AchBankMaintDoc" />
    <property name="summary" value="Ach bank maintenance document" />
    <property name="description" value="Document used to create or update Ach Bank objects" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-PD"
            p:parameterName="BANK"
            p:parameterDetailType="AchBank" />
    </property>
    <property name="documentTypeName" value="AchBankMaintenanceDocument" />
    <property name="businessRulesClass" value="org.kuali.module.pdp.rules.AchBankRule" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="ABNK" />
    <property name="workflowProperties" >
      <ref bean="AchBankMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>PD_VIEW_ACH</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="AchBankMaintenanceDocument-EditACHBank" parent="AchBankMaintenanceDocument-EditACHBank-parentBean" />

  <bean id="AchBankMaintenanceDocument-EditACHBank-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit ACH Bank" />
    <property name="title" value="Edit ACH Bank" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="bankRoutingNumber"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="bankOfficeCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="bankServiceNumber"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="bankTypeCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="bankNewRoutingNumber"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="bankName"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="bankStreetAddress"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="bankCityName"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="bankStateCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="bankZipCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="bankZipExtensionCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="bankPhoneAreaCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="bankPhonePrefixNumber"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="bankPhoneSuffixNumber"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="bankInstitutionStatusCode" />
        <bean parent="MaintainableFieldDefinition"
              p:name="bankDataViewCode" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="AchBankMaintenanceDocument-workflowProperties" parent="AchBankMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="AchBankMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
