<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xmlns:dd="http://rice.kuali.org/dd" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd         http://rice.kuali.org/dd         http://rice.kuali.org/dd/dd.xsd">

  <bean id="OrganizationTypeMaintenanceDocument" parent="OrganizationTypeMaintenanceDocument-parentBean"/>

  <bean id="OrganizationTypeMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.coa.businessobject.OrganizationType"/>
    <property name="maintainableClass" value="org.kuali.rice.kns.maintenance.KualiMaintainableImpl"/>
    <property name="maintainableSections">
      <list>
        <ref bean="OrganizationTypeMaintenanceDocument-EditOrganizationType"/>
      </list>
    </property>
    <property name="lockingKeys">
      <list>
        <value>organizationTypeCode</value>
      </list>
    </property>
    
    
    
    
    
    <property name="documentTypeName" value="ORTY"/>
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase"/>
    <property name="workflowAttributes">
    	<ref bean="OrganizationTypeMaintenanceDocument-workflowAttributes"/>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->

  <bean id="OrganizationTypeMaintenanceDocument-EditOrganizationType" parent="OrganizationTypeMaintenanceDocument-EditOrganizationType-parentBean"/>

  <bean id="OrganizationTypeMaintenanceDocument-EditOrganizationType-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Organization Type"/>
    <property name="title" value="Edit Organization Type"/>
    <property name="maintainableItems">
      <list>
        <bean parent="MaintainableFieldDefinition" p:name="organizationTypeCode" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="organizationTypeName" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:defaultValue="true" p:name="active"/>
      </list>
    </property>
  </bean>
  
  <!-- workflow attributes -->
  <bean id="OrganizationTypeMaintenanceDocument-workflowAttributes" parent="OrganizationTypeMaintenanceDocument-workflowAttributes-parentBean"/>
  
  <bean id="OrganizationTypeMaintenanceDocument-workflowAttributes-parentBean" class="org.kuali.rice.kns.datadictionary.WorkflowAttributes" abstract="true"/>
</beans>