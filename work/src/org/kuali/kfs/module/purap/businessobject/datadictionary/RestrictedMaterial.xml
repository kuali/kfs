<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="RestrictedMaterial" parent="RestrictedMaterial-parentBean" />

  <bean id="RestrictedMaterial-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.purap.bo.RestrictedMaterial" />
    <property name="inquiryDefinition" >
      <ref bean="RestrictedMaterial-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="RestrictedMaterial-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="restrictedMaterialCode" />
    <property name="objectLabel" value="Restricted Material" />
    <property name="attributes" >
      <list>
        <ref bean="RestrictedMaterial-restrictedMaterialCode" />
        <ref bean="RestrictedMaterial-restrictedMaterialDefaultDescription" />
        <ref bean="RestrictedMaterial-restrictedMaterialDescription" />
        <ref bean="RestrictedMaterial-restrictedMaterialWorkgroupName" />
        <ref bean="RestrictedMaterial-selected" />
        <ref bean="RestrictedMaterial-active" />
        <ref bean="RestrictedMaterial-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="RestrictedMaterial-restrictedMaterialCode" parent="RestrictedMaterial-restrictedMaterialCode-parentBean" />

  <bean id="RestrictedMaterial-restrictedMaterialCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="restrictedMaterialCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Restricted Material Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="6" />
    </property>
  </bean>
  <bean id="RestrictedMaterial-restrictedMaterialDefaultDescription" parent="RestrictedMaterial-restrictedMaterialDefaultDescription-parentBean" />

  <bean id="RestrictedMaterial-restrictedMaterialDefaultDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="restrictedMaterialDefaultDescription" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Restricted Material Default Description" />
    <property name="shortLabel" value="Description" />
    <property name="maxLength" value="45" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="47" />
    </property>
  </bean>
  <bean id="RestrictedMaterial-restrictedMaterialDescription" parent="RestrictedMaterial-restrictedMaterialDescription-parentBean" />

  <bean id="RestrictedMaterial-restrictedMaterialDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="restrictedMaterialDescription" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Restricted Material Description" />
    <property name="shortLabel" value="Description" />
    <property name="maxLength" value="45" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="47" />
    </property>
  </bean>
  <bean id="RestrictedMaterial-restrictedMaterialWorkgroupName" parent="RestrictedMaterial-restrictedMaterialWorkgroupName-parentBean" />

  <bean id="RestrictedMaterial-restrictedMaterialWorkgroupName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="restrictedMaterialWorkgroupName" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Restricted Material Workgroup Name" />
    <property name="shortLabel" value="Workgroup Name" />
    <property name="maxLength" value="45" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="WorkflowWorkgroupControlDefinition" />
    </property>
  </bean>
  <bean id="RestrictedMaterial-selected" parent="RestrictedMaterial-selected-parentBean" />

  <bean id="RestrictedMaterial-selected-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="selected" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Is Selected Indicator" />
    <property name="shortLabel" value="Is Selected" />
    <property name="maxLength" value="1" />
    <property name="control" >
      <bean parent="CheckboxControlDefinition" />
    </property>
  </bean>
  <bean id="RestrictedMaterial-active" parent="RestrictedMaterial-active-parentBean" />

  <bean id="RestrictedMaterial-active-parentBean" abstract="true" parent="AttributeReferenceDummy-activeIndicator">
    <property name="name" value="active" />
  </bean>
  <bean id="RestrictedMaterial-versionNumber" parent="RestrictedMaterial-versionNumber-parentBean" />

  <bean id="RestrictedMaterial-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="RestrictedMaterial-inquiryDefinition" parent="RestrictedMaterial-inquiryDefinition-parentBean" />

  <bean id="RestrictedMaterial-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Restricted Material Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="restrictedMaterialCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="restrictedMaterialDescription" />
              <bean parent="FieldDefinition"
                    p:attributeName="restrictedMaterialDefaultDescription" />
              <bean parent="FieldDefinition"
                    p:attributeName="restrictedMaterialWorkgroupName" />
              <bean parent="FieldDefinition"
                    p:attributeName="active" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="RestrictedMaterial-lookupDefinition" parent="RestrictedMaterial-lookupDefinition-parentBean" />

  <bean id="RestrictedMaterial-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Restricted Material Lookup" />
    <property name="instructions" value="Lookup an Restricted Material" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="restrictedMaterialCode" />
        <bean parent="FieldDefinition"
              p:attributeName="restrictedMaterialDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="restrictedMaterialDefaultDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="restrictedMaterialWorkgroupName" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="restrictedMaterialCode" />
        <bean parent="FieldDefinition"
              p:attributeName="restrictedMaterialDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="restrictedMaterialDefaultDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="restrictedMaterialWorkgroupName" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
  </bean>
</beans>
