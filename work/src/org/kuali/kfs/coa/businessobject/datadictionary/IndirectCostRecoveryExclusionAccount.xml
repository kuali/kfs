<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../core/datadictionary/dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.chart.bo.IndirectCostRecoveryExclusionAccount">


        <inquiry>
            <title>Indirect Cost Recovery Exclusion by Account Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="chartOfAccountsCode" />
                <field attributeName="accountNumber" />
                <field attributeName="financialObjectChartOfAccountCode" />
                <field attributeName="financialObjectCode" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Indirect Cost Recovery Exclusion by Account Lookup</title>

            <instructions>Lookup an Indirect Cost Recovery Exclusion by Account</instructions>

            <defaultSort attributeName="accountNumber" sortAscending="true" />

            <lookupFields>
                <lookupField attributeName="chartOfAccountsCode" required="false" />
                <lookupField attributeName="accountNumber" required="false" />
                <lookupField attributeName="financialObjectChartOfAccountCode" required="false" />
                <lookupField attributeName="financialObjectCode" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="chartOfAccountsCode" />
                <field attributeName="accountNumber" />
                <field attributeName="financialObjectChartOfAccountCode" />
                <field attributeName="financialObjectCode" />
            </resultFields>
        </lookup>

        <attributes>
            <attributeReference name="chartOfAccountsCode" sourceClassName="org.kuali.module.chart.bo.Chart" sourceAttributeName="chartOfAccountsCode">
                <required>true</required>
            </attributeReference>


            <attribute name="accountNumber" forceUppercase="false">
                <label>Account Number</label>
                <shortLabel>Acct</shortLabel>

                <maxLength>7</maxLength>
                <validationPattern>
                    <anyCharacter />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="7" />
                </control>

                <summary>Account Number</summary>
                <description>Account Number</description>
            </attribute>

            <attribute name="financialObjectCode" forceUppercase="true">
                <label>Object Code</label>
                <shortLabel>Object</shortLabel>

                <maxLength>4</maxLength>
                <required>true</required>
                <control>
                    <text size="4" />
                </control>

                <summary>Object Code</summary>
                <description>Object Code...</description>
            </attribute>


            <attributeReference name="financialObjectChartOfAccountCode" sourceClassName="org.kuali.module.chart.bo.Chart" sourceAttributeName="chartOfAccountsCode">
                <label>Object Chart</label>
                <shortLabel>Object Chart</shortLabel>
                <required>true</required>
               <control>
                    <text size="2" />
                </control>
                <summary>Object Chart</summary>
                <description>Object Chart...</description>
            </attributeReference>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
        
        <objectLabel>Indirect Cost Recovery Exclusion by Account</objectLabel>
    </businessObject>
