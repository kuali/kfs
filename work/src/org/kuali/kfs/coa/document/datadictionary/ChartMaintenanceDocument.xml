<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="ChartMaintenanceDocument" parent="ChartMaintenanceDocument-parentBean" />

  <bean id="ChartMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.chart.bo.Chart" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="ChartMaintenanceDocument-EditChart" />
      </list>
    </property>
    <property name="defaultExistenceChecks" >
      <list>
        <bean parent="ReferenceDefinition"
              p:attributeName="finCoaManagerUniversal"
              p:attributeToHighlightOnFail="finCoaManagerUniversal.personUserIdentifier"
              p:displayFieldName="Chart Manager" />
        <bean parent="ReferenceDefinition"
              p:attributeName="financialCashObject"
              p:attributeToHighlightOnFail="financialCashObjectCode" />
        <bean parent="ReferenceDefinition"
              p:attributeName="finAccountsPayableObject"
              p:attributeToHighlightOnFail="finAccountsPayableObjectCode" />
        <bean parent="ReferenceDefinition"
              p:attributeName="finAccountsReceivableObj"
              p:attributeToHighlightOnFail="finAccountsReceivableObjCode" />
        <bean parent="ReferenceDefinition"
              p:attributeName="incBdgtEliminationsFinObj"
              p:attributeToHighlightOnFail="incBdgtEliminationsFinObjCd" />
        <bean parent="ReferenceDefinition"
              p:attributeName="expBdgtEliminationsFinObj"
              p:attributeToHighlightOnFail="expBdgtEliminationsFinObjCd" />
        <bean parent="ReferenceDefinition"
              p:attributeName="finInternalEncumbranceObj"
              p:attributeToHighlightOnFail="finInternalEncumbranceObjCd" />
        <bean parent="ReferenceDefinition"
              p:attributeName="finExternalEncumbranceObj"
              p:attributeToHighlightOnFail="finExternalEncumbranceObjCd" />
        <bean parent="ReferenceDefinition"
              p:attributeName="finPreEncumbranceObject"
              p:attributeToHighlightOnFail="finPreEncumbranceObjectCode" />
        <bean parent="ReferenceDefinition"
              p:attributeName="icrIncomeFinancialObject"
              p:attributeToHighlightOnFail="icrIncomeFinancialObjectCode" />
        <bean parent="ReferenceDefinition"
              p:attributeName="icrExpenseFinancialObject"
              p:attributeToHighlightOnFail="icrExpenseFinancialObjectCd" />
        <bean parent="ReferenceDefinition"
              p:attributeName="fundBalanceObject"
              p:attributeToHighlightOnFail="fundBalanceObject" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>chartOfAccountsCode</value>
      </list>
    </property>
    <property name="label" value="Chart" />
    <property name="shortLabel" value="Chart" />
    <property name="summary" value="Chart maintenance document" />
    <property name="description" value="Document used to create or update Chart objects" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-CA"
            p:parameterName="CHART"
            p:parameterDetailType="Chart" />
    </property>
    <property name="documentTypeName" value="ChartMaintenanceDocument" />
    <property name="businessRulesClass" value="org.kuali.module.chart.rules.ChartRule" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="COAT" />
    <property name="workflowProperties" >
      <ref bean="ChartMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>SY_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="ChartMaintenanceDocument-EditChart" parent="ChartMaintenanceDocument-EditChart-parentBean" />

  <bean id="ChartMaintenanceDocument-EditChart-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Chart" />
    <property name="title" value="Edit Chart" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="chartOfAccountsCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="finCoaManagerUniversalId"
              p:readOnly="true"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="finCoaManagerUniversal.personUserIdentifier"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="finCoaManagerUniversal.personName"
              p:readOnly="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="finChartOfAccountDescription"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="finChartOfAccountActiveIndicator" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialCashObjectCode" />
        <bean parent="MaintainableFieldDefinition"
              p:name="finAccountsPayableObjectCode" />
        <bean parent="MaintainableFieldDefinition"
              p:name="incBdgtEliminationsFinObjCd" />
        <bean parent="MaintainableFieldDefinition"
              p:name="expBdgtEliminationsFinObjCd" />
        <bean parent="MaintainableFieldDefinition"
              p:name="reportsToChartOfAccountsCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="finAccountsReceivableObjCode" />
        <bean parent="MaintainableFieldDefinition"
              p:name="finInternalEncumbranceObjCd" />
        <bean parent="MaintainableFieldDefinition"
              p:name="finExternalEncumbranceObjCd" />
        <bean parent="MaintainableFieldDefinition"
              p:name="finPreEncumbranceObjectCode" />
        <bean parent="MaintainableFieldDefinition"
              p:name="icrIncomeFinancialObjectCode" />
        <bean parent="MaintainableFieldDefinition"
              p:name="icrExpenseFinancialObjectCd" />
        <bean parent="MaintainableFieldDefinition"
              p:name="fundBalanceObjectCode" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="ChartMaintenanceDocument-workflowProperties" parent="ChartMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="ChartMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
