<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="WireCharge" parent="WireCharge-parentBean" />

  <bean id="WireCharge-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.financial.bo.WireCharge" />
    <property name="inquiryDefinition" >
      <ref bean="WireCharge-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="WireCharge-lookupDefinition" />
    </property>
    <property name="objectLabel" value="Disbursement Voucher Wire Charge" />
    <property name="attributes" >
      <list>
        <ref bean="WireCharge-universityFiscalYear" />
        <ref bean="WireCharge-chartOfAccountsCode" />
        <ref bean="WireCharge-accountNumber" />
        <ref bean="WireCharge-incomeFinancialObjectCode" />
        <ref bean="WireCharge-expenseFinancialObjectCode" />
        <ref bean="WireCharge-domesticChargeAmt" />
        <ref bean="WireCharge-foreignChargeAmt" />
        <ref bean="WireCharge-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="WireCharge-universityFiscalYear" parent="WireCharge-universityFiscalYear-parentBean" />

  <bean id="WireCharge-universityFiscalYear-parentBean" abstract="true" parent="GenericAttributes-genericFiscalYear">
    <property name="name" value="universityFiscalYear" />
  </bean>
  <bean id="WireCharge-chartOfAccountsCode" parent="WireCharge-chartOfAccountsCode-parentBean" />

  <bean id="WireCharge-chartOfAccountsCode-parentBean" abstract="true" parent="Chart-chartOfAccountsCode">
  </bean>
  <bean id="WireCharge-accountNumber" parent="WireCharge-accountNumber-parentBean" />

  <bean id="WireCharge-accountNumber-parentBean" abstract="true" parent="Account-accountNumber">
  </bean>
  <bean id="WireCharge-incomeFinancialObjectCode" parent="WireCharge-incomeFinancialObjectCode-parentBean" />

  <bean id="WireCharge-incomeFinancialObjectCode-parentBean" abstract="true" parent="ObjectCode-financialObjectCode">
    <property name="name" value="incomeFinancialObjectCode" />
    <property name="label" value="Income Object Code" />
    <property name="shortLabel" value="Inc Obj Cd" />
  </bean>
  <bean id="WireCharge-expenseFinancialObjectCode" parent="WireCharge-expenseFinancialObjectCode-parentBean" />

  <bean id="WireCharge-expenseFinancialObjectCode-parentBean" abstract="true" parent="ObjectCode-financialObjectCode">
    <property name="name" value="expenseFinancialObjectCode" />
    <property name="label" value="Expense Object Code" />
    <property name="shortLabel" value="Exp Obj Cd" />
  </bean>
  <bean id="WireCharge-domesticChargeAmt" parent="WireCharge-domesticChargeAmt-parentBean" />

  <bean id="WireCharge-domesticChargeAmt-parentBean" abstract="true" parent="GenericAttributes-genericAmount">
    <property name="name" value="domesticChargeAmt" />
    <property name="label" value="Domestic Charge Amount" />
    <property name="shortLabel" value="Dom Amt" />
  </bean>
  <bean id="WireCharge-foreignChargeAmt" parent="WireCharge-foreignChargeAmt-parentBean" />

  <bean id="WireCharge-foreignChargeAmt-parentBean" abstract="true" parent="GenericAttributes-genericAmount">
    <property name="name" value="foreignChargeAmt" />
    <property name="label" value="Foreign Charge Amount" />
    <property name="shortLabel" value="For Amt" />
  </bean>
  <bean id="WireCharge-versionNumber" parent="WireCharge-versionNumber-parentBean" />

  <bean id="WireCharge-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="WireCharge-inquiryDefinition" parent="WireCharge-inquiryDefinition-parentBean" />

  <bean id="WireCharge-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Disbursement Voucher Wire Charge Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="universityFiscalYear" />
              <bean parent="FieldDefinition"
                    p:attributeName="chartOfAccountsCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="accountNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="incomeFinancialObjectCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="expenseFinancialObjectCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="domesticChargeAmt" />
              <bean parent="FieldDefinition"
                    p:attributeName="foreignChargeAmt" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="WireCharge-lookupDefinition" parent="WireCharge-lookupDefinition-parentBean" />

  <bean id="WireCharge-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Disbursement Voucher Wire Charge Lookup" />
    <property name="instructions" value="Lookup a Wire Charge" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>accountNumber</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalYear" />
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="accountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="incomeFinancialObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="expenseFinancialObjectCode" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalYear" />
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="accountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="incomeFinancialObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="expenseFinancialObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="domesticChargeAmt" />
        <bean parent="FieldDefinition"
              p:attributeName="foreignChargeAmt" />
      </list>
    </property>
  </bean>
</beans>
