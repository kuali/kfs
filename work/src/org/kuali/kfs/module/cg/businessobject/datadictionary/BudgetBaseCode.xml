<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="BudgetBaseCode" parent="BudgetBaseCode-parentBean" />

  <bean id="BudgetBaseCode-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.kra.budget.bo.BudgetBaseCode" />
    <property name="inquiryDefinition" >
      <ref bean="BudgetBaseCode-inquiryDefinition" />
    </property>
    <property name="objectLabel" value="Budget Base Code" />
    <property name="attributes" >
      <list>
        <ref bean="BudgetBaseCode-budgetBaseCode" />
        <ref bean="BudgetBaseCode-budgetBaseDescription" />
        <ref bean="BudgetBaseCode-active" />
        <ref bean="BudgetBaseCode-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="BudgetBaseCode-budgetBaseCode" parent="BudgetBaseCode-budgetBaseCode-parentBean" />

  <bean id="BudgetBaseCode-budgetBaseCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="budgetBaseCode" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Budget Base Code" />
    <property name="shortLabel" value="baseCode" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="2" />
    </property>
    <property name="summary" value="Budget Base Code Name" />
    <property name="description" value="The code assigned to this base." />
  </bean>
  <bean id="BudgetBaseCode-budgetBaseDescription" parent="BudgetBaseCode-budgetBaseDescription-parentBean" />

  <bean id="BudgetBaseCode-budgetBaseDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="budgetBaseDescription" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Budget Base Description" />
    <property name="shortLabel" value="baseDesc" />
    <property name="maxLength" value="40" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="40" />
    </property>
    <property name="summary" value="Budget Base Code Description" />
    <property name="description" value="The description assigned to this base code." />
  </bean>
  <bean id="BudgetBaseCode-active" parent="BudgetBaseCode-active-parentBean" />

  <bean id="BudgetBaseCode-active-parentBean" abstract="true" parent="AttributeReferenceDummy-activeIndicator">
    <property name="name" value="active" />
  </bean>
  <bean id="BudgetBaseCode-versionNumber" parent="BudgetBaseCode-versionNumber-parentBean" />

  <bean id="BudgetBaseCode-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="BudgetBaseCode-inquiryDefinition" parent="BudgetBaseCode-inquiryDefinition-parentBean" />

  <bean id="BudgetBaseCode-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Budget Base Code Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="budgetBaseCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="budgetBaseDescription" />
              <bean parent="FieldDefinition"
                    p:attributeName="active" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>
</beans>
