<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../core/datadictionary/dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.chart.bo.ObjectCons">
        <customAttributeEnabled>false</customAttributeEnabled>

        <inquiry>
            <title>Object Consolidation Inquiry</title>
            <inquiryFields>
                <field attributeName="chartOfAccountsCode" />
                <field attributeName="finConsolidationObjectCode" />
                <field attributeName="finConsolidationObjectName" />
                <field attributeName="finConsolidationObjShortName" />
                <field attributeName="finConsolidationObjActiveIndicator" />
                <field attributeName="financialEliminationsObjectCode" />
                <field attributeName="financialReportingSortCode" />

            </inquiryFields>
        </inquiry>

        <lookup>
            <title>Object Consolidation Lookup</title>

            <menubar>
                <![CDATA[<a href="maintenance.do?methodToCall=start&businessObjectClassName=org.kuali.module.chart.bo.ObjectCons"><img src="images/tinybutton-createnew.gif" alt="create new" width="70" height="15"/></a>]]>
            </menubar>

            <instructions>Lookup an Object Consolidation</instructions>

            <defaultSort attributeName="finConsolidationObjectCode" sortAscending="true" />

            <lookupFields>
                <lookupField attributeName="chartOfAccountsCode" required="false" />
                <lookupField attributeName="finConsolidationObjectCode" required="false" />
                <lookupField attributeName="finConsolidationObjectName" required="false" />
                <lookupField attributeName="finConsolidationObjShortName" required="false" />
                <lookupField attributeName="financialEliminationsObjectCode" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="chartOfAccountsCode" />
                <field attributeName="finConsolidationObjectCode" />
                <field attributeName="finConsolidationObjectName" />
                <field attributeName="finConsolidationObjShortName" />
                <field attributeName="financialEliminationsObjectCode" />
                <field attributeName="financialReportingSortCode" />
                <field attributeName="finConsolidationObjActiveIndicator" />                              
            </resultFields>
        </lookup>

        <titleAttribute>finConsolidationObjectCode</titleAttribute>

        <attributes>

            <attributeReference name="chartOfAccountsCode" sourceClassName="org.kuali.module.chart.bo.Chart" sourceAttributeName="chartOfAccountsCode">

                <required>true</required>
                <control>
                    <text size="2" />
                </control>
            </attributeReference>

            <attribute name="finConsolidationObjectCode" forceUppercase="true">
                <label>Consolidation Code</label>
                <shortLabel>Cons Code</shortLabel>

                <maxLength>4</maxLength>
                <required>true</required>
                <control>
                    <text size="4" />
                </control>

                <summary>Consolidation Code</summary>
                <description>Consolidation Code...</description>
            </attribute>

            <attribute name="finConsolidationObjectName" forceUppercase="true">
                <label>Consolidation Object Name</label>
                <shortLabel>Long Name</shortLabel>

                <maxLength>40</maxLength>
                <required>true</required>
                <control>
                    <text size="40" />
                </control>

                <summary>Consolidation Object Name</summary>
                <description>Consolidation Object Name...</description>
            </attribute>

            <attribute name="finConsolidationObjShortName" forceUppercase="true">
                <label>Consolidation Object Short Name</label>
                <shortLabel>Short Name</shortLabel>

                <maxLength>12</maxLength>
                <required>true</required>
                <control>
                    <text size="12" />
                </control>

                <summary>Consolidation Object Short Name</summary>
                <description>Consolidation Object Short Name...</description>
            </attribute>

            <attributeReference name="finConsolidationObjActiveIndicator" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="activeIndicator" />

            <attribute name="financialEliminationsObjectCode" forceUppercase="true">
                <label>Elimination Object Code</label>
                <shortLabel>Elim Obj Code</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern>
                    <anyCharacter allowWhitespace="true" />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="4" />
                </control>

                <summary>Elimination Object Code</summary>
                <description>Elimination Object Code</description>
            </attribute>

            <attribute name="financialReportingSortCode" forceUppercase="false">
                <label>Reporting Sort Code</label>
                <shortLabel>Sort Code</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern>
                    <numeric />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="2" />
                </control>

                <summary>Reporting Sort Code</summary>
                <description>Reporting Sort Code...</description>
            </attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>

