<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="AccountBalance" parent="AccountBalance-parentBean" />

  <bean id="AccountBalance-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.gl.bo.AccountBalance" />
    <property name="inquiryDefinition" >
      <ref bean="AccountBalance-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="AccountBalance-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="AccountBalance-universityFiscalYear" />
        <ref bean="AccountBalance-chartOfAccountsCode" />
        <ref bean="AccountBalance-accountNumber" />
        <ref bean="AccountBalance-subAccountNumber" />
        <ref bean="AccountBalance-objectCode" />
        <ref bean="AccountBalance-subObjectCode" />
        <ref bean="AccountBalance-currentBudgetLineBalanceAmount" />
        <ref bean="AccountBalance-accountLineActualsBalanceAmount" />
        <ref bean="AccountBalance-accountLineEncumbranceBalanceAmount" />
        <ref bean="AccountBalance-timestamp" />
        <ref bean="AccountBalance-dummyBusinessObject.pendingEntryOption" />
        <ref bean="AccountBalance-dummyBusinessObject.consolidationOption" />
        <ref bean="AccountBalance-dummyBusinessObject.genericAmount" />
        <ref bean="AccountBalance-dummyBusinessObject.linkButtonOption" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="AccountBalance-universityFiscalYear" parent="AccountBalance-universityFiscalYear-parentBean" />

  <bean id="AccountBalance-universityFiscalYear-parentBean" abstract="true" parent="GenericAttributes-genericFiscalYear">
    <property name="name" value="universityFiscalYear" />
    <property name="label" value="Fiscal Year" />
    <property name="shortLabel" value="Fiscal Year" />
  </bean>
  <bean id="AccountBalance-chartOfAccountsCode" parent="AccountBalance-chartOfAccountsCode-parentBean" />

  <bean id="AccountBalance-chartOfAccountsCode-parentBean" abstract="true" parent="Chart-chartOfAccountsCode">
  </bean>
  <bean id="AccountBalance-accountNumber" parent="AccountBalance-accountNumber-parentBean" />

  <bean id="AccountBalance-accountNumber-parentBean" abstract="true" parent="Account-accountNumber">
  </bean>
  <bean id="AccountBalance-subAccountNumber" parent="AccountBalance-subAccountNumber-parentBean" />

  <bean id="AccountBalance-subAccountNumber-parentBean" abstract="true" parent="SubAccount-subAccountNumber">
  </bean>
  <bean id="AccountBalance-objectCode" parent="AccountBalance-objectCode-parentBean" />

  <bean id="AccountBalance-objectCode-parentBean" abstract="true" parent="ObjectCode-financialObjectCode">
    <property name="name" value="objectCode" />
  </bean>
  <bean id="AccountBalance-subObjectCode" parent="AccountBalance-subObjectCode-parentBean" />

  <bean id="AccountBalance-subObjectCode-parentBean" abstract="true" parent="SubObjCd-financialSubObjectCode">
    <property name="name" value="subObjectCode" />
  </bean>
  <bean id="AccountBalance-currentBudgetLineBalanceAmount" parent="AccountBalance-currentBudgetLineBalanceAmount-parentBean" />

  <bean id="AccountBalance-currentBudgetLineBalanceAmount-parentBean" abstract="true" parent="GenericAttributes-genericAmount">
    <property name="name" value="currentBudgetLineBalanceAmount" />
    <property name="label" value="Budget Amount" />
    <property name="shortLabel" value="Amount" />
    <property name="formatterClass" value="org.kuali.core.web.format.CurrencyFormatter" />
  </bean>
  <bean id="AccountBalance-accountLineActualsBalanceAmount" parent="AccountBalance-accountLineActualsBalanceAmount-parentBean" />

  <bean id="AccountBalance-accountLineActualsBalanceAmount-parentBean" abstract="true" parent="GenericAttributes-genericAmount">
    <property name="name" value="accountLineActualsBalanceAmount" />
    <property name="label" value="Actuals Amount" />
    <property name="shortLabel" value="Actuals Balance Amount" />
    <property name="formatterClass" value="org.kuali.core.web.format.CurrencyFormatter" />
  </bean>
  <bean id="AccountBalance-accountLineEncumbranceBalanceAmount" parent="AccountBalance-accountLineEncumbranceBalanceAmount-parentBean" />

  <bean id="AccountBalance-accountLineEncumbranceBalanceAmount-parentBean" abstract="true" parent="GenericAttributes-genericAmount">
    <property name="name" value="accountLineEncumbranceBalanceAmount" />
    <property name="label" value="Encumbrance Amount" />
    <property name="shortLabel" value="Encumbrance Balance" />
    <property name="formatterClass" value="org.kuali.core.web.format.CurrencyFormatter" />
  </bean>
  <bean id="AccountBalance-timestamp" parent="AccountBalance-timestamp-parentBean" />

  <bean id="AccountBalance-timestamp-parentBean" abstract="true" parent="GenericAttributes-genericTimestamp">
    <property name="name" value="timestamp" />
    <property name="label" value="Transaction Entry Processed" />
    <property name="shortLabel" value="Entry Processed" />
  </bean>
  <bean id="AccountBalance-dummyBusinessObject.pendingEntryOption" parent="AccountBalance-dummyBusinessObject.pendingEntryOption-parentBean" />

  <bean id="AccountBalance-dummyBusinessObject.pendingEntryOption-parentBean" abstract="true" parent="TransientBalanceInquiryAttributes-pendingEntryOption">
    <property name="name" value="dummyBusinessObject.pendingEntryOption" />
  </bean>
  <bean id="AccountBalance-dummyBusinessObject.consolidationOption" parent="AccountBalance-dummyBusinessObject.consolidationOption-parentBean" />

  <bean id="AccountBalance-dummyBusinessObject.consolidationOption-parentBean" abstract="true" parent="TransientBalanceInquiryAttributes-consolidationOption">
    <property name="name" value="dummyBusinessObject.consolidationOption" />
  </bean>
  <bean id="AccountBalance-dummyBusinessObject.genericAmount" parent="AccountBalance-dummyBusinessObject.genericAmount-parentBean" />

  <bean id="AccountBalance-dummyBusinessObject.genericAmount-parentBean" abstract="true" parent="GenericAttributes-genericAmount">
    <property name="name" value="dummyBusinessObject.genericAmount" />
    <property name="label" value="Variance" />
    <property name="shortLabel" value="Variance" />
    <property name="formatterClass" value="org.kuali.core.web.format.CurrencyFormatter" />
  </bean>
  <bean id="AccountBalance-dummyBusinessObject.linkButtonOption" parent="AccountBalance-dummyBusinessObject.linkButtonOption-parentBean" />

  <bean id="AccountBalance-dummyBusinessObject.linkButtonOption-parentBean" abstract="true" parent="TransientBalanceInquiryAttributes-linkButtonOption">
    <property name="name" value="dummyBusinessObject.linkButtonOption" />
    <property name="label" value="Drill Down" />
    <property name="shortLabel" value="Drill Down" />
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="AccountBalance-inquiryDefinition" parent="AccountBalance-inquiryDefinition-parentBean" />

  <bean id="AccountBalance-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Available Balances" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="universityFiscalYear" />
              <bean parent="FieldDefinition"
                    p:attributeName="chartOfAccountsCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="accountNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="subAccountNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="objectCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="subObjectCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="currentBudgetLineBalanceAmount" />
              <bean parent="FieldDefinition"
                    p:attributeName="accountLineActualsBalanceAmount" />
              <bean parent="FieldDefinition"
                    p:attributeName="accountLineEncumbranceBalanceAmount" />
              <bean parent="FieldDefinition"
                    p:attributeName="dummyBusinessObject.genericAmount" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="AccountBalance-lookupDefinition" parent="AccountBalance-lookupDefinition-parentBean" />

  <bean id="AccountBalance-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Available Balances Lookup" />
    <property name="lookupableID" value="glAccountBalanceLookupable" />
    <property name="instructions" value="Lookup Available Account Balance for the specific account." />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>objectCode</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:required="true"
              p:attributeName="universityFiscalYear"
              p:defaultValueFinderClass="org.kuali.kfs.lookup.valuefinder.FiscalYearFinder" />
        <bean parent="FieldDefinition"
              p:required="true"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:required="true"
              p:attributeName="accountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="subAccountNumber"
              p:forceLookup="true" />
        <bean parent="FieldDefinition"
              p:attributeName="objectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="subObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="dummyBusinessObject.consolidationOption"
              p:defaultValueFinderClass="org.kuali.module.gl.web.optionfinder.GLConsolidationOptionFinder" />
        <bean parent="FieldDefinition"
              p:attributeName="dummyBusinessObject.pendingEntryOption"
              p:defaultValueFinderClass="org.kuali.module.gl.web.optionfinder.GLPendingEntryOptionFinder" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalYear" />
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="accountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="subAccountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="objectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="subObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="currentBudgetLineBalanceAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="accountLineActualsBalanceAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="accountLineEncumbranceBalanceAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="dummyBusinessObject.genericAmount" />
      </list>
    </property>
  </bean>
</beans>
