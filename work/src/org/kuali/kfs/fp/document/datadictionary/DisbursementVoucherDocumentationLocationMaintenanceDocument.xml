<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="DisbursementVoucherDocumentationLocationMaintenanceDocument" parent="DisbursementVoucherDocumentationLocationMaintenanceDocument-parentBean" />

  <bean id="DisbursementVoucherDocumentationLocationMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.financial.bo.DisbursementVoucherDocumentationLocation" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="DisbursementVoucherDocumentationLocationMaintenanceDocument-EditDocumentationLocation" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>disbursementVoucherDocumentationLocationCode</value>
      </list>
    </property>
    <property name="label" value="Disbursement Voucher Documentation Location" />
    <property name="shortLabel" value="Disbursement Voucher Documentation Location" />
    <property name="summary" value="Documentation Location Maintenance Document" />
    <property name="description" value="Document used to create or update Documentation Location objects" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-FP"
            p:parameterName="DOCUMENTATION_LOCATION"
            p:parameterDetailType="DisbursementVoucher" />
    </property>
    <property name="documentTypeName" value="DisbursementVoucherDocumentationLocationMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="DVDL" />
    <property name="workflowProperties" >
      <ref bean="DisbursementVoucherDocumentationLocationMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>SY_MAINTAINENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="DisbursementVoucherDocumentationLocationMaintenanceDocument-EditDocumentationLocation" parent="DisbursementVoucherDocumentationLocationMaintenanceDocument-EditDocumentationLocation-parentBean" />

  <bean id="DisbursementVoucherDocumentationLocationMaintenanceDocument-EditDocumentationLocation-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Documentation Location" />
    <property name="title" value="Edit Documentation Location" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="disbursementVoucherDocumentationLocationCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="disbursementVoucherDocumentationLocationName"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="disbursementVoucherDocumentationLocationAddress"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="DisbursementVoucherDocumentationLocationMaintenanceDocument-workflowProperties" parent="DisbursementVoucherDocumentationLocationMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="DisbursementVoucherDocumentationLocationMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
