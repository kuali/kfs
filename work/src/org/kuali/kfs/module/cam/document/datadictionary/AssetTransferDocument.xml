  <bean id="AssetTransferDocument-representativeUniversalIdentifier" parent="AssetTransferDocument-representativeUniversalIdentifier-parentBean" />

  <bean id="AssetTransferDocument-representativeUniversalIdentifier-parentBean" abstract="true" parent="UniversalUser-personUniversalIdentifier">
    <property name="name" value="representativeUniversalIdentifier" />
    <property name="label" value="Asset Representative" />
    <property name="shortLabel" value="Representative" />
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
  </bean>
  <bean id="AssetTransferDocument-assetRepresentative.personUserIdentifier" parent="AssetTransferDocument-assetRepresentative.personUserIdentifier-parentBean" />

  <bean id="AssetTransferDocument-assetRepresentative.personUserIdentifier-parentBean" abstract="true" parent="UniversalUser-personUserIdentifier">
    <property name="name" value="assetRepresentative.personUserIdentifier" />
    <property name="label" value="Asset Representative" />
    <property name="shortLabel" value="UserID" />
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="KualiUserControlDefinition"
            p:personNameAttributeName="assetRepresentative.personName"
            p:universalIdAttributeName="representativeUniversalIdentifier"
            p:userIdAttributeName="assetRepresentative.personUserIdentifier" />
    </property>
  </bean>
  <bean id="AssetTransferDocument-assetRepresentative.personName" parent="AssetTransferDocument-assetRepresentative.personName-parentBean" />

