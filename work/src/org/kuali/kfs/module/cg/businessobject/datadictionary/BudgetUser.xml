<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="BudgetUser" parent="BudgetUser-parentBean" />

  <bean id="BudgetUser-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.kra.budget.bo.BudgetUser" />
    <property name="inquiryDefinition" >
      <ref bean="BudgetUser-inquiryDefinition" />
    </property>
    <property name="objectLabel" value="Budget User/Personnel" />
    <property name="attributes" >
      <list>
        <ref bean="BudgetUser-documentNumber" />
        <ref bean="BudgetUser-budgetUserSequenceNumber" />
        <ref bean="BudgetUser-fiscalCampusCode" />
        <ref bean="BudgetUser-primaryDepartmentCode" />
        <ref bean="BudgetUser-personNamePrefixText" />
        <ref bean="BudgetUser-personNameSuffixText" />
        <ref bean="BudgetUser-personSeniorKeyIndicator" />
        <ref bean="BudgetUser-personProjectDirectorIndicator" />
        <ref bean="BudgetUser-personSecretarialClericalIndicator" />
        <ref bean="BudgetUser-personPostDoctoralIndicator" />
        <ref bean="BudgetUser-objectId" />
        <ref bean="BudgetUser-versionNumber" />
        <ref bean="BudgetUser-baseSalary" />
        <ref bean="BudgetUser-role" />
        <ref bean="BudgetUser-personSalaryJustificationText" />
        <ref bean="BudgetUser-appointmentTypeCode" />
        <ref bean="BudgetUser-personHourlyNumber" />
        <ref bean="BudgetUser-user.personUniversalIdentifier" />
        <ref bean="BudgetUser-user.personUserIdentifier" />
        <ref bean="BudgetUser-user.personName" />
        <ref bean="BudgetUser-user.campusCode" />
        <ref bean="BudgetUser-user.primaryDepartmentCode" />
        <ref bean="BudgetUser-user.employeeStatusCode" />
        <ref bean="BudgetUser-user.employeeTypeCode" />
        <ref bean="BudgetUser-delete" />
      </list>
    </property>
    <property name="relationships" >
      <list>
        <bean parent="RelationshipDefinition">
          <property name="objectAttributeName" value="user" />
          <property name="primitiveAttributes" >
            <list>
              <bean parent="PrimitiveAttributeDefinition"
                    p:sourceName="personUniversalIdentifier"
                    p:targetName="personUniversalIdentifier" />
            </list>
          </property>
          <property name="supportAttributes" >
            <list>
              <bean parent="SupportAttributeDefinition"
                    p:identifier="true"
                    p:sourceName="user.personUserIdentifier"
                    p:targetName="personUserIdentifier" />
              <bean parent="SupportAttributeDefinition"
                    p:sourceName="user.personName"
                    p:targetName="personName" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="BudgetUser-documentNumber" parent="BudgetUser-documentNumber-parentBean" />

  <bean id="BudgetUser-documentNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="documentNumber" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Document Header Id" />
    <property name="shortLabel" value="Document Id" />
    <property name="maxLength" value="14" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="16" />
    </property>
    <property name="summary" value="The document&apos;s displayable identifier." />
    <property name="description" value="The document id is generated by the workflow environment and is unique to each installation of Kuali..." />
  </bean>
  <bean id="BudgetUser-budgetUserSequenceNumber" parent="BudgetUser-budgetUserSequenceNumber-parentBean" />

  <bean id="BudgetUser-budgetUserSequenceNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="budgetUserSequenceNumber" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Budget Period Sequence Number" />
    <property name="shortLabel" value="Budget Pd. Seq. No." />
    <property name="maxLength" value="2" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    <property name="summary" value="The user sequence number." />
    <property name="description" value="Sequence number of the user within a budget." />
  </bean>
  <bean id="BudgetUser-fiscalCampusCode" parent="BudgetUser-fiscalCampusCode-parentBean" />

  <bean id="BudgetUser-fiscalCampusCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="fiscalCampusCode" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Fiscal Campus Code" />
    <property name="shortLabel" value="Fiscal Campus Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="2" />
    </property>
    <property name="summary" value="Fiscal Campus Code" />
    <property name="description" value="Fiscal Campus Code" />
  </bean>
  <bean id="BudgetUser-primaryDepartmentCode" parent="BudgetUser-primaryDepartmentCode-parentBean" />

  <bean id="BudgetUser-primaryDepartmentCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="primaryDepartmentCode" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Primary Department Code" />
    <property name="shortLabel" value="Pri. Dept. Code" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    <property name="summary" value="Primary Department Code" />
    <property name="description" value="Primary Department Code" />
  </bean>
  <bean id="BudgetUser-personNamePrefixText" parent="BudgetUser-personNamePrefixText-parentBean" />

  <bean id="BudgetUser-personNamePrefixText-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="personNamePrefixText" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Prefix" />
    <property name="shortLabel" value="Pre" />
    <property name="maxLength" value="15" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="15" />
    </property>
    <property name="summary" value="Prefix" />
    <property name="description" value="Prefix" />
  </bean>
  <bean id="BudgetUser-personNameSuffixText" parent="BudgetUser-personNameSuffixText-parentBean" />

  <bean id="BudgetUser-personNameSuffixText-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="personNameSuffixText" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Suffix" />
    <property name="shortLabel" value="Suff" />
    <property name="maxLength" value="15" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="15" />
    </property>
    <property name="summary" value="Suffix" />
    <property name="description" value="Suffix" />
  </bean>
  <bean id="BudgetUser-personSeniorKeyIndicator" parent="BudgetUser-personSeniorKeyIndicator-parentBean" />

  <bean id="BudgetUser-personSeniorKeyIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="personSeniorKeyIndicator" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Senior Key Personnel" />
    <property name="shortLabel" value="Sr. Person" />
    <property name="maxLength" value="1" />
    <property name="control" >
      <bean parent="CheckboxControlDefinition" />
    </property>
    <property name="summary" value="Senior Key Personnel" />
    <property name="description" value="Senior Key Personnel" />
    <property name="formatterClass" value="org.kuali.core.web.format.BooleanFormatter" />
  </bean>
  <bean id="BudgetUser-personProjectDirectorIndicator" parent="BudgetUser-personProjectDirectorIndicator-parentBean" />

  <bean id="BudgetUser-personProjectDirectorIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="personProjectDirectorIndicator" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Project Directory" />
    <property name="shortLabel" value="PD" />
    <property name="maxLength" value="1" />
    <property name="control" >
      <bean parent="CheckboxControlDefinition" />
    </property>
    <property name="summary" value="Project Directory" />
    <property name="description" value="Project Directory" />
    <property name="formatterClass" value="org.kuali.core.web.format.BooleanFormatter" />
  </bean>
  <bean id="BudgetUser-personSecretarialClericalIndicator" parent="BudgetUser-personSecretarialClericalIndicator-parentBean" />

  <bean id="BudgetUser-personSecretarialClericalIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="personSecretarialClericalIndicator" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Secretarial/Clerical" />
    <property name="shortLabel" value="Sec/Cl" />
    <property name="maxLength" value="1" />
    <property name="control" >
      <bean parent="CheckboxControlDefinition" />
    </property>
    <property name="summary" value="Secretarial/Clerical" />
    <property name="description" value="Secretarial/Clerical" />
    <property name="formatterClass" value="org.kuali.core.web.format.BooleanFormatter" />
  </bean>
  <bean id="BudgetUser-personPostDoctoralIndicator" parent="BudgetUser-personPostDoctoralIndicator-parentBean" />

  <bean id="BudgetUser-personPostDoctoralIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="personPostDoctoralIndicator" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Post Doctoral" />
    <property name="shortLabel" value="Post Doc" />
    <property name="maxLength" value="1" />
    <property name="control" >
      <bean parent="CheckboxControlDefinition" />
    </property>
    <property name="summary" value="Post Doctoral" />
    <property name="description" value="Post Doctoral" />
    <property name="formatterClass" value="org.kuali.core.web.format.BooleanFormatter" />
  </bean>
  <bean id="BudgetUser-objectId" parent="BudgetUser-objectId-parentBean" />

  <bean id="BudgetUser-objectId-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="objectId" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Object Id" />
    <property name="shortLabel" value="GUID" />
    <property name="maxLength" value="36" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="36" />
    </property>
    <property name="summary" value="objectId summary..." />
    <property name="description" value="objectId description..." />
  </bean>
  <bean id="BudgetUser-versionNumber" parent="BudgetUser-versionNumber-parentBean" />

  <bean id="BudgetUser-versionNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="versionNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Version Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="-1" />
    </property>
    <property name="summary" value="versionNumber summary..." />
    <property name="description" value="versionNumber description..." />
  </bean>
  <bean id="BudgetUser-baseSalary" parent="BudgetUser-baseSalary-parentBean" />

  <bean id="BudgetUser-baseSalary-parentBean" abstract="true" parent="GenericAttributes-genericAmount">
    <property name="name" value="baseSalary" />
  </bean>
  <bean id="BudgetUser-role" parent="BudgetUser-role-parentBean" />

  <bean id="BudgetUser-role-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="role" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Role" />
    <property name="shortLabel" value="Role" />
    <property name="maxLength" value="20" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="20" />
    </property>
    <property name="summary" value="Role" />
    <property name="description" value="Role" />
  </bean>
  <bean id="BudgetUser-personSalaryJustificationText" parent="BudgetUser-personSalaryJustificationText-parentBean" />

  <bean id="BudgetUser-personSalaryJustificationText-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="personSalaryJustificationText" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Justification for rate change" />
    <property name="shortLabel" value="Justification" />
    <property name="maxLength" value="250" />
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="TextareaControlDefinition"
            p:rows="3"
            p:cols="30" />
    </property>
    <property name="summary" value="Justification for Rate Change" />
    <property name="description" value="A place to describe why a change was made to the base salary of the Personnel record." />
  </bean>
  <bean id="BudgetUser-appointmentTypeCode" parent="BudgetUser-appointmentTypeCode-parentBean" />

  <bean id="BudgetUser-appointmentTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="appointmentTypeCode" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Appointment Type Code" />
    <property name="shortLabel" value="Appt Type Code" />
    <property name="maxLength" value="10" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.module.kra.budget.lookup.keyvalues.BudgetAppointmentTypeValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
    <property name="summary" value="Appointment Type Code" />
    <property name="description" value="Appointment Type Code" />
  </bean>
  <bean id="BudgetUser-personHourlyNumber" parent="BudgetUser-personHourlyNumber-parentBean" />

  <bean id="BudgetUser-personHourlyNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="personHourlyNumber" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Number of Hourlies" />
    <property name="shortLabel" value="# Hourlies" />
    <property name="maxLength" value="2" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    <property name="summary" value="Number of Hourlies" />
    <property name="description" value="The total number of hourly employees that this record represents." />
  </bean>
  <bean id="BudgetUser-user.personUniversalIdentifier" parent="BudgetUser-user.personUniversalIdentifier-parentBean" />

  <bean id="BudgetUser-user.personUniversalIdentifier-parentBean" abstract="true" parent="GenericAttributes-genericSystemId">
    <property name="name" value="user.personUniversalIdentifier" />
    <property name="label" value="Institution ID" />
    <property name="shortLabel" value="Univ ID" />
    <property name="summary" value="Institution ID" />
    <property name="description" value="Institution ID" />
  </bean>
  <bean id="BudgetUser-user.personUserIdentifier" parent="BudgetUser-user.personUserIdentifier-parentBean" />

  <bean id="BudgetUser-user.personUserIdentifier-parentBean" abstract="true" parent="UniversalUser-personUserIdentifier">
    <property name="name" value="user.personUserIdentifier" />
  </bean>
  <bean id="BudgetUser-user.personName" parent="BudgetUser-user.personName-parentBean" />

  <bean id="BudgetUser-user.personName-parentBean" abstract="true" parent="UniversalUser-personName">
    <property name="name" value="user.personName" />
    <property name="label" value="Project Director Name" />
    <property name="shortLabel" value="Project Director Name" />
    <property name="summary" value="Project Director Name" />
    <property name="description" value="Project Director Name" />
  </bean>
  <bean id="BudgetUser-user.campusCode" parent="BudgetUser-user.campusCode-parentBean" />

  <bean id="BudgetUser-user.campusCode-parentBean" abstract="true" parent="Campus-campusCode">
    <property name="name" value="user.campusCode" />
  </bean>
  <bean id="BudgetUser-user.primaryDepartmentCode" parent="BudgetUser-user.primaryDepartmentCode-parentBean" />

  <bean id="BudgetUser-user.primaryDepartmentCode-parentBean" abstract="true" parent="UniversalUser-primaryDepartmentCode">
    <property name="name" value="user.primaryDepartmentCode" />
    <property name="label" value="Dept ID" />
    <property name="shortLabel" value="Dept ID" />
    <property name="summary" value="Dept ID" />
    <property name="description" value="Dept ID" />
  </bean>
  <bean id="BudgetUser-user.employeeStatusCode" parent="BudgetUser-user.employeeStatusCode-parentBean" />

  <bean id="BudgetUser-user.employeeStatusCode-parentBean" abstract="true" parent="UniversalUser-employeeStatusCode">
    <property name="name" value="user.employeeStatusCode" />
    <property name="label" value="Employee Status" />
    <property name="shortLabel" value="Employee Status" />
    <property name="summary" value="Employee Statuse" />
    <property name="description" value="Employee Status" />
  </bean>
  <bean id="BudgetUser-user.employeeTypeCode" parent="BudgetUser-user.employeeTypeCode-parentBean" />

  <bean id="BudgetUser-user.employeeTypeCode-parentBean" abstract="true" parent="UniversalUser-employeeTypeCode">
    <property name="name" value="user.employeeTypeCode" />
    <property name="label" value="Employee Type" />
    <property name="shortLabel" value="Employee Typee" />
    <property name="summary" value="Employee Type" />
    <property name="description" value="Employee Type" />
  </bean>
  <bean id="BudgetUser-delete" parent="BudgetUser-delete-parentBean" />

  <bean id="BudgetUser-delete-parentBean" abstract="true" parent="GenericAttributes-genericBoolean">
    <property name="name" value="delete" />
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="BudgetUser-inquiryDefinition" parent="BudgetUser-inquiryDefinition-parentBean" />

  <bean id="BudgetUser-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Personnel Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="user.personUniversalIdentifier" />
              <bean parent="FieldDefinition"
                    p:attributeName="user.personUserIdentifier" />
              <bean parent="FieldDefinition"
                    p:attributeName="user.personName" />
              <bean parent="FieldDefinition"
                    p:attributeName="user.campusCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="user.primaryDepartmentCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="user.employeeStatusCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="user.employeeTypeCode" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>
</beans>
