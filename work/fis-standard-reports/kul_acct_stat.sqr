   let #total_objcon_ot_encb = 0
   let #total_objcon_av_bl = 0
   let #total_objlvl_bdgt_ytd = 0
   let #total_objlvl_act_trn = 0
   let #total_objlvl_actual_ytd = 0
   let #total_objlvl_ot_encb = 0
