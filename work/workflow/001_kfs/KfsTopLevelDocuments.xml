<data xmlns="ns:workflow" xsi:schemaLocation="ns:workflow resource:WorkflowData" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <ruleAttributes xmlns="ns:workflow/RuleAttribute" xsi:schemaLocation="ns:workflow/RuleAttribute resource:RuleAttribute">
    <ruleAttribute>
      <name>KFSDocumentSearchCustomizer</name>
      <label>KFS Document Search Customizer</label>
      <className>{KFS}KFSDocumentSearchCustomizer</className>
      <type>DocumentSearchCustomizer</type>
      <applicationId>KFS</applicationId>
    </ruleAttribute>
    <ruleAttribute>
      <name>FinancialSystemSearchableAttribute</name>
      <className>{KFS}FinancialSystemSearchableAttribute</className>
      <label>Financial System Searchable Attribute</label>
      <description>Financial System Searchable Attribute</description>
      <type>SearchableAttribute</type>
      <applicationId>KFS</applicationId>
    </ruleAttribute>
  </ruleAttributes>
  <documentTypes xmlns="ns:workflow/DocumentType" xsi:schemaLocation="ns:workflow/DocumentType resource:DocumentType">
    <documentType>
      <name>
        KFS
      </name>
      <parent>
        KualiDocument
      </parent>
      <label>
        FinancialSystemDocument
      </label>
      <serviceNamespace>
        KFS
      </serviceNamespace>
      <docHandler>
        ${kfs.url}/kr/maintenance.do?methodToCall=docHandler
      </docHandler>
      <postProcessorName>
        org.kuali.kfs.sys.document.workflow.PostProcessor
      </postProcessorName>
      <active>
        true
      </active>
      <attributes>
        <attribute>
          <name>FinancialSystemSearchableAttribute</name>
        </attribute>
        <attribute>
          <name>KFSDocumentSearchCustomizer</name>
        </attribute>
      </attributes>
      <routingVersion>
        2
      </routingVersion>
    </documentType>
    <documentType>
      <name>
        FSSM
      </name>
      <parent>
        KFS
      </parent>
      <label>
        FinancialSystemSimpleMaintenanceDocument
      </label>
      <active>
        true
      </active>
      <routingVersion>
        2
      </routingVersion>
      <routePaths>
        <routePath>
          <start name="AdHoc"/>
        </routePath>
      </routePaths>
      <routeNodes>
        <start name="AdHoc"/>
      </routeNodes>
    </documentType>
    <documentType>
      <name>
        KFSM
      </name>
      <parent>
        KFS
      </parent>
      <label>
        FinancialSystemComplexMaintenanceDocument
      </label>
      <active>
        true
      </active>
      <routingVersion>
        2
      </routingVersion>
      <routePaths>
        <routePath>
          <start name="AdHoc" nextNode="OrganizationHierarchy"/>
          <role name="OrganizationHierarchy"/>
        </routePath>
      </routePaths>
      <routeNodes>
        <start name="AdHoc"/>
        <role name="OrganizationHierarchy">
          <qualifierResolverClass>
            org.kuali.rice.krad.workflow.attribute.DataDictionaryQualifierResolver
          </qualifierResolverClass>
        </role>
      </routeNodes>
    </documentType>
    <documentType>
      <name>
        KFST
      </name>
      <parent>
        KFS
      </parent>
      <label>
        FinancialSystemTransactionalDocument
      </label>
      <docSearchHelpURL>
        default.htm?turl=WordDocuments%2Ffinancialtransactions.htm
      </docSearchHelpURL>
      <active>
        true
      </active>
      <!-- RICE20 Removing as this is making all transactional documents fail to authorize
      <security>
        <securityAttribute class="org.kuali.kfs.sys.document.workflow.SensitiveDataSecurityAttribute"/>
      </security>
      -->
      <routingVersion>
        2
      </routingVersion>
    </documentType>
    <documentType>
      <name>
        FSLO
      </name>
      <label>
        Financial System - Ledger Only
      </label>
      <active>
        true
      </active>
      <routingVersion>
        2
      </routingVersion>
      <routePaths/>
    </documentType>
  </documentTypes>
</data>
