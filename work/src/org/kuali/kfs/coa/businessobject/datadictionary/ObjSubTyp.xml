<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="ObjSubTyp" parent="ObjSubTyp-parentBean" />

  <bean id="ObjSubTyp-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.chart.bo.ObjSubTyp" />
    <property name="inquiryDefinition" >
      <ref bean="ObjSubTyp-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="ObjSubTyp-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="code" />
    <property name="objectLabel" value="Object Sub-Type" />
    <property name="attributes" >
      <list>
        <ref bean="ObjSubTyp-code" />
        <ref bean="ObjSubTyp-name" />
        <ref bean="ObjSubTyp-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="ObjSubTyp-code" parent="ObjSubTyp-code-parentBean" />

  <bean id="ObjSubTyp-code-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="code" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Object Sub-Type Code" />
    <property name="shortLabel" value="Object Sub-Type Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AlphaNumericValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="2" />
    </property>
  </bean>
  <bean id="ObjSubTyp-name" parent="ObjSubTyp-name-parentBean" />

  <bean id="ObjSubTyp-name-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="name" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Object Sub-Type Name" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="40" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="40" />
    </property>
  </bean>
  <bean id="ObjSubTyp-versionNumber" parent="ObjSubTyp-versionNumber-parentBean" />

  <bean id="ObjSubTyp-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="ObjSubTyp-inquiryDefinition" parent="ObjSubTyp-inquiryDefinition-parentBean" />

  <bean id="ObjSubTyp-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Object Sub-Type Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="code" />
              <bean parent="FieldDefinition"
                    p:attributeName="name" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="ObjSubTyp-lookupDefinition" parent="ObjSubTyp-lookupDefinition-parentBean" />

  <bean id="ObjSubTyp-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Object Sub-Type Lookup" />
    <property name="instructions" value="Lookup an Object Sub-Type" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>code</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="code" />
        <bean parent="FieldDefinition"
              p:attributeName="name" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="code" />
        <bean parent="FieldDefinition"
              p:attributeName="name" />
      </list>
    </property>
  </bean>
</beans>
