<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="PurchaseOrderCostSource" parent="PurchaseOrderCostSource-parentBean" />

  <bean id="PurchaseOrderCostSource-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.vendor.bo.PurchaseOrderCostSource" />
    <property name="inquiryDefinition" >
      <ref bean="PurchaseOrderCostSource-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="PurchaseOrderCostSource-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="purchaseOrderCostSourceCode" />
    <property name="objectLabel" value="Cost Source" />
    <property name="attributes" >
      <list>
        <ref bean="PurchaseOrderCostSource-purchaseOrderCostSourceCode" />
        <ref bean="PurchaseOrderCostSource-purchaseOrderCostSourceDescription" />
        <ref bean="PurchaseOrderCostSource-active" />
        <ref bean="PurchaseOrderCostSource-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="PurchaseOrderCostSource-purchaseOrderCostSourceCode" parent="PurchaseOrderCostSource-purchaseOrderCostSourceCode-parentBean" />

  <bean id="PurchaseOrderCostSource-purchaseOrderCostSourceCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="purchaseOrderCostSourceCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Cost Source Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="6" />
    </property>
  </bean>
  <bean id="PurchaseOrderCostSource-purchaseOrderCostSourceDescription" parent="PurchaseOrderCostSource-purchaseOrderCostSourceDescription-parentBean" />

  <bean id="PurchaseOrderCostSource-purchaseOrderCostSourceDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="purchaseOrderCostSourceDescription" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Cost Source Description" />
    <property name="shortLabel" value="Description" />
    <property name="maxLength" value="45" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="47" />
    </property>
  </bean>
  <bean id="PurchaseOrderCostSource-active" parent="PurchaseOrderCostSource-active-parentBean" />

  <bean id="PurchaseOrderCostSource-active-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
    <property name="name" value="active" />
  </bean>
  <bean id="PurchaseOrderCostSource-versionNumber" parent="PurchaseOrderCostSource-versionNumber-parentBean" />

  <bean id="PurchaseOrderCostSource-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="PurchaseOrderCostSource-inquiryDefinition" parent="PurchaseOrderCostSource-inquiryDefinition-parentBean" />

  <bean id="PurchaseOrderCostSource-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Cost Source Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="purchaseOrderCostSourceCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="purchaseOrderCostSourceDescription" />
              <bean parent="FieldDefinition"
                    p:attributeName="active" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="PurchaseOrderCostSource-lookupDefinition" parent="PurchaseOrderCostSource-lookupDefinition-parentBean" />

  <bean id="PurchaseOrderCostSource-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Cost Source Lookup" />
    <property name="instructions" value="Lookup a Cost Source" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="purchaseOrderCostSourceCode" />
        <bean parent="FieldDefinition"
              p:attributeName="purchaseOrderCostSourceDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="purchaseOrderCostSourceCode" />
        <bean parent="FieldDefinition"
              p:attributeName="purchaseOrderCostSourceDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
  </bean>
</beans>
