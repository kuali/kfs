<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="ObjLevelMaintenanceDocument" parent="ObjLevelMaintenanceDocument-parentBean" />

  <bean id="ObjLevelMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.chart.bo.ObjLevel" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="ObjLevelMaintenanceDocument-EditObjectLevel" />
      </list>
    </property>
    <property name="defaultExistenceChecks" >
      <list>
        <bean parent="ReferenceDefinition"
              p:attributeName="chartOfAccounts"
              p:attributeToHighlightOnFail="chartOfAccountsCode" />
        <bean parent="ReferenceDefinition"
              p:attributeName="financialConsolidationObject"
              p:attributeToHighlightOnFail="financialConsolidationObjectCode" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>chartOfAccountsCode</value>
        <value>financialObjectLevelCode</value>
      </list>
    </property>
    <property name="label" value="Object Level" />
    <property name="shortLabel" value="Object Level" />
    <property name="summary" value="Object Level maintenance document" />
    <property name="description" value="Document used to create or update Object Level objects" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-CA"
            p:parameterName="OBJECT_LEVEL"
            p:parameterDetailType="ObjLevel" />
    </property>
    <property name="documentTypeName" value="ObjLevelMaintenanceDocument" />
    <property name="businessRulesClass" value="org.kuali.module.chart.rules.ObjectLevelRule" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="OBJL" />
    <property name="workflowProperties" >
      <ref bean="ObjLevelMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>SY_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="ObjLevelMaintenanceDocument-EditObjectLevel" parent="ObjLevelMaintenanceDocument-EditObjectLevel-parentBean" />

  <bean id="ObjLevelMaintenanceDocument-EditObjectLevel-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Object Level" />
    <property name="title" value="Edit Object Level" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="chartOfAccountsCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialObjectLevelCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialObjectLevelName"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialObjectLevelShortNm"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialConsolidationObjectCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialObjectLevelActiveIndicator"
              p:defaultValue="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialReportingSortCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="ObjLevelMaintenanceDocument-workflowProperties" parent="ObjLevelMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="ObjLevelMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
