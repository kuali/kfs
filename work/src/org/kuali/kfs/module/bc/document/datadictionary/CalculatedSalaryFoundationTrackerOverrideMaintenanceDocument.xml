<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="CalculatedSalaryFoundationTrackerOverrideMaintenanceDocument" parent="CalculatedSalaryFoundationTrackerOverrideMaintenanceDocument-parentBean" />

  <bean id="CalculatedSalaryFoundationTrackerOverrideMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.budget.bo.CalculatedSalaryFoundationTrackerOverride" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="CalculatedSalaryFoundationTrackerOverrideMaintenanceDocument-EditCalculatedSalaryFoundationTrackerOverride" />
      </list>
    </property>
    <property name="defaultExistenceChecks" >
      <list>
        <bean parent="ReferenceDefinition"
              p:attributeName="chartOfAccounts"
              p:activeIndicatorAttributeName="finChartOfAccountActiveIndicator"
              p:attributeToHighlightOnFail="chartOfAccountsCode"
              p:displayFieldName="Chart Of Accounts Code" />
        <bean parent="ReferenceDefinition"
              p:attributeName="account"
              p:activeIndicatorAttributeName="accountClosedIndicator"
              p:activeIndicatorReversed="true"
              p:attributeToHighlightOnFail="accountNumber"
              p:displayFieldName="Account Number" />
        <bean parent="ReferenceDefinition"
              p:attributeName="financialObject"
              p:activeIndicatorAttributeName="financialObjectActiveCode"
              p:attributeToHighlightOnFail="financialObjectCode" />
        <bean parent="ReferenceDefinition"
              p:attributeName="universityFiscal"
              p:attributeToHighlightOnFail="universityFiscalYear" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>universityFiscalYear</value>
        <value>chartOfAccountsCode</value>
        <value>accountNumber</value>
        <value>subAccountNumber</value>
        <value>financialObjectCode</value>
        <value>financialSubObjectCode</value>
        <value>positionNumber</value>
        <value>emplid</value>
      </list>
    </property>
    <property name="label" value="Calculated Salary Foundation Tracker Override" />
    <property name="shortLabel" value="CSF Tracker Maint Override Doc" />
    <property name="summary" value="Calculated Salary Foundation Tracker Override Maintenance Document" />
    <property name="description" value="Document used to create or update Calculated Salary Foundation Tracker Override Maintenance Document objects" />
    <property name="documentTypeName" value="CalculatedSalaryFoundationTrackerOverrideMaintenanceDocument" />
    <property name="businessRulesClass" value="org.kuali.module.budget.rules.CalculatedSalaryFoundationTrackerOverrideRule" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="CSFO" />
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>SY_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="CalculatedSalaryFoundationTrackerOverrideMaintenanceDocument-EditCalculatedSalaryFoundationTrackerOverride" parent="CalculatedSalaryFoundationTrackerOverrideMaintenanceDocument-EditCalculatedSalaryFoundationTrackerOverride-parentBean" />

  <bean id="CalculatedSalaryFoundationTrackerOverrideMaintenanceDocument-EditCalculatedSalaryFoundationTrackerOverride-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Calculated Salary Foundation Tracker Override" />
    <property name="title" value="Edit Calculated Salary Foundation Tracker Override" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="universityFiscalYear"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="chartOfAccountsCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="accountNumber"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="subAccountNumber"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialObjectCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialSubObjectCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="positionNumber"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="emplid"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="csfAmount"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="csfFullTimeEmploymentQuantity"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="csfFundingStatusCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="csfTimePercent"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="csfDeleteCode" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>
</beans>
