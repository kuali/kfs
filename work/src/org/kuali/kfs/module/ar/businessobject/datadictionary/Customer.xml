		<property name="label" value="Last Address Change Date" />
		<property name="shortLabel" value="Date" />
		<property name="summary" value="customerAddressChangeDate" />
		<property name="description"
			value="customerAddressChangeDate description..." />
	</bean>
	<bean id="Customer-customerBirthDate"
		parent="Customer-customerBirthDate-parentBean" />

	<bean id="Customer-customerBirthDate-parentBean" abstract="true"
		parent="GenericAttributes-genericDate">
		<property name="name" value="customerBirthDate" />
		<property name="forceUppercase" value="true" />
		<property name="label" value="Birth Date" />
		<property name="shortLabel" value="Date" />
		<property name="summary" value="customerBirthDate" />
		<property name="description"
			value="customerBirthDate description..." />
	</bean>
	<bean id="Customer-customerContactName"
		parent="Customer-customerContactName-parentBean" />

	<bean id="Customer-customerContactName-parentBean" abstract="true"
