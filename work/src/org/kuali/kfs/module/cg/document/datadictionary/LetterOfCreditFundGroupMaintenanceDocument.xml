<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="LetterOfCreditFundGroupMaintenanceDocument" parent="LetterOfCreditFundGroupMaintenanceDocument-parentBean" />

  <bean id="LetterOfCreditFundGroupMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.cg.bo.LetterOfCreditFundGroup" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="LetterOfCreditFundGroupMaintenanceDocument-EditLetterofCreditFundGroup" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>letterOfCreditFundGroupCode</value>
      </list>
    </property>
    <property name="label" value="Letter Of Credit Fund Group" />
    <property name="shortLabel" value="Letter Of Credit Fund Group" />
    <property name="summary" value="Letter of Credit Fund Group Maintenance Document" />
    <property name="description" value="Document used to create or update Letter of Credit Fund Group objects" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-CG"
            p:parameterName="LETTER_OF_CREDIT_FUND_GROUP"
            p:parameterDetailType="LetterOfCreditFundGroup" />
    </property>
    <property name="documentTypeName" value="LetterOfCreditFundGroupMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="LFGR" />
    <property name="workflowProperties" >
      <ref bean="LetterOfCreditFundGroupMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>kualiUniversalGroup</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="LetterOfCreditFundGroupMaintenanceDocument-EditLetterofCreditFundGroup" parent="LetterOfCreditFundGroupMaintenanceDocument-EditLetterofCreditFundGroup-parentBean" />

  <bean id="LetterOfCreditFundGroupMaintenanceDocument-EditLetterofCreditFundGroup-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Letter of Credit Fund Group" />
    <property name="title" value="Edit Letter of Credit Fund Group" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="letterOfCreditFundGroupCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="letterOfCreditFundGroupDescription"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="rowActiveIndicator"
              p:defaultValue="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="LetterOfCreditFundGroupMaintenanceDocument-workflowProperties" parent="LetterOfCreditFundGroupMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="LetterOfCreditFundGroupMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
