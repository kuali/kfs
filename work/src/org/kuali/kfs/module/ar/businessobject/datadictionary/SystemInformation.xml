    </property>
  </bean>
  
  <bean id="SystemInformation-processingOrganization.codeAndDescription" parent="SystemInformation-processingOrganization.codeAndDescription-parentBean" />
  <bean id="SystemInformation-processingOrganization.codeAndDescription-parentBean" abstract="true" parent="Organization-codeAndDescription-parentBean">
  	<property name="name" value="processingOrganization.codeAndDescription" />
  	<property name="label" value="Processing Organization Code" />
  </bean>    
  
  <bean id="SystemInformation-discountObjectCode" parent="SystemInformation-discountObjectCode-parentBean" />
  <bean id="SystemInformation-discountObjectCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="discountObjectCode" />
