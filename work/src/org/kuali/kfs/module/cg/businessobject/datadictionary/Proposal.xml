<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="Proposal" parent="Proposal-parentBean" />

  <bean id="Proposal-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.cg.bo.Proposal" />
    <property name="inquiryDefinition" >
      <ref bean="Proposal-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="Proposal-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="proposalNumber" />
    <property name="objectLabel" value="Proposal" />
    <property name="attributes" >
      <list>
        <ref bean="Proposal-routingChart" />
        <ref bean="Proposal-routingOrg" />
        <ref bean="Proposal-award.proposalNumber" />
        <ref bean="Proposal-proposalOrganizations.organization.organizationCode" />
        <ref bean="Proposal-proposalSubcontractors.subcontractor.subcontractorNumber" />
        <ref bean="Proposal-proposalProjectDirectors.projectDirector.personUniversalIdentifier" />
        <ref bean="Proposal-lookupPersonUniversalIdentifier" />
        <ref bean="Proposal-lookupUniversalUser.personUserIdentifier" />
        <ref bean="Proposal-lookupUniversalUser.personName" />
        <ref bean="Proposal-proposal.grantNumber" />
        <ref bean="Proposal-agency.fullName" />
        <ref bean="Proposal-agencyNumber" />
        <ref bean="Proposal-agency.reportingName" />
        <ref bean="Proposal-cfdaNumber" />
        <ref bean="Proposal-federalPassThroughAgencyNumber" />
        <ref bean="Proposal-federalPassThroughAgency.fullName" />
        <ref bean="Proposal-grantNumber" />
        <ref bean="Proposal-oldProposalNumber" />
        <ref bean="Proposal-proposalAwardTypeCode" />
        <ref bean="Proposal-proposalBeginningDate" />
        <ref bean="Proposal-proposalClosingDate" />
        <ref bean="Proposal-proposalDirectCostAmount" />
        <ref bean="Proposal-proposalDueDate" />
        <ref bean="Proposal-proposalEndingDate" />
        <ref bean="Proposal-proposalFederalPassThroughIndicator" />
        <ref bean="Proposal-proposalFellowName" />
        <ref bean="Proposal-proposalIndirectCostAmount" />
        <ref bean="Proposal-proposalLastUpdateDate" />
        <ref bean="Proposal-proposalNumber" />
        <ref bean="Proposal-proposalProjectTitle" />
        <ref bean="Proposal-proposalPurposeCode" />
        <ref bean="Proposal-proposalRejectedDate" />
        <ref bean="Proposal-proposalStatusCode" />
        <ref bean="Proposal-proposalSubmissionDate" />
        <ref bean="Proposal-proposalTotalAmount" />
        <ref bean="Proposal-proposalTotalProjectAmount" />
        <ref bean="Proposal-versionNumber" />
      </list>
    </property>
    <property name="collections" >
      <list>
        <bean parent="CollectionDefinition"
              p:name="proposalOrganizations"
              p:label="Proposal Organizations"
              p:shortLabel="Organizations"
              p:elementLabel="Organization" />
        <bean parent="CollectionDefinition"
              p:name="proposalProjectDirectors"
              p:label="Proposal Project Directors"
              p:shortLabel="Project Directors"
              p:elementLabel="Project Director" />
        <bean parent="CollectionDefinition"
              p:name="proposalSubcontractors"
              p:label="Proposal Subcontractor"
              p:shortLabel="Subcontractors"
              p:elementLabel="Subcontractor" />
        <bean parent="CollectionDefinition"
              p:name="proposalResearchRisks"
              p:label="Proposal Research Risks"
              p:shortLabel="Research Risks"
              p:elementLabel="Research Risk" />
      </list>
    </property>
    <property name="relationships" >
      <list>
        <bean parent="RelationshipDefinition">
          <property name="objectAttributeName" value="proposalOrganizations.organization" />
          <property name="primitiveAttributes" >
            <list>
              <bean parent="PrimitiveAttributeDefinition"
                    p:sourceName="proposalOrganizations.organization.organizationCode"
                    p:targetName="organizationCode" />
            </list>
          </property>
        </bean>
        <bean parent="RelationshipDefinition">
          <property name="objectAttributeName" value="proposalSubcontractors.subcontractor" />
          <property name="primitiveAttributes" >
            <list>
              <bean parent="PrimitiveAttributeDefinition"
                    p:sourceName="proposalSubcontractors.subcontractor.subcontractorNumber"
                    p:targetName="subcontractorNumber" />
            </list>
          </property>
        </bean>
        <bean parent="RelationshipDefinition">
          <property name="objectAttributeName" value="proposalProjectDirectors.projectDirector" />
          <property name="primitiveAttributes" >
            <list>
              <bean parent="PrimitiveAttributeDefinition"
                    p:sourceName="proposalProjectDirectors.projectDirector.personUniversalIdentifier"
                    p:targetName="personUniversalIdentifier" />
            </list>
          </property>
        </bean>
        <bean parent="RelationshipDefinition">
          <property name="objectAttributeName" value="lookupUniversalUser" />
          <property name="primitiveAttributes" >
            <list>
              <bean parent="PrimitiveAttributeDefinition"
                    p:sourceName="lookupPersonUniversalIdentifier"
                    p:targetName="personUniversalIdentifier" />
            </list>
          </property>
          <property name="supportAttributes" >
            <list>
              <bean parent="SupportAttributeDefinition"
                    p:identifier="true"
                    p:sourceName="lookupUniversalUser.personUserIdentifier"
                    p:targetName="personUserIdentifier" />
              <bean parent="SupportAttributeDefinition"
                    p:sourceName="lookupUniversalUser.personName"
                    p:targetName="personName" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

