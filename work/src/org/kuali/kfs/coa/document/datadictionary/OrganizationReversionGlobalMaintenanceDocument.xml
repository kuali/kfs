<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../../core/datadictionary/dataDictionary-1_0.dtd">

<dictionaryEntry>
    <maintenanceDocument>
        <businessObjectClass>org.kuali.module.chart.bo.OrganizationReversionGlobal</businessObjectClass>
        <maintainableClass>org.kuali.module.chart.maintenance.OrganizationReversionGlobalMaintainableImpl</maintainableClass>

        <businessRulesClass>org.kuali.module.chart.rules.OrganizationReversionGlobalRule</businessRulesClass>
        <preRulesCheckClass>org.kuali.module.chart.rules.OrganizationReversionGlobalPreRules</preRulesCheckClass>
        <documentAuthorizerClass>org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase</documentAuthorizerClass>
        <authorizations>
            <authorization action="initiate">
                <workgroups>
                    <workgroup>CA_ORGANIZATION_REVERSION_USERS</workgroup>
                </workgroups>
            </authorization>
        </authorizations>

        <documentTypeName>OrganizationReversionGlobalMaintenanceDocument</documentTypeName>
        <documentTypeCode>GORV</documentTypeCode>
        
        <webScriptFile>../dwr/interface/ObjectCodeService.js</webScriptFile>
        <webScriptFile>../scripts/chart/orgReversionChangeDocument.js</webScriptFile>

        <label>Organization Reversion Global</label>

        <summary>Global Organization Reversion Maintenance Document</summary>
        <description>Global Organization Reversion Maintenance Document</description>
        <help parameterNamespace="KFS-CA" parameterDetailType="OrganizationReversionGlobal" parameterName="ORGANIZATION_REVERSION_GLOBAL" />

        <lockingKeys>
            <field attributeName="documentNumber" />
        </lockingKeys>
        
        <allowsNewOrCopy>true</allowsNewOrCopy>
        
        <defaultExistenceChecks>
            <reference attributeName="universityFiscal" attributeToHighlightOnFail="universityFiscalYear" />
            <reference attributeName="budgetReversionChartOfAccounts" activeIndicatorAttributeName="finChartOfAccountActiveIndicator" attributeToHighlightOnFail="budgetReversionChartOfAccountsCode" />
            <reference attributeName="budgetReversionAccount" attributeToHighlightOnFail="budgetReversionAccountNumber" activeIndicatorAttributeName="accountClosedIndicator" activeIndicatorReversed="true" />
            <reference attributeName="cashReversionFinancialChartOfAccounts" activeIndicatorAttributeName="finChartOfAccountActiveIndicator" attributeToHighlightOnFail="cashReversionFinancialChartOfAccountsCode" />
            <reference attributeName="cashReversionAccount" attributeToHighlightOnFail="cashReversionAccountNumber" activeIndicatorAttributeName="accountClosedIndicator" activeIndicatorReversed="true" />
        </defaultExistenceChecks>
        
        <maintainableSections>
        	<maintainableSection title="Edit Global Organization Reversion Organizations">
            	  <maintainableItems>
            		<maintainableCollection name="organizationReversionGlobalOrganizations" businessObjectClass="org.kuali.module.chart.bo.OrganizationReversionGlobalOrganization" summaryTitle="Organization" sourceClassName="org.kuali.module.chart.bo.Org" sourceAttributeName="organizationCode">
            		    <maintainableField name="chartOfAccountsCode" template="chartOfAccountsCode" required="true" />
                        <maintainableField name="organizationCode" template="organizationCode" required="true" />
                        <maintainableField name="newCollectionRecord" />
                        <maintainableField name="versionNumber" />
                        <summaryFields>
                        	<summaryField name="chartOfAccountsCode" />
                        	<summaryField name="organizationCode" />
                        </summaryFields>
                  	</maintainableCollection>
            	  </maintainableItems>
            </maintainableSection>
            <maintainableSection title="Edit Global Organization Reversion">
                <maintainableItems>
                    <maintainableField name="universityFiscalYear" required="true" />
                    <maintainableField name="budgetReversionChartOfAccountsCode" />
                    <maintainableField name="budgetReversionAccountNumber" />
                    <maintainableField name="cashReversionFinancialChartOfAccountsCode" />
                    <maintainableField name="cashReversionAccountNumber" />
                    <maintainableField name="carryForwardByObjectCodeIndicator" />
                    <maintainableField name="versionNumber" />
	              </maintainableItems>
            </maintainableSection>
            <maintainableSection title="Edit Global Organization Reversion Details">
            	  <maintainableItems>
                    <maintainableCollection name="organizationReversionGlobalDetails" businessObjectClass="org.kuali.module.chart.bo.OrganizationReversionGlobalDetail" includeAddLine="false" summaryTitle="Organization Reversion Detail">
                        <maintainableField name="organizationReversionCategoryCode" required="true" readOnly="true"/>
                        <maintainableField name="organizationReversionObjectCode" webUILeaveFieldFunction="updateObjectNames" />
                        <maintainableField name="objectCodeNames" readOnly="true" />
                        <maintainableField name="organizationReversionCode" />
                        <maintainableField name="newCollectionRecord" />
                        <maintainableField name="versionNumber" />
                    </maintainableCollection>
            	  </maintainableItems>
            </maintainableSection>
        </maintainableSections>
    </maintenanceDocument>
</dictionaryEntry>