  <bean id="AICPAFunctionMaintenanceDocument" parent="AICPAFunctionMaintenanceDocument-parentBean"/>

  <bean id="AICPAFunctionMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.coa.businessobject.AICPAFunction"/>
    <property name="maintainableClass" value="org.kuali.kfs.sys.document.FinancialSystemMaintainable"/>
    <property name="maintainableSections">
      <list>
        <ref bean="AICPAFunctionMaintenanceDocument-EditAICPAFunction"/>
      </list>
    </property>
    <property name="lockingKeys">
      <list>
        <value>financialAicpaFunctionCode</value>
      </list>
    </property>
    
    
    
    
    
    <property name="documentTypeName" value="AFUN"/>
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase"/>
    <property name="workflowAttributes">
      <ref bean="AICPAFunctionMaintenanceDocument-workflowAttributes"/>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="AICPAFunctionMaintenanceDocument-EditAICPAFunction" parent="AICPAFunctionMaintenanceDocument-EditAICPAFunction-parentBean"/>

  <bean id="AICPAFunctionMaintenanceDocument-EditAICPAFunction-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit AICPA Function"/>
    <property name="title" value="Edit AICPA Function"/>
    <property name="maintainableItems">
      <list>
        <bean parent="MaintainableFieldDefinition" p:name="financialAicpaFunctionCode" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="financialAicpaFunctionName" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:defaultValue="true" p:name="active"/>
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="AICPAFunctionMaintenanceDocument-workflowAttributes" parent="AICPAFunctionMaintenanceDocument-workflowAttributes-parentBean"/>

  <bean id="AICPAFunctionMaintenanceDocument-workflowAttributes-parentBean" abstract="true" class="org.kuali.rice.krad.datadictionary.WorkflowAttributes">
  </bean>
</beans>
