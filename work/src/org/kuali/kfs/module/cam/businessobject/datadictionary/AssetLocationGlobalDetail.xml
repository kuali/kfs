<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="AssetLocationGlobalDetail" parent="AssetLocationGlobalDetail-parentBean" />

  <bean id="AssetLocationGlobalDetail-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.cams.bo.AssetLocationGlobalDetail" />
    <property name="inquiryDefinition" >
      <ref bean="AssetLocationGlobalDetail-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="AssetLocationGlobalDetail-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="AssetLocationGlobalDetail-buildingCode" />
        <ref bean="AssetLocationGlobalDetail-buildingRoomNumber" />
        <ref bean="AssetLocationGlobalDetail-buildingSubRoomNumber" />
        <ref bean="AssetLocationGlobalDetail-campusCode" />
        <ref bean="AssetLocationGlobalDetail-campusTagNumber" />
        <ref bean="AssetLocationGlobalDetail-capitalAssetNumber" />
        <ref bean="AssetLocationGlobalDetail-documentNumber" />
        <ref bean="AssetLocationGlobalDetail-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="AssetLocationGlobalDetail-buildingCode" parent="AssetLocationGlobalDetail-buildingCode-parentBean" />

  <bean id="AssetLocationGlobalDetail-buildingCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="buildingCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Building Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
    <property name="summary" value="buildingCode" />
    <property name="description" value="buildingCode description..." />
  </bean>
  <bean id="AssetLocationGlobalDetail-buildingRoomNumber" parent="AssetLocationGlobalDetail-buildingRoomNumber-parentBean" />

  <bean id="AssetLocationGlobalDetail-buildingRoomNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="buildingRoomNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Building Room Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
    <property name="summary" value="buildingRoomNumber" />
    <property name="description" value="buildingRoomNumber description..." />
  </bean>
  <bean id="AssetLocationGlobalDetail-buildingSubRoomNumber" parent="AssetLocationGlobalDetail-buildingSubRoomNumber-parentBean" />

  <bean id="AssetLocationGlobalDetail-buildingSubRoomNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="buildingSubRoomNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Building Sub Room Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    <property name="summary" value="buildingSubRoomNumber" />
    <property name="description" value="buildingSubRoomNumber description..." />
  </bean>
  <bean id="AssetLocationGlobalDetail-campusCode" parent="AssetLocationGlobalDetail-campusCode-parentBean" />

  <bean id="AssetLocationGlobalDetail-campusCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="campusCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Campus Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    <property name="summary" value="campusCode" />
    <property name="description" value="campusCode description..." />
  </bean>
  <bean id="AssetLocationGlobalDetail-campusTagNumber" parent="AssetLocationGlobalDetail-campusTagNumber-parentBean" />

  <bean id="AssetLocationGlobalDetail-campusTagNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="campusTagNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Campus Tag Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
    <property name="summary" value="campusTagNumber" />
    <property name="description" value="campusTagNumber description..." />
  </bean>
  <bean id="AssetLocationGlobalDetail-capitalAssetNumber" parent="AssetLocationGlobalDetail-capitalAssetNumber-parentBean" />

  <bean id="AssetLocationGlobalDetail-capitalAssetNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="capitalAssetNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Capital Asset Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="12" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="14" />
    </property>
    <property name="summary" value="capitalAssetNumber" />
    <property name="description" value="capitalAssetNumber description..." />
  </bean>
  <bean id="AssetLocationGlobalDetail-documentNumber" parent="AssetLocationGlobalDetail-documentNumber-parentBean" />

  <bean id="AssetLocationGlobalDetail-documentNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="documentNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Document Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="14" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="16" />
    </property>
    <property name="summary" value="documentNumber" />
    <property name="description" value="documentNumber description..." />
  </bean>
  <bean id="AssetLocationGlobalDetail-versionNumber" parent="AssetLocationGlobalDetail-versionNumber-parentBean" />

  <bean id="AssetLocationGlobalDetail-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="AssetLocationGlobalDetail-inquiryDefinition" parent="AssetLocationGlobalDetail-inquiryDefinition-parentBean" />

  <bean id="AssetLocationGlobalDetail-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Asset Location Global Detail Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="buildingCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="buildingRoomNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="buildingSubRoomNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="campusCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="campusTagNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="capitalAssetNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="documentNumber" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="AssetLocationGlobalDetail-lookupDefinition" parent="AssetLocationGlobalDetail-lookupDefinition-parentBean" />

  <bean id="AssetLocationGlobalDetail-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Asset Location Global Detail Lookup" />
    <property name="instructions" value="Look up an Asset Location Global Detail" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="buildingCode" />
        <bean parent="FieldDefinition"
              p:attributeName="buildingRoomNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="buildingSubRoomNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="campusCode" />
        <bean parent="FieldDefinition"
              p:attributeName="campusTagNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="buildingCode" />
        <bean parent="FieldDefinition"
              p:attributeName="buildingRoomNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="buildingSubRoomNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="campusCode" />
        <bean parent="FieldDefinition"
              p:attributeName="campusTagNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
      </list>
    </property>
  </bean>
</beans>
