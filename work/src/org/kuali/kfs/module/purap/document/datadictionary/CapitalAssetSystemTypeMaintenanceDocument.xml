<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="CapitalAssetSystemTypeMaintenanceDocument" parent="CapitalAssetSystemTypeMaintenanceDocument-parentBean" />

  <bean id="CapitalAssetSystemTypeMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.purap.bo.CapitalAssetSystemType" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="CapitalAssetSystemTypeMaintenanceDocument-EditCapitalAssetSystemType" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>capitalAssetSystemTypeCode</value>
      </list>
    </property>
    <property name="label" value="Capital Asset System Type" />
    <property name="shortLabel" value="Capital Asset System Type" />
    <property name="documentTypeName" value="CapitalAssetSystemTypeMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="PMSY" />
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>PA_PUR_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="CapitalAssetSystemTypeMaintenanceDocument-EditCapitalAssetSystemType" parent="CapitalAssetSystemTypeMaintenanceDocument-EditCapitalAssetSystemType-parentBean" />

  <bean id="CapitalAssetSystemTypeMaintenanceDocument-EditCapitalAssetSystemType-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Capital Asset System Type" />
    <property name="title" value="Edit Capital Asset System Type" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="capitalAssetSystemTypeCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="capitalAssetSystemTypeDescription"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="active"
              p:defaultValue="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>
</beans>
