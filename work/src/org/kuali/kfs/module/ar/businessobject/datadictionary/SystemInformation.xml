  <bean id="SystemInformation-universityClearingChartOfAccountsCode" parent="SystemInformation-universityClearingChartOfAccountsCode-parentBean" />
  <bean id="SystemInformation-universityClearingChartOfAccountsCode-parentBean" abstract="true" parent="Chart-chartOfAccountsCode">
    <property name="name" value="universityClearingChartOfAccountsCode" />
    <property name="label" value="Chart Of Account" />
    <property name="shortLabel" value="University Clearing Chart Of Accounts Code" />
  </bean>

  <bean id="SystemInformation-universityClearingAccountNumber" parent="SystemInformation-universityClearingAccountNumber-parentBean" />
  <bean id="SystemInformation-universityClearingAccountNumber-parentBean" abstract="true" parent="Account-accountNumber">
    <property name="name" value="universityClearingAccountNumber" />
    <property name="shortLabel" value="University Clearing Account Number" />
  </bean>

  <bean id="SystemInformation-universityClearingObjectCode" parent="SystemInformation-universityClearingObjectCode-parentBean" />
  <bean id="SystemInformation-universityClearingObjectCode-parentBean" abstract="true" parent="ObjectCode-financialObjectCode">
    <property name="name" value="universityClearingObjectCode" />
