<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../datadictionary/dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.core.bo.ParameterNamespace">

        <inquiry>
            <title>Parameter Module Inquiry</title>
            <inquirySections>
                <inquirySection title="Module Information">
                    <inquiryFields>
                        <field attributeName="code" />
                        <field attributeName="name" noInquiry="true" />
                        <field attributeName="active" />
                    </inquiryFields>
                </inquirySection>
            </inquirySections>
        </inquiry>
        <lookup>
            <title>Parameter Module Lookup</title>

            <instructions>Look up a Module.</instructions>
            <defaultSort attributeName="name" sortAscending="true" />
            <lookupFields>
                <lookupField attributeName="code" required="false"
                    noLookup="true"
                />
                <lookupField attributeName="name" required="false" />
                <lookupField attributeName="active" required="false">
                    <defaultValue>Y</defaultValue>
                </lookupField>
            </lookupFields>
            <resultFields>
                <field attributeName="code" forceInquiry="true" />
                <field attributeName="name" />
                <field attributeName="active" />
            </resultFields>
        </lookup>

        <titleAttribute>name</titleAttribute>

        <attributes>
            <attribute name="code" forceUppercase="true">
                <label>Module Code</label>
                <shortLabel>Module Cd</shortLabel>
                <maxLength>20</maxLength>
                <validationPattern>
                    <anyCharacter />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="10" />
                </control>
                <summary>Code identifying the module.</summary>
                <description>
                    Code identifying the module.
                </description>
            </attribute>
            <attribute name="name" forceUppercase="false">
                <label>Module Name</label>
                <shortLabel>Module Nm</shortLabel>
                <maxLength>40</maxLength>
                <validationPattern>
                    <anyCharacter allowWhitespace="true" />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="40" />
                </control>
                <summary>The name of the module.</summary>
                <description>
                    The name of the module.
                </description>
            </attribute>
            <attribute name="codeAndDescription" forceUppercase="false">
                <label>Module</label>
                <shortLabel>Module</shortLabel>
                <maxLength>100</maxLength>
                <validationPattern>
                    <anyCharacter allowWhitespace="true" />
                </validationPattern>
                <required>false</required>
                <control>
                    <text size="40" />
                </control>
                <summary>The name of the module.</summary>
                <description>
                    The name of the module.
                </description>
            </attribute>
            <attributeReference name="active"
                sourceClassName="org.kuali.core.bo.AttributeReferenceDummy"
                sourceAttributeName="activeIndicator"
            >
            </attributeReference>
            <attributeReference name="objectId"
                sourceClassName="org.kuali.core.bo.AttributeReferenceDummy"
                sourceAttributeName="objectId"
            />
            <attributeReference name="versionNumber"
                sourceClassName="org.kuali.core.bo.AttributeReferenceDummy"
                sourceAttributeName="versionNumber"
            />
        </attributes>
        <objectLabel>Parameter Module</objectLabel>
    </businessObject>
</dictionaryEntry>
