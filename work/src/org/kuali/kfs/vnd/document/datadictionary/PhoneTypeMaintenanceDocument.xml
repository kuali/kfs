<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="PhoneTypeMaintenanceDocument" parent="PhoneTypeMaintenanceDocument-parentBean" />

  <bean id="PhoneTypeMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.vendor.bo.PhoneType" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="PhoneTypeMaintenanceDocument-EditPhoneType" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>vendorPhoneTypeCode</value>
      </list>
    </property>
    <property name="label" value="Phone Type" />
    <property name="shortLabel" value="Phone Type" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-VN"
            p:parameterName="PHONE_TYPE"
            p:parameterDetailType="PhoneType" />
    </property>
    <property name="documentTypeName" value="PhoneTypeMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="PMPT" />
    <property name="workflowProperties" >
      <ref bean="PhoneTypeMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>PA_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="PhoneTypeMaintenanceDocument-EditPhoneType" parent="PhoneTypeMaintenanceDocument-EditPhoneType-parentBean" />

  <bean id="PhoneTypeMaintenanceDocument-EditPhoneType-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Phone Type" />
    <property name="title" value="Edit Phone Type" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="vendorPhoneTypeCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="vendorPhoneTypeDescription"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="active" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="PhoneTypeMaintenanceDocument-workflowProperties" parent="PhoneTypeMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="PhoneTypeMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
