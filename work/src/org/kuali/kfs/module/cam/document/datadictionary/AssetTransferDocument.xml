    <property name="forceUppercase" value="true" />
    <property name="label" value="Building Sub Room Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    <property name="summary" value="buildingSubRoomNumber" />
    <property name="description" value="buildingSubRoomNumber description..." />
  </bean>
  <bean id="AssetTransferDocument-campusCode" parent="AssetTransferDocument-campusCode-parentBean" />

  <bean id="AssetTransferDocument-campusCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="campusCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Campus" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    <property name="summary" value="campusCode" />
    <property name="description" value="campusCode description..." />
  </bean>
  <bean id="AssetTransferDocument-interdepartmentalSalesIndicator" parent="AssetTransferDocument-interdepartmentalSalesIndicator-parentBean" />

