<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="GraduateAssistantRate" parent="GraduateAssistantRate-parentBean" />

  <bean id="GraduateAssistantRate-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.kra.budget.bo.GraduateAssistantRate" />
    <property name="inquiryDefinition" >
      <ref bean="GraduateAssistantRate-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="GraduateAssistantRate-lookupDefinition" />
    </property>
    <property name="objectLabel" value="Graduate Assistant Rate" />
    <property name="objectDescription" value="words" />
    <property name="attributes" >
      <list>
        <ref bean="GraduateAssistantRate-campusCode" />
        <ref bean="GraduateAssistantRate-active" />
        <ref bean="GraduateAssistantRate-campusMaximumPeriod1Rate" />
        <ref bean="GraduateAssistantRate-campusMaximumPeriod2Rate" />
        <ref bean="GraduateAssistantRate-campusMaximumPeriod3Rate" />
        <ref bean="GraduateAssistantRate-campusMaximumPeriod4Rate" />
        <ref bean="GraduateAssistantRate-campusMaximumPeriod5Rate" />
        <ref bean="GraduateAssistantRate-campusMaximumPeriod6Rate" />
        <ref bean="GraduateAssistantRate-objectId" />
        <ref bean="GraduateAssistantRate-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="GraduateAssistantRate-campusCode" parent="GraduateAssistantRate-campusCode-parentBean" />

  <bean id="GraduateAssistantRate-campusCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="campusCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Campus Code" />
    <property name="shortLabel" value="Campus Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="2" />
    </property>
    <property name="summary" value="Campus Code..." />
    <property name="description" value="Campus Code..." />
  </bean>
  <bean id="GraduateAssistantRate-active" parent="GraduateAssistantRate-active-parentBean" />

  <bean id="GraduateAssistantRate-active-parentBean" abstract="true" parent="AttributeReferenceDummy-activeIndicator">
    <property name="name" value="active" />
  </bean>
  <bean id="GraduateAssistantRate-campusMaximumPeriod1Rate" parent="GraduateAssistantRate-campusMaximumPeriod1Rate-parentBean" />

  <bean id="GraduateAssistantRate-campusMaximumPeriod1Rate-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="campusMaximumPeriod1Rate" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Period 1 Remission Rate" />
    <property name="shortLabel" value="Period 1 Remission Rate" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="FloatingPointValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
    <property name="summary" value="Period 1 Remission Rate" />
    <property name="description" value="Period 1 Remission Rate..." />
  </bean>
  <bean id="GraduateAssistantRate-campusMaximumPeriod2Rate" parent="GraduateAssistantRate-campusMaximumPeriod2Rate-parentBean" />

  <bean id="GraduateAssistantRate-campusMaximumPeriod2Rate-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="campusMaximumPeriod2Rate" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Period 2 Remission Rate" />
    <property name="shortLabel" value="Period 2 Remission Rate" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="FloatingPointValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
    <property name="summary" value="Period 2 Remission Rate" />
    <property name="description" value="Period 2 Remission Rate..." />
  </bean>
  <bean id="GraduateAssistantRate-campusMaximumPeriod3Rate" parent="GraduateAssistantRate-campusMaximumPeriod3Rate-parentBean" />

  <bean id="GraduateAssistantRate-campusMaximumPeriod3Rate-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="campusMaximumPeriod3Rate" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Period 3 Remission Rate" />
    <property name="shortLabel" value="Period 3 Remission Rate" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="FloatingPointValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
    <property name="summary" value="Period 3 Remission Rate" />
    <property name="description" value="Period 3 Remission Rate..." />
  </bean>
  <bean id="GraduateAssistantRate-campusMaximumPeriod4Rate" parent="GraduateAssistantRate-campusMaximumPeriod4Rate-parentBean" />

  <bean id="GraduateAssistantRate-campusMaximumPeriod4Rate-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="campusMaximumPeriod4Rate" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Period 4 Remission Rate" />
    <property name="shortLabel" value="Period 4 Remission Rate" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="FloatingPointValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
    <property name="summary" value="Period 4 Remission Rate" />
    <property name="description" value="Period 4 Remission Rate..." />
  </bean>
  <bean id="GraduateAssistantRate-campusMaximumPeriod5Rate" parent="GraduateAssistantRate-campusMaximumPeriod5Rate-parentBean" />

  <bean id="GraduateAssistantRate-campusMaximumPeriod5Rate-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="campusMaximumPeriod5Rate" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Period 5 Remission Rate" />
    <property name="shortLabel" value="Period 5 Remission Rate" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="FloatingPointValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
    <property name="summary" value="Period 5 Remission Rate" />
    <property name="description" value="Period 5 Remission Rate..." />
  </bean>
  <bean id="GraduateAssistantRate-campusMaximumPeriod6Rate" parent="GraduateAssistantRate-campusMaximumPeriod6Rate-parentBean" />

  <bean id="GraduateAssistantRate-campusMaximumPeriod6Rate-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="campusMaximumPeriod6Rate" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Period 6 Remission Rate" />
    <property name="shortLabel" value="Period 6 Remission Rate" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="FloatingPointValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
    <property name="summary" value="Period 6 Remission Rate" />
    <property name="description" value="Period 6 Remission Rate..." />
  </bean>
  <bean id="GraduateAssistantRate-objectId" parent="GraduateAssistantRate-objectId-parentBean" />

  <bean id="GraduateAssistantRate-objectId-parentBean" abstract="true" parent="AttributeReferenceDummy-objectId">
  </bean>
  <bean id="GraduateAssistantRate-versionNumber" parent="GraduateAssistantRate-versionNumber-parentBean" />

  <bean id="GraduateAssistantRate-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="GraduateAssistantRate-inquiryDefinition" parent="GraduateAssistantRate-inquiryDefinition-parentBean" />

  <bean id="GraduateAssistantRate-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Graduate Assistant Rate Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="campusCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="active" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="GraduateAssistantRate-lookupDefinition" parent="GraduateAssistantRate-lookupDefinition-parentBean" />

  <bean id="GraduateAssistantRate-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Graduate Assistant Rate Lookup" />
    <property name="instructions" value="Lookup a Graduate Assistant Rate" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>campusCode</value>
          </list>
        </property>
        <property name="sortAscending" value="false" />
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="campusCode" />
        <bean parent="FieldDefinition"
              p:defaultValue="Y"
              p:attributeName="active" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="campusCode" />
        <bean parent="FieldDefinition"
              p:attributeName="campusMaximumPeriod1Rate" />
        <bean parent="FieldDefinition"
              p:attributeName="campusMaximumPeriod2Rate" />
        <bean parent="FieldDefinition"
              p:attributeName="campusMaximumPeriod3Rate" />
        <bean parent="FieldDefinition"
              p:attributeName="campusMaximumPeriod4Rate" />
        <bean parent="FieldDefinition"
              p:attributeName="campusMaximumPeriod5Rate" />
        <bean parent="FieldDefinition"
              p:attributeName="campusMaximumPeriod6Rate" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
  </bean>
</beans>
