<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="AgencyTypeMaintenanceDocument" parent="AgencyTypeMaintenanceDocument-parentBean" />

  <bean id="AgencyTypeMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.cg.bo.AgencyType" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="AgencyTypeMaintenanceDocument-EditAgencyTypeCode" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>code</value>
      </list>
    </property>
    <property name="label" value="Agency Type" />
    <property name="shortLabel" value="Agency Type" />
    <property name="summary" value="Agency Type maintenance document" />
    <property name="description" value="Document used to create or update Agency Type objects" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-CG"
            p:parameterName="AGENCY_TYPE"
            p:parameterDetailType="AgencyType" />
    </property>
    <property name="documentTypeName" value="AgencyTypeMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="AGTY" />
    <property name="workflowProperties" >
      <ref bean="AgencyTypeMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>RA_PRE_AWARD_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="AgencyTypeMaintenanceDocument-EditAgencyTypeCode" parent="AgencyTypeMaintenanceDocument-EditAgencyTypeCode-parentBean" />

  <bean id="AgencyTypeMaintenanceDocument-EditAgencyTypeCode-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Agency Type Code" />
    <property name="title" value="Edit Agency Type Code" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="code"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="name"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="rowActiveIndicator"
              p:defaultValue="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="AgencyTypeMaintenanceDocument-workflowProperties" parent="AgencyTypeMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="AgencyTypeMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
