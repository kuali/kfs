<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xmlns:dd="http://rice.kuali.org/dd"
    xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
http://rice.kuali.org/dd http://rice.kuali.org/dd/dd.xsd">

  <bean id="CustomerAddress" parent="CustomerAddress-parentBean" />

  <bean id="CustomerAddress-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.ar.businessobject.CustomerAddress" />
    <property name="inquiryDefinition" >
      <ref bean="CustomerAddress-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="CustomerAddress-lookupDefinition" />
    </property>
    <property name="objectLabel" value="Customer Address" />
    <property name="attributes" >
      <list>
        <ref bean="CustomerAddress-customerAddressEndDate" />
        <ref bean="CustomerAddress-customerAddressIdentifier" />
        <ref bean="CustomerAddress-customerAddressName" />
        <ref bean="CustomerAddress-customerAddressTypeCode" />
        <ref bean="CustomerAddress-customerCityName" />
        <ref bean="CustomerAddress-customerEmailAddress" />
		<ref bean="CustomerAddress-customerInternationalMailCode" />
        <ref bean="CustomerAddress-customerLine1StreetAddress" />
        <ref bean="CustomerAddress-customerLine2StreetAddress" />
        <ref bean="CustomerAddress-customerNumber" />
        <ref bean="CustomerAddress-customerStateCode" />
        <ref bean="CustomerAddress-customerZipCode" />
        <ref bean="CustomerAddress-customerCountryCode" />
        <ref bean="CustomerAddress-customerAddressInternationalProvinceName" />
      </list>
    </property>
    <property name="relationships" >
      <list>
        <dd:relationship objectAttribute="customerCountry">
            <dd:primitiveAttribute source="customerCountryCode" target="postalCountryCode" />
        </dd:relationship>
      </list>
    </property>    
  </bean>

<!-- Attribute Definitions -->


  <bean id="CustomerAddress-customerAddressEndDate" parent="CustomerAddress-customerAddressEndDate-parentBean" />

  <bean id="CustomerAddress-customerAddressEndDate-parentBean" abstract="true" parent="GenericAttributes-genericDate">
    <property name="name" value="customerAddressEndDate" />
    <property name="label" value="Address End Date" />
    <property name="shortLabel" value="Date" />
  </bean>
  <bean id="CustomerAddress-customerAddressIdentifier" parent="CustomerAddress-customerAddressIdentifier-parentBean" />

  <bean id="CustomerAddress-customerAddressIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="customerAddressIdentifier" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Customer Address Identifier" />
    <property name="shortLabel" value="Identifier" />
    <property name="maxLength" value="7" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
  </bean>
  
  <bean id="CustomerAddress-customerAddressTypeCode" parent="CustomerAddress-customerAddressTypeCode-parentBean" />

  <bean id="CustomerAddress-customerAddressTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="customerAddressTypeCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Address Type" />
    <property name="shortLabel" value="Type" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.kfs.module.ar.businessobject.options.CustomerAddressTypeValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
  </bean>
  
  <bean id="CustomerAddress-customerAddressName" parent="CustomerAddress-customerAddressName-parentBean" />

  <bean id="CustomerAddress-customerAddressName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="customerAddressName" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Address Name" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="40" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="42" />
    </property>
  </bean>
  <bean id="CustomerAddress-customerCityName" parent="CustomerAddress-customerCityName-parentBean" />

  <bean id="CustomerAddress-customerCityName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="customerCityName" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="City" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="25" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="27" />
    </property>
  </bean>
  <bean id="CustomerAddress-customerEmailAddress" parent="CustomerAddress-customerEmailAddress-parentBean" />

  <bean id="CustomerAddress-customerEmailAddress-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="customerEmailAddress" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Email Address" />
    <property name="shortLabel" value="Email" />
    <property name="maxLength" value="60" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="62" />
    </property>
  </bean>

   <bean id="CustomerAddress-customerInternationalMailCode" parent="CustomerAddress-customerInternationalMailCode-parentBean" />

  <bean id="CustomerAddress-customerInternationalMailCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="customerInternationalMailCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="International Postal Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="20" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="22" />
    </property>
  </bean>

  <bean id="CustomerAddress-customerLine1StreetAddress" parent="CustomerAddress-customerLine1StreetAddress-parentBean" />

  <bean id="CustomerAddress-customerLine1StreetAddress-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="customerLine1StreetAddress" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Address 1" />
    <property name="shortLabel" value="Address1" />
    <property name="maxLength" value="30" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="32" />
    </property>
  </bean>
  <bean id="CustomerAddress-customerLine2StreetAddress" parent="CustomerAddress-customerLine2StreetAddress-parentBean" />

  <bean id="CustomerAddress-customerLine2StreetAddress-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="customerLine2StreetAddress" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Address 2" />
    <property name="shortLabel" value="Address2" />
    <property name="maxLength" value="30" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="32" />
    </property>
  </bean>
  <bean id="CustomerAddress-customerNumber" parent="CustomerAddress-customerNumber-parentBean" />

  <bean id="CustomerAddress-customerNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="customerNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Customer Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="9" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="11" />
    </property>
  </bean>
  <bean id="CustomerAddress-customerStateCode" parent="CustomerAddress-customerStateCode-parentBean" />

  <bean id="CustomerAddress-customerStateCode-parentBean" abstract="true" parent="ExternalizableAttributeDefinitionProxy"
  	p:sourceExternalizableBusinessObjectInterface="org.kuali.rice.kns.bo.State"
	p:sourceAttributeName="postalStateCode">
    <property name="name" value="customerStateCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="State" />
    <property name="shortLabel" value="State" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.kfs.sys.businessobject.options.StateValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
  </bean>
  <bean id="CustomerAddress-customerZipCode" parent="CustomerAddress-customerZipCode-parentBean" />

  <bean id="CustomerAddress-customerZipCode-parentBean" abstract="true" parent="ExternalizableAttributeDefinitionProxy"
  	p:sourceExternalizableBusinessObjectInterface="org.kuali.rice.kns.bo.PostalCode"
	p:sourceAttributeName="postalCode">
    <property name="name" value="customerZipCode" />
  </bean>
  
  <bean id="CustomerAddress-customerCountryCode" parent="CustomerAddress-customerCountryCode-parentBean" />

  <bean id="CustomerAddress-customerCountryCode-parentBean" abstract="true" parent="ExternalizableAttributeDefinitionProxy"
  	p:sourceExternalizableBusinessObjectInterface="org.kuali.rice.kns.bo.Country"
	p:sourceAttributeName="postalCountryCode">
    <property name="name" value="customerCountryCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Country" />
    <property name="shortLabel" value="Country" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.kfs.sys.businessobject.options.CountryNotRestrictedValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
  </bean>
  
   <bean id="CustomerAddress-customerAddressInternationalProvinceName" parent="CustomerAddress-customerAddressInternationalProvinceName-parentBean" />
    
   <bean id="CustomerAddress-customerAddressInternationalProvinceName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="customerAddressInternationalProvinceName" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="International Province" />
    <property name="shortLabel" value="Province" />
    <property name="maxLength" value="45" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="45" />
    </property>
  </bean>
  

<!-- Business Object Inquiry Definition -->


  <bean id="CustomerAddress-inquiryDefinition" parent="CustomerAddress-inquiryDefinition-parentBean" />

  <bean id="CustomerAddress-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Customer Address Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="customerAddressIdentifier" />
              <bean parent="FieldDefinition"
                    p:attributeName="customerAddressTypeCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="customerAddressName" />
              <bean parent="FieldDefinition"
                    p:attributeName="customerLine1StreetAddress" />
              <bean parent="FieldDefinition"
                    p:attributeName="customerLine2StreetAddress" />
              <bean parent="FieldDefinition"
                    p:attributeName="customerCityName" />
              <bean parent="FieldDefinition"
                    p:attributeName="customerStateCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="customerZipCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="customerAddressInternationalProvinceName" />
              <bean parent="FieldDefinition"
                    p:attributeName="customerInternationalMailCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="customerCountryCode" />  
              <bean parent="FieldDefinition"
                    p:attributeName="customerEmailAddress" />
              <bean parent="FieldDefinition"
                    p:attributeName="customerAddressEndDate" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="CustomerAddress-lookupDefinition" parent="CustomerAddress-lookupDefinition-parentBean" />

  <bean id="CustomerAddress-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Customer Address Lookup" />
    <property name="instructions" value="Look up a Customer Address" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="customerNumber" />      
        <bean parent="FieldDefinition"
              p:attributeName="customerAddressTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="customerAddressName" />
        <bean parent="FieldDefinition"
              p:attributeName="customerLine1StreetAddress" />
        <bean parent="FieldDefinition"
              p:attributeName="customerLine2StreetAddress" />
        <bean parent="FieldDefinition"
              p:attributeName="customerCityName" />
        <bean parent="FieldDefinition"
              p:attributeName="customerStateCode" />
        <bean parent="FieldDefinition"
              p:attributeName="customerZipCode" />  
        <bean parent="FieldDefinition"
              p:attributeName="customerAddressInternationalProvinceName" />
        <bean parent="FieldDefinition"
              p:attributeName="customerInternationalMailCode" />
        <bean parent="FieldDefinition"
              p:attributeName="customerCountryCode" />
        <bean parent="FieldDefinition"
              p:attributeName="customerEmailAddress" />
        <bean parent="FieldDefinition"
              p:attributeName="customerAddressEndDate" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="customerAddressTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="customerAddressName" />
        <bean parent="FieldDefinition"
              p:attributeName="customerLine1StreetAddress" />
        <bean parent="FieldDefinition"
              p:attributeName="customerLine2StreetAddress" />
        <bean parent="FieldDefinition"
              p:attributeName="customerCityName" />
        <bean parent="FieldDefinition"
              p:attributeName="customerStateCode" />
        <bean parent="FieldDefinition"
              p:attributeName="customerZipCode" />
        <bean parent="FieldDefinition"
              p:attributeName="customerAddressInternationalProvinceName" />
        <bean parent="FieldDefinition"
              p:attributeName="customerInternationalMailCode" />
        <bean parent="FieldDefinition"
              p:attributeName="customerCountryCode" />
        <bean parent="FieldDefinition"
              p:attributeName="customerEmailAddress" />
        <bean parent="FieldDefinition"
              p:attributeName="customerAddressEndDate" />
      </list>
    </property>
  </bean>
</beans>
