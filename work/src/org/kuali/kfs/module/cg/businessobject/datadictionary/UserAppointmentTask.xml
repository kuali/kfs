<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="UserAppointmentTask" parent="UserAppointmentTask-parentBean" />

  <bean id="UserAppointmentTask-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.kra.budget.bo.UserAppointmentTask" />
    <property name="attributes" >
      <list>
        <ref bean="UserAppointmentTask-documentNumber" />
        <ref bean="UserAppointmentTask-budgetUserSequenceNumber" />
        <ref bean="UserAppointmentTask-budgetTaskSequenceNumber" />
        <ref bean="UserAppointmentTask-institutionAppointmentTypeCode" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="UserAppointmentTask-documentNumber" parent="UserAppointmentTask-documentNumber-parentBean" />

  <bean id="UserAppointmentTask-documentNumber-parentBean" abstract="true" parent="Budget-documentNumber">
  </bean>
  <bean id="UserAppointmentTask-budgetUserSequenceNumber" parent="UserAppointmentTask-budgetUserSequenceNumber-parentBean" />

  <bean id="UserAppointmentTask-budgetUserSequenceNumber-parentBean" abstract="true" parent="BudgetUser-budgetUserSequenceNumber">
  </bean>
  <bean id="UserAppointmentTask-budgetTaskSequenceNumber" parent="UserAppointmentTask-budgetTaskSequenceNumber-parentBean" />

  <bean id="UserAppointmentTask-budgetTaskSequenceNumber-parentBean" abstract="true" parent="BudgetTask-budgetTaskSequenceNumber">
  </bean>
  <bean id="UserAppointmentTask-institutionAppointmentTypeCode" parent="UserAppointmentTask-institutionAppointmentTypeCode-parentBean" />

  <bean id="UserAppointmentTask-institutionAppointmentTypeCode-parentBean" abstract="true" parent="BudgetUser-appointmentTypeCode">
    <property name="name" value="institutionAppointmentTypeCode" />
    <property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
  </bean>
</beans>
