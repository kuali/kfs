<?xml version="1.0" encoding="UTF-8"?>

<!-- DV, PREQ and CM Document Income Type Business Objects -->
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="DocumentIncomeType" parent="DocumentIncomeType-parentBean"/>

  <bean id="DocumentIncomeType-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="edu.arizona.kfs.tax.businessobject.DocumentIncomeType"/>
    <property name="objectLabel" value="Document Income Type"/>
    <property name="attributes">
      <list>
        <ref bean="DocumentIncomeType-lineNumber"/>
        <ref bean="DocumentIncomeType-documentIdentifier"/>
        <ref bean="DocumentIncomeType-chartOfAccountsCode"/>
        <ref bean="DocumentIncomeType-incomeTypeCode"/>
        <ref bean="DocumentIncomeType-amount"/>
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->
  <bean id="DocumentIncomeType-lineNumber" parent="DocumentIncomeType-lineNumber-parentBean"/>
  <bean id="DocumentIncomeType-lineNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="lineNumber"/>
    <property name="forceUppercase" value="true"/>
    <property name="label" value="Line Number"/>
    <property name="shortLabel" value="Line Number"/>
    <property name="maxLength" value="3"/>
    <property name="validationPattern">
      <bean parent="NumericValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="5"/>
    </property>
  </bean>

  <bean id="DocumentIncomeType-documentIdentifier" parent="DocumentIncomeType-documentIdentifier-parentBean"/>
  <bean id="DocumentIncomeType-documentIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="documentIdentifier"/>
    <property name="forceUppercase" value="true"/>
    <property name="label" value="Document Identifier"/>
    <property name="shortLabel" value="Document ID"/>
    <property name="maxLength" value="8"/>
    <property name="validationPattern">
      <bean parent="NumericValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="10"/>
    </property>
  </bean>

  <bean id="DocumentIncomeType-chartOfAccountsCode" parent="DocumentIncomeType-chartOfAccountsCode-parentBean"/>
  <bean id="DocumentIncomeType-chartOfAccountsCode-parentBean" abstract="true" parent="Chart-chartOfAccountsCode">
    <property name="control">
      <bean parent="SelectControlDefinition" p:valuesFinderClass="org.kuali.kfs.coa.businessobject.options.SimpleChartValuesFinder" p:includeKeyInLabel="false"/>
    </property>
  </bean>

  <bean id="DocumentIncomeType-incomeTypeCode" parent="DocumentIncomeType-incomeTypeCode-parentBean"/>
  <bean id="DocumentIncomeType-incomeTypeCode-parentBean" abstract="true" parent="IncomeType-incomeTypeCode">
    <property name="label" value="Income Type/Description"/>
    <property name="shortLabel" value="Income Type/Description"/>
    <property name="control">
      <bean parent="SelectControlDefinition" p:valuesFinderClass="edu.arizona.kfs.tax.businessobject.options.IncomeTypeValuesFinder"/>
    </property>
    <property name="required" value="true"/>
  </bean>

  <bean id="DocumentIncomeType-amount" parent="DocumentIncomeType-amount-parentBean"/>
  <bean id="DocumentIncomeType-amount-parentBean" abstract="true" parent="GenericAttributes-genericAmount">
    <property name="name" value="amount"/>
    <property name="label" value="Amount"/>
    <property name="shortLabel" value="Amount"/>
  </bean>

<!-- Business Object Lookup Definition -->

<!-- Business Object Inquiry Definition -->

</beans>
