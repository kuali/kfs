<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="RoutingFormOrganizationCreditPercent" parent="RoutingFormOrganizationCreditPercent-parentBean" />

  <bean id="RoutingFormOrganizationCreditPercent-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.kra.routingform.bo.RoutingFormOrganizationCreditPercent" />
    <property name="inquiryDefinition" >
      <ref bean="RoutingFormOrganizationCreditPercent-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="RoutingFormOrganizationCreditPercent-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="RoutingFormOrganizationCreditPercent-chartOfAccountsCode" />
        <ref bean="RoutingFormOrganizationCreditPercent-documentNumber" />
        <ref bean="RoutingFormOrganizationCreditPercent-organizationCode" />
        <ref bean="RoutingFormOrganizationCreditPercent-organizationCreditPercent" />
        <ref bean="RoutingFormOrganizationCreditPercent-organizationCreditRoleText" />
        <ref bean="RoutingFormOrganizationCreditPercent-organizationFinancialAidPercent" />
        <ref bean="RoutingFormOrganizationCreditPercent-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="RoutingFormOrganizationCreditPercent-chartOfAccountsCode" parent="RoutingFormOrganizationCreditPercent-chartOfAccountsCode-parentBean" />

  <bean id="RoutingFormOrganizationCreditPercent-chartOfAccountsCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="chartOfAccountsCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Chart" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    <property name="summary" value="chartOfAccountsCode" />
    <property name="description" value="chartOfAccountsCode description..." />
  </bean>
  <bean id="RoutingFormOrganizationCreditPercent-documentNumber" parent="RoutingFormOrganizationCreditPercent-documentNumber-parentBean" />

  <bean id="RoutingFormOrganizationCreditPercent-documentNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="documentNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Document Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="14" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="16" />
    </property>
    <property name="summary" value="documentNumber" />
    <property name="description" value="documentNumber description..." />
  </bean>
  <bean id="RoutingFormOrganizationCreditPercent-organizationCode" parent="RoutingFormOrganizationCreditPercent-organizationCode-parentBean" />

  <bean id="RoutingFormOrganizationCreditPercent-organizationCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="organizationCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Org" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="6" />
    </property>
    <property name="summary" value="organizationCode" />
    <property name="description" value="organizationCode description..." />
  </bean>
  <bean id="RoutingFormOrganizationCreditPercent-organizationCreditPercent" parent="RoutingFormOrganizationCreditPercent-organizationCreditPercent-parentBean" />

  <bean id="RoutingFormOrganizationCreditPercent-organizationCreditPercent-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="organizationCreditPercent" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="% Credit" />
    <property name="shortLabel" value="Percent" />
    <property name="maxLength" value="3" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="5" />
    </property>
    <property name="summary" value="organizationCreditPercent" />
    <property name="description" value="organizationCreditPercent description..." />
  </bean>
  <bean id="RoutingFormOrganizationCreditPercent-organizationCreditRoleText" parent="RoutingFormOrganizationCreditPercent-organizationCreditRoleText-parentBean" />

  <bean id="RoutingFormOrganizationCreditPercent-organizationCreditRoleText-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="organizationCreditRoleText" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Role" />
    <property name="shortLabel" value="Text" />
    <property name="maxLength" value="50" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="50" />
    </property>
    <property name="summary" value="organizationCreditRoleText" />
    <property name="description" value="organizationCreditRoleText description..." />
  </bean>
  <bean id="RoutingFormOrganizationCreditPercent-organizationFinancialAidPercent" parent="RoutingFormOrganizationCreditPercent-organizationFinancialAidPercent-parentBean" />

  <bean id="RoutingFormOrganizationCreditPercent-organizationFinancialAidPercent-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="organizationFinancialAidPercent" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="% F&amp;A" />
    <property name="shortLabel" value="Percent" />
    <property name="maxLength" value="3" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="5" />
    </property>
    <property name="summary" value="organizationFinancialAidPercent" />
    <property name="description" value="organizationFinancialAidPercent description..." />
  </bean>
  <bean id="RoutingFormOrganizationCreditPercent-versionNumber" parent="RoutingFormOrganizationCreditPercent-versionNumber-parentBean" />

  <bean id="RoutingFormOrganizationCreditPercent-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="RoutingFormOrganizationCreditPercent-inquiryDefinition" parent="RoutingFormOrganizationCreditPercent-inquiryDefinition-parentBean" />

  <bean id="RoutingFormOrganizationCreditPercent-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Routing Form Organization Credit Percent Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="chartOfAccountsCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="documentNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="organizationCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="organizationCreditPercent" />
              <bean parent="FieldDefinition"
                    p:attributeName="organizationCreditRoleText" />
              <bean parent="FieldDefinition"
                    p:attributeName="organizationFinancialAidPercent" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="RoutingFormOrganizationCreditPercent-lookupDefinition" parent="RoutingFormOrganizationCreditPercent-lookupDefinition-parentBean" />

  <bean id="RoutingFormOrganizationCreditPercent-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Routing Form Organization Credit Percent Lookup" />
    <property name="instructions" value="Look up a Routing Form Organization Credit Percent" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationCode" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationCreditPercent" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationCreditRoleText" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationFinancialAidPercent" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationCode" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationCreditPercent" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationCreditRoleText" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationFinancialAidPercent" />
      </list>
    </property>
  </bean>
</beans>
