    </property>
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.kfs.lookup.keyvalues.CountryNotRestrictedValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
  </bean>
  <bean id="VendorGenericAttributes-internationalProvinceName" parent="VendorGenericAttributes-internationalProvinceName-parentBean" />

  <bean id="VendorGenericAttributes-internationalProvinceName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="internationalProvinceName" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Province" />
