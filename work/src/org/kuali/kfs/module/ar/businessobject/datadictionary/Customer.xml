	<bean id="Customer-versionNumber"
		parent="Customer-versionNumber-parentBean" />

	<bean id="Customer-versionNumber-parentBean" abstract="true"
		parent="GenericAttributes-versionNumber">
	</bean>

	<!-- Business Object Inquiry Definition -->

	<bean id="Customer-inquiryDefinition"
		parent="Customer-inquiryDefinition-parentBean" />

	<bean id="Customer-inquiryDefinition-parentBean" abstract="true"
		parent="InquiryDefinition">
		<property name="title" value="Customer Inquiry" />
		<property name="inquirySections">
			<list>
				<bean parent="InquirySectionDefinition">
					<property name="title" value="General Information" />
					<property name="numberOfColumns" value="1" />
					<property name="inquiryFields">
						<list>
							<bean parent="FieldDefinition"
								p:attributeName="customerNumber" />
							<bean parent="FieldDefinition"
								p:attributeName="customerParentCompanyNumber" />
							<bean parent="FieldDefinition"
								p:attributeName="customerName" />
							<bean parent="FieldDefinition"
								p:attributeName="customerTypeCode" />
			                <bean parent="FieldDefinition"
								p:attributeName="customerActiveIndicator" />
							<bean parent="FieldDefinition"
								p:attributeName="customerRecordAddDate" />
							<bean parent="FieldDefinition"
								p:attributeName="customerLastActivityDate" />
							<bean parent="FieldDefinition"
								p:attributeName="customerAddressChangeDate" />
						</list>
					</property>
				</bean>

                <bean parent="InquirySectionDefinition">
					<property name="title" value="Corporate Information" />
					<property name="numberOfColumns" value="1" />
					<property name="inquiryFields">
						<list>
							<bean parent="FieldDefinition"
								p:attributeName="customerSocialSecurityNumberIdentifier" />
							<bean parent="FieldDefinition"
								p:attributeName="customerFederalIdentifierNumber" />
							<bean parent="FieldDefinition"
								p:attributeName="customerTaxExemptIndicator" />
					        <bean parent="FieldDefinition"
								p:attributeName="customerBirthDate" />
							<bean parent="FieldDefinition"
								p:attributeName="customerCreditApprovedByName" />
							<bean parent="FieldDefinition"
								p:attributeName="customerCreditLimitAmount" />
						</list>
					</property>
				</bean>
				
				 <bean parent="InquirySectionDefinition">
					<property name="title" value="Contact Information" />
					<property name="numberOfColumns" value="1" />
					<property name="inquiryFields">
						<list>
							<bean parent="FieldDefinition"
								p:attributeName="customerEmailAddress" />
						    <bean parent="FieldDefinition"
								p:attributeName="customerPhoneNumber" />
							<bean parent="FieldDefinition"
								p:attributeName="customer800PhoneNumber" />
							<bean parent="FieldDefinition"
								p:attributeName="customerFaxNumber" />
							<bean parent="FieldDefinition"
								p:attributeName="customerContactName" />
							<bean parent="FieldDefinition"
								p:attributeName="customerContactPhoneNumber" />
						</list>
					</property>
				</bean>
				
				<bean parent="InquirySectionDefinition">
					<property name="title" value="Address" />
					<property name="numberOfColumns" value="1" />
					<property name="inquiryFields">
						<list>
							<bean
								parent="InquiryCollectionDefinition">
								<property name="attributeName"
									value="customerAddresses" />
								<property name="businessObjectClass"
									value="org.kuali.module.ar.bo.CustomerAddress" />
								<property name="numberOfColumns"
									value="1" />
								<property name="inquiryFields">
									<list>
									   <bean parent="FieldDefinition"
											p:attributeName="customerAddressTypeCode" />
										<bean parent="FieldDefinition"
											p:attributeName="customerAddressName" />
										<bean parent="FieldDefinition"
											p:attributeName="customerLine1StreetAddress" />
										<bean parent="FieldDefinition"
											p:attributeName="customerLine2StreetAddress" />
										<bean parent="FieldDefinition"
											p:attributeName="customerCityName" />
									    <bean parent="FieldDefinition"
											p:attributeName="customerStateCode" />
										<bean parent="FieldDefinition"
											p:attributeName="customerZipCode" />
										<bean parent="FieldDefinition"
											p:attributeName="customerAddressInternationalProvinceName" />	
										<bean parent="FieldDefinition"
											p:attributeName="customerInternationalMailCode" />
										<bean parent="FieldDefinition"
											p:attributeName="customerCountryCode" />
										<bean parent="FieldDefinition"
											p:attributeName="customerEmailAddress" />
										<bean parent="FieldDefinition"
											p:attributeName="customerAddressEndDate" />
									</list>
								</property>
								<property name="summaryFields">
									<list>
										<bean parent="FieldDefinition"
											p:attributeName="customerAddressTypeCode" />
									</list>
								</property>
							</bean>
						</list>
					</property>
				</bean>

				<bean parent="InquirySectionDefinition">
					<property name="title" value="Notes" />
					<property name="numberOfColumns" value="1" />
					<property name="inquiryFields">
						<list>
							<bean
								parent="InquiryCollectionDefinition">
								<property name="attributeName"
									value="boNotes" />
								<property name="businessObjectClass"
									value="org.kuali.core.bo.Note" />
								<property name="numberOfColumns"
									value="1" />
								<property name="inquiryFields">
									<list>
										<bean parent="FieldDefinition"
											p:attributeName="noteText" />
										<bean parent="FieldDefinition"
											p:attributeName="notePostedTimestamp" />
										<bean parent="FieldDefinition"
											p:attributeName="authorUniversal.personName" />
									</list>
								</property>
							</bean>
						</list>
					</property>
				</bean>

			</list>
		</property>
	</bean>

	<!-- Business Object Lookup Definition -->


	<bean id="Customer-lookupDefinition"
		parent="Customer-lookupDefinition-parentBean" />

	<bean id="Customer-lookupDefinition-parentBean" abstract="true"
		parent="LookupDefinition">
		<property name="title" value="Customer Lookup" />
		<property name="instructions" value="Look up a Customer" />
		<property name="defaultSort">
			<bean parent="SortDefinition"></bean>
		</property>
		<property name="lookupFields">
			<list>
				<bean parent="FieldDefinition"
					p:attributeName="customerNumber" />
				<bean parent="FieldDefinition"
					p:attributeName="customerName" />
				<bean parent="FieldDefinition"
					p:attributeName="customerPhoneNumber" />
				<bean parent="FieldDefinition"
					p:attributeName="customerFederalIdentifierNumber" />
				<bean parent="FieldDefinition"
					p:attributeName="customerActiveIndicator" />
			</list>
		</property>
		<property name="resultFields">
			<list>
				<bean parent="FieldDefinition"
					p:attributeName="customerNumber" />
				<bean parent="FieldDefinition"
					p:attributeName="customerName" />
				<bean parent="FieldDefinition"
					p:attributeName="customerPhoneNumber" />
				<bean parent="FieldDefinition"
					p:attributeName="customerFederalIdentifierNumber" />
				<bean parent="FieldDefinition"
					p:attributeName="customerActiveIndicator" />
			</list>
		</property>
	</bean>
</beans>
