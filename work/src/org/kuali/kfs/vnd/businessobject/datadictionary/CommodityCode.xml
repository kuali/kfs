<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="CommodityCode" parent="CommodityCode-parentBean" />

  <bean id="CommodityCode-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.vendor.bo.CommodityCode" />
    <property name="inquiryDefinition" >
      <ref bean="CommodityCode-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="CommodityCode-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="purchasingCommodityCode" />
    <property name="objectLabel" value="Commodity Code" />
    <property name="attributes" >
      <list>
        <ref bean="CommodityCode-purchasingCommodityCode" />
        <ref bean="CommodityCode-commodityDescription" />
        <ref bean="CommodityCode-salesTaxIndicator" />
        <ref bean="CommodityCode-restrictedItemsIndicator" />
        <ref bean="CommodityCode-restrictedMaterialCode" />
        <ref bean="CommodityCode-restrictedMaterial.restrictedMaterialDescription" />
        <ref bean="CommodityCode-active" />
        <ref bean="CommodityCode-versionNumber" />
        <ref bean="CommodityCode-commodityContractManagers.campusCode" />
        <ref bean="CommodityCode-commodityContractManagers.contractManagerCode" />
      </list>
    </property>
        <property name="collections" >
      <list>
        <bean parent="CollectionDefinition"
              p:name="commodityContractManagers"
              p:label="Contract Managers"
              p:shortLabel="Contract Managers"
              p:elementLabel="Contract Manager" />
      </list>
    </property>      
  </bean>

<!-- Attribute Definitions -->


  <bean id="CommodityCode-purchasingCommodityCode" parent="CommodityCode-purchasingCommodityCode-parentBean" />

  <bean id="CommodityCode-purchasingCommodityCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="purchasingCommodityCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Commodity Code" />
    <property name="shortLabel" value="Comm Code" />
    <property name="maxLength" value="40" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="42" />
    </property>
  </bean>
  <bean id="CommodityCode-commodityDescription" parent="CommodityCode-commodityDescription-parentBean" />

  <bean id="CommodityCode-commodityDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="commodityDescription" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Commodity Description" />
    <property name="shortLabel" value="Comm Desc" />
    <property name="maxLength" value="45" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="47" />
    </property>
  </bean>
  <bean id="CommodityCode-salesTaxIndicator" parent="CommodityCode-salesTaxIndicator-parentBean" />

  <bean id="CommodityCode-salesTaxIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="salesTaxIndicator" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Sales Tax Indicator" />
    <property name="shortLabel" value="Sales Tax" />
    <property name="maxLength" value="1" />
    <property name="control" >
      <bean parent="CheckboxControlDefinition" />
    </property>
  </bean>
  <bean id="CommodityCode-restrictedItemsIndicator" parent="CommodityCode-restrictedItemsIndicator-parentBean" />

  <bean id="CommodityCode-restrictedItemsIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="restrictedItemsIndicator" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Restricted Items Indicator" />
    <property name="shortLabel" value="Restricted Items" />
    <property name="maxLength" value="1" />
    <property name="control" >
      <bean parent="CheckboxControlDefinition" />
    </property>
  </bean>
  <bean id="CommodityCode-restrictedMaterialCode" parent="CommodityCode-restrictedMaterialCode-parentBean" />

  <bean id="CommodityCode-restrictedMaterialCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="restrictedMaterialCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Restricted Material" />
    <property name="shortLabel" value="Restricted Material" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.module.vendor.lookup.keyvalues.RestrictedMaterialValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
  </bean>
  <bean id="CommodityCode-restrictedMaterial.restrictedMaterialDescription" parent="CommodityCode-restrictedMaterial.restrictedMaterialDescription-parentBean" />

  <bean id="CommodityCode-restrictedMaterial.restrictedMaterialDescription-parentBean" abstract="true" parent="RestrictedMaterial-restrictedMaterialDescription">
    <property name="name" value="restrictedMaterial.restrictedMaterialDescription" />
    <property name="label" value="Restricted Material" />
  </bean>
  <bean id="CommodityCode-active" parent="CommodityCode-active-parentBean" />

  <bean id="CommodityCode-active-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
    <property name="name" value="active" />
  </bean>
  <bean id="CommodityCode-versionNumber" parent="CommodityCode-versionNumber-parentBean" />

  <bean id="CommodityCode-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>
  <bean id="CommodityCode-commodityContractManagers.campusCode" parent="CommodityCode-commodityContractManagers.campusCode-parentBean" />

  <bean id="CommodityCode-commodityContractManagers.campusCode-parentBean" abstract="true" parent="CommodityContractManager-campusCode">
    <property name="name" value="commodityContractManagers.campusCode" />
  </bean>  
  <bean id="CommodityCode-commodityContractManagers.contractManagerCode" parent="CommodityCode-commodityContractManagers.contractManagerCode-parentBean" />

  <bean id="CommodityCode-commodityContractManagers.contractManagerCode-parentBean" abstract="true" parent="CommodityContractManager-contractManagerCode">
    <property name="name" value="commodityContractManagers.contractManagerCode" />
  </bean>
  
<!-- Business Object Inquiry Definition -->


  <bean id="CommodityCode-inquiryDefinition" parent="CommodityCode-inquiryDefinition-parentBean" />

  <bean id="CommodityCode-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Commodity Code Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Commodity Code" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="purchasingCommodityCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="commodityDescription" />
              <bean parent="FieldDefinition"
                    p:attributeName="salesTaxIndicator" />
              <bean parent="FieldDefinition"
                    p:attributeName="restrictedItemsIndicator" />
              <bean parent="FieldDefinition"
                    p:attributeName="restrictedMaterialCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="active" />
            </list>
          </property>
        </bean>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Contract Managers" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="InquiryCollectionDefinition">
                <property name="attributeName" value="commodityContractManagers" />
                <property name="businessObjectClass" value="org.kuali.module.vendor.bo.CommodityContractManager" />
                <property name="numberOfColumns" value="1" />            
                <property name="inquiryFields" >
                  <list>
			              <bean parent="FieldDefinition"
      			              p:attributeName="campusCode" />
              			<bean parent="FieldDefinition"
                   				p:attributeName="contractManagerCode" />          			                    
                  </list>
                </property>
              </bean>
            </list>
          </property>
        </bean>                        
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="CommodityCode-lookupDefinition" parent="CommodityCode-lookupDefinition-parentBean" />

  <bean id="CommodityCode-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Commodity Code Lookup" />
    <property name="instructions" value="Lookup a Commodity Code" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="purchasingCommodityCode" />
        <bean parent="FieldDefinition"
              p:attributeName="commodityDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="salesTaxIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="restrictedItemsIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="restrictedMaterialCode" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
        <bean parent="FieldDefinition"
              p:attributeName="commodityContractManagers.campusCode" />              
        <bean parent="FieldDefinition"
              p:attributeName="commodityContractManagers.contractManagerCode" />              
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="purchasingCommodityCode" />
        <bean parent="FieldDefinition"
              p:attributeName="commodityDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="salesTaxIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="restrictedItemsIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="restrictedMaterial.restrictedMaterialDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />                      
      </list>
    </property>
  </bean>
</beans>
