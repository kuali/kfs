  <bean id="PurchaseOrderItem-itemLineNumber" parent="PurchaseOrderItem-itemLineNumber-parentBean" />

  <bean id="PurchaseOrderItem-itemLineNumber-parentBean" abstract="true" parent="AttributeDefinition">
