	        </maintainableSection>
	        <maintainableSection title="Account Responsibility">
                <maintainableItems>
		            <maintainableField name="accountFiscalOfficerSystemIdentifier" required="true" />
		            <maintainableField name="accountsSupervisorySystemsIdentifier" required="true" />
		            <maintainableField name="accountManagerSystemIdentifier" required="true" />
		            <maintainableField name="continuationFinChrtOfAcctCd" required="false" />
		            <maintainableField name="continuationAccountNumber" required="false" />    
