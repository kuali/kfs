  <bean id="PurchaseOrderCapitalAssetLocation-capitalAssetCountryCode" parent="PurchaseOrderCapitalAssetLocation-capitalAssetCountryCode-parentBean"/>
  <bean id="PurchaseOrderCapitalAssetLocation-capitalAssetCountryCode-parentBean" parent="AttributeDefinition" abstract="true" p:name="capitalAssetCountryCode" p:forceUppercase="false" p:label="Country" p:shortLabel="Country" p:maxLength="10" p:required="false">
