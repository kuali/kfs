<dictionaryEntry>
  <businessObject businessObjectClass="org.kuali.module.chart.bo.Org">
    <customAttributeEnabled>false</customAttributeEnabled>

    <inquiry>
      <title>Organization Inquiry</title>

      <inquiryFields>
        <field attributeName="organizationStateCode" />
        <field attributeName="organizationActiveIndicator" />
        <field attributeName="hrisOrganization.organizationCode" />
        <field attributeName="organizationType.organizationTypeCode" />
        <field
          attributeName="chartOfAccounts.chartOfAccountsCode"
          noInquiry="true" />
        <field attributeName="organizationName" />
        <field
          attributeName="reportsToOrganization.chartOfAccounts.chartOfAccountsCode" />
        <field attributeName="organizationZipCode" />
        <field attributeName="organizationPlantChart.chartOfAccountsCode" />
        <field attributeName="campusPlantAccount.accountNumber" />
        <field attributeName="reportsToOrganization.organizationCode" />
        <field
          attributeName="organizationCode"
          noInquiry="true" />
        <field attributeName="campusPlantAccount.chartOfAccounts.chartOfAccountsCode" />
        <field attributeName="organizationCityName" />
        <field attributeName="responsibilityCenter.responsibilityCenterCode" />
        <field attributeName="organizationBeginDate" />
        <field
          attributeName="organizationManagerUniversal.personUniversalIdentifier" />
        <field attributeName="organizationStreetAddress" />
      </inquiryFields>
    </inquiry>

    <lookup>
      <title>Organization Lookup</title>

      <menubar>
        <![CDATA[<a href="maintenance.do?methodToCall=start&businessObjectClassName=org.kuali.module.chart.bo.Org">Create New</a>]]>
      </menubar>

      <instructions>Lookup an Organization</instructions>

      <defaultSort
        attributeName="chartOfAccounts.chartOfAccountsCode"
        sortAscending="false" />

      <lookupFields>
        <lookupField
          attributeName="chartOfAccounts.chartOfAccountsCode"
          required="false" />
        <lookupField
          attributeName="organizationCode"
          required="false"
          forceLookup="true" />
        <lookupField
          attributeName="responsibilityCenter.responsibilityCenterCode"
          required="false" />
        <lookupField
          attributeName="organizationType.organizationTypeCode"
          required="false" />
        <lookupField
          attributeName="organizationName"
          required="false" />
        <lookupField
          attributeName="reportsToOrganization.chartOfAccounts.chartOfAccountsCode"
          required="false"
          noLookup="true" />
        <lookupField
          attributeName="reportsToOrganization.organizationCode"
          required="false" />
      </lookupFields>

      <resultFields>
        <field
          attributeName="chartOfAccounts.chartOfAccountsCode"
          noInquiry="true" />
        <field attributeName="organizationCode" />
        <field attributeName="responsibilityCenter.responsibilityCenterCode" />
        <field attributeName="organizationType.organizationTypeCode" />
        <field attributeName="organizationName" />
        <field attributeName="organizationActiveIndicator" />
      </resultFields>
    </lookup>

    <titleAttribute>organizationCode</titleAttribute>

    <attributes>
