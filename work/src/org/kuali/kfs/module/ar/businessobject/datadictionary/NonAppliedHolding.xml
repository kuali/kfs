<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="NonAppliedHolding" parent="NonAppliedHolding-parentBean" />

  <bean id="NonAppliedHolding-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.ar.bo.NonAppliedHolding" />
    <property name="inquiryDefinition" >
      <ref bean="NonAppliedHolding-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="NonAppliedHolding-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="NonAppliedHolding-customerNumber" />
        <ref bean="NonAppliedHolding-financialDocumentLineAmount" />
        <ref bean="NonAppliedHolding-referenceFinancialDocumentNumber" />
        <ref bean="NonAppliedHolding-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="NonAppliedHolding-customerNumber" parent="NonAppliedHolding-customerNumber-parentBean" />

  <bean id="NonAppliedHolding-customerNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="customerNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Customer Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="9" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="11" />
    </property>
    <property name="summary" value="customerNumber" />
    <property name="description" value="customerNumber description..." />
  </bean>
  <bean id="NonAppliedHolding-financialDocumentLineAmount" parent="NonAppliedHolding-financialDocumentLineAmount-parentBean" />

  <bean id="NonAppliedHolding-financialDocumentLineAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialDocumentLineAmount" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Financial Document Line Amount" />
    <property name="shortLabel" value="Amount" />
    <property name="maxLength" value="19" />
    <property name="validationPattern" >
      <bean parent="FloatingPointValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="21" />
    </property>
    <property name="summary" value="financialDocumentLineAmount" />
    <property name="description" value="financialDocumentLineAmount description..." />
  </bean>
  <bean id="NonAppliedHolding-referenceFinancialDocumentNumber" parent="NonAppliedHolding-referenceFinancialDocumentNumber-parentBean" />

  <bean id="NonAppliedHolding-referenceFinancialDocumentNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="referenceFinancialDocumentNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Reference Financial Document Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="14" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="16" />
    </property>
    <property name="summary" value="referenceFinancialDocumentNumber" />
    <property name="description" value="referenceFinancialDocumentNumber description..." />
  </bean>
  <bean id="NonAppliedHolding-versionNumber" parent="NonAppliedHolding-versionNumber-parentBean" />

  <bean id="NonAppliedHolding-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="NonAppliedHolding-inquiryDefinition" parent="NonAppliedHolding-inquiryDefinition-parentBean" />

  <bean id="NonAppliedHolding-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Non-Applied Holding Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="customerNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialDocumentLineAmount" />
              <bean parent="FieldDefinition"
                    p:attributeName="referenceFinancialDocumentNumber" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="NonAppliedHolding-lookupDefinition" parent="NonAppliedHolding-lookupDefinition-parentBean" />

  <bean id="NonAppliedHolding-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Non-Applied Holding Lookup" />
    <property name="instructions" value="Look up a Non-Applied Holding" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="customerNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentLineAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="referenceFinancialDocumentNumber" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="customerNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentLineAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="referenceFinancialDocumentNumber" />
      </list>
    </property>
  </bean>
</beans>
