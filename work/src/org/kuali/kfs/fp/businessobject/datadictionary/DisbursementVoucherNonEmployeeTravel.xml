                </control>

                <summary>
                    <![CDATA[&nbsp;]]>
                </summary>
                <description>The sum of Per Diem: Actual Amount, Personal Vehicle: Actual Amount, and Traveler Expenses: Expense Sub Total.</description>
            </attribute>

            <attribute name="totalPrePaidAmount">
                <label>Pre Paid Sub Total</label>
                <shortLabel>Total</shortLabel>

                <maxLength>22</maxLength>

                <required>false</required>

                <control>
