<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="ns:workflow" xmlns:fo="http://www.w3.org/1999/XSL/Format" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
<documentTypes xmlns="ns:workflow/DocumentType" xsi:schemaLocation="ns:workflow/DocumentType resource:DocumentType">
<documentType>
<name>VendorMaintenanceDocument</name>
<parent>VendorComplexMaintenanceDocument</parent>
<routePaths>
        <routePath>
          <start name="AdHoc" nextNode="Approval No Approval Split"/>
          <split name="Approval No Approval Split">
            <branch name="ApprovalBranch">
              <requests name="Management" nextNode="Initiator"/>
              <requests name="Initiator" nextNode="Join"/>
            </branch>
            <branch name="NoApprovalBranch">
              <simple name="Do Nothing" nextNode="Join"/>
            </branch>
            <join name="Join"/>
          </split>
        </routePath>
      </routePaths>
<routeNodes>
<start name="AdHoc"/>
<role name="Management">
<!--<ruleTemplate>KualiVendorDocumentTypeTemplate</ruleTemplate>-->
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
<role name="Initiator">
<!--<ruleTemplate>KualiVendorNotificationTemplate</ruleTemplate>-->
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
<split name="Approval No Approval Split">
<type>org.kuali.kfs.vnd.document.routing.node.VendorMaintenanceDocumentApprovalNoApprovalSplitNode</type>
</split>
<simple name="Do Nothing">
<type>org.kuali.rice.kew.engine.node.NoOpNode</type>
</simple>
<join name="Join"/>
</routeNodes>
</documentType>
</documentTypes>
</data>
