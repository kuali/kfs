        <ref bean="ReceivingCorrectionDocument-vendorDetailAssignedIdentifier" />
        <ref bean="ReceivingCorrectionDocument-vendorCityName" />
        <ref bean="ReceivingCorrectionDocument-vendorCountryCode" />
        <ref bean="ReceivingCorrectionDocument-vendorLine1Address" />
        <ref bean="ReceivingCorrectionDocument-vendorLine2Address" />
        <ref bean="ReceivingCorrectionDocument-vendorStateCode" />
        <ref bean="ReceivingCorrectionDocument-vendorPostalCode" />
        <ref bean="ReceivingCorrectionDocument-versionNumber" />
      </list>
    </property>
