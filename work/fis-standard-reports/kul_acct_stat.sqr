BEGIN-SELECT
TO_CHAR(SYSDATE, 'MM/DD/YY')                 &report_date
a.account_nbr                                &account_nbr        ()        ON-BREAK LEVEL=1 PRINT=NEVER PROCEDURE=MakeNewPageRenum
