<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../core/datadictionary/dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.chart.bo.codes.BudgetAggregationCode">
        <customAttributeEnabled>true</customAttributeEnabled>

        <inquiry>
            <title>Budget Aggregation Code Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="code" />
                <field attributeName="name" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Budget Aggregation Code Lookup</title>

            <instructions>Lookup a Budget Aggregation Code</instructions>

            <defaultSort attributeName="code" sortAscending="true" />

            <lookupFields>
                <lookupField attributeName="code" required="false" />
                <lookupField attributeName="name" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="code" />
                <field attributeName="name" />
            </resultFields>
        </lookup>

        <attributes>
            <attribute name="code" forceUppercase="true">
                <label>Budget Aggregation Code</label>
                <shortLabel>Budget Aggregation Code</shortLabel>

                <maxLength>1</maxLength>
                <validationPattern>
                    <alphaNumeric />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="1" />
                </control>

                <summary>Budget Aggregation Code</summary>
                <description>The code for a budget aggregation.</description>
            </attribute>

            <attribute name="name" forceUppercase="true">
                <label>Budget Aggregation Name</label>
                <shortLabel>Budget Aggregation Name</shortLabel>

                <maxLength>40</maxLength>

                <required>true</required>
                <control>
                    <text size="40" />
                </control>

                <summary>Budget Aggregation Name</summary>
                <description>Budget Aggregation Name...</description>
            </attribute>

            <attributeReference name="objectId" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="objectId" />

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

            <attributeReference name="active" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="activeIndicator" />
                        
        </attributes>
        
        <objectLabel>Budget Aggregation Code</objectLabel>
    </businessObject>
</dictionaryEntry>
