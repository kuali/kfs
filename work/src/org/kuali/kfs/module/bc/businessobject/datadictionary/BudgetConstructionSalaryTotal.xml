      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="6" />
    </property>
    <property name="summary" value="organizationCode" />
    <property name="description" value="organizationCode description..." />
  </bean>
  <bean id="BudgetConstructionSalaryTotal-personUniversalIdentifier" parent="BudgetConstructionSalaryTotal-personUniversalIdentifier-parentBean" />

