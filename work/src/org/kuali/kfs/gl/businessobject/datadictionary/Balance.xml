<!-- Business Object Inquiry Definition -->


  <bean id="Balance-inquiryDefinition" parent="Balance-inquiryDefinition-parentBean" />

  <bean id="Balance-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="General Ledger Balance" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="universityFiscalYear"
                    p:forceInquiry="true" />
              <bean parent="FieldDefinition"
                    p:attributeName="chartOfAccountsCode"
                    p:forceInquiry="true" />
              <bean parent="FieldDefinition"
                    p:attributeName="accountNumber"
                    p:forceInquiry="true" />
              <bean parent="FieldDefinition"
                    p:attributeName="subAccountNumber"
                    p:forceInquiry="true" />
              <bean parent="FieldDefinition"
                    p:attributeName="objectCode"
                    p:forceInquiry="true" />
              <bean parent="FieldDefinition"
                    p:attributeName="subObjectCode"
                    p:forceInquiry="true" />
              <bean parent="FieldDefinition"
                    p:attributeName="balanceTypeCode"
                    p:forceInquiry="true" />
              <bean parent="FieldDefinition"
                    p:attributeName="dummyBusinessObject.consolidationOption"
                    p:forceInquiry="true" />
              <bean parent="FieldDefinition"
                    p:attributeName="dummyBusinessObject.amountViewOption"
                    p:forceInquiry="true" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="Balance-lookupDefinition" parent="Balance-lookupDefinition-parentBean" />

  <bean id="Balance-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="General Ledger Balance Lookup" />
    <property name="lookupableID" value="glBalanceLookupable" />
    <property name="instructions" value="Lookup General Ledger balance for the specific accounts." />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>accountNumber</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:required="true"
              p:attributeName="universityFiscalYear"
              p:defaultValueFinderClass="org.kuali.kfs.lookup.valuefinder.FiscalYearFinder" />
        <bean parent="FieldDefinition"
              p:required="true"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:required="true"
              p:attributeName="accountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="subAccountNumber"
              p:forceLookup="true" />
        <bean parent="FieldDefinition"
              p:attributeName="objectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="subObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="balanceTypeCode"
              p:defaultValueFinderClass="org.kuali.module.gl.web.optionfinder.GLActualBalanceTypeOptionFinder" />
        <bean parent="FieldDefinition"
              p:attributeName="objectTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="dummyBusinessObject.consolidationOption"
              p:defaultValueFinderClass="org.kuali.module.gl.web.optionfinder.GLConsolidationOptionFinder" />
        <bean parent="FieldDefinition"
              p:attributeName="dummyBusinessObject.amountViewOption"
              p:defaultValueFinderClass="org.kuali.module.gl.web.optionfinder.GLAmountViewOptionFinder" />
        <bean parent="FieldDefinition"
              p:attributeName="dummyBusinessObject.pendingEntryOption"
              p:defaultValueFinderClass="org.kuali.module.gl.web.optionfinder.GLPendingEntryOptionFinder" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalYear" />
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="accountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="subAccountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="objectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="subObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="balanceTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="objectTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="beginningBalanceLineAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="contractsGrantsBeginningBalanceAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="accountLineAnnualBalanceAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="month1Amount" />
        <bean parent="FieldDefinition"
              p:attributeName="month2Amount" />
        <bean parent="FieldDefinition"
              p:attributeName="month3Amount" />
        <bean parent="FieldDefinition"
              p:attributeName="month4Amount" />
        <bean parent="FieldDefinition"
              p:attributeName="month5Amount" />
        <bean parent="FieldDefinition"
              p:attributeName="month6Amount" />
        <bean parent="FieldDefinition"
              p:attributeName="month7Amount" />
        <bean parent="FieldDefinition"
              p:attributeName="month8Amount" />
        <bean parent="FieldDefinition"
              p:attributeName="month9Amount" />
        <bean parent="FieldDefinition"
              p:attributeName="month10Amount" />
        <bean parent="FieldDefinition"
              p:attributeName="month11Amount" />
        <bean parent="FieldDefinition"
              p:attributeName="month12Amount" />
        <bean parent="FieldDefinition"
              p:attributeName="month13Amount" />
      </list>
    </property>
  </bean>
</beans>
