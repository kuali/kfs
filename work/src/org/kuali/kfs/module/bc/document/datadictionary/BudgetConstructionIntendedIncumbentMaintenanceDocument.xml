<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="IntendedIncumbentMaintenanceDocument" parent="IntendedIncumbentMaintenanceDocument-parentBean" />

  <bean id="IntendedIncumbentMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.budget.bo.BudgetConstructionIntendedIncumbent" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="IntendedIncumbentMaintenanceDocument-EditBCIntendedIncumbent" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>emplid</value>
      </list>
    </property>
    <property name="label" value="Intended Incumbent" />
    <property name="shortLabel" value="BC - BC Intended Incumbent Code Maint Doc" />
    <property name="summary" value="Budget Construction Intended Incumbent Maintenance Document" />
    <property name="description" value="Document used to create or update Budget Construction Intended Incumbent Maintenance Document objects" />
    <property name="documentTypeName" value="IntendedIncumbentMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="IINC" />
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>SY_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="IntendedIncumbentMaintenanceDocument-EditBCIntendedIncumbent" parent="IntendedIncumbentMaintenanceDocument-EditBCIntendedIncumbent-parentBean" />

  <bean id="IntendedIncumbentMaintenanceDocument-EditBCIntendedIncumbent-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit BC Intended Incumbent" />
    <property name="title" value="Edit BC Intended Incumbent" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="emplid"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="personName"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="setidSalary" />
        <bean parent="MaintainableFieldDefinition"
              p:name="salaryAdministrationPlan" />
        <bean parent="MaintainableFieldDefinition"
              p:name="grade" />
        <bean parent="MaintainableFieldDefinition"
              p:name="iuClassificationLevel" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>
</beans>
