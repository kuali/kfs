<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.financial.bo.ProcurementCardHolder">


        <inquiry>
            <title>Procurement Card Holder Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="documentNumber" />
                <field attributeName="cardCycleAmountLimit" />
                <field attributeName="cardCycleVolumeLimit" />
                <field attributeName="cardHolderName" />
                <field attributeName="cardHolderAlternateName" />
                <field attributeName="cardHolderCityName" />
                <field attributeName="cardHolderLine1Address" />
                <field attributeName="cardHolderLine2Address" />
                <field attributeName="cardHolderStateCode" />
                <field attributeName="cardHolderWorkPhoneNumber" />
                <field attributeName="cardHolderZipCode" />
                <field attributeName="cardLimit" />
                <field attributeName="cardNoteText" />
                <field attributeName="cardStatusCode" />
                <field attributeName="chartOfAccountsCode" />
                <field attributeName="accountNumber" />
                <field attributeName="subAccountNumber" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <attributes>

            <attributeReference name="accountNumber" sourceClassName="org.kuali.module.chart.bo.Account" sourceAttributeName="accountNumber">
                <label>Default Account</label>
                <shortLabel>Dflt Acct</shortLabel>
            </attributeReference>

            <attribute name="cardCycleAmountLimit" forceUppercase="true">
                <label>Card Cycle Amount Limit</label>
                <shortLabel>Limit</shortLabel>

                <maxLength>19</maxLength>
                <validationPattern>
                    <floatingPoint />
                </validationPattern>

                <control>
                    <text size="21" />
                </control>
            </attribute>

            <attribute name="cardCycleVolumeLimit" forceUppercase="true">
                <label>Card Cycle Volume Limit</label>
                <shortLabel>Limit</shortLabel>

                <maxLength>19</maxLength>
                <validationPattern>
                    <floatingPoint />
                </validationPattern>

                <control>
                    <text size="21" />
                </control>
            </attribute>

            <attribute name="cardHolderAlternateName" forceUppercase="true">
                <label>Card Holder Alternate Name</label>
                <shortLabel>Name</shortLabel>

                <maxLength>45</maxLength>

                <control>
                    <text size="47" />
                </control>
            </attribute>

            <attribute name="cardHolderCityName" forceUppercase="true">
                <label>Card Holder City Name</label>
                <shortLabel>Name</shortLabel>

                <maxLength>45</maxLength>

                <control>
                    <text size="47" />
                </control>
            </attribute>

            <attribute name="cardHolderLine1Address" forceUppercase="true">
                <label>Card Holder Line1 Address</label>
                <shortLabel>Address</shortLabel>

                <maxLength>45</maxLength>

                <control>
                    <text size="47" />
                </control>
            </attribute>

            <attribute name="cardHolderLine2Address" forceUppercase="true">
                <label>Card Holder Line2 Address</label>
                <shortLabel>Address</shortLabel>

                <maxLength>45</maxLength>

                <control>
                    <text size="47" />
                </control>
            </attribute>

            <attribute name="cardHolderName" forceUppercase="true">
                <label>Card Holder Name</label>
                <shortLabel>Name</shortLabel>

                <maxLength>35</maxLength>

                <control>
                    <text size="37" />
                </control>
            </attribute>

            <attribute name="cardHolderStateCode" forceUppercase="true">
                <label>Card Holder State Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>2</maxLength>

                <control>
                    <text size="4" />
                </control>
            </attribute>

            <attribute name="cardHolderWorkPhoneNumber" forceUppercase="true">
                <label>Card Holder Work Phone Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>10</maxLength>

                <control>
                    <text size="12" />
                </control>
            </attribute>

            <attribute name="cardHolderZipCode" forceUppercase="true">
                <label>Card Holder Postal (ZIP) Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>20</maxLength>

                <control>
                    <text size="22" />
                </control>
            </attribute>

            <attribute name="cardLimit" forceUppercase="true">
                <label>Card Limit</label>
                <shortLabel>Limit</shortLabel>

                <maxLength>19</maxLength>
                <validationPattern>
                    <floatingPoint />
                </validationPattern>

                <control>
                    <text size="21" />
                </control>
            </attribute>

            <attribute name="cardNoteText" forceUppercase="true">
                <label>Card Note Text</label>
                <shortLabel>Text</shortLabel>

                <maxLength>50</maxLength>

                <control>
                    <text size="52" />
                </control>
            </attribute>

            <attribute name="cardStatusCode" forceUppercase="true">
                <label>Card Status Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>2</maxLength>

                <control>
                    <text size="4" />
                </control>
            </attribute>

            <attributeReference name="chartOfAccountsCode" sourceClassName="org.kuali.module.chart.bo.Chart" sourceAttributeName="chartOfAccountsCode">
                <label>Default Chart</label>
                <shortLabel>Dflt Chrt</shortLabel>
            </attributeReference>

            <attributeReference name="documentNumber" sourceClassName="org.kuali.core.bo.DocumentHeader" sourceAttributeName="documentNumber" />

            <attributeReference name="subAccountNumber" sourceClassName="org.kuali.module.chart.bo.SubAccount" sourceAttributeName="subAccountNumber">
                <label>Default Sub-Account</label>
                <shortLabel>Dflt Sub-Acct</shortLabel>
                <required>false</required>
            </attributeReference>

            <attribute name="transactionCreditCardNumber" forceUppercase="true">
                <label>Transaction Credit Card Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>16</maxLength>

                <control>
                    <text size="18" />
                </control>
            </attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
