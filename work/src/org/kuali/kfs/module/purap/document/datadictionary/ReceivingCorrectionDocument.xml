        </bean>
      </list>
    </property>
    <property name="attributes" >
      <list>
        <ref bean="ReceivingCorrectionDocument-documentNumber" />
        <ref bean="ReceivingCorrectionDocument-receivingLineDocumentNumber" />
        <ref bean="ReceivingCorrectionDocument-carrierCode" />
        <ref bean="ReceivingCorrectionDocument-deliveryBuildingCode" />
        <ref bean="ReceivingCorrectionDocument-deliveryBuildingLine1Address" />
        <ref bean="ReceivingCorrectionDocument-deliveryBuildingLine2Address" />
        <ref bean="ReceivingCorrectionDocument-deliveryBuildingName" />
        <ref bean="ReceivingCorrectionDocument-deliveryBuildingOther" />
        <ref bean="ReceivingCorrectionDocument-deliveryBuildingRoomNumber" />
        <ref bean="ReceivingCorrectionDocument-deliveryCampusCode" />
        <ref bean="ReceivingCorrectionDocument-deliveryCityName" />
        <ref bean="ReceivingCorrectionDocument-deliveryCountryCode" />
        <ref bean="ReceivingCorrectionDocument-deliveryInstructionText" />
        <ref bean="ReceivingCorrectionDocument-deliveryPostalCode" />
        <ref bean="ReceivingCorrectionDocument-deliveryRequiredDate" />
        <ref bean="ReceivingCorrectionDocument-deliveryRequiredDateReasonCode" />
        <ref bean="ReceivingCorrectionDocument-deliveryStateCode" />
        <ref bean="ReceivingCorrectionDocument-deliveryToEmailAddress" />
        <ref bean="ReceivingCorrectionDocument-deliveryToName" />
