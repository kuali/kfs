<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../core/datadictionary/dataDictionary-1_0.dtd">

<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.gl.bo.CashBalance">
        <customAttributeEnabled>false</customAttributeEnabled>
        <inquiry>
            <title>Cash Balance</title>
            <inquiryFields>
                <field attributeName="universityFiscalYear" />
                <field attributeName="chartOfAccountsCode" />
                <field attributeName="accountNumber" />
                <field attributeName="subAccountNumber" />
            </inquiryFields>
        </inquiry>

        <lookup>
            <lookupableID>glCashBalanceLookupable</lookupableID>
            <title>Cash Balance</title>
            <instructions>Lookup Cash Balance for the specific account.</instructions>

            <defaultSort attributeName="accountNumber" />
			
            <lookupFields>
                <lookupField attributeName="universityFiscalYear" required="true">
                    <defaultValueFinderClass>org.kuali.core.lookup.valueFinder.FiscalYearFinder</defaultValueFinderClass>
                </lookupField>

                <lookupField attributeName="chartOfAccountsCode" required="true" />
                <lookupField attributeName="accountNumber" required="true" />
                <lookupField attributeName="subAccountNumber" forceLookup="true" />

                <lookupField attributeName="dummyBusinessObject.consolidationOption">
                    <defaultValueFinderClass>org.kuali.module.gl.web.optionfinder.GLConsolidationOptionFinder</defaultValueFinderClass>
                </lookupField>

                <lookupField attributeName="dummyBusinessObject.pendingEntryOption">
                    <defaultValueFinderClass>org.kuali.module.gl.web.optionfinder.GLPendingEntryOptionFinder</defaultValueFinderClass>
                </lookupField>
            </lookupFields>

            <resultFields>
                <field attributeName="universityFiscalYear" />
                <field attributeName="chartOfAccountsCode" />
                <field attributeName="accountNumber" />
                <field attributeName="subAccountNumber" />
                <field attributeName="beginningBalanceLineAmount" />
                <field attributeName="contractsGrantsBeginningBalanceAmount" />
                <field attributeName="accountLineAnnualBalanceAmount" />
                <field attributeName="dummyBusinessObject.genericAmount" />
            </resultFields>
        </lookup>

        <attributes>
            <attributeReference name="universityFiscalYear" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericFiscalYear">
                <label>Fiscal Year</label>
                <description>Identifies the Fiscal Year to which the balances apply.</description>
            </attributeReference>

            <attributeReference name="chartOfAccountsCode" sourceClassName="org.kuali.module.chart.bo.Chart" sourceAttributeName="chartOfAccountsCode">
                <control>
                    <text size="2" />
                </control>
                <description>The Chart of Accounts for an Account Number. Kuali accommodates more than one Chart of Accounts. Each chart has its own set of Account Numbers, Object Codes, and Organizations.</description>
            </attributeReference>

            <attributeReference name="accountNumber" sourceClassName="org.kuali.module.chart.bo.Account" sourceAttributeName="accountNumber">
                <description>An identifier for a pool of funds assigned to a specific division for a stated purpose.</description>
            </attributeReference>

            <attributeReference name="subAccountNumber" sourceClassName="org.kuali.module.chart.bo.SubAccount" sourceAttributeName="subAccountNumber">
                <description>A code that partitions an Account Number into greater detail. Sub-Accounts take on most of the attributes of the Account Number to which they report.</description>
            </attributeReference>

            <attributeReference name="objectCode" sourceClassName="org.kuali.module.chart.bo.ObjectCode" sourceAttributeName="financialObjectCode" />

            <attributeReference name="subObjectCode" sourceClassName="org.kuali.module.chart.bo.SubObjCd" sourceAttributeName="financialSubObjectCode" />

            <attribute name="objectTypeCode">
                <label>Object Type Code</label>
                <shortLabel>Object Type Code</shortLabel>
                <maxLength>2</maxLength>
                <control>
                    <text size="3" />
                </control>
                <summary>objectTypeCode</summary>
                <description>objectTypeCode description...</description>
            </attribute>

            <attributeReference name="balanceTypeCode" sourceClassName="org.kuali.module.chart.bo.codes.BalanceTyp" sourceAttributeName="financialBalanceTypeShortNm">
                <label>Balance Type Code</label>
                <shortLabel>Balance Type Code</shortLabel>
                <maxLength>2</maxLength>
                <control>
                    <text size="3" />
                </control>
                <summary>balanceTypeCode</summary>
                <description>balanceTypeCode description...</description>
            </attributeReference>

            <attributeReference name="accountLineAnnualBalanceAmount" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericAmount">
                <label>Account Line Annual Balance Amount</label>
                <shortLabel>Account Line Annual Balance Amount</shortLabel>
                <summary>accountLineAnnualBalanceAmount</summary>
                <description>accountLineAnnualBalanceAmount description...</description>
                <formatterClass>org.kuali.core.web.format.CurrencyFormatter</formatterClass>
            </attributeReference>

            <attributeReference name="beginningBalanceLineAmount" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericAmount">
                <label>Beginning Balance Line Amount</label>
