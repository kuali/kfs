<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="NonpersonnelCategory" parent="NonpersonnelCategory-parentBean" />

  <bean id="NonpersonnelCategory-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.kra.budget.bo.NonpersonnelCategory" />
    <property name="inquiryDefinition" >
      <ref bean="NonpersonnelCategory-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="NonpersonnelCategory-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="code" />
    <property name="objectLabel" value="Non-Personnel Category" />
    <property name="attributes" >
      <list>
        <ref bean="NonpersonnelCategory-code" />
        <ref bean="NonpersonnelCategory-active" />
        <ref bean="NonpersonnelCategory-name" />
        <ref bean="NonpersonnelCategory-sortNumber" />
        <ref bean="NonpersonnelCategory-objectId" />
        <ref bean="NonpersonnelCategory-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="NonpersonnelCategory-code" parent="NonpersonnelCategory-code-parentBean" />

  <bean id="NonpersonnelCategory-code-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="code" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Nonpersonnel Category Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AlphaValidationPattern"
            p:exactLength="2" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    <property name="summary" value="Nonpersonnel Category Code" />
    <property name="description" value="Nonpersonnel Category Code" />
  </bean>
  <bean id="NonpersonnelCategory-active" parent="NonpersonnelCategory-active-parentBean" />

  <bean id="NonpersonnelCategory-active-parentBean" abstract="true" parent="AttributeReferenceDummy-activeIndicator">
    <property name="name" value="active" />
  </bean>
  <bean id="NonpersonnelCategory-name" parent="NonpersonnelCategory-name-parentBean" />

  <bean id="NonpersonnelCategory-name-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="name" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Nonpersonnel Category Name" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="40" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="42" />
    </property>
    <property name="summary" value="Nonpersonnel Category Name" />
    <property name="description" value="The name of the Nonpersonnel Category." />
  </bean>
  <bean id="NonpersonnelCategory-sortNumber" parent="NonpersonnelCategory-sortNumber-parentBean" />

  <bean id="NonpersonnelCategory-sortNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="sortNumber" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Nonpersonnel Category Sort Number" />
    <property name="shortLabel" value="Sort Number" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="2" />
    </property>
    <property name="summary" value="Nonpersonnel Category Sort Number" />
    <property name="description" value="The order in which Nonpersonnel Categories will be displayed within the application." />
  </bean>
  <bean id="NonpersonnelCategory-objectId" parent="NonpersonnelCategory-objectId-parentBean" />

  <bean id="NonpersonnelCategory-objectId-parentBean" abstract="true" parent="AttributeReferenceDummy-objectId">
  </bean>
  <bean id="NonpersonnelCategory-versionNumber" parent="NonpersonnelCategory-versionNumber-parentBean" />

  <bean id="NonpersonnelCategory-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="NonpersonnelCategory-inquiryDefinition" parent="NonpersonnelCategory-inquiryDefinition-parentBean" />

  <bean id="NonpersonnelCategory-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Non-Personnel Category Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="code" />
              <bean parent="FieldDefinition"
                    p:attributeName="name" />
              <bean parent="FieldDefinition"
                    p:attributeName="sortNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="active" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="NonpersonnelCategory-lookupDefinition" parent="NonpersonnelCategory-lookupDefinition-parentBean" />

  <bean id="NonpersonnelCategory-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Non-Personnel Category Lookup" />
    <property name="instructions" value="Lookup a Nonpersonnel Category" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>code</value>
          </list>
        </property>
        <property name="sortAscending" value="false" />
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="code" />
        <bean parent="FieldDefinition"
              p:attributeName="name" />
        <bean parent="FieldDefinition"
              p:defaultValue="Y"
              p:attributeName="active" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="code"
              p:noInquiry="true" />
        <bean parent="FieldDefinition"
              p:attributeName="name" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
  </bean>
</beans>
