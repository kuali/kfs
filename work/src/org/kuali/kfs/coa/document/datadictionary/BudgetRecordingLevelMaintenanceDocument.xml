<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xmlns:dd="http://rice.kuali.org/dd" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd         http://rice.kuali.org/dd         http://rice.kuali.org/dd/dd.xsd">

  <bean id="BudgetRecordingLevelMaintenanceDocument" parent="BudgetRecordingLevelMaintenanceDocument-parentBean"/>

  <bean id="BudgetRecordingLevelMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.coa.businessobject.BudgetRecordingLevel"/>
    <property name="maintainableClass" value="org.kuali.kfs.sys.document.FinancialSystemMaintainable"/>
    <property name="maintainableSections">
      <list>
        <ref bean="BudgetRecordingLevelMaintenanceDocument-EditBudgetRecordingLevel"/>
      </list>
    </property>
    <property name="lockingKeys">
      <list>
        <value>code</value>
      </list>
    </property>
    
    
    
    
    
    <property name="documentTypeName" value="BRL"/>
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase"/>
    <property name="workflowAttributes">
    	<ref bean="BudgetRecordingLevelMaintenanceDocument-workflowAttributes"/>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->

  <bean id="BudgetRecordingLevelMaintenanceDocument-EditBudgetRecordingLevel" parent="BudgetRecordingLevelMaintenanceDocument-EditBudgetRecordingLevel-parentBean"/>

  <bean id="BudgetRecordingLevelMaintenanceDocument-EditBudgetRecordingLevel-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Budget Recording Level"/>
    <property name="title" value="Edit Budget Recording Level"/>
    <property name="maintainableItems">
      <list>
        <bean parent="MaintainableFieldDefinition" p:name="code" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="name" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="active" p:defaultValue="true"/>
      </list>
    </property>
  </bean>
  
  <!-- workflow attributes -->
  <bean id="BudgetRecordingLevelMaintenanceDocument-workflowAttributes" parent="BudgetRecordingLevelMaintenanceDocument-workflowAttributes-parentBean"/>
  
  <bean id="BudgetRecordingLevelMaintenanceDocument-workflowAttributes-parentBean" class="org.kuali.rice.kns.datadictionary.WorkflowAttributes" abstract="true">
  </bean>
</beans>