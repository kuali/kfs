  <bean id="CustomerInvoiceDocument-invoiceWriteoffAmount" parent="CustomerInvoiceDocument-invoiceWriteoffAmount-parentBean" />

  <bean id="CustomerInvoiceDocument-invoiceWriteoffAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="invoiceWriteoffAmount" />
    <property name="forceUppercase" value="false" />
