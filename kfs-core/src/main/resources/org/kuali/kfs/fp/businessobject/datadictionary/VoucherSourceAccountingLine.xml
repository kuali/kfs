    <property name="label" value="Sales Tax Required"/>
    <property name="shortLabel" value="Sales Tax Reqd"/>
    <property name="maxLength" value="10"/>
    <property name="required" value="false"/>
    <property name="control">
      <ref bean="HiddenControl" />
    </property>
    
    
  </bean>
