<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="ObjectCodeGlobalMaintenanceDocument" parent="ObjectCodeGlobalMaintenanceDocument-parentBean" />

  <bean id="ObjectCodeGlobalMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.chart.bo.ObjectCodeGlobal" />
    <property name="maintainableClass" value="org.kuali.module.chart.maintenance.ObjectCodeGlobalMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="ObjectCodeGlobalMaintenanceDocument-GlobalObjectCodeMaintenance" />
        <ref bean="ObjectCodeGlobalMaintenanceDocument-EditMultipleYearandChart" />
      </list>
    </property>
    <property name="defaultExistenceChecks" >
      <list>
        <bean parent="ReferenceDefinition"
              p:attributeName="finObjMandatoryTrnfrelim"
              p:attributeToHighlightOnFail="finObjMandatoryTrnfrOrElimCd" />
        <bean parent="ReferenceDefinition"
              p:attributeName="financialBudgetAggregation"
              p:attributeToHighlightOnFail="financialBudgetAggregationCd" />
        <bean parent="ReferenceDefinition"
              p:attributeName="financialFederalFunded"
              p:attributeToHighlightOnFail="financialFederalFundedCode" />
        <bean parent="ReferenceDefinition"
              p:attributeName="financialObjectType"
              p:attributeToHighlightOnFail="financialObjectTypeCode" />
        <bean parent="ReferenceDefinition"
              p:attributeName="financialObjectSubType"
              p:attributeToHighlightOnFail="financialObjectSubTypeCode" />
        <bean parent="ReferenceDefinition"
              p:attributeName="reportsToFinancialObject"
              p:attributeToHighlightOnFail="reportsToFinancialObjectCode" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>documentNumber</value>
      </list>
    </property>
    <property name="label" value="Object Code Global" />
    <property name="shortLabel" value="Object Code Global" />
    <property name="summary" value="Global Object Code Maintenance Document" />
    <property name="description" value="Document used to create or update global account document." />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-CA"
            p:parameterName="OBJECT_CODE_GLOBAL"
            p:parameterDetailType="ObjectCodeGlobal" />
    </property>
    <property name="documentTypeName" value="ObjectCodeGlobalMaintenanceDocument" />
    <property name="businessRulesClass" value="org.kuali.module.chart.rules.ObjectCodeGlobalRule" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="preRulesCheckClass" value="org.kuali.module.chart.rules.ObjectCodeGlobalPreRules" />
    <property name="documentTypeCode" value="GOBJ" />
    <property name="workflowProperties" >
      <ref bean="ObjectCodeGlobalMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>Kuali_ROLE_GLOBAL_OBJECT_CODE_USER</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="ObjectCodeGlobalMaintenanceDocument-GlobalObjectCodeMaintenance" parent="ObjectCodeGlobalMaintenanceDocument-GlobalObjectCodeMaintenance-parentBean" />

  <bean id="ObjectCodeGlobalMaintenanceDocument-GlobalObjectCodeMaintenance-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Global Object Code Maintenance" />
    <property name="title" value="Global Object Code Maintenance" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="financialObjectCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialObjectCodeName"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialObjectCodeShortName"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="reportsToFinancialObjectCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialObjectTypeCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialObjectLevelCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialObjectSubTypeCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="historicalFinancialObjectCode" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialObjectActiveIndicator"
              p:defaultValue="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialBudgetAggregationCd"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="finObjMandatoryTrnfrOrElimCd"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialFederalFundedCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="nextYearFinancialObjectCode" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>
  <bean id="ObjectCodeGlobalMaintenanceDocument-EditMultipleYearandChart" parent="ObjectCodeGlobalMaintenanceDocument-EditMultipleYearandChart-parentBean" />

  <bean id="ObjectCodeGlobalMaintenanceDocument-EditMultipleYearandChart-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Multiple Year and Chart" />
    <property name="title" value="Edit Multiple Year and Chart" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableCollectionDefinition">
          <property name="name" value="objectCodeGlobalDetails" />
          <property name="businessObjectClass" value="org.kuali.module.chart.bo.ObjectCodeGlobalDetail" />
          <property name="summaryTitle" value="Year and Chart" />
          <property name="summaryFields" >
            <list>
              <bean parent="MaintainableFieldDefinition"
                    p:name="universityFiscalYear" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="chartOfAccountsCode" />
            </list>
          </property>
          <property name="maintainableFields" >
            <list>
              <bean parent="MaintainableFieldDefinition"
                    p:name="universityFiscalYear"
                    p:defaultValueFinderClass="org.kuali.kfs.lookup.valuefinder.FiscalYearFinder"
                    p:readOnlyAfterAdd="true" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="chartOfAccountsCode"
                    p:readOnlyAfterAdd="true"
                    p:template="chartOfAccountsCode" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="versionNumber" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="newCollectionRecord" />
            </list>
          </property>
          <property name="duplicateIdentificationFields">
          	<list>
          		<bean parent="MaintainableFieldDefinition" p:name="chartOfAccountsCode" />
          	</list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="ObjectCodeGlobalMaintenanceDocument-workflowProperties" parent="ObjectCodeGlobalMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="ObjectCodeGlobalMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
    <property name="workflowPropertyGroups" >
      <list>
        <bean parent="WorkflowPropertyGroup">
          <property name="workflowProperties" >
            <list>
              <bean parent="WorkflowProperty"
                    p:path="newMaintainableObject.businessObject.objectCodeGlobalDetails" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>
</beans>
