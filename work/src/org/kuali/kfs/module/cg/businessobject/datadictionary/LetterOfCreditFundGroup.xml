<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="LetterOfCreditFundGroup" parent="LetterOfCreditFundGroup-parentBean"/>

  <bean id="LetterOfCreditFundGroup-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.cg.businessobject.LetterOfCreditFundGroup"/>
    <property name="inquiryDefinition">
      <ref bean="LetterOfCreditFundGroup-inquiryDefinition"/>
    </property>
    <property name="lookupDefinition">
      <ref bean="LetterOfCreditFundGroup-lookupDefinition"/>
    </property>
    <property name="objectLabel" value="Letter Of Credit Fund Group"/>
    <property name="attributes">
      <list>
        <ref bean="LetterOfCreditFundGroup-letterOfCreditFundGroupCode"/>
        <ref bean="LetterOfCreditFundGroup-letterOfCreditFundGroupDescription"/>
        <ref bean="LetterOfCreditFundGroup-active"/>
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="LetterOfCreditFundGroup-letterOfCreditFundGroupCode" parent="LetterOfCreditFundGroup-letterOfCreditFundGroupCode-parentBean"/>

  <bean id="LetterOfCreditFundGroup-letterOfCreditFundGroupCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="letterOfCreditFundGroupCode"/>
    <property name="forceUppercase" value="true"/>
    <property name="label" value="Letter Of Credit Fund Group Code"/>
    <property name="shortLabel" value="Code"/>
    <property name="maxLength" value="4"/>
    <property name="validationPattern">
      <bean parent="AlphaNumericValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="6"/>
    </property>
  </bean>
  <bean id="LetterOfCreditFundGroup-letterOfCreditFundGroupDescription" parent="LetterOfCreditFundGroup-letterOfCreditFundGroupDescription-parentBean"/>

  <bean id="LetterOfCreditFundGroup-letterOfCreditFundGroupDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="letterOfCreditFundGroupDescription"/>
    <property name="label" value="Letter Of Credit Fund Group Description"/>
    <property name="shortLabel" value="Description"/>
    <property name="maxLength" value="40"/>
    <property name="validationPattern">
      <bean parent="AnyCharacterValidationPattern" p:allowWhitespace="true"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="42"/>
    </property>
  </bean>
  <bean id="LetterOfCreditFundGroup-active" parent="LetterOfCreditFundGroup-active-parentBean"/>

  <bean id="LetterOfCreditFundGroup-active-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
    <property name="name" value="active"/>
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="LetterOfCreditFundGroup-inquiryDefinition" parent="LetterOfCreditFundGroup-inquiryDefinition-parentBean"/>

  <bean id="LetterOfCreditFundGroup-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Letter Of Credit Fund Group Inquiry"/>
    <property name="inquirySections">
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Letter of Credit Fund Group Details"/>
          <property name="numberOfColumns" value="1"/>
          <property name="inquiryFields">
            <list>
              <bean parent="FieldDefinition" p:attributeName="letterOfCreditFundGroupCode"/>
              <bean parent="FieldDefinition" p:attributeName="letterOfCreditFundGroupDescription"/>
              <bean parent="FieldDefinition" p:attributeName="active"/>
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="LetterOfCreditFundGroup-lookupDefinition" parent="LetterOfCreditFundGroup-lookupDefinition-parentBean"/>

  <bean id="LetterOfCreditFundGroup-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Letter Of Credit Fund Group Lookup"/>
    
    <property name="defaultSort">
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="letterOfCreditFundGroupCode"/>
        <bean parent="FieldDefinition" p:attributeName="letterOfCreditFundGroupDescription"/>
        <bean parent="FieldDefinition" p:attributeName="active"/>
      </list>
    </property>
    <property name="resultFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="letterOfCreditFundGroupCode"/>
        <bean parent="FieldDefinition" p:attributeName="letterOfCreditFundGroupDescription"/>
        <bean parent="FieldDefinition" p:attributeName="active"/>
      </list>
    </property>
  </bean>
</beans>