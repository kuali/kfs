<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="PositionObjectBenefitMaintenanceDocument" parent="PositionObjectBenefitMaintenanceDocument-parentBean" />

  <bean id="PositionObjectBenefitMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.labor.bo.PositionObjectBenefit" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="PositionObjectBenefitMaintenanceDocument-EditPositionObjectBenefit" />
      </list>
    </property>
    <property name="defaultExistenceChecks" >
      <list>
        <bean parent="ReferenceDefinition"
              p:attributeName="universityFiscal"
              p:attributeToHighlightOnFail="universityFiscalYear" />
        <bean parent="ReferenceDefinition"
              p:attributeName="chartOfAccounts"
              p:attributeToHighlightOnFail="chartOfAccountsCode" />
        <bean parent="ReferenceDefinition"
              p:attributeName="financialObject"
              p:attributeToHighlightOnFail="financialObjectCode" />
        <bean parent="ReferenceDefinition"
              p:attributeName="financialObjectBenefitsType"
              p:attributeToHighlightOnFail="financialObjectBenefitsTypeCode" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>universityFiscalYear</value>
        <value>chartOfAccountsCode</value>
        <value>financialObjectCode</value>
        <value>financialObjectBenefitsTypeCode</value>
      </list>
    </property>
    <property name="label" value="Labor Object Code Benefits Maintenance Document" />
    <property name="shortLabel" value="Object Code Benefit Maint Doc" />
    <property name="summary" value="Labor Object Code Benefits Maintenance Document" />
    <property name="description" value="Document used to create or update Labor Object Code Benefit objects" />
    <property name="documentTypeName" value="PositionObjectBenefitMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="LOBN" />
    <property name="workflowProperties" >
      <ref bean="PositionObjectBenefitMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>SY_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="PositionObjectBenefitMaintenanceDocument-EditPositionObjectBenefit" parent="PositionObjectBenefitMaintenanceDocument-EditPositionObjectBenefit-parentBean" />

  <bean id="PositionObjectBenefitMaintenanceDocument-EditPositionObjectBenefit-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Position Object Benefit" />
    <property name="title" value="Edit Position Object Benefit" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="universityFiscalYear"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="chartOfAccountsCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialObjectCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialObjectBenefitsTypeCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="PositionObjectBenefitMaintenanceDocument-workflowProperties" parent="PositionObjectBenefitMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="PositionObjectBenefitMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
