        	</headerNavigationTab>
        	<headerNavigationTab>
        		<navigateTo>notes</navigateTo>
        		<displayName>Notes</displayName>
        		<pageHelp parameterNamespace="KFS-RA" parameterDetailType="Budget" parameterName="BUDGET_NOTES_PAGE"/>
        	</headerNavigationTab>
        </headerNavigation>
        
        <allowsErrorCorrection>false</allowsErrorCorrection>
        <allowsNoteDelete>true</allowsNoteDelete>
        <displayTopicFieldInNotes>true</displayTopicFieldInNotes>
