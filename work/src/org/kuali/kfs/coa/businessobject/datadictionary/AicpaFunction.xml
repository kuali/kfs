<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../core/datadictionary/dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.chart.bo.AicpaFunction">
        <customAttributeEnabled>true</customAttributeEnabled>

        <inquiry>
            <title>AICPA Function Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="financialAicpaFunctionCode" />
                <field attributeName="financialAicpaFunctionName" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>AICPA Function Lookup</title>

            <instructions>Lookup an AICPA Function</instructions>

            <defaultSort attributeName="financialAicpaFunctionCode" sortAscending="true" />

            <lookupFields>
                <lookupField attributeName="financialAicpaFunctionCode" />
                <lookupField attributeName="financialAicpaFunctionName" />
            </lookupFields>

            <resultFields>
                <field attributeName="financialAicpaFunctionCode" forceInquiry="true" />
                <field attributeName="financialAicpaFunctionName" />
            </resultFields>
        </lookup>

        <titleAttribute>financialAicpaFunctionCode</titleAttribute>

        <attributes>

            <attribute name="financialAicpaFunctionCode" forceUppercase="true">
                <label>AICPA Function Code</label>
                <shortLabel>AICPA</shortLabel>

                <maxLength>3</maxLength>
                <validationPattern>
                    <anyCharacter />
                </validationPattern>
                <required>true</required>

                <control>
                    <text size="3" />
                </control>

                <summary>AICPA Function Code</summary>
                <description>The code uniquely identifying a particular AICPA Function.</description>
            </attribute>

            <attribute name="financialAicpaFunctionName" forceUppercase="true">
                <label>AICPA Function Name</label>
                <shortLabel>Name</shortLabel>

                <maxLength>40</maxLength>
                <required>true</required>

                <control>
                    <text size="40" />
                </control>

                <summary>AICPA Function Name</summary>
                <description>AICPA Function Name...</description>
            </attribute>


            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
        
        <objectLabel>AICPA Function</objectLabel>
    </businessObject>
</dictionaryEntry>

