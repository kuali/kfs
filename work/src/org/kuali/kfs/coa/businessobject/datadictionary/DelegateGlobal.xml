<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="DelegateGlobal" parent="DelegateGlobal-parentBean" />

  <bean id="DelegateGlobal-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.chart.bo.DelegateGlobal" />
    <property name="objectLabel" value="Account Delegate Global" />
    <property name="attributes" >
      <list>
        <ref bean="DelegateGlobal-model.chartOfAccountsCode" />
        <ref bean="DelegateGlobal-model.organizationCode" />
        <ref bean="DelegateGlobal-model.organizationRoutingModelName" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="DelegateGlobal-model.chartOfAccountsCode" parent="DelegateGlobal-model.chartOfAccountsCode-parentBean" />

  <bean id="DelegateGlobal-model.chartOfAccountsCode-parentBean" abstract="true" parent="OrganizationRoutingModelName-chartOfAccountsCode">
    <property name="name" value="model.chartOfAccountsCode" />
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.module.chart.lookup.keyvalues.ChartValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
  </bean>
  <bean id="DelegateGlobal-model.organizationCode" parent="DelegateGlobal-model.organizationCode-parentBean" />

  <bean id="DelegateGlobal-model.organizationCode-parentBean" abstract="true" parent="OrganizationRoutingModelName-organizationCode">
    <property name="name" value="model.organizationCode" />
    <property name="shortLabel" value="Org" />
  </bean>
  <bean id="DelegateGlobal-model.organizationRoutingModelName" parent="DelegateGlobal-model.organizationRoutingModelName-parentBean" />

  <bean id="DelegateGlobal-model.organizationRoutingModelName-parentBean" abstract="true" parent="OrganizationRoutingModelName-organizationRoutingModelName">
    <property name="name" value="model.organizationRoutingModelName" />
    <property name="label" value="Model Name" />
    <property name="shortLabel" value="Load" />
    <property name="required" value="false" />
  </bean>
</beans>
