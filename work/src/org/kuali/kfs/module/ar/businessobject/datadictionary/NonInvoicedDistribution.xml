<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="NonInvoicedDistribution" parent="NonInvoicedDistribution-parentBean" />

  <bean id="NonInvoicedDistribution-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.ar.bo.NonInvoicedDistribution" />
    <property name="inquiryDefinition" >
      <ref bean="NonInvoicedDistribution-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="NonInvoicedDistribution-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="NonInvoicedDistribution-documentNumber" />
        <ref bean="NonInvoicedDistribution-financialDocumentLineAmount" />
        <ref bean="NonInvoicedDistribution-financialDocumentLineNumber" />
        <ref bean="NonInvoicedDistribution-referenceFinancialDocumentNumber" />
        <ref bean="NonInvoicedDistribution-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="NonInvoicedDistribution-documentNumber" parent="NonInvoicedDistribution-documentNumber-parentBean" />

  <bean id="NonInvoicedDistribution-documentNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="documentNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Document Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="14" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="16" />
    </property>
    <property name="summary" value="documentNumber" />
    <property name="description" value="documentNumber description..." />
  </bean>
  <bean id="NonInvoicedDistribution-financialDocumentLineAmount" parent="NonInvoicedDistribution-financialDocumentLineAmount-parentBean" />

  <bean id="NonInvoicedDistribution-financialDocumentLineAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialDocumentLineAmount" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Financial Document Line Amount" />
    <property name="shortLabel" value="Amount" />
    <property name="maxLength" value="19" />
    <property name="validationPattern" >
      <bean parent="FloatingPointValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="21" />
    </property>
    <property name="summary" value="financialDocumentLineAmount" />
    <property name="description" value="financialDocumentLineAmount description..." />
  </bean>
  <bean id="NonInvoicedDistribution-financialDocumentLineNumber" parent="NonInvoicedDistribution-financialDocumentLineNumber-parentBean" />

  <bean id="NonInvoicedDistribution-financialDocumentLineNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialDocumentLineNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Financial Document Line Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="7" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="9" />
    </property>
    <property name="summary" value="financialDocumentLineNumber" />
    <property name="description" value="financialDocumentLineNumber description..." />
  </bean>
  <bean id="NonInvoicedDistribution-referenceFinancialDocumentNumber" parent="NonInvoicedDistribution-referenceFinancialDocumentNumber-parentBean" />

  <bean id="NonInvoicedDistribution-referenceFinancialDocumentNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="referenceFinancialDocumentNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Reference Financial Document Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="14" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="16" />
    </property>
    <property name="summary" value="referenceFinancialDocumentNumber" />
    <property name="description" value="referenceFinancialDocumentNumber description..." />
  </bean>
  <bean id="NonInvoicedDistribution-versionNumber" parent="NonInvoicedDistribution-versionNumber-parentBean" />

  <bean id="NonInvoicedDistribution-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="NonInvoicedDistribution-inquiryDefinition" parent="NonInvoicedDistribution-inquiryDefinition-parentBean" />

  <bean id="NonInvoicedDistribution-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Non-Invoiced Distribution Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="documentNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialDocumentLineAmount" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialDocumentLineNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="referenceFinancialDocumentNumber" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="NonInvoicedDistribution-lookupDefinition" parent="NonInvoicedDistribution-lookupDefinition-parentBean" />

  <bean id="NonInvoicedDistribution-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Non-Invoiced Distribution Lookup" />
    <property name="instructions" value="Look up a Non-Invoiced Distribution" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentLineAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentLineNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="referenceFinancialDocumentNumber" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentLineAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentLineNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="referenceFinancialDocumentNumber" />
      </list>
    </property>
  </bean>
</beans>
