                <shortLabel>Justification</shortLabel>

                <maxLength>200</maxLength>

                <required>false</required>

                <control>
                    <textarea rows="3" cols="30" />
                </control>

                <summary>
                    <![CDATA[&nbsp;]]>
                </summary>
                <description>A free-form text field that describes the reason for any disparity between the Calculated and Actual Per Diem Amounts for the Non-Employee Travel.</description>
            </attribute>

            <attribute name="disbVchrAutoFromCityName">
                <label>From (City/State)</label>
                <shortLabel>From</shortLabel>

                <maxLength>40</maxLength>

                <required>false</required>

                <control>
                    <text size="40" />
                </control>

                <summary>
                    <![CDATA[&nbsp;]]>
                </summary>
                <description>The city and state of the traveler's departure.</description>
            </attribute>

            <attribute name="disbVchrAutoFromStateCode">
                <label>From (City/State)</label>
                <shortLabel>St</shortLabel>

                <maxLength>2</maxLength>

                <required>false</required>

