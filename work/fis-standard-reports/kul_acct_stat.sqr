   print $total_objcon_actual_ytd (,74,15)  edit 99,999,999.99pf
   print $total_objcon_ot_encb    (,90,14)  edit 9,999,999.99pf
   print $total_objcon_av_bl      (,106,15) edit 99,999,999.99pf
