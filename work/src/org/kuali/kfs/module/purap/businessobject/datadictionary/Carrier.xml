<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.purap.bo.Carrier">


        <inquiry>
            <title>Carrier Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="carrierCode" />
                <field attributeName="carrierDescription" />
                <field attributeName="active" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Carrier Lookup</title>

            <instructions>Lookup a Carrier</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="carrierDescription" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="carrierCode" required="false" />
                <lookupField attributeName="carrierDescription" required="false" />
                <lookupField attributeName="active" required="false" >
                	<defaultValue>Y</defaultValue>
                </lookupField>
            </lookupFields>

            <resultFields>
                <field attributeName="carrierCode" />
                <field attributeName="carrierDescription" />
                <field attributeName="active" />
            </resultFields>
        </lookup>

        <titleAttribute>carrierCode</titleAttribute>

        <attributes>

            <attribute name="carrierCode" forceUppercase="true">
                <label>Carrier Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="6" />
                </control>

                <summary>carrierCode</summary>
                <description>carrierCode description...</description>
			</attribute>

            <attribute name="carrierDescription" forceUppercase="true">
                <label>Carrier Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>45</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true" /></validationPattern>

                <control>
                    <text size="47" />
                </control>

                <summary>carrierDescription</summary>
                <description>carrierDescription description...</description>
			</attribute>

            <attributeReference name="active" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="activeIndicator" />

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
