<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
    <form-beans>
        <!--  Rice Bean Definitions : This is a bean declared in Rice, but is needed throughout this config -->
        <form-bean name="KualiForm" type="org.kuali.core.web.struts.form.KualiForm" />

        <!--  KFS Core Bean Definitions -->
        <form-bean name="KualiBalanceInquiryReportMenuForm" type="org.kuali.kfs.web.struts.form.KualiBalanceInquiryReportMenuForm" />
        <form-bean name="KualiBatchInputFileForm" type="org.kuali.kfs.web.struts.form.KualiBatchInputFileForm" />
        <form-bean name="KualiBatchInputFileSetForm" type="org.kuali.kfs.web.struts.form.KualiBatchInputFileSetForm" />
        
        <!--  Financial Module Bean Definitions -->
        <form-bean name="DepositWizardForm" type="org.kuali.module.financial.web.struts.form.DepositWizardForm" />
        <form-bean name="CashReceiptForm" type="org.kuali.module.financial.web.struts.form.CashReceiptForm" />
        <form-bean name="CreditCardReceiptForm" type="org.kuali.module.financial.web.struts.form.CreditCardReceiptForm" />
        <form-bean name="AdvanceDepositForm" type="org.kuali.module.financial.web.struts.form.AdvanceDepositForm" />
        <form-bean name="InternalBillingForm" type="org.kuali.module.financial.web.struts.form.InternalBillingForm" />
        <form-bean name="ServiceBillingForm" type="org.kuali.module.financial.web.struts.form.ServiceBillingForm" />
        <form-bean name="TransferOfFundsForm" type="org.kuali.module.financial.web.struts.form.TransferOfFundsForm" />
        <form-bean name="YearEndTransferOfFundsForm" type="org.kuali.module.financial.web.struts.form.YearEndTransferOfFundsForm" />
        <form-bean name="GeneralErrorCorrectionForm" type="org.kuali.module.financial.web.struts.form.GeneralErrorCorrectionForm" />
        <form-bean name="YearEndGeneralErrorCorrectionForm" type="org.kuali.module.financial.web.struts.form.YearEndGeneralErrorCorrectionForm" />
        <form-bean name="DistributionOfIncomeAndExpenseForm" type="org.kuali.module.financial.web.struts.form.DistributionOfIncomeAndExpenseForm" />
        <form-bean name="YearEndDistributionOfIncomeAndExpenseForm" type="org.kuali.module.financial.web.struts.form.YearEndDistributionOfIncomeAndExpenseForm" />
        <form-bean name="AuxiliaryVoucherForm" type="org.kuali.module.financial.web.struts.form.AuxiliaryVoucherForm" />
        <form-bean name="BursarControlScreenForm" type="org.kuali.module.financial.web.struts.form.BursarControlScreenForm" />
        <form-bean name="JournalVoucherForm" type="org.kuali.module.financial.web.struts.form.JournalVoucherForm" />
        <form-bean name="IndirectCostAdjustmentForm" type="org.kuali.module.financial.web.struts.form.IndirectCostAdjustmentForm" />
        <form-bean name="NonCheckDisbursementForm" type="org.kuali.module.financial.web.struts.form.NonCheckDisbursementForm" />
        <form-bean name="PreEncumbranceForm" type="org.kuali.module.financial.web.struts.form.PreEncumbranceForm" />
        <form-bean name="DisbursementVoucherForm" type="org.kuali.module.financial.web.struts.form.DisbursementVoucherForm" />
        <form-bean name="CashManagementForm" type="org.kuali.module.financial.web.struts.form.CashManagementForm" />
        <form-bean name="CashManagementStatusForm" type="org.kuali.module.financial.web.struts.form.CashManagementStatusForm" />
        <form-bean name="ProcurementCardForm" type="org.kuali.module.financial.web.struts.form.ProcurementCardForm" />
        <form-bean name="BudgetAdjustmentForm" type="org.kuali.module.financial.web.struts.form.BudgetAdjustmentForm" />
        <form-bean name="YearEndBudgetAdjustmentForm" type="org.kuali.module.financial.web.struts.form.YearEndBudgetAdjustmentForm" />
        
		<!-- Labor Distribution Bean Definitions -->
   		<form-bean name="LaborSalaryExpenseTransferForm" type="org.kuali.module.labor.web.struts.form.SalaryExpenseTransferForm" />
   		<form-bean name="LaborBenefitExpenseTransferForm" type="org.kuali.module.labor.web.struts.form.BenefitExpenseTransferForm" />
   		<form-bean name="LaborJournalVoucherForm" type="org.kuali.module.labor.web.struts.form.LaborJournalVoucherForm" />
   		<form-bean name="PersonFundingInquiryForm" type="org.kuali.module.labor.web.struts.form.PersonFundingInquiryForm" />
		<form-bean name="LaborCorrectionForm" type="org.kuali.module.labor.web.struts.form.LaborCorrectionForm" />

		<!-- Budget Construction Bean Definitions -->
   		<form-bean name="BudgetConstructionSelectionForm" type="org.kuali.module.budget.web.struts.form.BudgetConstructionSelectionForm" />
   		<form-bean name="SalarySettingForm" type="org.kuali.module.budget.web.struts.form.SalarySettingForm" />
   		<form-bean name="BudgetConstructionForm" type="org.kuali.module.budget.web.struts.form.BudgetConstructionForm" />
   		<form-bean name="MonthlyBudgetForm" type="org.kuali.module.budget.web.struts.form.MonthlyBudgetForm" />
   		<form-bean name="PositionSalarySettingForm" type="org.kuali.module.budget.web.struts.form.PositionSalarySettingForm" />
   		<form-bean name="IncumbentSalarySettingForm" type="org.kuali.module.budget.web.struts.form.IncumbentSalarySettingForm" />

        <!-- KRA Module Bean Definitions -->
        <form-bean name="BudgetForm" type="org.kuali.module.kra.budget.web.struts.form.BudgetForm" />
        <form-bean name="RoutingForm" type="org.kuali.module.kra.routingform.web.struts.form.RoutingForm" />

        <!-- General Ledger Bean Definitions -->
        <form-bean name="CorrectionForm" type="org.kuali.module.gl.web.struts.form.CorrectionForm" />
        <form-bean name="BalanceInquiryForm" type="org.kuali.module.gl.web.struts.form.BalanceInquiryForm" />
        <form-bean name="BalanceInquiryLookupForm" type="org.kuali.module.gl.web.struts.form.BalanceInquiryLookupForm" />

        <!-- PURAP Module Bean Definitions -->
        <form-bean name="AssignContractManagerForm" type="org.kuali.module.purap.web.struts.form.AssignContractManagerForm" />
        <form-bean name="RequisitionForm" type="org.kuali.module.purap.web.struts.form.RequisitionForm" />
        <form-bean name="PurchaseOrderForm" type="org.kuali.module.purap.web.struts.form.PurchaseOrderForm" />
        <form-bean name="PaymentRequestForm" type="org.kuali.module.purap.web.struts.form.PaymentRequestForm" />
        <form-bean name="CreditMemoForm" type="org.kuali.module.purap.web.struts.form.CreditMemoForm" />

        <!-- CG Module Bean Definitions -->
        <form-bean name="CloseForm" type="org.kuali.module.cg.web.struts.form.CloseForm" />

    </form-beans>

    <global-exceptions>
        <exception type="org.kuali.module.financial.exceptions.CashDrawerStateException" key="unused" path="/cashManagementStatus.do" />
        <exception type="org.kuali.core.exceptions.AuthorizationException" key="meaningless" path="/authorizationException.do" />
        <exception type="org.kuali.core.exceptions.DocumentInitiationAuthorizationException" key="meaningless" path="/initiationAuthorizationException.do" />
        <exception type="org.kuali.core.exceptions.UnknownDocumentIdException" key="error.document.noLongerExistsInKuali" path="/kr/WEB-INF/jsp/core/Exception.jsp" />
        <exception type="org.kuali.core.exceptions.FileUploadLimitExceededException" key="error.document.attachmentMaximumFileSizeExceeded" path="/kr/WEB-INF/jsp/core/Exception.jsp" />
    </global-exceptions>


    <global-forwards>
        <forward name="error" path="/kr/WEB-INF/jsp/core/KualiError.jsp" />
        <forward name="save" path="/index.jsp" redirect="true" />
        <forward name="cancel" path="/index.jsp" redirect="true" />
        <forward name="close" path="/index.jsp" redirect="true" />
        <forward name="portal" path="/index.jsp" redirect="true" />
    </global-forwards>

    <action-mappings>

		<!-- Rice Pass-through Actions : These delegate to the Rice (kr) struts module -->
        <action path="/Exception" type="org.kuali.core.web.struts.action.KualiExceptionAction">
            <forward name="basic" path="/kr/Exception.do" />
        </action>
        <action path="/authorizationException" type="org.kuali.core.web.struts.action.KualiAuthorizationExceptionAction">
            <forward name="basic" path="/kr/authorizationException.do" />
        </action> 
        <action path="/initiationAuthorizationException" type="org.kuali.core.web.struts.action.KualiDocumentInitiationAuthorizationExceptionAction">
            <forward name="basic" path="/kr/initiationAuthorizationException.do" />
        </action>
		<!-- End Rice Pass-through Actions -->
                            
        <!--  Core Action Mappings -->
        <action path="/portal" name="KualiForm" type="org.kuali.core.web.struts.action.KualiSimpleAction">
            <forward name="basic" path="/portal.jsp" />
        </action>

        <action path="/index" name="KualiForm" type="org.kuali.core.web.struts.action.KualiSimpleAction">
            <forward name="basic" path="/portal.jsp" />
        </action>

        <action path="/balanceInquiryReportMenu" name="KualiBalanceInquiryReportMenuForm" scope="request" attribute="KualiForm" parameter="methodToCall" type="org.kuali.kfs.web.struts.action.KualiBalanceInquiryReportMenuAction">
            <set-property property="cancellable" value="true" />
            <forward name="basic" path="/jsp/modules/generalLedger/KualiBalanceInquiryReportMenu.jsp" />
        </action>

        <action path="/dvPerDiem" name="DisbursementVoucherForm" scope="request" attribute="KualiForm" parameter="methodToCall" type="org.kuali.core.web.struts.action.KualiHelpAction">
            <set-property property="cancellable" value="true" />
            <forward name="basic" path="/kr/WEB-INF/jsp/core/KualiHelp.jsp" />
            <forward name="dvPerDiemLinks" path="/jsp/modules/financial/DisbursementVoucherTravelPerDiemLinks.jsp" />
        </action>

		<!-- KFS Action Mappings -->
		<action path="/batchModify" name="KualiForm" scope="request" attribute="KualiForm" parameter="methodToCall" type="org.kuali.kfs.web.struts.action.KualiBatchJobModifyAction">
            <set-property property="cancellable" value="true" />
            <forward name="basic" path="/jsp/kfs/KualiBatchJobModify.jsp" />
        </action>

        <!--  Module Action Mappings -->
        <action path="/financial*" name="{1}Form" input="/jsp/modules/financial/{1}.jsp" type="org.kuali.module.financial.web.struts.action.{1}Action" scope="request" parameter="methodToCall" validate="true" attribute="KualiForm">
            <set-property property="cancellable" value="true" />
            <forward name="basic" path="/jsp/modules/financial/{1}.jsp" />
        </action>

        <action path="/depositWizard" name="DepositWizardForm" scope="request" attribute="KualiForm" parameter="methodToCall" type="org.kuali.module.financial.web.struts.action.DepositWizardAction">
            <set-property property="cancellable" value="true" />
            <forward name="basic" path="/jsp/modules/financial/DepositWizard.jsp" />
        </action>

        <action path="/generalLedger*" name="{1}Form" input="/jsp/modules/generalLedger/{1}.jsp" type="org.kuali.module.gl.web.struts.action.{1}Action" scope="request" parameter="methodToCall" validate="true" attribute="KualiForm">
            <set-property property="cancellable" value="true" />
            <forward name="basic" path="/jsp/modules/generalLedger/{1}.jsp" />
        </action>

        <action path="/glBalanceInquiry" name="BalanceInquiryForm" scope="request" attribute="KualiForm" parameter="methodToCall" type="org.kuali.module.gl.web.struts.action.BalanceInquiryAction">
            <set-property property="cancellable" value="true" />
            <forward name="basic" path="/jsp/modules/generalLedger/BalanceInquiry.jsp" />
        </action>

        <action path="/glBalanceInquiryLookup" name="BalanceInquiryLookupForm" scope="request" attribute="KualiForm" parameter="methodToCall" type="org.kuali.module.gl.web.struts.action.BalanceInquiryLookupAction">
            <set-property property="cancellable" value="true" />
            <forward name="basic" path="/jsp/modules/generalLedger/BalanceInquiryLookup.jsp" />
        </action>

        <action path="/glModifiedInquiry" name="BalanceInquiryForm" scope="request" attribute="KualiForm" parameter="methodToCall" type="org.kuali.module.gl.web.struts.action.BalanceInquiryAction">
            <set-property property="cancellable" value="true" />
            <forward name="basic" path="/jsp/modules/generalLedger/ModifiedInquiry.jsp" />
        </action>

        <action path="/glAccountBalanceByConsolidationLookup" name="BalanceInquiryForm" scope="request" attribute="KualiForm" parameter="methodToCall" type="org.kuali.module.gl.web.struts.action.BalanceInquiryAction">
            <set-property property="cancellable" value="true" />
            <forward name="basic" path="/jsp/modules/generalLedger/AccountBalanceByConsolidationLookup.jsp" />
        </action>

        <action path="/labor*" name="Labor{1}Form" input="/jsp/modules/labor/{1}.jsp" type="org.kuali.module.labor.web.struts.action.{1}Action" scope="request" parameter="methodToCall" validate="true" attribute="KualiForm"> 
            <set-property property="cancellable" value="true" />
            <forward name="basic" path="/jsp/modules/labor/{1}.jsp" /> 
        </action> 

        <action path="/laborLedgerCorrection" name="LaborCorrectionForm" input="/jsp/modules/generalLedger/Correction.jsp" type="org.kuali.module.labor.web.struts.action.LaborCorrectionAction" scope="request" parameter="methodToCall" validate="true" attribute="KualiForm"> 
            <set-property property="cancellable" value="true" />
            <forward name="basic" path="/jsp/modules/generalLedger/Correction.jsp" /> 
        </action> 
        
        <action path="/laborLongRowTableInquiry" name="BalanceInquiryForm" scope="request" attribute="KualiForm" parameter="methodToCall" type="org.kuali.module.gl.web.struts.action.BalanceInquiryAction">
            <set-property property="cancellable" value="true" />
            <forward name="basic" path="/jsp/modules/labor/LongRowTableInquiry.jsp" />
        </action>

		<action path="/personFundingInquiry" name="PersonFundingInquiryForm" scope="request" attribute="KualiForm" parameter="methodToCall" type="org.kuali.module.labor.web.struts.action.PersonFundingInquiryAction">
            <set-property property="cancellable" value="true" />
            <forward name="basic" path="/jsp/modules/labor/PersonFundingInquiry.jsp" />
			 <forward name="BalanceInquiry" path="/jsp/modules/generalLedger/BalanceInquiry.jsp" />
        </action>

        <action path="/budget*" name="{1}Form" input="/jsp/modules/budget/{1}.jsp" type="org.kuali.module.budget.web.struts.action.{1}Action" scope="request" parameter="methodToCall" validate="true" attribute="KualiForm"> 
            <set-property property="cancellable" value="true" />
            <forward name="basic" path="/jsp/modules/budget/{1}.jsp" /> 
        </action> 

        <action path="/researchBudget*" name="BudgetForm" input="/jsp/modules/research/budget/Budget{1}.jsp" type="org.kuali.module.kra.budget.web.struts.action.Budget{1}Action" scope="request" parameter="methodToCall" validate="true" attribute="KualiForm">
            <set-property property="cancellable" value="true" />
            <forward name="basic" path="/jsp/modules/research/budget/Budget{1}.jsp" />
            <forward name="parameters" path="/jsp/modules/research/budget/BudgetParameters.jsp" redirect="false" />
            <forward name="overview" path="/jsp/modules/research/budget/BudgetOverview.jsp" redirect="false" />
            <forward name="template" path="/jsp/modules/research/budget/BudgetTemplate.jsp" redirect="false" />
            <forward name="nonpersonnel" path="/jsp/modules/research/budget/BudgetNonpersonnel.jsp" redirect="false" />
            <forward name="nonpersonnelCopyOver" path="/jsp/modules/research/budget/BudgetNonpersonnelCopyOver.jsp" redirect="false" />
            <forward name="personnel" path="/jsp/modules/research/budget/BudgetPersonnel.jsp" redirect="false" />
            <forward name="costshare" path="/jsp/modules/research/budget/BudgetCostShare.jsp" redirect="false" />
            <forward name="output" path="/jsp/modules/research/budget/BudgetOutput.jsp" redirect="false" />
            <forward name="modular" path="/jsp/modules/research/budget/BudgetModular.jsp" redirect="false" />
            <forward name="indirectcost" path="/jsp/modules/research/budget/BudgetIndirectCost.jsp" redirect="false" />
            <forward name="auditmode" path="/jsp/modules/research/budget/BudgetAuditMode.jsp" redirect="false" />
            <forward name="permissions" path="/jsp/modules/research/budget/BudgetPermissions.jsp" redirect="false" />
            <forward name="notes" path="/jsp/modules/research/budget/BudgetNotes.jsp" redirect="false" />
        </action>

        <action path="/researchRoutingForm*" name="RoutingForm" input="/jsp/modules/research/routingform/RoutingForm{1}.jsp" type="org.kuali.module.kra.routingform.web.struts.action.RoutingForm{1}Action" scope="request" parameter="methodToCall" validate="true" attribute="KualiForm">
            <set-property property="cancellable" value="true" />
            <forward name="basic" path="/jsp/modules/research/routingform/RoutingForm{1}.jsp" />
            <forward name="mainpage" path="/jsp/modules/research/routingform/RoutingFormMainPage.jsp" redirect="false" />
            <forward name="personnel" path="/jsp/modules/research/routingform/RoutingFormPersonnel.jsp" redirect="false" />
            <forward name="researchrisks" path="/jsp/modules/research/routingform/RoutingFormResearchRisks.jsp" redirect="false" />
            <forward name="projectdetails" path="/jsp/modules/research/routingform/RoutingFormProjectDetails.jsp" redirect="false" />       
            <forward name="agencyspecific" path="/jsp/modules/research/routingform/RoutingFormAgencySpecific.jsp" redirect="false" />       
            <forward name="sf424" path="/jsp/modules/research/routingform/RoutingFormSF424.jsp" redirect="false" />       
            <forward name="link" path="/jsp/modules/research/routingform/RoutingFormLink.jsp" redirect="false" />       
            <forward name="notes" path="/jsp/modules/research/routingform/RoutingFormNotes.jsp" redirect="false" />       
            <forward name="output" path="/jsp/modules/research/routingform/RoutingFormOutput.jsp" redirect="false" />       
            <forward name="template" path="/jsp/modules/research/routingform/RoutingFormTemplate.jsp" redirect="false" />       
            <forward name="auditmode" path="/jsp/modules/research/routingform/RoutingFormAuditMode.jsp" redirect="false" />
            <forward name="permissions" path="/jsp/modules/research/routingform/RoutingFormPermissions.jsp" redirect="false" />
            <forward name="approvals" path="/jsp/modules/research/routingform/RoutingFormApprovals.jsp" redirect="false" />
        </action>

        <action path="/cashManagementStatus" name="CashManagementStatusForm" type="org.kuali.module.financial.web.struts.action.CashManagementStatusAction" scope="request" attribute="KualiForm">
            <set-property property="cancellable" value="true" />
            <forward name="basic" path="/jsp/modules/financial/CashManagementStatus.jsp" />
        </action>

        <action path="/purap*" name="{1}Form" input="/jsp/modules/purap/{1}.jsp" type="org.kuali.module.purap.web.struts.action.{1}Action" scope="request" parameter="methodToCall" validate="true" attribute="KualiForm">
            <set-property property="cancellable" value="true" />
            <forward name="basic" path="/jsp/modules/purap/{1}.jsp" />
            <forward name="viewRelatedDocuments" path="/jsp/modules/purap/ViewRelatedDocuments.jsp" redirect="false" />
            <forward name="viewPaymentHistory" path="/jsp/modules/purap/ViewPaymentHistory.jsp" redirect="false" />
        </action>

        <action path="/cg*" name="{1}Form" input="/jsp/modules/cg/{1}.jsp" type="org.kuali.module.cg.web.struts.action.{1}Action" scope="request" parameter="methodToCall" validate="true" attribute="KualiForm">
            <set-property property="cancellable" value="true" />
            <forward name="basic" path="/jsp/modules/cg/{1}.jsp" />
        </action>

        <action path="/batchUpload" name="KualiBatchInputFileForm" scope="request" attribute="KualiForm" parameter="methodToCall" type="org.kuali.kfs.web.struts.action.KualiBatchInputFileAction">
            <set-property property="cancellable" value="true" />
            <forward name="basic" path="/jsp/kfs/KualiBatchInputFile.jsp" />
        </action>
        
        <action path="/batchUploadFileSet" name="KualiBatchInputFileSetForm" scope="request" attribute="KualiForm" parameter="methodToCall" type="org.kuali.kfs.web.struts.action.KualiBatchInputFileSetAction">
            <set-property property="cancellable" value="true" />
            <forward name="basic" path="/jsp/kfs/KualiBatchInputFileSet.jsp" />
        </action>
        
    </action-mappings>

    <controller processorClass="org.kuali.core.web.struts.action.KualiRequestProcessor" maxFileSize="25M" />

    <message-resources parameter="ApplicationResources" />
    <message-resources key="kraResources" parameter="org.kuali.module.kra.KraResources" />

    <plug-in className="org.kuali.core.web.struts.pojo.PojoPlugin" />
</struts-config>
