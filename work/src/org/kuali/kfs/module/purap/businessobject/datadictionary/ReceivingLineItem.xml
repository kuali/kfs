<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="ReceivingLineItem" parent="ReceivingLineItem-parentBean" />

  <bean id="ReceivingLineItem-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.purap.bo.ReceivingLineItem" />
    <property name="inquiryDefinition" >
      <ref bean="ReceivingLineItem-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="ReceivingLineItem-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="ReceivingLineItem-documentNumber" />
        <ref bean="ReceivingLineItem-itemCatalogNumber" />
        <ref bean="ReceivingLineItem-itemDamagedTotalQuantity" />
        <ref bean="ReceivingLineItem-itemDescription" />
        <ref bean="ReceivingLineItem-itemLineNumber" />
        <ref bean="ReceivingLineItem-itemOrderedQuantity" />
        <ref bean="ReceivingLineItem-itemReceivedPriorQuantity" />
        <ref bean="ReceivingLineItem-itemReceivedToBeQuantity" />
        <ref bean="ReceivingLineItem-itemReasonAddedCode" />
        <ref bean="ReceivingLineItem-itemReceivedTotalQuantity" />
        <ref bean="ReceivingLineItem-itemReturnedTotalQuantity" />
        <ref bean="ReceivingLineItem-itemTypeCode" />
        <ref bean="ReceivingLineItem-itemUnitOfMeasureCode" />
        <ref bean="ReceivingLineItem-purchaseOrderIdentifier" />
        <ref bean="ReceivingLineItem-receivingItemIdentifier" />
        <ref bean="ReceivingLineItem-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="ReceivingLineItem-documentNumber" parent="ReceivingLineItem-documentNumber-parentBean" />

  <bean id="ReceivingLineItem-documentNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="documentNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Document Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="14" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="16" />
    </property>
    <property name="summary" value="documentNumber" />
    <property name="description" value="documentNumber description..." />
  </bean>
  <bean id="ReceivingLineItem-itemCatalogNumber" parent="ReceivingLineItem-itemCatalogNumber-parentBean" />

  <bean id="ReceivingLineItem-itemCatalogNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="itemCatalogNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Item Catalog Number" />
    <property name="shortLabel" value="Catalog #" />
    <property name="maxLength" value="30" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="15" />
    </property>
    <property name="summary" value="itemCatalogNumber" />
    <property name="description" value="itemCatalogNumber description..." />
  </bean>
  <bean id="ReceivingLineItem-itemDamagedTotalQuantity" parent="ReceivingLineItem-itemDamagedTotalQuantity-parentBean" />

  <bean id="ReceivingLineItem-itemDamagedTotalQuantity-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="itemDamagedTotalQuantity" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Item Damaged Total Quantity" />
    <property name="shortLabel" value="Qty Damaged" />
    <property name="maxLength" value="11" />
    <property name="validationPattern" >
      <bean parent="FloatingPointValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="13" />
    </property>
    <property name="summary" value="itemDamagedTotalQuantity" />
    <property name="description" value="itemDamagedTotalQuantity description..." />
  </bean>
  <bean id="ReceivingLineItem-itemDescription" parent="ReceivingLineItem-itemDescription-parentBean" />

  <bean id="ReceivingLineItem-itemDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="itemDescription" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Item Description" />
    <property name="shortLabel" value="Description" />
    <property name="maxLength" value="4000" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextareaControlDefinition"
            p:rows="1"
            p:cols="45" />
    </property>
    <property name="summary" value="itemDescription" />
    <property name="description" value="itemDescription description..." />
  </bean>
  <bean id="ReceivingLineItem-itemLineNumber" parent="ReceivingLineItem-itemLineNumber-parentBean" />

  <bean id="ReceivingLineItem-itemLineNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="itemLineNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Item Line Number" />
    <property name="shortLabel" value="Line #" />
    <property name="maxLength" value="3" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="5" />
    </property>
    <property name="summary" value="itemLineNumber" />
    <property name="description" value="itemLineNumber description..." />
  </bean>
  <bean id="ReceivingLineItem-itemOrderedQuantity" parent="ReceivingLineItem-itemOrderedQuantity-parentBean" />

  <bean id="ReceivingLineItem-itemOrderedQuantity-parentBean" abstract="true" parent="PurApGenericAttributes-genericItemQuantity">
    <property name="name" value="itemOrderedQuantity" />
    <property name="label" value="Item Ordered Quantity" />
    <property name="shortLabel" value="Qty Ordered" />
    <property name="summary" value="itemOrderedQuantity" />
    <property name="description" value="itemOrderedQuantity description..." />
  </bean>
  <bean id="ReceivingLineItem-itemReceivedPriorQuantity" parent="ReceivingLineItem-itemReceivedPriorQuantity-parentBean" />

  <bean id="ReceivingLineItem-itemReceivedPriorQuantity-parentBean" abstract="true" parent="PurApGenericAttributes-genericItemQuantity">
    <property name="name" value="itemReceivedPriorQuantity" />
    <property name="label" value="Prior Qty Received" />
    <property name="shortLabel" value="Prior Qty Received" />
    <property name="summary" value="itemReceivedPriorQuantity" />
    <property name="description" value="itemReceivedPriorQuantity description..." />
  </bean>
  <bean id="ReceivingLineItem-itemReceivedToBeQuantity" parent="ReceivingLineItem-itemReceivedToBeQuantity-parentBean" />

  <bean id="ReceivingLineItem-itemReceivedToBeQuantity-parentBean" abstract="true" parent="PurApGenericAttributes-genericItemQuantity">
    <property name="name" value="itemReceivedToBeQuantity" />
    <property name="label" value="To be Received" />
    <property name="shortLabel" value="To be Received" />
    <property name="summary" value="itemReceivedToBeQuantity" />
    <property name="description" value="itemReceivedToBeQuantity description..." />
  </bean>
  <bean id="ReceivingLineItem-itemReasonAddedCode" parent="ReceivingLineItem-itemReasonAddedCode-parentBean" />

  <bean id="ReceivingLineItem-itemReasonAddedCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="itemReasonAddedCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Item Reason Added Code" />
    <property name="shortLabel" value="Addition Reason" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.module.purap.lookup.keyvalues.ItemReasonAddedValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
    <property name="summary" value="itemReasonAddedCode" />
    <property name="description" value="itemReasonAddedCode description..." />
  </bean>
  <bean id="ReceivingLineItem-itemReceivedTotalQuantity" parent="ReceivingLineItem-itemReceivedTotalQuantity-parentBean" />

  <bean id="ReceivingLineItem-itemReceivedTotalQuantity-parentBean" abstract="true" parent="PurApGenericAttributes-genericItemQuantity">
    <property name="name" value="itemReceivedTotalQuantity" />
    <property name="label" value="Item Received Total Quantity" />
    <property name="shortLabel" value="Qty Received" />
    <property name="summary" value="itemReceivedTotalQuantity" />
    <property name="description" value="itemReceivedTotalQuantity description..." />
  </bean>
  <bean id="ReceivingLineItem-itemReturnedTotalQuantity" parent="ReceivingLineItem-itemReturnedTotalQuantity-parentBean" />

  <bean id="ReceivingLineItem-itemReturnedTotalQuantity-parentBean" abstract="true" parent="PurApGenericAttributes-genericItemQuantity">
    <property name="name" value="itemReturnedTotalQuantity" />
    <property name="label" value="Item Returned Total Quantity" />
    <property name="shortLabel" value="Qty Returned" />
    <property name="summary" value="itemReturnedTotalQuantity" />
    <property name="description" value="itemReturnedTotalQuantity description..." />
  </bean>
  <bean id="ReceivingLineItem-itemTypeCode" parent="ReceivingLineItem-itemTypeCode-parentBean" />

  <bean id="ReceivingLineItem-itemTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="itemTypeCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Item Type Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="6" />
    </property>
    <property name="summary" value="itemTypeCode" />
    <property name="description" value="itemTypeCode description..." />
  </bean>
  <bean id="ReceivingLineItem-itemUnitOfMeasureCode" parent="ReceivingLineItem-itemUnitOfMeasureCode-parentBean" />

  <bean id="ReceivingLineItem-itemUnitOfMeasureCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="itemUnitOfMeasureCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Item Unit Of Measure Code" />
    <property name="shortLabel" value="UOM" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.kfs.lookup.keyvalues.UnitOfMeasureValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
  </bean>
  <bean id="ReceivingLineItem-purchaseOrderIdentifier" parent="ReceivingLineItem-purchaseOrderIdentifier-parentBean" />

  <bean id="ReceivingLineItem-purchaseOrderIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="purchaseOrderIdentifier" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Purchase Order Identifier" />
    <property name="shortLabel" value="Identifier" />
    <property name="maxLength" value="9" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="11" />
    </property>
    <property name="summary" value="purchaseOrderIdentifier" />
    <property name="description" value="purchaseOrderIdentifier description..." />
  </bean>
  <bean id="ReceivingLineItem-receivingItemIdentifier" parent="ReceivingLineItem-receivingItemIdentifier-parentBean" />

  <bean id="ReceivingLineItem-receivingItemIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="receivingItemIdentifier" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Receiving Line Item Identifier" />
    <property name="shortLabel" value="Identifier" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
    <property name="summary" value="receivingItemIdentifier" />
    <property name="description" value="receivingItemIdentifier description..." />
  </bean>
  <bean id="ReceivingLineItem-versionNumber" parent="ReceivingLineItem-versionNumber-parentBean" />

  <bean id="ReceivingLineItem-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="ReceivingLineItem-inquiryDefinition" parent="ReceivingLineItem-inquiryDefinition-parentBean" />

  <bean id="ReceivingLineItem-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Receiving Line Item Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="documentNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="itemCatalogNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="itemDamagedTotalQuantity" />
              <bean parent="FieldDefinition"
                    p:attributeName="itemDescription" />
              <bean parent="FieldDefinition"
                    p:attributeName="itemLineNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="itemOrderedQuantity" />
              <bean parent="FieldDefinition"
                    p:attributeName="itemReasonAddedCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="itemReceivedTotalQuantity" />
              <bean parent="FieldDefinition"
                    p:attributeName="itemReturnedTotalQuantity" />
              <bean parent="FieldDefinition"
                    p:attributeName="itemTypeCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="itemUnitOfMeasureCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="purchaseOrderIdentifier" />
              <bean parent="FieldDefinition"
                    p:attributeName="receivingItemIdentifier" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="ReceivingLineItem-lookupDefinition" parent="ReceivingLineItem-lookupDefinition-parentBean" />

  <bean id="ReceivingLineItem-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Receiving Line Item Lookup" />
    <property name="instructions" value="Look up a Receiving Line Item" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="itemCatalogNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="itemDamagedTotalQuantity" />
        <bean parent="FieldDefinition"
              p:attributeName="itemDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="itemLineNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="itemOrderedQuantity" />
        <bean parent="FieldDefinition"
              p:attributeName="itemReasonAddedCode" />
        <bean parent="FieldDefinition"
              p:attributeName="itemReceivedTotalQuantity" />
        <bean parent="FieldDefinition"
              p:attributeName="itemReturnedTotalQuantity" />
        <bean parent="FieldDefinition"
              p:attributeName="itemTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="itemUnitOfMeasureCode" />
        <bean parent="FieldDefinition"
              p:attributeName="purchaseOrderIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="receivingItemIdentifier" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="itemCatalogNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="itemDamagedTotalQuantity" />
        <bean parent="FieldDefinition"
              p:attributeName="itemDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="itemLineNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="itemOrderedQuantity" />
        <bean parent="FieldDefinition"
              p:attributeName="itemReasonAddedCode" />
        <bean parent="FieldDefinition"
              p:attributeName="itemReceivedTotalQuantity" />
        <bean parent="FieldDefinition"
              p:attributeName="itemReturnedTotalQuantity" />
        <bean parent="FieldDefinition"
              p:attributeName="itemTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="itemUnitOfMeasureCode" />
        <bean parent="FieldDefinition"
              p:attributeName="purchaseOrderIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="receivingItemIdentifier" />
      </list>
    </property>
  </bean>
</beans>
