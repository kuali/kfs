<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
    <bean id="budgetService" class="org.kuali.module.kra.service.impl.BudgetServiceImpl">
        <property name="budgetFringeRateService">
            <ref bean="budgetFringeRateService" />
        </property>
        <property name="budgetGraduateAssistantRateService">
            <ref bean="budgetGraduateAssistantRateService" />
        </property>
        <property name="budgetPeriodDao">
            <ref bean="budgetPeriodDao"/>
        </property>
        <property name="budgetTaskDao">
            <ref bean="budgetTaskDao" />
        </property>
    </bean>

    <bean id="budgetFringeRateService" class="org.kuali.module.kra.service.impl.BudgetFringeRateServiceImpl">
        <property name="appointmentTypeDao">
            <ref bean="appointmentTypeDao" />
        </property>
        <property name="budgetFringeRateDao">
            <ref bean="budgetFringeRateDao" />
        </property>
    </bean>
    
    <bean id="budgetNonpersonnelService" class="org.kuali.module.kra.service.impl.BudgetNonpersonnelServiceImpl">
        <property name="budgetNonpersonnelDao">
            <ref bean="budgetNonpersonnelDao" />
        </property>
        <property name="nonpersonnelCategoryDao">
            <ref bean="nonpersonnelCategoryDao" />
        </property>
    </bean>
    
    <bean id="budgetGraduateAssistantRateService" class="org.kuali.module.kra.service.impl.BudgetGraduateAssistantRateServiceImpl">
        <property name="graduateAssistantRateDao">
            <ref bean="graduateAssistantRateDao" />
        </property>
        <property name="budgetGraduateAssistantRateDao">
            <ref bean="budgetGraduateAssistantRateDao" />
        </property>
    </bean>
    
    <bean id="budgetPeriodService" class="org.kuali.module.kra.service.impl.BudgetPeriodServiceImpl">
        <property name="budgetPeriodDao">
            <ref bean="budgetPeriodDao"/>
        </property>
        <property name="dateTimeService">
            <ref bean="dateTimeService" />
        </property>
    </bean>
    
    <bean id="budgetTaskService" class="org.kuali.module.kra.service.impl.BudgetTaskServiceImpl">
        <property name="budgetTaskDao">
            <ref bean="budgetTaskDao" />
        </property>
    </bean>
    
    <bean id="researchDocumentService" class="org.kuali.module.kra.service.impl.ResearchDocumentServiceImpl">
        <property name="budgetService">
            <ref bean="budgetService" />
        </property>
    </bean>
    
    <bean id="budgetPersonnelService" class="org.kuali.module.kra.service.impl.BudgetPersonnelServiceImpl">
        <property name="dateTimeService">
            <ref bean="dateTimeService" />
        </property>
    </bean>
</beans>