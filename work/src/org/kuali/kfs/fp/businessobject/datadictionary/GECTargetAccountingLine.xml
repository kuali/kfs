<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd" [
<!ENTITY accountingLineBaseAttributes SYSTEM "../../../kfs/entities/AccountingLineBaseAttributes.xml">
]>
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.financial.bo.GECTargetAccountingLine">


        <attributes>
            &accountingLineBaseAttributes;
            
			<attribute name="referenceNumber" forceUppercase="true" override="true">
			    <label>Reference Number</label>
			    <shortLabel>Ref Number</shortLabel>
			
			    <maxLength>14</maxLength>
			
			    <validationPattern>
			        <alphaNumeric />
			    </validationPattern>
			    <required>false</required>
			    <control>
			        <text size="16" />
			    </control>
			
			    <summary>
			        <![CDATA[&nbsp;]]>
			    </summary>
			    <description>
			        The transaction number on the deposit slip assigned by the bank.
			    </description>
			</attribute>

			<attribute name="referenceOriginCode" forceUppercase="true" override="true">
			    <label>Reference Origin Code</label>
			    <shortLabel>Ref Origin Code</shortLabel>
			
			    <maxLength>2</maxLength>
			
			    <validationPattern>
			        <anyCharacter />
			    </validationPattern>
			    <required>false</required>
			    <control>
			        <text size="4" />
			    </control>
			
			    <summary>
			        <![CDATA[&nbsp;]]>
			    </summary>
			    <description>
			        The system code for the system from which the original encumbering transaction came, selected from a pulldown list.
			    </description>
			</attribute>
        </attributes>

    </businessObject>
</dictionaryEntry>
