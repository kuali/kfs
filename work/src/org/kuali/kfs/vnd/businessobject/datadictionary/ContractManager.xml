<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="ContractManager" parent="ContractManager-parentBean" />

  <bean id="ContractManager-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.vendor.bo.ContractManager" />
    <property name="inquiryDefinition" >
      <ref bean="ContractManager-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="ContractManager-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="contractManagerCode" />
    <property name="objectLabel" value="Contract Manager" />
    <property name="attributes" >
      <list>
        <ref bean="ContractManager-contractManagerCode" />
        <ref bean="ContractManager-contractManagerDelegationDollarLimit" />
        <ref bean="ContractManager-contractManagerFaxNumber" />
        <ref bean="ContractManager-contractManagerName" />
        <ref bean="ContractManager-contractManagerPhoneNumber" />
        <ref bean="ContractManager-contractManagerSignatureImageLocationDescription" />
        <ref bean="ContractManager-contractManagerUserIdentifier" />
        <ref bean="ContractManager-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="ContractManager-contractManagerCode" parent="ContractManager-contractManagerCode-parentBean" />

  <bean id="ContractManager-contractManagerCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="contractManagerCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Contract Manager Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
  </bean>
  <bean id="ContractManager-contractManagerDelegationDollarLimit" parent="ContractManager-contractManagerDelegationDollarLimit-parentBean" />

  <bean id="ContractManager-contractManagerDelegationDollarLimit-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="contractManagerDelegationDollarLimit" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Contract Manager Delegation Dollar Limit" />
    <property name="shortLabel" value="Limit" />
    <property name="maxLength" value="19" />
    <property name="validationPattern" >
      <bean parent="FloatingPointValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="21" />
    </property>
    <property name="formatterClass" value="org.kuali.core.web.format.CurrencyFormatter" />
  </bean>
  <bean id="ContractManager-contractManagerFaxNumber" parent="ContractManager-contractManagerFaxNumber-parentBean" />

  <bean id="ContractManager-contractManagerFaxNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="contractManagerFaxNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Contract Manager Fax Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="45" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="47" />
    </property>
  </bean>
  <bean id="ContractManager-contractManagerName" parent="ContractManager-contractManagerName-parentBean" />

  <bean id="ContractManager-contractManagerName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="contractManagerName" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Contract Manager Name" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="45" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="47" />
    </property>
  </bean>
  <bean id="ContractManager-contractManagerPhoneNumber" parent="ContractManager-contractManagerPhoneNumber-parentBean" />

  <bean id="ContractManager-contractManagerPhoneNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="contractManagerPhoneNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Contract Manager Phone Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="45" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="47" />
    </property>
  </bean>
  <bean id="ContractManager-contractManagerSignatureImageLocationDescription" parent="ContractManager-contractManagerSignatureImageLocationDescription-parentBean" />

  <bean id="ContractManager-contractManagerSignatureImageLocationDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="contractManagerSignatureImageLocationDescription" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Contract Manager Signature Image Location Description" />
    <property name="shortLabel" value="Description" />
    <property name="maxLength" value="45" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="47" />
    </property>
  </bean>
  <bean id="ContractManager-contractManagerUserIdentifier" parent="ContractManager-contractManagerUserIdentifier-parentBean" />

  <bean id="ContractManager-contractManagerUserIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="contractManagerUserIdentifier" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Contract Manager User Identifier" />
    <property name="shortLabel" value="Identifier" />
    <property name="maxLength" value="11" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="13" />
    </property>
  </bean>
  <bean id="ContractManager-versionNumber" parent="ContractManager-versionNumber-parentBean" />

  <bean id="ContractManager-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="ContractManager-inquiryDefinition" parent="ContractManager-inquiryDefinition-parentBean" />

  <bean id="ContractManager-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Contract Manager Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="contractManagerCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="contractManagerUserIdentifier" />
              <bean parent="FieldDefinition"
                    p:attributeName="contractManagerName" />
              <bean parent="FieldDefinition"
                    p:attributeName="contractManagerPhoneNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="contractManagerFaxNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="contractManagerDelegationDollarLimit" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="ContractManager-lookupDefinition" parent="ContractManager-lookupDefinition-parentBean" />

  <bean id="ContractManager-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Contract Manager Lookup" />
    <property name="instructions" value="Lookup a Contract Manager" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="contractManagerCode" />
        <bean parent="FieldDefinition"
              p:attributeName="contractManagerUserIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="contractManagerName" />
        <bean parent="FieldDefinition"
              p:attributeName="contractManagerPhoneNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="contractManagerFaxNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="contractManagerDelegationDollarLimit" />
        <bean parent="FieldDefinition"
              p:attributeName="contractManagerSignatureImageLocationDescription" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="contractManagerCode" />
        <bean parent="FieldDefinition"
              p:attributeName="contractManagerUserIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="contractManagerName" />
        <bean parent="FieldDefinition"
              p:attributeName="contractManagerPhoneNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="contractManagerFaxNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="contractManagerDelegationDollarLimit" />
        <bean parent="FieldDefinition"
              p:attributeName="contractManagerSignatureImageLocationDescription" />
      </list>
    </property>
  </bean>
</beans>
