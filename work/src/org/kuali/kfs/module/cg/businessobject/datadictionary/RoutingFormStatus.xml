<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.kra.routingform.bo.RoutingFormStatus">


        <inquiry>
            <title>Routing Form Status Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="dataObjectMaintenanceCodeActiveIndicator" />
                <field attributeName="routingFormStatusCode" />
                <field attributeName="routingFormStatusDescription" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Routing Form Status Lookup</title>

            <instructions>Look up a Routing Form Status</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="routingFormStatusCode" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="dataObjectMaintenanceCodeActiveIndicator" required="false" />
                <lookupField attributeName="routingFormStatusCode" required="false" />
                <lookupField attributeName="routingFormStatusDescription" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="dataObjectMaintenanceCodeActiveIndicator" />
                <field attributeName="routingFormStatusCode" />
                <field attributeName="routingFormStatusDescription" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="dataObjectMaintenanceCodeActiveIndicator" forceUppercase="true">
                <label>Data Object Maintenance Code Active Indicator</label>
                <shortLabel>Indicator</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <checkbox />
                </control>

                <summary>dataObjectMaintenanceCodeActiveIndicator</summary>
                <description>dataObjectMaintenanceCodeActiveIndicator description...</description>
			</attribute>

            <attribute name="routingFormStatusCode" forceUppercase="true">
                <label>Routing Form Status Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="4" />
                </control>

                <summary>routingFormStatusCode</summary>
                <description>routingFormStatusCode description...</description>
			</attribute>

            <attribute name="routingFormStatusDescription" forceUppercase="true">
                <label>Routing Form Status Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>15</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="17" />
                </control>

                <summary>routingFormStatusDescription</summary>
                <description>routingFormStatusDescription description...</description>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
