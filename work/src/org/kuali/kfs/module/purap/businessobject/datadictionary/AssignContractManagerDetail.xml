<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="AssignContractManagerDetail" parent="AssignContractManagerDetail-parentBean" />

  <bean id="AssignContractManagerDetail-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.purap.bo.AssignContractManagerDetail" />
    <property name="attributes" >
      <list>
        <ref bean="AssignContractManagerDetail-contractManagerCode" />
        <ref bean="AssignContractManagerDetail-documentNumber" />
        <ref bean="AssignContractManagerDetail-requisitionIdentifier" />
        <ref bean="AssignContractManagerDetail-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="AssignContractManagerDetail-contractManagerCode" parent="AssignContractManagerDetail-contractManagerCode-parentBean" />

  <bean id="AssignContractManagerDetail-contractManagerCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="contractManagerCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Contract Manager Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
  </bean>
  <bean id="AssignContractManagerDetail-documentNumber" parent="AssignContractManagerDetail-documentNumber-parentBean" />

  <bean id="AssignContractManagerDetail-documentNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="documentNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Document Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="14" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="16" />
    </property>
  </bean>
  <bean id="AssignContractManagerDetail-requisitionIdentifier" parent="AssignContractManagerDetail-requisitionIdentifier-parentBean" />

  <bean id="AssignContractManagerDetail-requisitionIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="requisitionIdentifier" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Requisition Identifier" />
    <property name="shortLabel" value="Identifier" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
  </bean>
  <bean id="AssignContractManagerDetail-versionNumber" parent="AssignContractManagerDetail-versionNumber-parentBean" />

  <bean id="AssignContractManagerDetail-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>
</beans>
