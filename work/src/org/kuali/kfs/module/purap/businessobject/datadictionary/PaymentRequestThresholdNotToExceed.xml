<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="PaymentRequestThresholdNotToExceed" parent="PaymentRequestThresholdNotToExceed-parentBean" />

  <bean id="PaymentRequestThresholdNotToExceed-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.purap.bo.PaymentRequestThresholdNotToExceed" />
    <property name="inquiryDefinition" >
      <ref bean="PaymentRequestThresholdNotToExceed-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="PaymentRequestThresholdNotToExceed-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="PaymentRequestThresholdNotToExceed-accountsPayableThresholdNotToExceedAmount" />
        <ref bean="PaymentRequestThresholdNotToExceed-versionNumber" />
        <ref bean="PaymentRequestThresholdNotToExceed-chartOfAccountsCode" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="PaymentRequestThresholdNotToExceed-accountsPayableThresholdNotToExceedAmount" parent="PaymentRequestThresholdNotToExceed-accountsPayableThresholdNotToExceedAmount-parentBean" />

  <bean id="PaymentRequestThresholdNotToExceed-accountsPayableThresholdNotToExceedAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="accountsPayableThresholdNotToExceedAmount" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Accounts Payable Threshold Not To Exceed Amount" />
    <property name="shortLabel" value="Amount" />
    <property name="maxLength" value="19" />
    <property name="validationPattern" >
      <bean parent="FloatingPointValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="21" />
    </property>
  </bean>
  <bean id="PaymentRequestThresholdNotToExceed-versionNumber" parent="PaymentRequestThresholdNotToExceed-versionNumber-parentBean" />

  <bean id="PaymentRequestThresholdNotToExceed-versionNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="versionNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Version Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
  </bean>
  <bean id="PaymentRequestThresholdNotToExceed-chartOfAccountsCode" parent="PaymentRequestThresholdNotToExceed-chartOfAccountsCode-parentBean" />

  <bean id="PaymentRequestThresholdNotToExceed-chartOfAccountsCode-parentBean" abstract="true" parent="Chart-chartOfAccountsCode">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="PaymentRequestThresholdNotToExceed-inquiryDefinition" parent="PaymentRequestThresholdNotToExceed-inquiryDefinition-parentBean" />

  <bean id="PaymentRequestThresholdNotToExceed-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Payment Request Threshold Not To Exceed Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="accountsPayableThresholdNotToExceedAmount" />
              <bean parent="FieldDefinition"
                    p:attributeName="chartOfAccountsCode" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="PaymentRequestThresholdNotToExceed-lookupDefinition" parent="PaymentRequestThresholdNotToExceed-lookupDefinition-parentBean" />

  <bean id="PaymentRequestThresholdNotToExceed-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Payment Request Threshold Not To Exceed Lookup" />
    <property name="instructions" value="Lookup a Payment Request Threshold Not To Exceed" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="accountsPayableThresholdNotToExceedAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="accountsPayableThresholdNotToExceedAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
      </list>
    </property>
  </bean>
</beans>
