<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="OwnershipTypeMaintenanceDocument" parent="OwnershipTypeMaintenanceDocument-parentBean" />

  <bean id="OwnershipTypeMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.vendor.bo.OwnershipType" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="OwnershipTypeMaintenanceDocument-EditOwnershipType" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>vendorOwnershipCode</value>
      </list>
    </property>
    <property name="label" value="Ownership Type" />
    <property name="shortLabel" value="Ownership Type" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-VN"
            p:parameterName="OWNERSHIP_TYPE"
            p:parameterDetailType="OwnershipType" />
    </property>
    <property name="documentTypeName" value="OwnershipTypeMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="PMOT" />
    <property name="workflowProperties" >
      <ref bean="OwnershipTypeMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>PA_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="OwnershipTypeMaintenanceDocument-EditOwnershipType" parent="OwnershipTypeMaintenanceDocument-EditOwnershipType-parentBean" />

  <bean id="OwnershipTypeMaintenanceDocument-EditOwnershipType-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Ownership Type" />
    <property name="title" value="Edit Ownership Type" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="vendorOwnershipCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="vendorOwnershipDescription"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="vendorOwnershipCategoryAllowedIndicator" />
        <bean parent="MaintainableFieldDefinition"
              p:name="active" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="OwnershipTypeMaintenanceDocument-workflowProperties" parent="OwnershipTypeMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="OwnershipTypeMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
