<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.kra.routingform.bo.ControlAttributeType">
        <customAttributeEnabled>false</customAttributeEnabled>

        <inquiry>
            <title>Control Attribute Type Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="controlAttributeTypeCode" />
                <field attributeName="controlAttributeTypeDescription" />
                <field attributeName="dataObjectMaintenanceCodeActiveIndicator" />
                <field attributeName="requiredIndicator" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Control Attribute Type Lookup</title>

            <instructions>Look up a Control Attribute Type</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="controlAttributeTypeCode" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="controlAttributeTypeCode" required="false" />
                <lookupField attributeName="controlAttributeTypeDescription" required="false" />
                <lookupField attributeName="dataObjectMaintenanceCodeActiveIndicator" required="false" />
                <lookupField attributeName="requiredIndicator" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="controlAttributeTypeCode" />
                <field attributeName="controlAttributeTypeDescription" />
                <field attributeName="dataObjectMaintenanceCodeActiveIndicator" />
                <field attributeName="requiredIndicator" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="controlAttributeTypeCode" forceUppercase="true">
                <label>Control Attribute Type Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>1</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="3" />
                </control>

                <summary>controlAttributeTypeCode</summary>
                <description>controlAttributeTypeCode description...</description>
			</attribute>
			
			<attribute name="controlAttributeTypeDescription" forceUppercase="false">
                <label>Control Attribute Type Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>100</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true" /></validationPattern>

                <control>
                    <text size="20" />
                </control>

                <summary>controlAttributeTypeDescription</summary>
                <description>controlAttributeTypeDescription description...</description>
			</attribute>

            <attribute name="dataObjectMaintenanceCodeActiveIndicator" forceUppercase="true">
                <label>Data Object Maintenance Code Active Indicator</label>
                <shortLabel>Indicator</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <checkbox />
                </control>

                <summary>dataObjectMaintenanceCodeActiveIndicator</summary>
                <description>dataObjectMaintenanceCodeActiveIndicator description...</description>
			</attribute>

            <attribute name="requiredIndicator" forceUppercase="true">
                <label>Required Indicator</label>
                <shortLabel>Indicator</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <checkbox />
                </control>

                <summary>requiredIndicator</summary>
                <description>requiredIndicator description...</description>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
