<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="AssetGlobal" parent="AssetGlobal-parentBean" />

  <bean id="AssetGlobal-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.cams.bo.AssetGlobal" />
    <property name="objectLabel" value="Asset Global" />
    <property name="attributes" >
      <list>
        <ref bean="AssetGlobal-assetHeader.organizationOwnerChartOfAccountsCode" />
        <ref bean="AssetGlobal-assetHeader.organizationOwnerAccountNumber" />
        <ref bean="AssetGlobal-assetHeader.agencyNumber" />
        <ref bean="AssetGlobal-assetHeader.versionNumber" />
        <ref bean="AssetGlobal-acquisitionTypeCode" />
        <ref bean="AssetGlobal-inventoryStatusCode" />
        <ref bean="AssetGlobal-conditionCode" />
        <ref bean="AssetGlobal-capitalAssetDescription" />
        <ref bean="AssetGlobal-capitalAssetTypeCode" />
        <ref bean="AssetGlobal-vendorName" />
        <ref bean="AssetGlobal-manufacturerName" />
        <ref bean="AssetGlobal-manufacturerModelNumber" />
        <ref bean="AssetGlobal-organizationText" />
        <ref bean="AssetGlobal-assetHeader.representativeUniversalIdentifier" />
        <ref bean="AssetGlobal-assetHeader.assetRepresentative.personUserIdentifier" />
        <ref bean="AssetGlobal-assetHeader.assetRepresentative.personName" />
        <ref bean="AssetGlobal-lastInventoryDate" />
        <ref bean="AssetGlobal-createDate" />
        <ref bean="AssetGlobal-capitalAssetInServiceDate" />
        <ref bean="AssetGlobal-capitalAssetDepreciationDate" />
        <ref bean="AssetGlobal-landCountyName" />
        <ref bean="AssetGlobal-landAcreageSize" />
        <ref bean="AssetGlobal-landParcelNumber" />
        <ref bean="AssetGlobal-financialDocumentNextLineNumber" />
        <ref bean="AssetGlobal-financialDocumentTypeCode" />
        <ref bean="AssetGlobal-versionNumber" />
      </list>
    </property>
    <property name="relationships" >
      <list>
        <bean parent="RelationshipDefinition">
          <property name="objectAttributeName" value="assetHeader.assetRepresentative" />
          <property name="primitiveAttributes" >
            <list>
              <bean parent="PrimitiveAttributeDefinition"
                    p:sourceName="assetHeader.representativeUniversalIdentifier"
                    p:targetName="personUniversalIdentifier" />
            </list>
          </property>
          <property name="supportAttributes" >
            <list>
              <bean parent="SupportAttributeDefinition"
                    p:identifier="true"
                    p:sourceName="assetHeader.assetRepresentative.personUserIdentifier"
                    p:targetName="personUserIdentifier" />
              <bean parent="SupportAttributeDefinition"
                    p:sourceName="assetHeader.assetRepresentative.personName"
                    p:targetName="personName" />
            </list>
          </property>
        </bean>
        <bean parent="RelationshipDefinition">
          <property name="objectAttributeName" value="assetHeader.organizationOwnerChartOfAccounts" />
          <property name="primitiveAttributes" >
            <list>
              <bean parent="PrimitiveAttributeDefinition"
                    p:sourceName="assetHeader.organizationOwnerChartOfAccountsCode"
                    p:targetName="chartOfAccountsCode" />
            </list>
          </property>
        </bean>
        <bean parent="RelationshipDefinition">
          <property name="objectAttributeName" value="assetHeader.organizationOwnerAccount" />
          <property name="primitiveAttributes" >
            <list>
              <bean parent="PrimitiveAttributeDefinition"
                    p:sourceName="assetHeader.organizationOwnerChartOfAccountsCode"
                    p:targetName="chartOfAccountsCode" />
              <bean parent="PrimitiveAttributeDefinition"
                    p:sourceName="assetHeader.organizationOwnerAccountNumber"
                    p:targetName="accountNumber" />
            </list>
          </property>
        </bean>
        <bean parent="RelationshipDefinition">
          <property name="objectAttributeName" value="assetHeader.agency" />
          <property name="primitiveAttributes" >
            <list>
              <bean parent="PrimitiveAttributeDefinition"
                    p:sourceName="assetHeader.agencyNumber"
                    p:targetName="agencyNumber" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="AssetGlobal-assetHeader.organizationOwnerChartOfAccountsCode" parent="AssetGlobal-assetHeader.organizationOwnerChartOfAccountsCode-parentBean" />

  <bean id="AssetGlobal-assetHeader.organizationOwnerChartOfAccountsCode-parentBean" abstract="true" parent="AssetHeader-organizationOwnerChartOfAccountsCode">
    <property name="name" value="assetHeader.organizationOwnerChartOfAccountsCode" />
  </bean>
  <bean id="AssetGlobal-assetHeader.organizationOwnerAccountNumber" parent="AssetGlobal-assetHeader.organizationOwnerAccountNumber-parentBean" />

  <bean id="AssetGlobal-assetHeader.organizationOwnerAccountNumber-parentBean" abstract="true" parent="AssetHeader-organizationOwnerAccountNumber">
    <property name="name" value="assetHeader.organizationOwnerAccountNumber" />
  </bean>
  <bean id="AssetGlobal-assetHeader.agencyNumber" parent="AssetGlobal-assetHeader.agencyNumber-parentBean" />

  <bean id="AssetGlobal-assetHeader.agencyNumber-parentBean" abstract="true" parent="AssetHeader-agencyNumber">
    <property name="name" value="assetHeader.agencyNumber" />
  </bean>
   <bean id="AssetGlobal-assetHeader.versionNumber" parent="AssetGlobal-assetHeader.versionNumber-parentBean" />

  <bean id="AssetGlobal-assetHeader.versionNumber-parentBean" abstract="true" parent="AssetHeader-versionNumber">
    <property name="name" value="assetHeader.versionNumber" />
  </bean>
  <bean id="AssetGlobal-acquisitionTypeCode" parent="AssetGlobal-acquisitionTypeCode-parentBean" />

  <bean id="AssetGlobal-acquisitionTypeCode-parentBean" abstract="true" parent="Asset-acquisitionTypeCode">
  </bean>
  <bean id="AssetGlobal-inventoryStatusCode" parent="AssetGlobal-inventoryStatusCode-parentBean" />

  <bean id="AssetGlobal-inventoryStatusCode-parentBean" abstract="true" parent="Asset-inventoryStatusCode">
  </bean>
  <bean id="AssetGlobal-conditionCode" parent="AssetGlobal-conditionCode-parentBean" />

  <bean id="AssetGlobal-conditionCode-parentBean" abstract="true" parent="AssetCondition-assetConditionCode">
    <property name="name" value="conditionCode" />
    <property name="control">
    	<bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.module.cams.lookup.valuefinder.AssetConditionValuesFinder"
            />
    </property>
  </bean>
  <bean id="AssetGlobal-capitalAssetDescription" parent="AssetGlobal-capitalAssetDescription-parentBean" />

  <bean id="AssetGlobal-capitalAssetDescription-parentBean" abstract="true" parent="Asset-capitalAssetDescription">
  </bean>
  <bean id="AssetGlobal-capitalAssetTypeCode" parent="AssetGlobal-capitalAssetTypeCode-parentBean" />

  <bean id="AssetGlobal-capitalAssetTypeCode-parentBean" abstract="true" parent="AssetType-capitalAssetTypeCode">
    <property name="label" value="Asset Type Code" />
  </bean>
  <bean id="AssetGlobal-vendorName" parent="AssetGlobal-vendorName-parentBean" />

  <bean id="AssetGlobal-vendorName-parentBean" abstract="true" parent="Asset-vendorName">
  </bean>
  <bean id="AssetGlobal-manufacturerName" parent="AssetGlobal-manufacturerName-parentBean" />

  <bean id="AssetGlobal-manufacturerName-parentBean" abstract="true" parent="Asset-manufacturerName">
  </bean>
  <bean id="AssetGlobal-manufacturerModelNumber" parent="AssetGlobal-manufacturerModelNumber-parentBean" />

  <bean id="AssetGlobal-manufacturerModelNumber-parentBean" abstract="true" parent="Asset-manufacturerModelNumber">
  </bean>
  <bean id="AssetGlobal-organizationText" parent="AssetGlobal-organizationText-parentBean" />

  <bean id="AssetGlobal-organizationText-parentBean" abstract="true" parent="AssetOrganization-organizationText">
  </bean>
  <bean id="AssetGlobal-assetHeader.representativeUniversalIdentifier" parent="AssetGlobal-assetHeader.representativeUniversalIdentifier-parentBean" />

  <bean id="AssetGlobal-assetHeader.representativeUniversalIdentifier-parentBean" abstract="true" parent="AssetHeader-representativeUniversalIdentifier">
    <property name="name" value="assetHeader.representativeUniversalIdentifier" />
    <property name="label" value="Asset Representitive" />
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
    <property name="displayLabelAttribute" value="assetHeader.assetRepresentative.personUserIdentifier" />
  </bean>
  <bean id="AssetGlobal-assetHeader.assetRepresentative.personUserIdentifier" parent="AssetGlobal-assetHeader.assetRepresentative.personUserIdentifier-parentBean" />

  <bean id="AssetGlobal-assetHeader.assetRepresentative.personUserIdentifier-parentBean" abstract="true" parent="UniversalUser-personUserIdentifier">
    <property name="name" value="assetHeader.assetRepresentative.personUserIdentifier" />
    <property name="label" value="Asset Representitive" />
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="KualiUserControlDefinition"
            p:personNameAttributeName="assetHeader.assetRepresentative.personName"
            p:universalIdAttributeName="assetHeader.representativeUniversalIdentifier"
            p:userIdAttributeName="assetHeader.assetRepresentative.personUserIdentifier" />
    </property>
  </bean>
  <bean id="AssetGlobal-assetHeader.assetRepresentative.personName" parent="AssetGlobal-assetHeader.assetRepresentative.personName-parentBean" />

  <bean id="AssetGlobal-assetHeader.assetRepresentative.personName-parentBean" abstract="true" parent="UniversalUser-personName">
    <property name="name" value="assetHeader.assetRepresentative.personName" />
    <property name="label" value="Asset Representitive" />
    <property name="required" value="false" />
  </bean>
  <bean id="AssetGlobal-lastInventoryDate" parent="AssetGlobal-lastInventoryDate-parentBean" />

  <bean id="AssetGlobal-lastInventoryDate-parentBean" abstract="true" parent="GenericAttributes-genericDate">
    <property name="name" value="lastInventoryDate" />
    <property name="label" value="Last Inventory Date" />
  </bean>
  <bean id="AssetGlobal-createDate" parent="AssetGlobal-createDate-parentBean" />

  <bean id="AssetGlobal-createDate-parentBean" abstract="true" parent="GenericAttributes-genericDate">
    <property name="name" value="createDate" />
    <property name="label" value="Create Date" />
  </bean>
  <bean id="AssetGlobal-capitalAssetInServiceDate" parent="AssetGlobal-capitalAssetInServiceDate-parentBean" />

  <bean id="AssetGlobal-capitalAssetInServiceDate-parentBean" abstract="true" parent="GenericAttributes-genericDate">
    <property name="name" value="capitalAssetInServiceDate" />
    <property name="label" value="In-service Date" />
  </bean>
  <bean id="AssetGlobal-capitalAssetDepreciationDate" parent="AssetGlobal-capitalAssetDepreciationDate-parentBean" />

  <bean id="AssetGlobal-capitalAssetDepreciationDate-parentBean" abstract="true" parent="GenericAttributes-genericDate">
    <property name="name" value="capitalAssetDepreciationDate" />
    <property name="label" value="Depreciation Date" />
  </bean>
  <bean id="AssetGlobal-landCountyName" parent="AssetGlobal-landCountyName-parentBean" />

  <bean id="AssetGlobal-landCountyName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="landCountyName" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Land County Name" />
    <property name="shortLabel" value="Country" />
    <property name="maxLength" value="20" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="22" />
    </property>
    <property name="summary" value="landCountyName" />
    <property name="description" value="landCountyName description..." />
  </bean>
  <bean id="AssetGlobal-landAcreageSize" parent="AssetGlobal-landAcreageSize-parentBean" />

  <bean id="AssetGlobal-landAcreageSize-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="landAcreageSize" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Land Acreage" />
    <property name="shortLabel" value="Acreage" />
    <property name="maxLength" value="7" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="9" />
    </property>
    <property name="summary" value="landAcreageSize" />
    <property name="description" value="landAcreageSize description..." />
  </bean>
  <bean id="AssetGlobal-landParcelNumber" parent="AssetGlobal-landParcelNumber-parentBean" />

  <bean id="AssetGlobal-landParcelNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="landParcelNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Land Parcel Number" />
    <property name="shortLabel" value="Parcel" />
    <property name="maxLength" value="30" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="32" />
    </property>
    <property name="summary" value="landParcelNumber" />
    <property name="description" value="landParcelNumber description..." />
  </bean>
  <bean id="AssetGlobal-financialDocumentNextLineNumber" parent="AssetGlobal-financialDocumentNextLineNumber-parentBean" />

  <bean id="AssetGlobal-financialDocumentNextLineNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialDocumentNextLineNumber" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Financial Document Next LineNumber" />
    <property name="shortLabel" value="financialDocumentNextLineNumber" />
    <property name="maxLength" value="32" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
    <property name="summary" value="financialDocumentNextLineNumber" />
    <property name="description" value="financialDocumentNextLineNumber description..." />
  </bean>
  
  <bean id="AssetGlobal-financialDocumentTypeCode" parent="AssetGlobal-financialDocumentTypeCode-parentBean" />

  <bean id="AssetGlobal-financialDocumentTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialDocumentTypeCode" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Financial Document Type Code" />
    <property name="shortLabel" value="financialDocumentTypeCode" />
    <property name="maxLength" value="6" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
    <property name="summary" value="financialDocumentTypeCode" />
    <property name="description" value="financialDocumentTypeCode description..." />
  </bean>
  
  <bean id="AssetGlobal-versionNumber" parent="AssetGlobal-versionNumber-parentBean" />

  <bean id="AssetGlobal-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>
</beans>
