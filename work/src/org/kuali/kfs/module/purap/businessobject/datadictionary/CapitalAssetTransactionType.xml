<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="CapitalAssetTransactionType" parent="CapitalAssetTransactionType-parentBean" />

  <bean id="CapitalAssetTransactionType-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.purap.bo.CapitalAssetTransactionType" />
    <property name="inquiryDefinition" >
      <ref bean="CapitalAssetTransactionType-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="CapitalAssetTransactionType-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="capitalAssetTransactionTypeCode" />
    <property name="objectLabel" value="Capital Asset Transaction Type" />
    <property name="attributes" >
      <list>
        <ref bean="CapitalAssetTransactionType-capitalAssetTransactionTypeCode" />
        <ref bean="CapitalAssetTransactionType-capitalAssetTransactionTypeDescription" />
        <ref bean="CapitalAssetTransactionType-capitalAssetTransactionTypeServiceIndicator" />
        <ref bean="CapitalAssetTransactionType-active" />
        <ref bean="CapitalAssetTransactionType-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="CapitalAssetTransactionType-capitalAssetTransactionTypeCode" parent="CapitalAssetTransactionType-capitalAssetTransactionTypeCode-parentBean" />

  <bean id="CapitalAssetTransactionType-capitalAssetTransactionTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="capitalAssetTransactionTypeCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="CA Transaction Type Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="6" />
    </property>
  </bean>
  <bean id="CapitalAssetTransactionType-capitalAssetTransactionTypeDescription" parent="CapitalAssetTransactionType-capitalAssetTransactionTypeDescription-parentBean" />

  <bean id="CapitalAssetTransactionType-capitalAssetTransactionTypeDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="capitalAssetTransactionTypeDescription" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="CA Transaction Type Description" />
    <property name="shortLabel" value="Description" />
    <property name="maxLength" value="45" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="47" />
    </property>
  </bean>
  <bean id="CapitalAssetTransactionType-capitalAssetTransactionTypeServiceIndicator" parent="CapitalAssetTransactionType-capitalAssetTransactionTypeServiceIndicator-parentBean" />

  <bean id="CapitalAssetTransactionType-capitalAssetTransactionTypeServiceIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="capitalAssetTransactionTypeServiceIndicator" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="CA Transaction Type Service Indicator" />
    <property name="shortLabel" value="Indicator" />
    <property name="maxLength" value="1" />
    <property name="control" >
      <bean parent="CheckboxControlDefinition" />
    </property>
  </bean>
  <bean id="CapitalAssetTransactionType-active" parent="CapitalAssetTransactionType-active-parentBean" />

  <bean id="CapitalAssetTransactionType-active-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
    <property name="name" value="active" />
  </bean>
  <bean id="CapitalAssetTransactionType-versionNumber" parent="CapitalAssetTransactionType-versionNumber-parentBean" />

  <bean id="CapitalAssetTransactionType-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="CapitalAssetTransactionType-inquiryDefinition" parent="CapitalAssetTransactionType-inquiryDefinition-parentBean" />

  <bean id="CapitalAssetTransactionType-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Capital Asset Transaction Type Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="capitalAssetTransactionTypeCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="capitalAssetTransactionTypeDescription" />
              <bean parent="FieldDefinition"
                    p:attributeName="capitalAssetTransactionTypeServiceIndicator" />
              <bean parent="FieldDefinition"
                    p:attributeName="active" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="CapitalAssetTransactionType-lookupDefinition" parent="CapitalAssetTransactionType-lookupDefinition-parentBean" />

  <bean id="CapitalAssetTransactionType-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Capital Asset Transaction Type Lookup" />
    <property name="instructions" value="Lookup a Capital Asset Transaction Type" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetTransactionTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetTransactionTypeDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetTransactionTypeServiceIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetTransactionTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetTransactionTypeDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetTransactionTypeServiceIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
  </bean>
</beans>
