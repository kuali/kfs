<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="ProposalProjectDirector" parent="ProposalProjectDirector-parentBean" />

  <bean id="ProposalProjectDirector-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.cg.bo.ProposalProjectDirector" />
    <property name="inquiryDefinition" >
      <ref bean="ProposalProjectDirector-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="ProposalProjectDirector-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="ProposalProjectDirector-personUniversalIdentifier" />
        <ref bean="ProposalProjectDirector-projectDirector.personUserIdentifier" />
        <ref bean="ProposalProjectDirector-projectDirector.personName" />
        <ref bean="ProposalProjectDirector-proposalNumber" />
        <ref bean="ProposalProjectDirector-proposalPrimaryProjectDirectorIndicator" />
        <ref bean="ProposalProjectDirector-proposalProjectDirectorProjectTitle" />
        <ref bean="ProposalProjectDirector-newCollectionRecord" />
        <ref bean="ProposalProjectDirector-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="ProposalProjectDirector-personUniversalIdentifier" parent="ProposalProjectDirector-personUniversalIdentifier-parentBean" />

  <bean id="ProposalProjectDirector-personUniversalIdentifier-parentBean" abstract="true" parent="ProjectDirector-personUniversalIdentifier">
    <property name="label" value="Project Director ID" />
    <property name="shortLabel" value="Proj Dir ID" />
    <property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
  </bean>
  <bean id="ProposalProjectDirector-projectDirector.personUserIdentifier" parent="ProposalProjectDirector-projectDirector.personUserIdentifier-parentBean" />

  <bean id="ProposalProjectDirector-projectDirector.personUserIdentifier-parentBean" abstract="true" parent="UniversalUser-personUserIdentifier">
    <property name="name" value="projectDirector.personUserIdentifier" />
    <property name="label" value="Project Director User ID" />
    <property name="required" value="false" />
  </bean>
  <bean id="ProposalProjectDirector-projectDirector.personName" parent="ProposalProjectDirector-projectDirector.personName-parentBean" />

  <bean id="ProposalProjectDirector-projectDirector.personName-parentBean" abstract="true" parent="UniversalUser-personName">
    <property name="name" value="projectDirector.personName" />
    <property name="label" value="Name" />
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="40" />
    </property>
  </bean>
  <bean id="ProposalProjectDirector-proposalNumber" parent="ProposalProjectDirector-proposalNumber-parentBean" />

  <bean id="ProposalProjectDirector-proposalNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="proposalNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Proposal Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="12" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="14" />
    </property>
  </bean>
  <bean id="ProposalProjectDirector-proposalPrimaryProjectDirectorIndicator" parent="ProposalProjectDirector-proposalPrimaryProjectDirectorIndicator-parentBean" />

  <bean id="ProposalProjectDirector-proposalPrimaryProjectDirectorIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="proposalPrimaryProjectDirectorIndicator" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Primary" />
    <property name="shortLabel" value="Primary" />
    <property name="maxLength" value="1" />
    <property name="control" >
      <bean parent="CheckboxControlDefinition" />
    </property>
  </bean>
  <bean id="ProposalProjectDirector-proposalProjectDirectorProjectTitle" parent="ProposalProjectDirector-proposalProjectDirectorProjectTitle-parentBean" />

  <bean id="ProposalProjectDirector-proposalProjectDirectorProjectTitle-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="proposalProjectDirectorProjectTitle" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Title" />
    <property name="shortLabel" value="Title" />
    <property name="maxLength" value="100" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="40" />
    </property>
  </bean>
  <bean id="ProposalProjectDirector-newCollectionRecord" parent="ProposalProjectDirector-newCollectionRecord-parentBean" />

  <bean id="ProposalProjectDirector-newCollectionRecord-parentBean" abstract="true" parent="GenericAttributes-newCollectionRecord">
  </bean>
  <bean id="ProposalProjectDirector-versionNumber" parent="ProposalProjectDirector-versionNumber-parentBean" />

  <bean id="ProposalProjectDirector-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="ProposalProjectDirector-inquiryDefinition" parent="ProposalProjectDirector-inquiryDefinition-parentBean" />

  <bean id="ProposalProjectDirector-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Proposal Project Director Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Proposal Project Director Details" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="personUniversalIdentifier" />
              <bean parent="FieldDefinition"
                    p:attributeName="proposalNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="proposalPrimaryProjectDirectorIndicator" />
              <bean parent="FieldDefinition"
                    p:attributeName="proposalProjectDirectorProjectTitle" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="ProposalProjectDirector-lookupDefinition" parent="ProposalProjectDirector-lookupDefinition-parentBean" />

  <bean id="ProposalProjectDirector-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Proposal Project Director Lookup" />
    <property name="instructions" value="Look up a Proposal Project Director" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="personUniversalIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="proposalNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="proposalPrimaryProjectDirectorIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="proposalProjectDirectorProjectTitle" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="personUniversalIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="proposalNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="proposalPrimaryProjectDirectorIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="proposalProjectDirectorProjectTitle" />
      </list>
    </property>
  </bean>
</beans>
