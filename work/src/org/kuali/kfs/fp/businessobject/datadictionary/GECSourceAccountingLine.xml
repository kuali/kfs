    <property name="required" value="false" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="2" />
    </property>
    <property name="summary" value="&amp;nbsp;" />
    <property name="description" value="If it&apos;s a &quot;C&quot; then this accounting line is a credit, if it&apos;s a &quot;D&quot;, then it&apos;s a debit." />
  </bean>
  <bean id="GECSourceAccountingLine-encumbranceUpdateCode" parent="GECSourceAccountingLine-encumbranceUpdateCode-parentBean" />

