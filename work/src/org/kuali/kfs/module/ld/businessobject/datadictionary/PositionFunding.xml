<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="PositionFunding" parent="PositionFunding-parentBean" />

  <bean id="PositionFunding-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.labor.bo.PositionFunding" />
    <property name="inquiryDefinition" >
      <ref bean="PositionFunding-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="PositionFunding-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="PositionFunding-universityFiscalYear" />
        <ref bean="PositionFunding-chartOfAccountsCode" />
        <ref bean="PositionFunding-accountNumber" />
        <ref bean="PositionFunding-subAccountNumber" />
        <ref bean="PositionFunding-financialObjectCode" />
        <ref bean="PositionFunding-financialSubObjectCode" />
        <ref bean="PositionFunding-csfCreateTimestamp" />
        <ref bean="PositionFunding-csfAmount" />
        <ref bean="PositionFunding-csfDeleteCode" />
        <ref bean="PositionFunding-csfTimePercent" />
        <ref bean="PositionFunding-csfFundingStatusCode" />
        <ref bean="PositionFunding-csfFullTimeEmploymentQuantity" />
        <ref bean="PositionFunding-effectiveSequence" />
        <ref bean="PositionFunding-earnCode" />
        <ref bean="PositionFunding-effectiveDate" />
        <ref bean="PositionFunding-emplid" />
        <ref bean="PositionFunding-personName" />
        <ref bean="PositionFunding-positionNumber" />
        <ref bean="PositionFunding-versionNumber" />
      </list>
    </property>
    <property name="relationships" >
      <list>
        <bean parent="RelationshipDefinition">
          <property name="objectAttributeName" value="ledgerPerson" />
          <property name="primitiveAttributes" >
            <list>
              <bean parent="PrimitiveAttributeDefinition"
                    p:sourceName="ledgerPerson.personUniversalIdentifier"
                    p:targetName="personUniversalIdentifier" />
            </list>
          </property>
          <property name="supportAttributes" >
            <list>
              <bean parent="SupportAttributeDefinition"
                    p:identifier="true"
                    p:sourceName="emplid"
                    p:targetName="personPayrollIdentifier" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="PositionFunding-universityFiscalYear" parent="PositionFunding-universityFiscalYear-parentBean" />

  <bean id="PositionFunding-universityFiscalYear-parentBean" abstract="true" parent="AttributeReferenceDummy-genericFiscalYear">
    <property name="name" value="universityFiscalYear" />
  </bean>
  <bean id="PositionFunding-chartOfAccountsCode" parent="PositionFunding-chartOfAccountsCode-parentBean" />

  <bean id="PositionFunding-chartOfAccountsCode-parentBean" abstract="true" parent="Chart-chartOfAccountsCode">
  </bean>
  <bean id="PositionFunding-accountNumber" parent="PositionFunding-accountNumber-parentBean" />

  <bean id="PositionFunding-accountNumber-parentBean" abstract="true" parent="Account-accountNumber">
  </bean>
  <bean id="PositionFunding-subAccountNumber" parent="PositionFunding-subAccountNumber-parentBean" />

  <bean id="PositionFunding-subAccountNumber-parentBean" abstract="true" parent="SubAccount-subAccountNumber">
  </bean>
  <bean id="PositionFunding-financialObjectCode" parent="PositionFunding-financialObjectCode-parentBean" />

  <bean id="PositionFunding-financialObjectCode-parentBean" abstract="true" parent="ObjectCode-financialObjectCode">
  </bean>
  <bean id="PositionFunding-financialSubObjectCode" parent="PositionFunding-financialSubObjectCode-parentBean" />

  <bean id="PositionFunding-financialSubObjectCode-parentBean" abstract="true" parent="SubObjCd-financialSubObjectCode">
  </bean>
  <bean id="PositionFunding-csfCreateTimestamp" parent="PositionFunding-csfCreateTimestamp-parentBean" />

  <bean id="PositionFunding-csfCreateTimestamp-parentBean" abstract="true" parent="CalculatedSalaryFoundationTracker-csfCreateTimestamp">
  </bean>
  <bean id="PositionFunding-csfAmount" parent="PositionFunding-csfAmount-parentBean" />

  <bean id="PositionFunding-csfAmount-parentBean" abstract="true" parent="CalculatedSalaryFoundationTracker-csfAmount">
  </bean>
  <bean id="PositionFunding-csfDeleteCode" parent="PositionFunding-csfDeleteCode-parentBean" />

  <bean id="PositionFunding-csfDeleteCode-parentBean" abstract="true" parent="CalculatedSalaryFoundationTracker-csfDeleteCode">
  </bean>
  <bean id="PositionFunding-csfTimePercent" parent="PositionFunding-csfTimePercent-parentBean" />

  <bean id="PositionFunding-csfTimePercent-parentBean" abstract="true" parent="CalculatedSalaryFoundationTracker-csfTimePercent">
  </bean>
  <bean id="PositionFunding-csfFundingStatusCode" parent="PositionFunding-csfFundingStatusCode-parentBean" />

  <bean id="PositionFunding-csfFundingStatusCode-parentBean" abstract="true" parent="CalculatedSalaryFoundationTracker-csfFundingStatusCode">
  </bean>
  <bean id="PositionFunding-csfFullTimeEmploymentQuantity" parent="PositionFunding-csfFullTimeEmploymentQuantity-parentBean" />

  <bean id="PositionFunding-csfFullTimeEmploymentQuantity-parentBean" abstract="true" parent="CalculatedSalaryFoundationTracker-csfFullTimeEmploymentQuantity">
  </bean>
  <bean id="PositionFunding-effectiveSequence" parent="PositionFunding-effectiveSequence-parentBean" />

  <bean id="PositionFunding-effectiveSequence-parentBean" abstract="true" parent="CalculatedSalaryFoundationTracker-effectiveSequence">
  </bean>
  <bean id="PositionFunding-earnCode" parent="PositionFunding-earnCode-parentBean" />

  <bean id="PositionFunding-earnCode-parentBean" abstract="true" parent="CalculatedSalaryFoundationTracker-earnCode">
  </bean>
  <bean id="PositionFunding-effectiveDate" parent="PositionFunding-effectiveDate-parentBean" />

  <bean id="PositionFunding-effectiveDate-parentBean" abstract="true" parent="CalculatedSalaryFoundationTracker-effectiveDate">
  </bean>
  <bean id="PositionFunding-emplid" parent="PositionFunding-emplid-parentBean" />

  <bean id="PositionFunding-emplid-parentBean" abstract="true" parent="UniversalUser-personPayrollIdentifier">
    <property name="name" value="emplid" />
  </bean>
  <bean id="PositionFunding-personName" parent="PositionFunding-personName-parentBean" />

  <bean id="PositionFunding-personName-parentBean" abstract="true" parent="UniversalUser-personName">
  </bean>
  <bean id="PositionFunding-positionNumber" parent="PositionFunding-positionNumber-parentBean" />

  <bean id="PositionFunding-positionNumber-parentBean" abstract="true" parent="PositionData-positionNumber">
  </bean>
  <bean id="PositionFunding-versionNumber" parent="PositionFunding-versionNumber-parentBean" />

  <bean id="PositionFunding-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="PositionFunding-inquiryDefinition" parent="PositionFunding-inquiryDefinition-parentBean" />

  <bean id="PositionFunding-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Calculated Salary Foundation Tracker Inquiry By position Number" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="accountNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="chartOfAccountsCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="csfAmount" />
              <bean parent="FieldDefinition"
                    p:attributeName="csfCreateTimestamp" />
              <bean parent="FieldDefinition"
                    p:attributeName="csfDeleteCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="csfFullTimeEmploymentQuantity" />
              <bean parent="FieldDefinition"
                    p:attributeName="csfFundingStatusCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="csfTimePercent" />
              <bean parent="FieldDefinition"
                    p:attributeName="earnCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="effectiveDate" />
              <bean parent="FieldDefinition"
                    p:attributeName="effectiveSequence" />
              <bean parent="FieldDefinition"
                    p:attributeName="emplid" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialObjectCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialSubObjectCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="positionNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="subAccountNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="universityFiscalYear" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="PositionFunding-lookupDefinition" parent="PositionFunding-lookupDefinition-parentBean" />

  <bean id="PositionFunding-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Position Funding" />
    <property name="lookupableID" value="laborPositionFundingLookupable" />
    <property name="instructions" value="Look up a Calculated Salary Foundation Tracker" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:required="true"
              p:attributeName="positionNumber" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalYear" />
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="accountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="subAccountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialSubObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="personName" />
        <bean parent="FieldDefinition"
              p:attributeName="positionNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="csfAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="csfFundingStatusCode" />
        <bean parent="FieldDefinition"
              p:attributeName="csfDeleteCode" />
      </list>
    </property>
  </bean>
</beans>
