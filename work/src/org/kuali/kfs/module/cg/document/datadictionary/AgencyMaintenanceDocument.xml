<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="AgencyMaintenanceDocument" parent="AgencyMaintenanceDocument-parentBean" />

  <bean id="AgencyMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.cg.bo.Agency" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="AgencyMaintenanceDocument-EditAgency" />
      </list>
    </property>
    <property name="defaultExistenceChecks" >
      <list>
        <bean parent="ReferenceDefinition"
              p:attributeName="agencyType"
              p:activeIndicatorAttributeName="active"
              p:attributeToHighlightOnFail="agencyTypeCode" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>agencyNumber</value>
      </list>
    </property>
    <property name="label" value="Agency" />
    <property name="shortLabel" value="Agency" />
    <property name="summary" value="Agency Maintenance Document" />
    <property name="description" value="Document used to create or update Agency objects" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-CG"
            p:parameterName="AGENCY"
            p:parameterDetailType="Agency" />
    </property>
    <property name="documentTypeName" value="AgencyMaintenanceDocument" />
    <property name="businessRulesClass" value="org.kuali.module.cg.rules.AgencyRule" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="AGCY" />
    <property name="workflowProperties" >
      <ref bean="AgencyMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>kualiUniversalGroup</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="AgencyMaintenanceDocument-EditAgency" parent="AgencyMaintenanceDocument-EditAgency-parentBean" />

  <bean id="AgencyMaintenanceDocument-EditAgency-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Agency" />
    <property name="title" value="Edit Agency" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="agencyNumber"
              p:readOnly="true"
              p:required="true"
              p:defaultValueFinderClass="org.kuali.module.cg.lookup.valuefinder.NextAgencyNumberFinder" />
        <bean parent="MaintainableFieldDefinition"
              p:name="reportingName"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="fullName"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="agencyTypeCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="historicalIndicator"
              p:defaultValue="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="inStateIndicator" />
        <bean parent="MaintainableFieldDefinition"
              p:name="indirectAmount" />
        <bean parent="MaintainableFieldDefinition"
              p:name="reportsToAgencyNumber" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="AgencyMaintenanceDocument-workflowProperties" parent="AgencyMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="AgencyMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
