<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xmlns:dd="http://rice.kuali.org/dd"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
        http://rice.kuali.org/dd
        http://rice.kuali.org/dd/dd.xsd">

  <bean id="EffortCertificationReportTypeMaintenanceDocument" parent="EffortCertificationReportTypeMaintenanceDocument-parentBean" />

  <bean id="EffortCertificationReportTypeMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.ec.businessobject.EffortCertificationReportType" />
    <property name="maintainableClass" value="org.kuali.rice.kns.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="EffortCertificationReportTypeMaintenanceDocument-EditReportType" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>effortCertificationReportTypeCode</value>
      </list>
    </property>
    <property name="label" value="Report Type" />
    <property name="shortLabel" value="Effort Cert Report Type Maintenance Document" />
    <property name="summary" value="Report Type Code Document" />
    <property name="description" value="Document used to create or update Effort Certification Report Type" />
    <property name="documentTypeName" value="ECRT" />
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase" />
    <property name="workflowAttributes">
    	<ref bean="EffortCertificationReportTypeMaintenanceDocument-workflowAttributes" />
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="EffortCertificationReportTypeMaintenanceDocument-EditReportType" parent="EffortCertificationReportTypeMaintenanceDocument-EditReportType-parentBean" />

  <bean id="EffortCertificationReportTypeMaintenanceDocument-EditReportType-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Report Type" />
    <property name="title" value="Edit Report Type" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="effortCertificationReportTypeCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="effortCertificationReportDescription"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="active"
              p:defaultValue="true"
              p:required="true" />
      </list>
    </property>
  </bean>
  
  <!-- workflow attributes -->
  <bean id="EffortCertificationReportTypeMaintenanceDocument-workflowAttributes" parent="EffortCertificationReportTypeMaintenanceDocument-workflowAttributes-parentBean" />
  
  <bean id="EffortCertificationReportTypeMaintenanceDocument-workflowAttributes-parentBean" class="org.kuali.rice.kns.datadictionary.WorkflowAttributes" abstract="true" />
</beans>
