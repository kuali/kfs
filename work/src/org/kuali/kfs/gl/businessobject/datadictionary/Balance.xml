  <bean id="Balance-subAccountNumber" parent="Balance-subAccountNumber-parentBean" />

  <bean id="Balance-subAccountNumber-parentBean" abstract="true" parent="SubAccount-subAccountNumber">
  </bean>
  <bean id="Balance-objectCode" parent="Balance-objectCode-parentBean" />

  <bean id="Balance-objectCode-parentBean" abstract="true" parent="ObjectCode-financialObjectCode">
