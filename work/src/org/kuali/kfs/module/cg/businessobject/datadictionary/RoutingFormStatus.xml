<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="RoutingFormStatus" parent="RoutingFormStatus-parentBean" />

  <bean id="RoutingFormStatus-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.kra.routingform.bo.RoutingFormStatus" />
    <property name="inquiryDefinition" >
      <ref bean="RoutingFormStatus-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="RoutingFormStatus-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="RoutingFormStatus-dataObjectMaintenanceCodeActiveIndicator" />
        <ref bean="RoutingFormStatus-routingFormStatusCode" />
        <ref bean="RoutingFormStatus-routingFormStatusDescription" />
        <ref bean="RoutingFormStatus-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="RoutingFormStatus-dataObjectMaintenanceCodeActiveIndicator" parent="RoutingFormStatus-dataObjectMaintenanceCodeActiveIndicator-parentBean" />

  <bean id="RoutingFormStatus-dataObjectMaintenanceCodeActiveIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="dataObjectMaintenanceCodeActiveIndicator" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Data Object Maintenance Code Active Indicator" />
    <property name="shortLabel" value="Indicator" />
    <property name="maxLength" value="1" />
    <property name="control" >
      <bean parent="CheckboxControlDefinition" />
    </property>
    <property name="summary" value="dataObjectMaintenanceCodeActiveIndicator" />
    <property name="description" value="dataObjectMaintenanceCodeActiveIndicator description..." />
  </bean>
  <bean id="RoutingFormStatus-routingFormStatusCode" parent="RoutingFormStatus-routingFormStatusCode-parentBean" />

  <bean id="RoutingFormStatus-routingFormStatusCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="routingFormStatusCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Routing Form Status Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    <property name="summary" value="routingFormStatusCode" />
    <property name="description" value="routingFormStatusCode description..." />
  </bean>
  <bean id="RoutingFormStatus-routingFormStatusDescription" parent="RoutingFormStatus-routingFormStatusDescription-parentBean" />

  <bean id="RoutingFormStatus-routingFormStatusDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="routingFormStatusDescription" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Routing Form Status Description" />
    <property name="shortLabel" value="Description" />
    <property name="maxLength" value="15" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="17" />
    </property>
    <property name="summary" value="routingFormStatusDescription" />
    <property name="description" value="routingFormStatusDescription description..." />
  </bean>
  <bean id="RoutingFormStatus-versionNumber" parent="RoutingFormStatus-versionNumber-parentBean" />

  <bean id="RoutingFormStatus-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="RoutingFormStatus-inquiryDefinition" parent="RoutingFormStatus-inquiryDefinition-parentBean" />

  <bean id="RoutingFormStatus-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Routing Form Status Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="dataObjectMaintenanceCodeActiveIndicator" />
              <bean parent="FieldDefinition"
                    p:attributeName="routingFormStatusCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="routingFormStatusDescription" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="RoutingFormStatus-lookupDefinition" parent="RoutingFormStatus-lookupDefinition-parentBean" />

  <bean id="RoutingFormStatus-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Routing Form Status Lookup" />
    <property name="instructions" value="Look up a Routing Form Status" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="dataObjectMaintenanceCodeActiveIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="routingFormStatusCode" />
        <bean parent="FieldDefinition"
              p:attributeName="routingFormStatusDescription" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="dataObjectMaintenanceCodeActiveIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="routingFormStatusCode" />
        <bean parent="FieldDefinition"
              p:attributeName="routingFormStatusDescription" />
      </list>
    </property>
  </bean>
</beans>
