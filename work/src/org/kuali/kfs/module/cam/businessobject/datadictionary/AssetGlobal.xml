  </bean>
  
  <bean id="AssetGlobal-landAcreageSize" parent="AssetGlobal-landAcreageSize-parentBean"/>
  <bean id="AssetGlobal-landAcreageSize-parentBean" abstract="true" parent="Asset-landAcreageSize">
