<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="RoutingFormQuestion" parent="RoutingFormQuestion-parentBean" />

  <bean id="RoutingFormQuestion-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.cg.businessobject.RoutingFormQuestion" />
    <property name="objectLabel" value="Routing Form Question" />
    <property name="attributes" >
      <list>
        <ref bean="RoutingFormQuestion-documentNumber" />
        <ref bean="RoutingFormQuestion-questionTypeCode" />
        <ref bean="RoutingFormQuestion-yesNoIndicator" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="RoutingFormQuestion-documentNumber" parent="RoutingFormQuestion-documentNumber-parentBean" />

  <bean id="RoutingFormQuestion-documentNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="documentNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Document Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="14" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="16" />
    </property>
    
    
  </bean>
  <bean id="RoutingFormQuestion-questionTypeCode" parent="RoutingFormQuestion-questionTypeCode-parentBean" />

  <bean id="RoutingFormQuestion-questionTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="questionTypeCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Question Type Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="1" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="3" />
    </property>
    
    
  </bean>
  <bean id="RoutingFormQuestion-yesNoIndicator" parent="RoutingFormQuestion-yesNoIndicator-parentBean" />

  <bean id="RoutingFormQuestion-yesNoIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="yesNoIndicator" />
    <property name="label" value="Yes/No Indicator" />
    <property name="shortLabel" value="Indicator" />
    <property name="maxLength" value="1" />
    <property name="control" >
      <bean parent="RadioControlDefinition"
            p:valuesFinderClass="org.kuali.rice.kns.lookup.keyvalues.IndicatorYNNullValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
    
    
  </bean>
</beans>
