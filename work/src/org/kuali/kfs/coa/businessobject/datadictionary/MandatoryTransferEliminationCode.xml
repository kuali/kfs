<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../core/datadictionary/dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.chart.bo.codes.MandatoryTransferEliminationCode">
        <customAttributeEnabled>true</customAttributeEnabled>

        <inquiry>
            <title>Mandatory Transfer Elimination Code Inquiry</title>
            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="code" />
                <field attributeName="name" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Mandatory Transfer Elimination Code Lookup</title>

            <instructions>Lookup a Mandatory Transfer Elimination Code</instructions>

            <defaultSort attributeName="code" sortAscending="true" />

            <lookupFields>
                <lookupField attributeName="code" required="false" />
                <lookupField attributeName="name" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="code" />
                <field attributeName="name" />
            </resultFields>
        </lookup>

        <titleAttribute>code</titleAttribute>

        <attributes>

            <attribute name="code" forceUppercase="true">
                <label>Mandatory Transfer Elimination Code</label>
                <shortLabel>Mand Xfr Elim Code</shortLabel>

                <maxLength>1</maxLength>
                <validationPattern>
                    <alphaNumeric />
                </validationPattern>
                <required>true</required>

                <control>
                    <text size="2" />
                </control>

                <summary>Mandatory Transfer Elimination Code</summary>
                <description>Mandatory Transfer Elimination Code</description>
            </attribute>

            <attribute name="name" forceUppercase="true">
                <label>Mandatory Transfer Elimination Name</label>
                <shortLabel>Name</shortLabel>

                <maxLength>40</maxLength>
                <required>true</required>

                <control>
                    <text size="40" />
                </control>

                <summary>Mandatory Transfer Elimination Name</summary>
                <description>Mandatory Transfer Elimination Name...</description>
            </attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
        <objectLabel>Mandatory Transfer Elimination Code</objectLabel>
    </businessObject>
</dictionaryEntry>

