<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="ProjectDirector" parent="ProjectDirector-parentBean" />

  <bean id="ProjectDirector-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.cg.bo.ProjectDirector" />
    <property name="inquiryDefinition" >
      <ref bean="ProjectDirector-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="ProjectDirector-lookupDefinition" />
    </property>
    <property name="objectLabel" value="Project Director" />
    <property name="attributes" >
      <list>
        <ref bean="ProjectDirector-universalUser.personPayrollIdentifier" />
        <ref bean="ProjectDirector-personUniversalIdentifier" />
        <ref bean="ProjectDirector-universalUser.personUserIdentifier" />
        <ref bean="ProjectDirector-universalUser.personName" />
        <ref bean="ProjectDirector-universalUser.personLastName" />
        <ref bean="ProjectDirector-universalUser.personFirstName" />
        <ref bean="ProjectDirector-universalUser.campusCode" />
        <ref bean="ProjectDirector-universalUser.primaryDepartmentCode" />
        <ref bean="ProjectDirector-universalUser.employeeStatusCode" />
        <ref bean="ProjectDirector-universalUser.employeeTypeCode" />
        <ref bean="ProjectDirector-versionNumber" />
      </list>
    </property>
    <property name="relationships" >
      <list>
        <bean parent="RelationshipDefinition">
          <property name="objectAttributeName" value="universalUser" />
          <property name="primitiveAttributes" >
            <list>
              <bean parent="PrimitiveAttributeDefinition"
                    p:sourceName="personUniversalIdentifier"
                    p:targetName="personUniversalIdentifier" />
            </list>
          </property>
          <property name="supportAttributes" >
            <list>
              <bean parent="SupportAttributeDefinition"
                    p:identifier="true"
                    p:sourceName="universalUser.personUserIdentifier"
                    p:targetName="personUserIdentifier" />
              <bean parent="SupportAttributeDefinition"
                    p:sourceName="universalUser.personName"
                    p:targetName="personName" />
              <bean parent="SupportAttributeDefinition"
                    p:sourceName="universalUser.primaryDepartmentCode"
                    p:targetName="primaryDepartmentCode" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="ProjectDirector-universalUser.personPayrollIdentifier" parent="ProjectDirector-universalUser.personPayrollIdentifier-parentBean" />

  <bean id="ProjectDirector-universalUser.personPayrollIdentifier-parentBean" abstract="true" parent="UniversalUser-personPayrollIdentifier">
    <property name="name" value="universalUser.personPayrollIdentifier" />
  </bean>
  <bean id="ProjectDirector-personUniversalIdentifier" parent="ProjectDirector-personUniversalIdentifier-parentBean" />

  <bean id="ProjectDirector-personUniversalIdentifier-parentBean" abstract="true" parent="GenericAttributes-genericSystemId">
    <property name="name" value="personUniversalIdentifier" />
    <property name="label" value="Universal ID" />
    <property name="shortLabel" value="ID" />
    <property name="required" value="true" />
  </bean>
  <bean id="ProjectDirector-universalUser.personUserIdentifier" parent="ProjectDirector-universalUser.personUserIdentifier-parentBean" />

  <bean id="ProjectDirector-universalUser.personUserIdentifier-parentBean" abstract="true" parent="UniversalUser-personUserIdentifier">
    <property name="name" value="universalUser.personUserIdentifier" />
  </bean>
  <bean id="ProjectDirector-universalUser.personName" parent="ProjectDirector-universalUser.personName-parentBean" />

  <bean id="ProjectDirector-universalUser.personName-parentBean" abstract="true" parent="UniversalUser-personName">
    <property name="name" value="universalUser.personName" />
    <property name="label" value="Project Director Name" />
    <property name="shortLabel" value="Proj Dir Name" />
  </bean>
  <bean id="ProjectDirector-universalUser.personLastName" parent="ProjectDirector-universalUser.personLastName-parentBean" />

  <bean id="ProjectDirector-universalUser.personLastName-parentBean" abstract="true" parent="UniversalUser-personLastName">
    <property name="name" value="universalUser.personLastName" />
    <property name="label" value="Project Director Last Name" />
    <property name="shortLabel" value="Proj Dir Last Name" />
  </bean>
  <bean id="ProjectDirector-universalUser.personFirstName" parent="ProjectDirector-universalUser.personFirstName-parentBean" />

  <bean id="ProjectDirector-universalUser.personFirstName-parentBean" abstract="true" parent="UniversalUser-personFirstName">
    <property name="name" value="universalUser.personFirstName" />
    <property name="label" value="Project Director First Name" />
    <property name="shortLabel" value="Proj Dir First Name" />
  </bean>
  <bean id="ProjectDirector-universalUser.campusCode" parent="ProjectDirector-universalUser.campusCode-parentBean" />

  <bean id="ProjectDirector-universalUser.campusCode-parentBean" abstract="true" parent="Campus-campusCode">
    <property name="name" value="universalUser.campusCode" />
  </bean>
  <bean id="ProjectDirector-universalUser.primaryDepartmentCode" parent="ProjectDirector-universalUser.primaryDepartmentCode-parentBean" />

  <bean id="ProjectDirector-universalUser.primaryDepartmentCode-parentBean" abstract="true" parent="UniversalUser-primaryDepartmentCode">
    <property name="name" value="universalUser.primaryDepartmentCode" />
    <property name="label" value="Dept ID" />
    <property name="shortLabel" value="Dept ID" />
  </bean>
  <bean id="ProjectDirector-universalUser.employeeStatusCode" parent="ProjectDirector-universalUser.employeeStatusCode-parentBean" />

  <bean id="ProjectDirector-universalUser.employeeStatusCode-parentBean" abstract="true" parent="UniversalUser-employeeStatusCode">
    <property name="name" value="universalUser.employeeStatusCode" />
    <property name="label" value="Employee Status" />
    <property name="shortLabel" value="Empl Status" />
  </bean>
  <bean id="ProjectDirector-universalUser.employeeTypeCode" parent="ProjectDirector-universalUser.employeeTypeCode-parentBean" />

  <bean id="ProjectDirector-universalUser.employeeTypeCode-parentBean" abstract="true" parent="UniversalUser-employeeTypeCode">
    <property name="name" value="universalUser.employeeTypeCode" />
    <property name="label" value="Employee Type" />
    <property name="shortLabel" value="Empl Type" />
  </bean>
  <bean id="ProjectDirector-versionNumber" parent="ProjectDirector-versionNumber-parentBean" />

  <bean id="ProjectDirector-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="ProjectDirector-inquiryDefinition" parent="ProjectDirector-inquiryDefinition-parentBean" />

  <bean id="ProjectDirector-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Project Director Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Project Director Details" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="personUniversalIdentifier" />
              <bean parent="FieldDefinition"
                    p:attributeName="universalUser.personUserIdentifier" />
              <bean parent="FieldDefinition"
                    p:attributeName="universalUser.personName" />
              <bean parent="FieldDefinition"
                    p:attributeName="universalUser.campusCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="universalUser.primaryDepartmentCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="universalUser.employeeStatusCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="universalUser.employeeTypeCode" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="ProjectDirector-lookupDefinition" parent="ProjectDirector-lookupDefinition-parentBean" />

  <bean id="ProjectDirector-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Project Director Lookup" />
    <property name="instructions" value="Lookup a Project Director" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>universalUser.personName</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="universalUser.personUserIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="personUniversalIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="universalUser.personPayrollIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="universalUser.personLastName" />
        <bean parent="FieldDefinition"
              p:attributeName="universalUser.personFirstName" />
        <bean parent="FieldDefinition"
              p:attributeName="universalUser.primaryDepartmentCode" />
        <bean parent="FieldDefinition"
              p:attributeName="universalUser.employeeStatusCode" />
        <bean parent="FieldDefinition"
              p:attributeName="universalUser.employeeTypeCode" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="universalUser.personUserIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="personUniversalIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="universalUser.personPayrollIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="universalUser.personName" />
        <bean parent="FieldDefinition"
              p:attributeName="universalUser.primaryDepartmentCode" />
        <bean parent="FieldDefinition"
              p:attributeName="universalUser.employeeStatusCode" />
        <bean parent="FieldDefinition"
              p:attributeName="universalUser.employeeTypeCode" />
      </list>
    </property>
  </bean>
</beans>
