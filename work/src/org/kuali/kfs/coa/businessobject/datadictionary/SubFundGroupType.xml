<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.chart.bo.SubFundGroupType">
        <customAttributeEnabled>true</customAttributeEnabled>

        <inquiry>
            <title>Sub-Fund Group Type Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="subFundGroupTypeCode" />
                <field attributeName="subFundGroupTypeDescription" />
                <field attributeName="subFundGroupTypeActiveIndicator" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Sub-Fund Group Type Lookup</title>

            <instructions>Look up a Sub-Fund Group Type</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="subFundGroupTypeCode" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
            <lookupField attributeName="subFundGroupTypeCode" required="false" />
            <lookupField attributeName="subFundGroupTypeDescription" required="false" />
            <lookupField attributeName="subFundGroupTypeActiveIndicator" required="false" />
    
            </lookupFields>

            <resultFields>
                <field attributeName="subFundGroupTypeCode" />
                <field attributeName="subFundGroupTypeDescription" />
                <field attributeName="subFundGroupTypeActiveIndicator" />
            </resultFields>
        </lookup>

        <titleAttribute>subFundGroupTypeCode</titleAttribute>

        <attributes>

            <attribute name="subFundGroupTypeActiveIndicator" forceUppercase="true">
                <label>Sub-Fund Group Type Active Indicator</label>
                <shortLabel>Indicator</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <checkbox />
                </control>

                <summary>subFundGroupTypeActiveIndicator</summary>
                <description>subFundGroupTypeActiveIndicator description...</description>
			</attribute>

            <attribute name="subFundGroupTypeCode" forceUppercase="true">
                <label>Sub-Fund Group Type Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>1</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="3" />
                </control>

                <summary>subFundGroupTypeCode</summary>
                <description>subFundGroupTypeCode description...</description>
			</attribute>

            <attribute name="subFundGroupTypeDescription" forceUppercase="true">
                <label>Sub-Fund Group Type Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>40</maxLength>
                
                <validationPattern>
                    <anyCharacter allowWhitespace="true" />
                </validationPattern>

                <control>
                    <text size="42" />
                </control>

                <summary>subFundGroupTypeDescription</summary>
                <description>subFundGroupTypeDescription description...</description>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />
        </attributes>
        
        <objectLabel>Sub-Fund Group Type</objectLabel>
    </businessObject>
</dictionaryEntry>
