<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.kra.routingform.bo.RoutingFormResearchRisk">
        <customAttributeEnabled>false</customAttributeEnabled>

        <inquiry>
            <title>Routing Form Research Risk Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="dataPresentIndicator" />
                <field attributeName="researchRiskDescription" />
                <field attributeName="documentNumber" />
                <field attributeName="researchRiskTypeCode" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Routing Form Research Risk Lookup</title>

            <instructions>Look up a Routing Form Research Risk</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="documentNumber" />
                    <sortAttribute attributeName="researchRiskTypeCode" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="dataPresentIndicator" required="false" />
                <lookupField attributeName="researchRiskDescription" required="false" />
                <lookupField attributeName="documentNumber" required="false" />
                <lookupField attributeName="researchRiskTypeCode" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="dataPresentIndicator" />
                <field attributeName="researchRiskDescription" />
                <field attributeName="documentNumber" />
                <field attributeName="researchRiskTypeCode" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="dataPresentIndicator" forceUppercase="true">
                <label>Data Present Indicator</label>
                <shortLabel>Indicator</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <radio valuesFinderClass="org.kuali.core.lookup.keyvalues.IndicatorYNOnlyValuesFinder" />
                </control>

                <summary>dataPresentIndicator</summary>
                <description>dataPresentIndicator description...</description>
			</attribute>

            <attribute name="researchRiskDescription" forceUppercase="true">
                <label>Research Risk Description</label>
                <shortLabel>Describe</shortLabel>

                <maxLength>500</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <textarea rows="5" cols="80"/>
                </control>

                <summary>researchRiskDescription</summary>
                <description>researchRiskDescription description...</description>
			</attribute>

            <attribute name="documentNumber" forceUppercase="true">
                <label>Document Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>14</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="16" />
                </control>

                <summary>documentNumber</summary>
                <description>documentNumber description...</description>
			</attribute>

            <attribute name="researchRiskTypeCode" forceUppercase="true">
                <label>Research Risk Type Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="4" />
                </control>

                <summary>researchRiskTypeCode</summary>
                <description>researchRiskTypeCode description...</description>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
