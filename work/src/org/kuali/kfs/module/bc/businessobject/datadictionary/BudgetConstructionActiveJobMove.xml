<!-- Attribute Definitions -->


  <bean id="BudgetConstructionActiveJobMove-departmentIdentifier" parent="BudgetConstructionActiveJobMove-departmentIdentifier-parentBean"/>

  <bean id="BudgetConstructionActiveJobMove-departmentIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="departmentIdentifier"/>
    <property name="forceUppercase" value="false"/>
    <property name="label" value="Department Identifier"/>
    <property name="shortLabel" value="Identifier"/>
    <property name="maxLength" value="10"/>
    <property name="validationPattern">
      <bean parent="AnyCharacterValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="12"/>
    </property>
    
    
  </bean>
  <bean id="BudgetConstructionActiveJobMove-effectiveDate" parent="BudgetConstructionActiveJobMove-effectiveDate-parentBean"/>

  <bean id="BudgetConstructionActiveJobMove-effectiveDate-parentBean" abstract="true" parent="AttributeDefinition">
