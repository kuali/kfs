    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="11" />
    </property>
    <property name="summary" value="offCampusZipCode" />
    <property name="description" value="offCampusZipCode description..." />
  </bean>
  <bean id="AssetTransferDocument-organizationCode" parent="AssetTransferDocument-organizationCode-parentBean" />

  <bean id="AssetTransferDocument-organizationCode-parentBean" abstract="true" parent="AttributeDefinition">
