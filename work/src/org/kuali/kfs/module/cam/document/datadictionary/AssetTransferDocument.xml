<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="AssetTransferDocument" parent="AssetTransferDocument-parentBean" />

  <bean id="AssetTransferDocument-parentBean" abstract="true" parent="TransactionalDocumentEntry">
    <property name="label" value="Asset Transfer" />
    <property name="shortLabel" value="Asset Transfer" />
    <property name="summary" value="&amp;nbsp;" />
    <property name="description" value="Asset Transfer Document" />
    <property name="documentTypeName" value="AssetTransferDocument" />
    <property name="documentClass" value="org.kuali.module.cams.document.AssetTransferDocument" />
    <property name="businessRulesClass" value="org.kuali.module.cams.rules.AssetTransferDocumentRule" />
    <property name="preRulesCheckClass" value="org.kuali.module.cams.rules.AssetTransferDocumentPreRules" />    
    <property name="documentAuthorizerClass" value="org.kuali.module.cams.document.authorization.AssetTransferDocumentAuthorizer" />
    <property name="documentTypeCode" value="AT" />
    <property name="workflowProperties" >
      <ref bean="AssetTransferDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>kualiUniversalGroup</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
    <property name="attributes" >
      <list>
        <ref bean="AssetTransferDocument-buildingCode" />
        <ref bean="AssetTransferDocument-buildingRoomNumber" />
        <ref bean="AssetTransferDocument-buildingSubRoomNumber" />
        <ref bean="AssetTransferDocument-campusCode" />
        <ref bean="AssetTransferDocument-interdepartmentalSalesIndicator" />
        <ref bean="AssetTransferDocument-offCampusAddress" />
        <ref bean="AssetTransferDocument-offCampusCityName" />
        <ref bean="AssetTransferDocument-offCampusStateCode" />
        <ref bean="AssetTransferDocument-offCampusZipCode" />
        <ref bean="AssetTransferDocument-organizationCode" />
        <ref bean="AssetTransferDocument-organizationInventoryName" />
        <ref bean="AssetTransferDocument-organizationOwnerAccountNumber" />
        <ref bean="AssetTransferDocument-organizationOwnerChartOfAccountsCode" />
        <ref bean="AssetTransferDocument-organizationTagNumber" />
        <ref bean="AssetTransferDocument-organizationText" />
        <ref bean="AssetTransferDocument-transferOfFundsFinancialDocumentNumber" />
        <ref bean="AssetTransferDocument-representativeUniversalIdentifier" />
        <ref bean="AssetTransferDocument-assetRepresentative.personUserIdentifier" />
        <ref bean="AssetTransferDocument-assetRepresentative.personName" />
        <ref bean="AssetTransferDocument-objectId" />
        <ref bean="AssetTransferDocument-versionNumber" />
        <ref bean="AssetTransferDocument-documentNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="AssetTransferDocument-buildingCode" parent="AssetTransferDocument-buildingCode-parentBean" />

  <bean id="AssetTransferDocument-buildingCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="buildingCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Building Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
    <property name="summary" value="buildingCode" />
    <property name="description" value="buildingCode description..." />
  </bean>
  <bean id="AssetTransferDocument-buildingRoomNumber" parent="AssetTransferDocument-buildingRoomNumber-parentBean" />

  <bean id="AssetTransferDocument-buildingRoomNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="buildingRoomNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Building Room Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
    <property name="summary" value="buildingRoomNumber" />
    <property name="description" value="buildingRoomNumber description..." />
  </bean>
  <bean id="AssetTransferDocument-buildingSubRoomNumber" parent="AssetTransferDocument-buildingSubRoomNumber-parentBean" />

