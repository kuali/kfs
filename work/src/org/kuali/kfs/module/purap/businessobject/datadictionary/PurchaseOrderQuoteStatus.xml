<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.purap.bo.PurchaseOrderQuoteStatus">


        <inquiry>
            <title>Purchase Order Quote Status Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="purchaseOrderQuoteStatusCode" />
                <field attributeName="purchaseOrderQuoteStatusDescription" />
                <field attributeName="active" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Purchase Order Quote Status Lookup</title>

            <instructions>Lookup a Purchase Order Quote Status</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="purchaseOrderQuoteStatusDescription" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="purchaseOrderQuoteStatusCode" required="false" />
                <lookupField attributeName="purchaseOrderQuoteStatusDescription" required="false" />
                <lookupField attributeName="active" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="purchaseOrderQuoteStatusCode" />
                <field attributeName="purchaseOrderQuoteStatusDescription" />
                <field attributeName="active" />
            </resultFields>
        </lookup>

        <titleAttribute>purchaseOrderQuoteStatusCode</titleAttribute>

        <attributes>

            <attribute name="purchaseOrderQuoteStatusCode" forceUppercase="true">
                <label>Purchase Order Quote Status Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="6" />
                </control>

                <summary>purchaseOrderQuoteStatusCode</summary>
                <description>purchaseOrderQuoteStatusCode description...</description>
			</attribute>

            <attribute name="purchaseOrderQuoteStatusDescription">
                <label>Purchase Order Quote Status Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>45</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true" /></validationPattern>

                <control>
                    <text size="47" />
                </control>

                <summary>purchaseOrderQuoteStatusDescription</summary>
                <description>purchaseOrderQuoteStatusDescription description...</description>
			</attribute>

            <attributeReference name="active" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="activeIndicator" />

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
