<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="VendorInactiveReason" parent="VendorInactiveReason-parentBean" />

  <bean id="VendorInactiveReason-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.vendor.bo.VendorInactiveReason" />
    <property name="inquiryDefinition" >
      <ref bean="VendorInactiveReason-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="VendorInactiveReason-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="vendorInactiveReasonCode" />
    <property name="objectLabel" value="Vendor Inactive Reason" />
    <property name="attributes" >
      <list>
        <ref bean="VendorInactiveReason-vendorInactiveReasonCode" />
        <ref bean="VendorInactiveReason-vendorInactiveReasonDescription" />
        <ref bean="VendorInactiveReason-versionNumber" />
        <ref bean="VendorInactiveReason-active" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="VendorInactiveReason-vendorInactiveReasonCode" parent="VendorInactiveReason-vendorInactiveReasonCode-parentBean" />

  <bean id="VendorInactiveReason-vendorInactiveReasonCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="vendorInactiveReasonCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Vendor Inactive Reason Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="6" />
    </property>
  </bean>
  <bean id="VendorInactiveReason-vendorInactiveReasonDescription" parent="VendorInactiveReason-vendorInactiveReasonDescription-parentBean" />

  <bean id="VendorInactiveReason-vendorInactiveReasonDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="vendorInactiveReasonDescription" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Vendor Inactive Reason Description" />
    <property name="shortLabel" value="Description" />
    <property name="maxLength" value="45" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="47" />
    </property>
  </bean>
  <bean id="VendorInactiveReason-versionNumber" parent="VendorInactiveReason-versionNumber-parentBean" />

  <bean id="VendorInactiveReason-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>
  <bean id="VendorInactiveReason-active" parent="VendorInactiveReason-active-parentBean" />

  <bean id="VendorInactiveReason-active-parentBean" abstract="true" parent="AttributeReferenceDummy-activeIndicator">
    <property name="name" value="active" />
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="VendorInactiveReason-inquiryDefinition" parent="VendorInactiveReason-inquiryDefinition-parentBean" />

  <bean id="VendorInactiveReason-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Vendor Inactive Reason Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="vendorInactiveReasonCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="vendorInactiveReasonDescription" />
              <bean parent="FieldDefinition"
                    p:attributeName="active" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="VendorInactiveReason-lookupDefinition" parent="VendorInactiveReason-lookupDefinition-parentBean" />

  <bean id="VendorInactiveReason-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Vendor Inactive Reason Lookup" />
    <property name="instructions" value="Look up a Vendor Inactive Reason" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="vendorInactiveReasonCode" />
        <bean parent="FieldDefinition"
              p:attributeName="vendorInactiveReasonDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="vendorInactiveReasonCode" />
        <bean parent="FieldDefinition"
              p:attributeName="vendorInactiveReasonDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
  </bean>
</beans>
