  <bean id="SystemInformation-universityClearingSubAccountNumber" parent="SystemInformation-universityClearingSubAccountNumber-parentBean" />
  <bean id="SystemInformation-universityClearingSubAccountNumber-parentBean" abstract="true" parent="AttributeDefinition">
