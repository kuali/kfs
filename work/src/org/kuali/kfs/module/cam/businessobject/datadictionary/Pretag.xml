<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="Pretag" parent="Pretag-parentBean" />

  <bean id="Pretag-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.cams.bo.Pretag" />
    <property name="inquiryDefinition" >
      <ref bean="Pretag-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="Pretag-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="Pretag-capitalAssetTypeCode" />
        <ref bean="Pretag-organizationCode" />
        <ref bean="Pretag-personUniversal.personUserIdentifier" />
        <ref bean="Pretag-personUniversal.personName" />
        <ref bean="Pretag-active" />
        <ref bean="Pretag-chartOfAccountsCode" />
        <ref bean="Pretag-purchaseOrderNumber" />
        <ref bean="Pretag-pretagCreateDate" />
        <ref bean="Pretag-assetTopsDescription" />
        <ref bean="Pretag-lineItemNumber" />
        <ref bean="Pretag-manufacturerModelNumber" />
        <ref bean="Pretag-manufacturerName" />
        <ref bean="Pretag-organizationDescription" />
        <ref bean="Pretag-organizationInventoryName" />
        <ref bean="Pretag-organizationText" />
        <ref bean="Pretag-quantityInvoiced" />
        <ref bean="Pretag-vendorName" />
        <ref bean="Pretag-campusTagNumbers" />
        <ref bean="Pretag-serialNumbers" />
        <ref bean="Pretag-pretagDetails.campusTagNumber" />
        <ref bean="Pretag-pretagDetails.serialNumber" />
        <ref bean="Pretag-versionNumber" />
      </list>
    </property>
    <property name="collections" >
      <list>
        <bean parent="CollectionDefinition"
              p:name="pretagDetails"
              p:label="Pretag Details"
              p:shortLabel="Details"
              p:elementLabel="Details" />
      </list>
    </property>
    <property name="relationships" >
      <list>
        <bean parent="RelationshipDefinition">
          <property name="objectAttributeName" value="personUniversal" />
          <property name="primitiveAttributes" >
            <list>
              <bean parent="PrimitiveAttributeDefinition"
                    p:sourceName="representativeUniversalIdentifier"
                    p:targetName="personUniversalIdentifier" />
            </list>
          </property>
          <property name="supportAttributes" >
            <list>
              <bean parent="SupportAttributeDefinition"
                    p:identifier="true"
                    p:sourceName="personUniversal.personUserIdentifier"
                    p:targetName="personUserIdentifier" />
              <bean parent="SupportAttributeDefinition"
                    p:sourceName="personUniversal.personName"
                    p:targetName="personName" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="Pretag-capitalAssetTypeCode" parent="Pretag-capitalAssetTypeCode-parentBean" />

  <bean id="Pretag-capitalAssetTypeCode-parentBean" abstract="true" parent="AssetType-capitalAssetTypeCode">
    <property name="label" value="Captial Asset Type Code" />
    <property name="shortLabel" value="Asset Type Code" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.module.cams.lookup.keyvalues.AssetTypeValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
  </bean>
  <bean id="Pretag-organizationCode" parent="Pretag-organizationCode-parentBean" />

  <bean id="Pretag-organizationCode-parentBean" abstract="true" parent="Org-organizationCode">
    <property name="shortLabel" value="Org Cd" />
    <property name="required" value="false" />
  </bean>
  <bean id="Pretag-personUniversal.personUserIdentifier" parent="Pretag-personUniversal.personUserIdentifier-parentBean" />

  <bean id="Pretag-personUniversal.personUserIdentifier-parentBean" abstract="true" parent="UniversalUser-personUserIdentifier">
    <property name="name" value="personUniversal.personUserIdentifier" />
    <property name="label" value="Representative Universal Identifier" />
    <property name="shortLabel" value="Identifier" />
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="KualiUserControlDefinition"
            p:personNameAttributeName="personUniversal.personName"
            p:universalIdAttributeName="representativeUniversalIdentifier"
            p:userIdAttributeName="personUniversal.personUserIdentifier" />
    </property>
  </bean>
  <bean id="Pretag-personUniversal.personName" parent="Pretag-personUniversal.personName-parentBean" />

  <bean id="Pretag-personUniversal.personName-parentBean" abstract="true" parent="UniversalUser-personName">
    <property name="name" value="personUniversal.personName" />
    <property name="required" value="false" />
  </bean>
  <bean id="Pretag-active" parent="Pretag-active-parentBean" />

  <bean id="Pretag-active-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
    <property name="name" value="active" />
  </bean>
  <bean id="Pretag-chartOfAccountsCode" parent="Pretag-chartOfAccountsCode-parentBean" />

  <bean id="Pretag-chartOfAccountsCode-parentBean" abstract="true" parent="Chart-chartOfAccountsCode">
    <property name="label" value="Chart of Accounts Code" />
    <property name="shortLabel" value="Fiscal Chart" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.module.chart.lookup.keyvalues.ChartValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
  </bean>
  <bean id="Pretag-purchaseOrderNumber" parent="Pretag-purchaseOrderNumber-parentBean" />

  <bean id="Pretag-purchaseOrderNumber-parentBean" abstract="true" parent="PendingPurchaseOrder-purchaseOrderNumber">
    <property name="required" value="true" />
  </bean>
  <bean id="Pretag-pretagCreateDate" parent="Pretag-pretagCreateDate-parentBean" />

  <bean id="Pretag-pretagCreateDate-parentBean" abstract="true" parent="GenericAttributes-genericTimestamp">
    <property name="name" value="pretagCreateDate" />
    <property name="label" value="Pretag Create Date" />
    <property name="shortLabel" value="Date" />
  </bean>
  <bean id="Pretag-assetTopsDescription" parent="Pretag-assetTopsDescription-parentBean" />

  <bean id="Pretag-assetTopsDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="assetTopsDescription" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Purchase Order Line Item Description" />
    <property name="shortLabel" value="Description" />
    <property name="maxLength" value="2000" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextareaControlDefinition"
            p:rows="5"
            p:cols="80" />
    </property>
    <property name="summary" value="assetTopsDescription" />
    <property name="description" value="assetTopsDescription description..." />
  </bean>
  <bean id="Pretag-lineItemNumber" parent="Pretag-lineItemNumber-parentBean" />

  <bean id="Pretag-lineItemNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="lineItemNumber" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Line Item Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="12" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="14" />
    </property>
    <property name="summary" value="lineItemNumber" />
    <property name="description" value="lineItemNumber description..." />
  </bean>
  <bean id="Pretag-manufacturerModelNumber" parent="Pretag-manufacturerModelNumber-parentBean" />

  <bean id="Pretag-manufacturerModelNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="manufacturerModelNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Manufacturer Model Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="25" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="27" />
    </property>
    <property name="summary" value="manufacturerModelNumber" />
    <property name="description" value="manufacturerModelNumber description..." />
  </bean>
  <bean id="Pretag-manufacturerName" parent="Pretag-manufacturerName-parentBean" />

  <bean id="Pretag-manufacturerName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="manufacturerName" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Manufacturer Name" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="40" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="42" />
    </property>
    <property name="summary" value="manufacturerName" />
    <property name="description" value="manufacturerName description..." />
  </bean>
  <bean id="Pretag-organizationDescription" parent="Pretag-organizationDescription-parentBean" />

  <bean id="Pretag-organizationDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="organizationDescription" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Organization Description" />
    <property name="shortLabel" value="Description" />
    <property name="maxLength" value="255" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextareaControlDefinition"
            p:rows="5"
            p:cols="80" />
    </property>
    <property name="summary" value="organizationDescription" />
    <property name="description" value="organizationDescription description..." />
  </bean>
  <bean id="Pretag-organizationInventoryName" parent="Pretag-organizationInventoryName-parentBean" />

  <bean id="Pretag-organizationInventoryName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="organizationInventoryName" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Organization Inventory Name" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="40" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="42" />
    </property>
    <property name="summary" value="organizationInventoryName" />
    <property name="description" value="organizationInventoryName description..." />
  </bean>
  <bean id="Pretag-organizationText" parent="Pretag-organizationText-parentBean" />

  <bean id="Pretag-organizationText-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="organizationText" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Organization Text" />
    <property name="shortLabel" value="Text" />
    <property name="maxLength" value="255" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextareaControlDefinition"
            p:rows="5"
            p:cols="80" />
    </property>
    <property name="summary" value="organizationText" />
    <property name="description" value="organizationText description..." />
  </bean>
  <bean id="Pretag-quantityInvoiced" parent="Pretag-quantityInvoiced-parentBean" />

  <bean id="Pretag-quantityInvoiced-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="quantityInvoiced" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Quantity Invoiced" />
    <property name="shortLabel" value="Invoiced" />
    <property name="maxLength" value="9" />
    <property name="validationPattern" >
      <bean parent="FloatingPointValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="11" />
    </property>
    <property name="summary" value="quantityInvoiced" />
    <property name="description" value="quantityInvoiced description..." />
  </bean>
  <bean id="Pretag-vendorName" parent="Pretag-vendorName-parentBean" />

  <bean id="Pretag-vendorName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="vendorName" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Vendor Name" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="40" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="42" />
    </property>
    <property name="summary" value="vendorName" />
    <property name="description" value="vendorName description..." />
  </bean>
  <bean id="Pretag-campusTagNumbers" parent="Pretag-campusTagNumbers-parentBean" />

  <bean id="Pretag-campusTagNumbers-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="campusTagNumbers" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Campus Tag Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
    <property name="summary" value="campusTagNumber" />
    <property name="description" value="campusTagNumber description..." />
  </bean>
  <bean id="Pretag-serialNumbers" parent="Pretag-serialNumbers-parentBean" />

  <bean id="Pretag-serialNumbers-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="serialNumbers" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Serial Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="25" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="27" />
    </property>
    <property name="summary" value="serialNumber" />
    <property name="description" value="serialNumber description..." />
  </bean>
  <bean id="Pretag-pretagDetails.campusTagNumber" parent="Pretag-pretagDetails.campusTagNumber-parentBean" />

  <bean id="Pretag-pretagDetails.campusTagNumber-parentBean" abstract="true" parent="PretagDetail-campusTagNumber">
    <property name="name" value="pretagDetails.campusTagNumber" />
  </bean>
  <bean id="Pretag-pretagDetails.serialNumber" parent="Pretag-pretagDetails.serialNumber-parentBean" />

  <bean id="Pretag-pretagDetails.serialNumber-parentBean" abstract="true" parent="PretagDetail-serialNumber">
    <property name="name" value="pretagDetails.serialNumber" />
  </bean>
  <bean id="Pretag-versionNumber" parent="Pretag-versionNumber-parentBean" />

  <bean id="Pretag-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="Pretag-inquiryDefinition" parent="Pretag-inquiryDefinition-parentBean" />

  <bean id="Pretag-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Pretag Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="purchaseOrderNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="lineItemNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="quantityInvoiced" />
              <bean parent="FieldDefinition"
                    p:attributeName="capitalAssetTypeCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="manufacturerName" />
              <bean parent="FieldDefinition"
                    p:attributeName="manufacturerModelNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="vendorName" />
              <bean parent="FieldDefinition"
                    p:attributeName="organizationCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="organizationDescription" />
              <bean parent="FieldDefinition"
                    p:attributeName="organizationInventoryName" />
              <bean parent="FieldDefinition"
                    p:attributeName="personUniversal.personName" />
              <bean parent="FieldDefinition"
                    p:attributeName="organizationText" />
              <bean parent="FieldDefinition"
                    p:attributeName="assetTopsDescription" />
              <bean parent="FieldDefinition"
                    p:attributeName="chartOfAccountsCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="pretagCreateDate" />
              <bean parent="FieldDefinition"
                    p:attributeName="active" />
            </list>
          </property>
        </bean>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Pretag Details" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="InquiryCollectionDefinition">
                <property name="attributeName" value="pretagDetails" />
                <property name="businessObjectClass" value="org.kuali.module.cams.bo.PretagDetail" />
                <property name="numberOfColumns" value="1" />
                <property name="summaryTitle" value="Pretag Detail" />
                <property name="inquiryFields" >
                  <list>
                    <bean parent="FieldDefinition"
                          p:attributeName="campusTagNumber" />
                    <bean parent="FieldDefinition"
                          p:attributeName="serialNumber" />
                    <bean parent="FieldDefinition"
                          p:attributeName="organizationTagNumber" />
                    <bean parent="FieldDefinition"
                          p:attributeName="governmentTagNumber" />
                    <bean parent="FieldDefinition"
                          p:attributeName="nationalStockNumber" />
                    <bean parent="FieldDefinition"
                          p:attributeName="campusCode" />
                    <bean parent="FieldDefinition"
                          p:attributeName="buildingCode" />
                    <bean parent="FieldDefinition"
                          p:attributeName="buildingRoomNumber" />
                    <bean parent="FieldDefinition"
                          p:attributeName="buildingSubRoomNumber" />
                    <bean parent="FieldDefinition"
                          p:attributeName="pretagTagCreateDate" />
                    <bean parent="FieldDefinition"
                          p:attributeName="active" />
                  </list>
                </property>
                <property name="summaryFields" >
                  <list>
                    <bean parent="FieldDefinition"
                          p:attributeName="campusTagNumber" />
                    <bean parent="FieldDefinition"
                          p:attributeName="serialNumber" />
                  </list>
                </property>
              </bean>
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="Pretag-lookupDefinition" parent="Pretag-lookupDefinition-parentBean" />

  <bean id="Pretag-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Pre-Asset Tagging Lookup" />
    <property name="instructions" value="Look up a Pretag" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="purchaseOrderNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="lineItemNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="quantityInvoiced" />
        <bean parent="FieldDefinition"
              p:attributeName="pretagDetails.campusTagNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="pretagDetails.serialNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="assetTopsDescription" />
        <bean parent="FieldDefinition"
              p:defaultValue="Y"
              p:attributeName="active" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="purchaseOrderNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="lineItemNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="quantityInvoiced" />
        <bean parent="FieldDefinition"
              p:attributeName="campusTagNumbers" />
        <bean parent="FieldDefinition"
              p:attributeName="serialNumbers" />
        <bean parent="FieldDefinition"
              p:attributeName="assetTopsDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
  </bean>
</beans>
