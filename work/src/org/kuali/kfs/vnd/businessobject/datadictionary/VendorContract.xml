<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="VendorContract" parent="VendorContract-parentBean" />

  <bean id="VendorContract-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.vendor.bo.VendorContract" />
    <property name="inquiryDefinition" >
      <ref bean="VendorContract-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="VendorContract-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="VendorContract-vendorContractName" />
        <ref bean="VendorContract-vendorContractDescription" />
        <ref bean="VendorContract-vendorCampusCode" />
        <ref bean="VendorContract-vendorContractBeginningDate" />
        <ref bean="VendorContract-vendorContractEndDate" />
        <ref bean="VendorContract-contractManagerCode" />
        <ref bean="VendorContract-vendorPaymentTermsCode" />
        <ref bean="VendorContract-purchaseOrderCostSourceCode" />
        <ref bean="VendorContract-purchaseOrderCostSource.purchaseOrderCostSourceDescription" />
        <ref bean="VendorContract-vendorContractExtensionDate" />
        <ref bean="VendorContract-vendorShippingPaymentTermsCode" />
        <ref bean="VendorContract-vendorShippingTitleCode" />
        <ref bean="VendorContract-vendorB2bIndicator" />
        <ref bean="VendorContract-organizationAutomaticPurchaseOrderLimit" />
        <ref bean="VendorContract-vendorContractGeneratedIdentifier" />
        <ref bean="VendorContract-vendorDetail.vendorName" />
        <ref bean="VendorContract-vendorDetail.vendorHeader.vendorSupplierDiversities.vendorSupplierDiversityCode" />
        <ref bean="VendorContract-newCollectionRecord" />
        <ref bean="VendorContract-vendorHeaderGeneratedIdentifier" />
        <ref bean="VendorContract-vendorDetailAssignedIdentifier" />
        <ref bean="VendorContract-versionNumber" />
        <ref bean="VendorContract-active" />
      </list>
    </property>
    <property name="collections" >
      <list>
        <bean parent="CollectionDefinition"
              p:name="vendorDetail.vendorHeader.vendorSupplierDiversities"
              p:label="Supplier Diversities"
              p:shortLabel="Supplier Diversities" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="VendorContract-vendorContractName" parent="VendorContract-vendorContractName-parentBean" />

  <bean id="VendorContract-vendorContractName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="vendorContractName" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Contract Name" />
    <property name="shortLabel" value="Contract Name" />
    <property name="maxLength" value="20" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="22" />
    </property>
  </bean>
  <bean id="VendorContract-vendorContractDescription" parent="VendorContract-vendorContractDescription-parentBean" />

  <bean id="VendorContract-vendorContractDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="vendorContractDescription" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Description" />
    <property name="shortLabel" value="Description" />
    <property name="maxLength" value="60" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="45" />
    </property>
  </bean>
  <bean id="VendorContract-vendorCampusCode" parent="VendorContract-vendorCampusCode-parentBean" />

  <bean id="VendorContract-vendorCampusCode-parentBean" abstract="true" parent="VendorAttributeReferenceDummy-campusCode">
    <property name="name" value="vendorCampusCode" />
    <property name="required" value="true" />
  </bean>
  <bean id="VendorContract-vendorContractBeginningDate" parent="VendorContract-vendorContractBeginningDate-parentBean" />

  <bean id="VendorContract-vendorContractBeginningDate-parentBean" abstract="true" parent="AttributeReferenceDummy-genericDate">
    <property name="name" value="vendorContractBeginningDate" />
    <property name="label" value="Begin Date" />
    <property name="shortLabel" value="Begin Date" />
    <property name="required" value="true" />
  </bean>
  <bean id="VendorContract-vendorContractEndDate" parent="VendorContract-vendorContractEndDate-parentBean" />

  <bean id="VendorContract-vendorContractEndDate-parentBean" abstract="true" parent="AttributeReferenceDummy-genericDate">
    <property name="name" value="vendorContractEndDate" />
    <property name="label" value="End Date" />
    <property name="shortLabel" value="End Date" />
    <property name="required" value="true" />
  </bean>
  <bean id="VendorContract-contractManagerCode" parent="VendorContract-contractManagerCode-parentBean" />

  <bean id="VendorContract-contractManagerCode-parentBean" abstract="true" parent="ContractManager-contractManagerCode">
    <property name="label" value="Contract Manager" />
    <property name="shortLabel" value="Contract Manager" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.module.vendor.lookup.keyvalues.ContractManagerValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
  </bean>
  <bean id="VendorContract-vendorPaymentTermsCode" parent="VendorContract-vendorPaymentTermsCode-parentBean" />

  <bean id="VendorContract-vendorPaymentTermsCode-parentBean" abstract="true" parent="PaymentTermType-vendorPaymentTermsCode">
    <property name="label" value="Payment Terms" />
    <property name="shortLabel" value="Payment Terms" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.module.vendor.lookup.keyvalues.PaymentTermsValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
  </bean>
  <bean id="VendorContract-purchaseOrderCostSourceCode" parent="VendorContract-purchaseOrderCostSourceCode-parentBean" />

  <bean id="VendorContract-purchaseOrderCostSourceCode-parentBean" abstract="true" parent="PurchaseOrderCostSource-purchaseOrderCostSourceCode">
    <property name="label" value="PO Cost Source" />
    <property name="shortLabel" value="PO Cost Source" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.module.vendor.lookup.keyvalues.PurchaseOrderCostSourceValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
  </bean>
  <bean id="VendorContract-purchaseOrderCostSource.purchaseOrderCostSourceDescription" parent="VendorContract-purchaseOrderCostSource.purchaseOrderCostSourceDescription-parentBean" />

  <bean id="VendorContract-purchaseOrderCostSource.purchaseOrderCostSourceDescription-parentBean" abstract="true" parent="PurchaseOrderCostSource-purchaseOrderCostSourceDescription">
    <property name="name" value="purchaseOrderCostSource.purchaseOrderCostSourceDescription" />
    <property name="label" value="PO Cost Source" />
    <property name="shortLabel" value="PO Cost Source" />
  </bean>
  <bean id="VendorContract-vendorContractExtensionDate" parent="VendorContract-vendorContractExtensionDate-parentBean" />

  <bean id="VendorContract-vendorContractExtensionDate-parentBean" abstract="true" parent="AttributeReferenceDummy-genericDate">
    <property name="name" value="vendorContractExtensionDate" />
    <property name="label" value="Extension Option Date" />
    <property name="shortLabel" value="Extension Option Date" />
    <property name="required" value="false" />
  </bean>
  <bean id="VendorContract-vendorShippingPaymentTermsCode" parent="VendorContract-vendorShippingPaymentTermsCode-parentBean" />

  <bean id="VendorContract-vendorShippingPaymentTermsCode-parentBean" abstract="true" parent="ShippingPaymentTerms-vendorShippingPaymentTermsCode">
    <property name="label" value="Shipping Terms" />
    <property name="shortLabel" value="Shipping Terms" />
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.module.vendor.lookup.keyvalues.ShippingPaymentTermsValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
  </bean>
  <bean id="VendorContract-vendorShippingTitleCode" parent="VendorContract-vendorShippingTitleCode-parentBean" />

  <bean id="VendorContract-vendorShippingTitleCode-parentBean" abstract="true" parent="ShippingTitle-vendorShippingTitleCode">
    <property name="label" value="Shipping Title" />
    <property name="shortLabel" value="Shipping Title" />
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.module.vendor.lookup.keyvalues.ShippingTitleValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
  </bean>
  <bean id="VendorContract-vendorB2bIndicator" parent="VendorContract-vendorB2bIndicator-parentBean" />

  <bean id="VendorContract-vendorB2bIndicator-parentBean" abstract="true" parent="VendorAttributeReferenceDummy-yesNoWithoutBlankIndicator">
    <property name="name" value="vendorB2bIndicator" />
    <property name="label" value="B2B Contract" />
    <property name="shortLabel" value="B2B Contract" />
    <property name="required" value="false" />
  </bean>
  <bean id="VendorContract-organizationAutomaticPurchaseOrderLimit" parent="VendorContract-organizationAutomaticPurchaseOrderLimit-parentBean" />

  <bean id="VendorContract-organizationAutomaticPurchaseOrderLimit-parentBean" abstract="true" parent="AttributeReferenceDummy-genericAmount">
    <property name="name" value="organizationAutomaticPurchaseOrderLimit" />
    <property name="label" value="Default APO Limit" />
    <property name="shortLabel" value="Default APO Limit" />
    <property name="required" value="false" />
  </bean>
  <bean id="VendorContract-vendorContractGeneratedIdentifier" parent="VendorContract-vendorContractGeneratedIdentifier-parentBean" />

  <bean id="VendorContract-vendorContractGeneratedIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="vendorContractGeneratedIdentifier" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Vendor Contract Generated Identifier" />
    <property name="shortLabel" value="Vendor Contract Generated Identifier" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
  </bean>
  <bean id="VendorContract-vendorDetail.vendorName" parent="VendorContract-vendorDetail.vendorName-parentBean" />

  <bean id="VendorContract-vendorDetail.vendorName-parentBean" abstract="true" parent="VendorDetail-vendorName">
    <property name="name" value="vendorDetail.vendorName" />
  </bean>
  <bean id="VendorContract-vendorDetail.vendorHeader.vendorSupplierDiversities.vendorSupplierDiversityCode" parent="VendorContract-vendorDetail.vendorHeader.vendorSupplierDiversities.vendorSupplierDiversityCode-parentBean" />

  <bean id="VendorContract-vendorDetail.vendorHeader.vendorSupplierDiversities.vendorSupplierDiversityCode-parentBean" abstract="true" parent="VendorSupplierDiversity-vendorSupplierDiversityCode">
    <property name="name" value="vendorDetail.vendorHeader.vendorSupplierDiversities.vendorSupplierDiversityCode" />
  </bean>
  <bean id="VendorContract-newCollectionRecord" parent="VendorContract-newCollectionRecord-parentBean" />

  <bean id="VendorContract-newCollectionRecord-parentBean" abstract="true" parent="AttributeReferenceDummy-newCollectionRecord">
  </bean>
  <bean id="VendorContract-vendorHeaderGeneratedIdentifier" parent="VendorContract-vendorHeaderGeneratedIdentifier-parentBean" />

  <bean id="VendorContract-vendorHeaderGeneratedIdentifier-parentBean" abstract="true" parent="VendorDetail-vendorHeaderGeneratedIdentifier">
  </bean>
  <bean id="VendorContract-vendorDetailAssignedIdentifier" parent="VendorContract-vendorDetailAssignedIdentifier-parentBean" />

  <bean id="VendorContract-vendorDetailAssignedIdentifier-parentBean" abstract="true" parent="VendorDetail-vendorDetailAssignedIdentifier">
  </bean>
  <bean id="VendorContract-versionNumber" parent="VendorContract-versionNumber-parentBean" />

  <bean id="VendorContract-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>
  <bean id="VendorContract-active" parent="VendorContract-active-parentBean" />

  <bean id="VendorContract-active-parentBean" abstract="true" parent="AttributeReferenceDummy-activeIndicator">
    <property name="name" value="active" />
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="VendorContract-inquiryDefinition" parent="VendorContract-inquiryDefinition-parentBean" />

  <bean id="VendorContract-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Vendor Contract Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="vendorContractName" />
              <bean parent="FieldDefinition"
                    p:attributeName="vendorContractDescription" />
              <bean parent="FieldDefinition"
                    p:attributeName="vendorCampusCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="vendorContractBeginningDate" />
              <bean parent="FieldDefinition"
                    p:attributeName="vendorContractEndDate" />
              <bean parent="FieldDefinition"
                    p:attributeName="contractManagerCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="vendorPaymentTermsCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="vendorContractExtensionDate" />
              <bean parent="FieldDefinition"
                    p:attributeName="vendorShippingPaymentTermsCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="vendorShippingTitleCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="organizationAutomaticPurchaseOrderLimit" />
              <bean parent="FieldDefinition"
                    p:attributeName="purchaseOrderCostSourceCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="vendorContractGeneratedIdentifier" />
              <bean parent="FieldDefinition"
                    p:attributeName="vendorDetailAssignedIdentifier" />
              <bean parent="FieldDefinition"
                    p:attributeName="vendorHeaderGeneratedIdentifier" />
              <bean parent="FieldDefinition"
                    p:attributeName="active" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="VendorContract-lookupDefinition" parent="VendorContract-lookupDefinition-parentBean" />

  <bean id="VendorContract-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Vendor Contract Lookup" />
    <property name="lookupableID" value="vendorContractLookupable" />
    <property name="instructions" value="Lookup a Vendor Contract" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="vendorCampusCode" />
        <bean parent="FieldDefinition"
              p:attributeName="contractManagerCode" />
        <bean parent="FieldDefinition"
              p:attributeName="vendorContractName" />
        <bean parent="FieldDefinition"
              p:attributeName="vendorContractDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="purchaseOrderCostSourceCode" />
        <bean parent="FieldDefinition"
              p:attributeName="vendorPaymentTermsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="vendorShippingPaymentTermsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="vendorShippingTitleCode" />
        <bean parent="FieldDefinition"
              p:defaultValue="Y"
              p:attributeName="active" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="vendorDetail.vendorName" />
        <bean parent="FieldDefinition"
              p:attributeName="vendorContractDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="vendorDetail.vendorHeader.vendorSupplierDiversities" />
        <bean parent="FieldDefinition"
              p:attributeName="purchaseOrderCostSource.purchaseOrderCostSourceDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
  </bean>
</beans>
