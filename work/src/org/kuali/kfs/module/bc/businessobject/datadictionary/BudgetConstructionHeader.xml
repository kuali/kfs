<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.budget.bo.BudgetConstructionHeader">


        <inquiry>
            <title>Budget Construction Header Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="accountNumber" />
                <field attributeName="budgetLockUserIdentifier" />
                <field attributeName="budgetTransactionLockUserIdentifier" />
                <field attributeName="chartOfAccountsCode" />
                <field attributeName="documentNumber" />
                <field attributeName="organizationLevelChartOfAccountsCode" />
                <field attributeName="organizationLevelCode" />
                <field attributeName="organizationLevelOrganizationCode" />
                <field attributeName="subAccountNumber" />
                <field attributeName="universityFiscalYear" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Budget Construction Header Lookup</title>

            <instructions>Look up a Budget Construction Header</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="universityFiscalYear" />
                    <sortAttribute attributeName="chartOfAccountsCode" />
                    <sortAttribute attributeName="accountNumber" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="accountNumber" required="false" />
                <lookupField attributeName="budgetLockUserIdentifier" required="false" />
                <lookupField attributeName="budgetTransactionLockUserIdentifier" required="false" />
                <lookupField attributeName="chartOfAccountsCode" required="false" />
                <lookupField attributeName="documentNumber" required="false" />
                <lookupField attributeName="organizationLevelChartOfAccountsCode" required="false" />
                <lookupField attributeName="organizationLevelCode" required="false" />
                <lookupField attributeName="organizationLevelOrganizationCode" required="false" />
                <lookupField attributeName="subAccountNumber" required="false" />
                <lookupField attributeName="universityFiscalYear" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="accountNumber" />
                <field attributeName="budgetLockUserIdentifier" />
                <field attributeName="budgetTransactionLockUserIdentifier" />
                <field attributeName="chartOfAccountsCode" />
                <field attributeName="documentNumber" />
                <field attributeName="organizationLevelChartOfAccountsCode" />
                <field attributeName="organizationLevelCode" />
                <field attributeName="organizationLevelOrganizationCode" />
                <field attributeName="subAccountNumber" />
                <field attributeName="universityFiscalYear" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="accountNumber" forceUppercase="true">
                <label>Account Number</label>
                <shortLabel>Account</shortLabel>

                <maxLength>7</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <required>true</required>
                <control>
                    <text size="9" />
                </control>

                <summary>accountNumber</summary>
                <description>accountNumber description...</description>
			</attribute>

            <attribute name="budgetLockUserIdentifier" forceUppercase="true">
                <label>Budget Lock User Identifier</label>
                <shortLabel>Identifier</shortLabel>

                <maxLength>10</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="11" />
                </control>

                <summary>budgetLockUserIdentifier</summary>
                <description>budgetLockUserIdentifier description...</description>
			</attribute>

            <attribute name="budgetTransactionLockUserIdentifier" forceUppercase="true">
                <label>Budget Transaction Lock User Identifier</label>
                <shortLabel>Identifier</shortLabel>

                <maxLength>10</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="11" />
                </control>

                <summary>budgetTransactionLockUserIdentifier</summary>
                <description>budgetTransactionLockUserIdentifier description...</description>
			</attribute>

            <attribute name="chartOfAccountsCode" forceUppercase="true">
                <label>Chart Of Accounts Code</label>
                <shortLabel>Chart</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <required>true</required>
                <control>
                    <!-- <text size="4" /-->
                    <select valuesFinderClass="org.kuali.module.chart.lookup.keyvalues.SimpleChartValuesFinder" />
                </control>

                <summary>chartOfAccountsCode</summary>
                <description>chartOfAccountsCode description...</description>
			</attribute>

            <attribute name="documentNumber" forceUppercase="true">
                <label>Document Number</label>
                <shortLabel>Doc#</shortLabel>

                <maxLength>14</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <required>true</required>
                <control>
                    <text size="15" />
                </control>

                <summary>documentNumber</summary>
                <description>documentNumber description...</description>
			</attribute>

            <attribute name="organizationLevelChartOfAccountsCode" forceUppercase="true">
                <label>Organization Level Chart Of Accounts Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="3" />
                </control>

                <summary>organizationLevelChartOfAccountsCode</summary>
                <description>organizationLevelChartOfAccountsCode description...</description>
			</attribute>

            <attribute name="organizationLevelCode" forceUppercase="true">
                <label>Organization Level Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>7</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="8" />
                </control>

                <summary>organizationLevelCode</summary>
                <description>organizationLevelCode description...</description>
			</attribute>

            <attribute name="organizationLevelOrganizationCode" forceUppercase="true">
                <label>Organization Level Organization Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="5" />
                </control>

                <summary>organizationLevelOrganizationCode</summary>
                <description>organizationLevelOrganizationCode description...</description>
			</attribute>

            <attribute name="subAccountNumber" forceUppercase="true">
                <label>Sub Account Number</label>
                <shortLabel>Sub Account</shortLabel>

                <maxLength>5</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="6" />
                </control>

                <summary>subAccountNumber</summary>
                <description>subAccountNumber description...</description>
			</attribute>

            <attribute name="universityFiscalYear" forceUppercase="true">
                <label>University Fiscal Year</label>
                <shortLabel>Year</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><numeric /></validationPattern>

                <required>true</required>
                <control>
                    <text size="5" />
                </control>

                <summary>universityFiscalYear</summary>
                <description>universityFiscalYear description...</description>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
