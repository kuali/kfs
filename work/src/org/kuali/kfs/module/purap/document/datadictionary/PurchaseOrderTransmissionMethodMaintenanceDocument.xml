<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="PurchaseOrderTransmissionMethodMaintenanceDocument" parent="PurchaseOrderTransmissionMethodMaintenanceDocument-parentBean" />

  <bean id="PurchaseOrderTransmissionMethodMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.purap.bo.PurchaseOrderTransmissionMethod" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="PurchaseOrderTransmissionMethodMaintenanceDocument-EditMethodofPOTransmission" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>purchaseOrderTransmissionMethodCode</value>
      </list>
    </property>
    <property name="label" value="Method of PO Transmission" />
    <property name="shortLabel" value="Method of PO Transmission" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-PA"
            p:parameterName="PURCHASE_ORDER_TRANSMISSION_METHOD"
            p:parameterDetailType="PurchaseOrderTransmissionMethod" />
    </property>
    <property name="documentTypeName" value="PurchaseOrderTransmissionMethodMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="PMTM" />
    <property name="workflowProperties" >
      <ref bean="PurchaseOrderTransmissionMethodMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>PA_PUR_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="PurchaseOrderTransmissionMethodMaintenanceDocument-EditMethodofPOTransmission" parent="PurchaseOrderTransmissionMethodMaintenanceDocument-EditMethodofPOTransmission-parentBean" />

  <bean id="PurchaseOrderTransmissionMethodMaintenanceDocument-EditMethodofPOTransmission-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Method of PO Transmission" />
    <property name="title" value="Edit Method of PO Transmission" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="purchaseOrderTransmissionMethodCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="purchaseOrderTransmissionMethodDescription"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="active" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="PurchaseOrderTransmissionMethodMaintenanceDocument-workflowProperties" parent="PurchaseOrderTransmissionMethodMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="PurchaseOrderTransmissionMethodMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
