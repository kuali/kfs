<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.purap.bo.AssignContractManagerDetail">
        <customAttributeEnabled>false</customAttributeEnabled>

        <attributes>

            <attribute name="contractManagerCode" forceUppercase="true">
                <label>Contract Manager Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="4" />
                </control>

                <summary>contractManagerCode</summary>
                <description>contractManagerCode description...</description>
			</attribute>

            <attribute name="documentNumber" forceUppercase="true">
                <label>Document Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>14</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="16" />
                </control>

                <summary>documentNumber</summary>
                <description>documentNumber description...</description>
			</attribute>

            <attribute name="requisitionIdentifier" forceUppercase="true">
                <label>Requisition Identifier</label>
                <shortLabel>Identifier</shortLabel>

                <maxLength>8</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="10" />
                </control>

                <summary>requisitionIdentifier</summary>
                <description>requisitionIdentifier description...</description>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
