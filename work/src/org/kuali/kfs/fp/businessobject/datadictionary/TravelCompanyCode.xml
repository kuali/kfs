<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="TravelCompanyCode" parent="TravelCompanyCode-parentBean" />

  <bean id="TravelCompanyCode-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.financial.bo.TravelCompanyCode" />
    <property name="inquiryDefinition" >
      <ref bean="TravelCompanyCode-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="TravelCompanyCode-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="code" />
    <property name="objectLabel" value="Disbursement Voucher Travel Company" />
    <property name="attributes" >
      <list>
        <ref bean="TravelCompanyCode-code" />
        <ref bean="TravelCompanyCode-travelExpenseTypeCode.prepaidExpense" />
        <ref bean="TravelCompanyCode-name" />
        <ref bean="TravelCompanyCode-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="TravelCompanyCode-code" parent="TravelCompanyCode-code-parentBean" />

  <bean id="TravelCompanyCode-code-parentBean" abstract="true" parent="TravelExpenseTypeCode-code">
    <property name="label" value="Company Type Code" />
  </bean>
  <bean id="TravelCompanyCode-travelExpenseTypeCode.prepaidExpense" parent="TravelCompanyCode-travelExpenseTypeCode.prepaidExpense-parentBean" />

  <bean id="TravelCompanyCode-travelExpenseTypeCode.prepaidExpense-parentBean" abstract="true" parent="TravelExpenseTypeCode-prepaidExpense">
    <property name="name" value="travelExpenseTypeCode.prepaidExpense" />
  </bean>
  <bean id="TravelCompanyCode-name" parent="TravelCompanyCode-name-parentBean" />

  <bean id="TravelCompanyCode-name-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="name" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Travel Company Name" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="25" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="25" />
    </property>
  </bean>
  <bean id="TravelCompanyCode-versionNumber" parent="TravelCompanyCode-versionNumber-parentBean" />

  <bean id="TravelCompanyCode-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="TravelCompanyCode-inquiryDefinition" parent="TravelCompanyCode-inquiryDefinition-parentBean" />

  <bean id="TravelCompanyCode-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Disbursement Voucher Travel Company Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="code" />
              <bean parent="FieldDefinition"
                    p:attributeName="name" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="TravelCompanyCode-lookupDefinition" parent="TravelCompanyCode-lookupDefinition-parentBean" />

  <bean id="TravelCompanyCode-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Disbursement Voucher Travel Company Lookup" />
    <property name="instructions" value="Lookup a Travel Company Code" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>code</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="code" />
        <bean parent="FieldDefinition"
              p:attributeName="travelExpenseTypeCode.prepaidExpense" />
        <bean parent="FieldDefinition"
              p:attributeName="name" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="code" />
        <bean parent="FieldDefinition"
              p:attributeName="name" />
      </list>
    </property>
  </bean>
</beans>
