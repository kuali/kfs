<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="FiscalYearFunctionControlMaintenanceDocument" parent="FiscalYearFunctionControlMaintenanceDocument-parentBean" />

  <bean id="FiscalYearFunctionControlMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.financial.bo.FiscalYearFunctionControl" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="FiscalYearFunctionControlMaintenanceDocument-EditFiscalYearFunctionControl" />
      </list>
    </property>
    <property name="defaultExistenceChecks" >
      <list>
        <bean parent="ReferenceDefinition"
              p:attributeName="universityFiscal"
              p:attributeToHighlightOnFail="universityFiscalYear" />
        <bean parent="ReferenceDefinition"
              p:attributeName="functionControl"
              p:attributeToHighlightOnFail="financialSystemFunctionControlCode" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>universityFiscalYear</value>
        <value>financialSystemFunctionControlCode</value>
      </list>
    </property>
    <property name="label" value="Fiscal Year Function Control" />
    <property name="shortLabel" value="Fiscal Year Function Control" />
    <property name="summary" value="Fiscal Year Function Control Maintenance Document" />
    <property name="description" value="Document used to create or update Fiscal Year Function Control objects" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-FP"
            p:parameterName="FUNCTION_CONTROL_CODE"
            p:parameterDetailType="FiscalYearFunctionControl" />
    </property>
    <property name="documentTypeName" value="FiscalYearFunctionControlMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="BFYC" />
    <property name="workflowProperties" >
      <ref bean="FiscalYearFunctionControlMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>SY_MAINTAINENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="FiscalYearFunctionControlMaintenanceDocument-EditFiscalYearFunctionControl" parent="FiscalYearFunctionControlMaintenanceDocument-EditFiscalYearFunctionControl-parentBean" />

  <bean id="FiscalYearFunctionControlMaintenanceDocument-EditFiscalYearFunctionControl-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Fiscal Year Function Control" />
    <property name="title" value="Edit Fiscal Year Function Control" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="universityFiscalYear"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialSystemFunctionControlCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialSystemFunctionActiveIndicator" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="FiscalYearFunctionControlMaintenanceDocument-workflowProperties" parent="FiscalYearFunctionControlMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="FiscalYearFunctionControlMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
