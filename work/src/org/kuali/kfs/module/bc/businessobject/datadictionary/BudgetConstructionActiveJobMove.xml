    </property>
    
    
  </bean>
  <bean id="BudgetConstructionActiveJobMove-positionNumber" parent="BudgetConstructionActiveJobMove-positionNumber-parentBean"/>

  <bean id="BudgetConstructionActiveJobMove-positionNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="positionNumber"/>
    <property name="forceUppercase" value="true"/>
    <property name="label" value="Position Number"/>
    <property name="shortLabel" value="Number"/>
    <property name="maxLength" value="8"/>
    <property name="validationPattern">
      <bean parent="AnyCharacterValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="10"/>
    </property>
    
    
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="BudgetConstructionActiveJobMove-inquiryDefinition" parent="BudgetConstructionActiveJobMove-inquiryDefinition-parentBean"/>

  <bean id="BudgetConstructionActiveJobMove-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Budget Construction Active Job Move Inquiry"/>
    <property name="inquirySections">
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value=""/>
          <property name="numberOfColumns" value="1"/>
          <property name="inquiryFields">
            <list>
              <bean parent="FieldDefinition" p:attributeName="departmentIdentifier"/>
              <bean parent="FieldDefinition" p:attributeName="effectiveDate"/>
              <bean parent="FieldDefinition" p:attributeName="effectiveSequence"/>
              <bean parent="FieldDefinition" p:attributeName="emplid"/>
              <bean parent="FieldDefinition" p:attributeName="employeeRecord"/>
              <bean parent="FieldDefinition" p:attributeName="employeeStatus"/>
              <bean parent="FieldDefinition" p:attributeName="principalId"/>
              <bean parent="FieldDefinition" p:attributeName="positionNumber"/>
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


