<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="BudgetConstructionAdministrativePost" parent="BudgetConstructionAdministrativePost-parentBean" />

  <bean id="BudgetConstructionAdministrativePost-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.budget.bo.BudgetConstructionAdministrativePost" />
    <property name="inquiryDefinition" >
      <ref bean="BudgetConstructionAdministrativePost-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="BudgetConstructionAdministrativePost-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="BudgetConstructionAdministrativePost-administrativePost" />
        <ref bean="BudgetConstructionAdministrativePost-emplid" />
        <ref bean="BudgetConstructionAdministrativePost-positionNumber" />
        <ref bean="BudgetConstructionAdministrativePost-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="BudgetConstructionAdministrativePost-administrativePost" parent="BudgetConstructionAdministrativePost-administrativePost-parentBean" />

  <bean id="BudgetConstructionAdministrativePost-administrativePost-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="administrativePost" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Administrative Post" />
    <property name="shortLabel" value="Post" />
    <property name="maxLength" value="15" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="17" />
    </property>
    <property name="summary" value="administrativePost" />
    <property name="description" value="administrativePost description..." />
  </bean>
  <bean id="BudgetConstructionAdministrativePost-emplid" parent="BudgetConstructionAdministrativePost-emplid-parentBean" />

  <bean id="BudgetConstructionAdministrativePost-emplid-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="emplid" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Emplid" />
    <property name="shortLabel" value="Emplid" />
    <property name="maxLength" value="11" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="13" />
    </property>
    <property name="summary" value="emplid" />
    <property name="description" value="emplid description..." />
  </bean>
  <bean id="BudgetConstructionAdministrativePost-positionNumber" parent="BudgetConstructionAdministrativePost-positionNumber-parentBean" />

  <bean id="BudgetConstructionAdministrativePost-positionNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="positionNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Position Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
    <property name="summary" value="positionNumber" />
    <property name="description" value="positionNumber description..." />
  </bean>
  <bean id="BudgetConstructionAdministrativePost-versionNumber" parent="BudgetConstructionAdministrativePost-versionNumber-parentBean" />

  <bean id="BudgetConstructionAdministrativePost-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="BudgetConstructionAdministrativePost-inquiryDefinition" parent="BudgetConstructionAdministrativePost-inquiryDefinition-parentBean" />

  <bean id="BudgetConstructionAdministrativePost-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Budget Construction Administrative Post Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="administrativePost" />
              <bean parent="FieldDefinition"
                    p:attributeName="emplid" />
              <bean parent="FieldDefinition"
                    p:attributeName="positionNumber" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="BudgetConstructionAdministrativePost-lookupDefinition" parent="BudgetConstructionAdministrativePost-lookupDefinition-parentBean" />

  <bean id="BudgetConstructionAdministrativePost-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Budget Construction Administrative Post Lookup" />
    <property name="instructions" value="Look up a Budget Construction Administrative Post" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="administrativePost" />
        <bean parent="FieldDefinition"
              p:attributeName="emplid" />
        <bean parent="FieldDefinition"
              p:attributeName="positionNumber" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="administrativePost" />
        <bean parent="FieldDefinition"
              p:attributeName="emplid" />
        <bean parent="FieldDefinition"
              p:attributeName="positionNumber" />
      </list>
    </property>
  </bean>
</beans>
