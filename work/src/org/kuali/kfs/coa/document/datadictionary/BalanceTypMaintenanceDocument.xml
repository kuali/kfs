<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="BalanceTypMaintenanceDocument" parent="BalanceTypMaintenanceDocument-parentBean" />

  <bean id="BalanceTypMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.chart.bo.codes.BalanceTyp" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="BalanceTypMaintenanceDocument-EditBalanceType" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>code</value>
      </list>
    </property>
    <property name="label" value="Balance Type" />
    <property name="shortLabel" value="Balance Type" />
    <property name="summary" value="Balance Type Maintenance Document" />
    <property name="description" value="Maintenance document which allows you to create new Balance Types, or modify existing Balance Types." />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-CA"
            p:parameterName="BALANCE_TYPE"
            p:parameterDetailType="BalanceTyp" />
    </property>
    <property name="documentTypeName" value="BalanceTypMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="BTYP" />
    <property name="workflowProperties" >
      <ref bean="BalanceTypMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>SY_MAINTAINENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="BalanceTypMaintenanceDocument-EditBalanceType" parent="BalanceTypMaintenanceDocument-EditBalanceType-parentBean" />

  <bean id="BalanceTypMaintenanceDocument-EditBalanceType-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Balance Type" />
    <property name="title" value="Edit Balance Type" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="code"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="name"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialBalanceTypeShortNm"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialOffsetGenerationIndicator" />
        <bean parent="MaintainableFieldDefinition"
              p:name="finBalanceTypeEncumIndicator" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="BalanceTypMaintenanceDocument-workflowProperties" parent="BalanceTypMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="BalanceTypMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
