<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="ShippingPaymentTermsMaintenanceDocument" parent="ShippingPaymentTermsMaintenanceDocument-parentBean" />

  <bean id="ShippingPaymentTermsMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.vendor.bo.ShippingPaymentTerms" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="ShippingPaymentTermsMaintenanceDocument-EditShippingPaymentTerms" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>vendorShippingPaymentTermsCode</value>
      </list>
    </property>
    <property name="label" value="Shipping Payment Terms" />
    <property name="shortLabel" value="Shipping Payment Terms" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-VN"
            p:parameterName="SHIPPING_PAYMENT_TERMS"
            p:parameterDetailType="ShippingPaymentTerms" />
    </property>
    <property name="documentTypeName" value="ShippingPaymentTermsMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="PMSP" />
    <property name="workflowProperties" >
      <ref bean="ShippingPaymentTermsMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>PA_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="ShippingPaymentTermsMaintenanceDocument-EditShippingPaymentTerms" parent="ShippingPaymentTermsMaintenanceDocument-EditShippingPaymentTerms-parentBean" />

  <bean id="ShippingPaymentTermsMaintenanceDocument-EditShippingPaymentTerms-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Shipping Payment Terms" />
    <property name="title" value="Edit Shipping Payment Terms" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="vendorShippingPaymentTermsCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="vendorShippingPaymentTermsDescription"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="vendorShippingPaymentTermsPayIndicator" />
        <bean parent="MaintainableFieldDefinition"
              p:name="active" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="ShippingPaymentTermsMaintenanceDocument-workflowProperties" parent="ShippingPaymentTermsMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="ShippingPaymentTermsMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
