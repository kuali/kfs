	<changeSet author="KFS50" id="KFS_50_COA_LBR_BEN_RT_CAT_CD">
		<comment>KFSMI-6987 Employee Benefit Expense Calculation</comment>
		<addColumn tableName="CA_ACCOUNT_T">
