                <required>false</required>

                <control>
                    <text size="40" />
                </control>

                <summary>
                    <![CDATA[&nbsp;]]>
                </summary>
                <description>The city and state of the traveler's destination.</description>
            </attribute>

            <attribute name="disbVchrAutoToStateCode">
                <label>To (City/State)</label>
                <shortLabel>St</shortLabel>

                <maxLength>2</maxLength>

                <required>false</required>

                <control>
                    <text size="2" />
                </control>

                <summary>
                    <![CDATA[&nbsp;]]>
                </summary>
                <description>
                    The city and state of the traveler's destination.

                </description>
            </attribute>

            <attribute name="disbVchrAutoRoundTripCode">
                <label>Round Trip</label>
                <shortLabel>Round Trip</shortLabel>

                <maxLength>1</maxLength>

                <required>false</required>

                <control>
                    <checkbox />
                </control>

                <summary>Round trip Indicator</summary>
                <description>A checkbox to indicate that the Non-Employee Travel is round trip.</description>
            </attribute>

