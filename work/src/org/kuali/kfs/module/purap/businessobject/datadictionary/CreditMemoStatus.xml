<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.purap.bo.CreditMemoStatus">
        <customAttributeEnabled>false</customAttributeEnabled>

        <inquiry>
            <title>Credit Memo Status Inquiry</title>

            <inquiryFields>
                <field attributeName="creditMemoStatusCode" />
                <field attributeName="creditMemoStatusDescription" />
                <field attributeName="dataObjectMaintenanceCodeActiveIndicator" />
            </inquiryFields>
        </inquiry>

        <lookup>
            <title>Credit Memo Status Lookup</title>

			<menubar><![CDATA[<a href="maintenance.do?methodToCall=start&businessObjectClassName=org.kuali.module.purap.bo.CreditMemoStatus">Create New</a>]]></menubar>

            <instructions>Lookup a Credit Memo Status</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="creditMemoStatusDescription" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="creditMemoStatusCode" required="false" />
                <lookupField attributeName="creditMemoStatusDescription" required="false" />
                <lookupField attributeName="dataObjectMaintenanceCodeActiveIndicator" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="creditMemoStatusCode" />
                <field attributeName="creditMemoStatusDescription" />
                <field attributeName="dataObjectMaintenanceCodeActiveIndicator" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="creditMemoStatusCode" forceUppercase="true">
                <label>Credit Memo Status Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="6" />
                </control>

                <summary>creditMemoStatusCode</summary>
                <description>creditMemoStatusCode description...</description>
			</attribute>

            <attribute name="creditMemoStatusDescription">
                <label>Credit Memo Status Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>45</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>

                <control>
                    <text size="47" />
                </control>

                <summary>creditMemoStatusDescription</summary>
                <description>creditMemoStatusDescription description...</description>
			</attribute>

            <attributeReference name="dataObjectMaintenanceCodeActiveIndicator" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="activeIndicator" />

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />


        </attributes>
    </businessObject>
</dictionaryEntry>
