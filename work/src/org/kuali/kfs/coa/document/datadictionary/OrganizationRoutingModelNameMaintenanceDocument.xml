<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="OrganizationRoutingModelNameMaintenanceDocument" parent="OrganizationRoutingModelNameMaintenanceDocument-parentBean" />

  <bean id="OrganizationRoutingModelNameMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.chart.bo.OrganizationRoutingModelName" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="OrganizationRoutingModelNameMaintenanceDocument-EditAccountDelegateModel" />
        <ref bean="OrganizationRoutingModelNameMaintenanceDocument-EditDelegatesforModel" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>chartOfAccountsCode</value>
        <value>organizationCode</value>
        <value>organizationRoutingModelName</value>
      </list>
    </property>
    <property name="label" value="Account Delegate Model" />
    <property name="shortLabel" value="Account Delegate Model" />
    <property name="summary" value="Account Delegate Model Maintenance Document" />
    <property name="description" value="Account Delegate Model Maintenance Document" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-CA"
            p:parameterName="DELEGATE_MODEL"
            p:parameterDetailType="OrganizationRoutingModelName" />
    </property>
    <property name="documentTypeName" value="OrganizationRoutingModelNameMaintenanceDocument" />
    <property name="businessRulesClass" value="org.kuali.module.chart.rules.OrganizationRoutingModelRule" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="preRulesCheckClass" value="org.kuali.module.chart.rules.OrganizationRoutingModelPreRules" />
    <property name="documentTypeCode" value="GDLM" />
    <property name="workflowProperties" >
      <ref bean="OrganizationRoutingModelNameMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>kualiUniversalGroup</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="OrganizationRoutingModelNameMaintenanceDocument-EditAccountDelegateModel" parent="OrganizationRoutingModelNameMaintenanceDocument-EditAccountDelegateModel-parentBean" />

  <bean id="OrganizationRoutingModelNameMaintenanceDocument-EditAccountDelegateModel-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Account Delegate  Model" />
    <property name="title" value="Edit Account Delegate  Model" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="chartOfAccountsCode"
              p:required="true"
              p:defaultValueFinderClass="org.kuali.module.chart.lookup.valuefinder.CurrentUserChartValueFinder" />
        <bean parent="MaintainableFieldDefinition"
              p:name="organizationCode"
              p:required="true"
              p:defaultValueFinderClass="org.kuali.module.chart.lookup.valuefinder.CurrentUserOrgValueFinder" />
        <bean parent="MaintainableFieldDefinition"
              p:name="organizationRoutingModelName"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>
  <bean id="OrganizationRoutingModelNameMaintenanceDocument-EditDelegatesforModel" parent="OrganizationRoutingModelNameMaintenanceDocument-EditDelegatesforModel-parentBean" />

  <bean id="OrganizationRoutingModelNameMaintenanceDocument-EditDelegatesforModel-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Delegates for Model" />
    <property name="title" value="Edit Delegates for Model" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableCollectionDefinition">
          <property name="name" value="organizationRoutingModel" />
          <property name="businessObjectClass" value="org.kuali.module.chart.bo.OrganizationRoutingModel" />
          <property name="summaryTitle" value="Account Delegate Model Delegate" />
          <property name="summaryFields" >
            <list>
              <bean parent="MaintainableFieldDefinition"
                    p:name="financialDocumentTypeCode" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="accountDelegatePrimaryRoutingIndicator" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="accountDelegate.personUserIdentifier" />
            </list>
          </property>
          <property name="maintainableFields" >
            <list>
              <bean parent="MaintainableFieldDefinition"
                    p:name="financialDocumentTypeCode"
                    p:required="true" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="accountDelegatePrimaryRoutingIndicator" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="accountDelegateStartDate"
                    p:required="true"
                    p:defaultValueFinderClass="org.kuali.kfs.lookup.valuefinder.CurrentDateMMDDYYYYFinder" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="approvalFromThisAmount" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="approvalToThisAmount" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="accountDelegate.personUserIdentifier"
                    p:required="true" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="accountDelegate.personName"
                    p:readOnly="true"
                    p:required="true" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="accountDelegateUniversalId"
                    p:readOnly="true"
                    p:required="true" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="active"
                    p:defaultValue="true" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="newCollectionRecord" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="versionNumber" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="OrganizationRoutingModelNameMaintenanceDocument-workflowProperties" parent="OrganizationRoutingModelNameMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="OrganizationRoutingModelNameMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
    <property name="workflowPropertyGroups" >
      <list>
        <bean parent="WorkflowPropertyGroup">
          <property name="workflowProperties" >
            <list>
              <bean parent="WorkflowProperty"
                    p:path="newMaintainableObject.businessObject" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>
</beans>
