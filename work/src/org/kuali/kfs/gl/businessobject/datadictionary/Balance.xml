  <bean id="Balance-timestamp" parent="Balance-timestamp-parentBean" />

  <bean id="Balance-timestamp-parentBean" abstract="true" parent="AttributeReferenceDummy-genericTimestamp">
    <property name="name" value="timestamp" />
    <property name="label" value="Transaction Entry Processed" />
    <property name="shortLabel" value="Entry Processed" />
  </bean>
  <bean id="Balance-dummyBusinessObject.consolidationOption" parent="Balance-dummyBusinessObject.consolidationOption-parentBean" />

  <bean id="Balance-dummyBusinessObject.consolidationOption-parentBean" abstract="true" parent="TransientBalanceInquiryAttributes-consolidationOption">
    <property name="name" value="dummyBusinessObject.consolidationOption" />
  </bean>
  <bean id="Balance-dummyBusinessObject.pendingEntryOption" parent="Balance-dummyBusinessObject.pendingEntryOption-parentBean" />

  <bean id="Balance-dummyBusinessObject.pendingEntryOption-parentBean" abstract="true" parent="TransientBalanceInquiryAttributes-pendingEntryOption">
    <property name="name" value="dummyBusinessObject.pendingEntryOption" />
  </bean>
  <bean id="Balance-dummyBusinessObject.amountViewOption" parent="Balance-dummyBusinessObject.amountViewOption-parentBean" />

  <bean id="Balance-dummyBusinessObject.amountViewOption-parentBean" abstract="true" parent="TransientBalanceInquiryAttributes-amountViewOption">
    <property name="name" value="dummyBusinessObject.amountViewOption" />
  </bean>
  <bean id="Balance-dummyBusinessObject.genericAmount" parent="Balance-dummyBusinessObject.genericAmount-parentBean" />

