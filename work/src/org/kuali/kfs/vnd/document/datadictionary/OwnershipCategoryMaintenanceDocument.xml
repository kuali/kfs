<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="OwnershipCategoryMaintenanceDocument" parent="OwnershipCategoryMaintenanceDocument-parentBean" />

  <bean id="OwnershipCategoryMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.vendor.bo.OwnershipCategory" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="OwnershipCategoryMaintenanceDocument-EditOwnershipCategory" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>vendorOwnershipCategoryCode</value>
      </list>
    </property>
    <property name="label" value="Ownership Type Category" />
    <property name="shortLabel" value="Ownership Type Category" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-VN"
            p:parameterName="OWNERSHIP_CATEGORY"
            p:parameterDetailType="OwnershipCategory" />
    </property>
    <property name="documentTypeName" value="OwnershipCategoryMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="PMOC" />
    <property name="workflowProperties" >
      <ref bean="OwnershipCategoryMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>PA_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="OwnershipCategoryMaintenanceDocument-EditOwnershipCategory" parent="OwnershipCategoryMaintenanceDocument-EditOwnershipCategory-parentBean" />

  <bean id="OwnershipCategoryMaintenanceDocument-EditOwnershipCategory-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Ownership Type Category" />
    <property name="title" value="Edit Ownership Type Category" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="vendorOwnershipCategoryCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="vendorOwnershipCategoryDescription"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="active" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="OwnershipCategoryMaintenanceDocument-workflowProperties" parent="OwnershipCategoryMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="OwnershipCategoryMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
