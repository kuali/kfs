<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="RequisitionItemCapitalAsset" parent="RequisitionItemCapitalAsset-parentBean" />

  <bean id="RequisitionItemCapitalAsset-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.purap.bo.RequisitionItemCapitalAsset" />
    <property name="attributes" >
      <list>
        <ref bean="RequisitionItemCapitalAsset-capitalAssetNumber" />
        <ref bean="RequisitionItemCapitalAsset-itemIdentifier" />
        <ref bean="RequisitionItemCapitalAsset-requisitionItemCapitalAssetIdentifier" />
        <ref bean="RequisitionItemCapitalAsset-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="RequisitionItemCapitalAsset-capitalAssetNumber" parent="RequisitionItemCapitalAsset-capitalAssetNumber-parentBean" />

  <bean id="RequisitionItemCapitalAsset-capitalAssetNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="capitalAssetNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Capital Asset Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="12" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="14" />
    </property>
  </bean>
  <bean id="RequisitionItemCapitalAsset-itemIdentifier" parent="RequisitionItemCapitalAsset-itemIdentifier-parentBean" />

  <bean id="RequisitionItemCapitalAsset-itemIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="itemIdentifier" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Item Identifier" />
    <property name="shortLabel" value="Identifier" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
  </bean>
  <bean id="RequisitionItemCapitalAsset-requisitionItemCapitalAssetIdentifier" parent="RequisitionItemCapitalAsset-requisitionItemCapitalAssetIdentifier-parentBean" />

  <bean id="RequisitionItemCapitalAsset-requisitionItemCapitalAssetIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="requisitionItemCapitalAssetIdentifier" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Requisition Item Capital Asset Identifier" />
    <property name="shortLabel" value="Identifier" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
  </bean>
  <bean id="RequisitionItemCapitalAsset-versionNumber" parent="RequisitionItemCapitalAsset-versionNumber-parentBean" />

  <bean id="RequisitionItemCapitalAsset-versionNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="versionNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Version Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
  </bean>
</beans>
