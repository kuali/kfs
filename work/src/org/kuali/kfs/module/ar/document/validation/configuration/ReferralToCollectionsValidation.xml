    <bean id="ReferralToCollections-saveDocumentValidation" parent="ReferralToCollections-saveDocumentValidation-parentBean" scope="prototype" />
	<bean id="ReferralToCollections-saveDocumentValidation-parentBean" parent="CompositeValidation" scope="prototype" abstract="true">
		<property name="validations">
