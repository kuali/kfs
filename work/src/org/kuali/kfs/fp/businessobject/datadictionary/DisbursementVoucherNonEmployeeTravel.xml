                <shortLabel>Calc Amt</shortLabel>

                <maxLength>22</maxLength>

                <required>false</required>

                <control>
                    <text size="9" />
                </control>

                <summary>
                    <![CDATA[&nbsp;]]>
                </summary>
                <description>The calculated Per Diem amount for the Non-Employee Travel, based the appropriate Rate.</description>
            </attribute>

            <attribute name="disbVchrPerdiemActualAmount">
                <label>Actual Amount</label>
                <shortLabel>Act Amt</shortLabel>

                <maxLength>22</maxLength>

                <required>false</required>

                <control>
                    <text size="9" />
                </control>

                <summary>
                    <![CDATA[&nbsp;]]>
                </summary>
                <description>The actual Per Diem amount for the Non-Employee Travel.</description>
            </attribute>

            <attribute name="dvPerdiemChangeReasonText">
