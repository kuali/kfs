<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="QuestionTypeMaintenanceDocument" parent="QuestionTypeMaintenanceDocument-parentBean" />

  <bean id="QuestionTypeMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.kra.routingform.bo.QuestionType" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="QuestionTypeMaintenanceDocument-EditQuestionType" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>questionTypeCode</value>
      </list>
    </property>
    <property name="label" value="Question Type" />
    <property name="shortLabel" value="Question Type" />
    <property name="summary" value="Question Type Maintenance Document" />
    <property name="description" value="Document used to create or update Question Type objects" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-RA"
            p:parameterName="QUESTION_TYPE"
            p:parameterDetailType="QuestionType" />
    </property>
    <property name="documentTypeName" value="QuestionTypeMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.module.kra.routingform.document.QuestionTypeMaintenanceDocumentAuthorizer" />
    <property name="documentTypeCode" value="QNT" />
    <property name="workflowProperties" >
      <ref bean="QuestionTypeMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>RA_PRE_AWARD_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="QuestionTypeMaintenanceDocument-EditQuestionType" parent="QuestionTypeMaintenanceDocument-EditQuestionType-parentBean" />

  <bean id="QuestionTypeMaintenanceDocument-EditQuestionType-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Question Type" />
    <property name="title" value="Edit Question Type" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="questionTypeCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="questionTypeDescription"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="questionTypeSortNumber"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="questionTypeWorkgroupName" />
        <bean parent="MaintainableFieldDefinition"
              p:name="questionTypeNotificationValue" />
        <bean parent="MaintainableFieldDefinition"
              p:name="dataObjectMaintenanceCodeActiveIndicator"
              p:defaultValue="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="QuestionTypeMaintenanceDocument-workflowProperties" parent="QuestionTypeMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="QuestionTypeMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
