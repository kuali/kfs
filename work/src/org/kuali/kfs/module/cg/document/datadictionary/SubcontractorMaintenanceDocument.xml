<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="SubcontractorMaintenanceDocument" parent="SubcontractorMaintenanceDocument-parentBean" />

  <bean id="SubcontractorMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.cg.bo.Subcontractor" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="SubcontractorMaintenanceDocument-EditSubcontractor" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>subcontractorNumber</value>
      </list>
    </property>
    <property name="label" value="Sub-Contractor" />
    <property name="shortLabel" value="Sub-Contractor" />
    <property name="summary" value="Subcontractor Maintenance Document" />
    <property name="description" value="Document used to create or update Subcontractor objects" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-CG"
            p:parameterName="SUBCONTRACTOR"
            p:parameterDetailType="Subcontractor" />
    </property>
    <property name="documentTypeName" value="SubcontractorMaintenanceDocument" />
    <property name="businessRulesClass" value="org.kuali.module.cg.rules.SubcontractorRule" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="SUBC" />
    <property name="workflowProperties" >
      <ref bean="SubcontractorMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>kualiUniversalGroup</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="SubcontractorMaintenanceDocument-EditSubcontractor" parent="SubcontractorMaintenanceDocument-EditSubcontractor-parentBean" />

  <bean id="SubcontractorMaintenanceDocument-EditSubcontractor-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Subcontractor" />
    <property name="title" value="Edit Subcontractor" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="subcontractorNumber"
              p:readOnly="true"
              p:required="true"
              p:defaultValueFinderClass="org.kuali.module.cg.lookup.valuefinder.NextSubcontractorNumberFinder" />
        <bean parent="MaintainableFieldDefinition"
              p:name="subcontractorName"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="subcontractorAddressLine1"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="subcontractorAddressLine2" />
        <bean parent="MaintainableFieldDefinition"
              p:name="subcontractorCity"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="subcontractorStateCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="subcontractorZipCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="subcontractorCountryCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="active"
              p:defaultValue="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="SubcontractorMaintenanceDocument-workflowProperties" parent="SubcontractorMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="SubcontractorMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
