 </bean>
 <bean id="AccountAutoCreateDefaults-inquirySectionDefinition-cgAccountDefaultsDescription" parent="AccountAutoCreateDefaults-inquirySectionDefinition-cgAccountDefaultsDescription-parentBean"/>
 <bean id="AccountAutoCreateDefaults-inquirySectionDefinition-cgAccountDefaultsDescription-parentBean" parent="InquirySectionDefinition" abstract="true" p:title="CG Account Defaults Description">        
