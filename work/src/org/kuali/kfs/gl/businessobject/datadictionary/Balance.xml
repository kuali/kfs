  <bean id="Balance-month2Amount-parentBean" abstract="true" parent="AttributeReferenceDummy-genericAmount">
    <property name="name" value="month2Amount" />
    <property name="label" value="August" />
    <property name="formatterClass" value="org.kuali.core.web.format.CurrencyFormatter" />
  </bean>
  <bean id="Balance-month3Amount" parent="Balance-month3Amount-parentBean" />

  <bean id="Balance-month3Amount-parentBean" abstract="true" parent="AttributeReferenceDummy-genericAmount">
    <property name="name" value="month3Amount" />
    <property name="label" value="September" />
    <property name="formatterClass" value="org.kuali.core.web.format.CurrencyFormatter" />
  </bean>
  <bean id="Balance-month4Amount" parent="Balance-month4Amount-parentBean" />

  <bean id="Balance-month4Amount-parentBean" abstract="true" parent="AttributeReferenceDummy-genericAmount">
    <property name="name" value="month4Amount" />
