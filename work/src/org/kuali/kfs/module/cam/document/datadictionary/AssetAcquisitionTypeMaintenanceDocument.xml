<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="AssetAcquisitionTypeMaintenanceDocument" parent="AssetAcquisitionTypeMaintenanceDocument-parentBean" />

  <bean id="AssetAcquisitionTypeMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.cams.bo.AssetAcquisitionType" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="AssetAcquisitionTypeMaintenanceDocument-EditAssetAcquisitionType" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>acquisitionTypeCode</value>
      </list>
    </property>
    <property name="label" value="Asset Acquisition Type" />
    <property name="shortLabel" value="Asset Acquisition Type" />
    <property name="summary" value="Asset acquisition maintenance document" />
    <property name="description" value="Document used to create or update asset acquisition objects" />
    <property name="documentTypeName" value="AssetAcquisitionTypeMaintenanceDocument" />
    <property name="businessRulesClass" value="org.kuali.core.maintenance.rules.MaintenanceDocumentRuleBase" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="ACQT" />
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>CM_ADMINISTRATORS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="AssetAcquisitionTypeMaintenanceDocument-EditAssetAcquisitionType" parent="AssetAcquisitionTypeMaintenanceDocument-EditAssetAcquisitionType-parentBean" />

  <bean id="AssetAcquisitionTypeMaintenanceDocument-EditAssetAcquisitionType-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Asset Acquisition Type" />
    <property name="title" value="Edit Asset Acquisition Type" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="acquisitionTypeCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="acquisitionTypeName"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="incomeAssetObjectCode" />
        <bean parent="MaintainableFieldDefinition"
              p:name="active" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>
</beans>
