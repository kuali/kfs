<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="ProposalAutoGeneratedMaintenanceDocument" parent="ProposalAutoGeneratedMaintenanceDocument-parentBean" />

  <bean id="ProposalAutoGeneratedMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.cg.bo.ProposalAutoGenerated" />
    <property name="maintainableClass" value="org.kuali.module.cg.maintenance.ProposalMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="ProposalAutoGeneratedMaintenanceDocument-ProposalMaintenance" />
        <ref bean="ProposalAutoGeneratedMaintenanceDocument-Organizations" />
        <ref bean="ProposalAutoGeneratedMaintenanceDocument-ProjectDirectors" />
        <ref bean="ProposalAutoGeneratedMaintenanceDocument-Subcontractors" />
        <ref bean="ProposalAutoGeneratedMaintenanceDocument-ResearchRisks" />
      </list>
    </property>
    <property name="defaultExistenceChecks" >
      <list>
        <bean parent="ReferenceDefinition"
              p:attributeName="proposalStatus"
              p:activeIndicatorAttributeName="rowActiveIndicator"
              p:attributeToHighlightOnFail="proposalStatusCode" />
        <bean parent="ReferenceDefinition"
              p:attributeName="agency"
              p:activeIndicatorAttributeName="historicalIndicator"
              p:attributeToHighlightOnFail="agencyNumber" />
        <bean parent="ReferenceDefinition"
              p:attributeName="proposalAwardType"
              p:activeIndicatorAttributeName="rowActiveIndicator"
              p:attributeToHighlightOnFail="proposalAwardTypeCode" />
        <bean parent="ReferenceDefinition"
              p:attributeName="proposalPurpose"
              p:activeIndicatorAttributeName="rowActiveIndicator"
              p:attributeToHighlightOnFail="proposalPurposeCode" />
        <bean parent="ReferenceDefinition"
              p:attributeName="federalPassThroughAgency"
              p:activeIndicatorAttributeName="historicalIndicator"
              p:attributeToHighlightOnFail="federalPassThroughAgencyNumber" />
        <bean parent="ReferenceDefinition"
              p:attributeName="cfda"
              p:activeIndicatorAttributeName="cfdaStatusCode"
              p:attributeToHighlightOnFail="cfdaNumber" />
        <bean parent="ReferenceDefinition"
              p:attributeName="organization"
              p:collection="proposalOrganizations"
              p:activeIndicatorAttributeName="organizationActiveIndicator"
              p:attributeToHighlightOnFail="organizationCode" />
        <bean parent="ReferenceDefinition"
              p:attributeName="subcontractor"
              p:collection="proposalSubcontractors"
              p:activeIndicatorAttributeName="active"
              p:attributeToHighlightOnFail="subcontractorNumber" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>proposalNumber</value>
      </list>
    </property>
    <property name="label" value="Proposal Auto-Generated" />
    <property name="shortLabel" value="Proposal Auto-Generated" />
    <property name="summary" value="Proposal maintenance document" />
    <property name="description" value="Document used to create or update Proposal objects" />
    <property name="documentTypeName" value="ProposalAutoGeneratedMaintenanceDocument" />
    <property name="businessRulesClass" value="org.kuali.module.cg.rules.ProposalRule" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="PRPL" />
    <property name="displayTopicFieldInNotes" value="true" />
    <property name="webScriptFiles" >
      <list>
        <value>../cg/proposalDocument.js</value>
      </list>
    </property>
    <property name="workflowProperties" >
      <ref bean="ProposalAutoGeneratedMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>kualiUniversalGroup</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="ProposalAutoGeneratedMaintenanceDocument-ProposalMaintenance" parent="ProposalAutoGeneratedMaintenanceDocument-ProposalMaintenance-parentBean" />

  <bean id="ProposalAutoGeneratedMaintenanceDocument-ProposalMaintenance-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Proposal Maintenance" />
    <property name="title" value="Proposal Maintenance" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="proposalNumber"
              p:required="true" />
      </list>
    </property>
  </bean>
  <bean id="ProposalAutoGeneratedMaintenanceDocument-Organizations" parent="ProposalAutoGeneratedMaintenanceDocument-Organizations-parentBean" />

  <bean id="ProposalAutoGeneratedMaintenanceDocument-Organizations-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Organizations" />
    <property name="title" value="Organizations" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableCollectionDefinition">
          <property name="name" value="proposalOrganizations" />
          <property name="businessObjectClass" value="org.kuali.module.cg.bo.ProposalOrganization" />
          <property name="maintainableFields" >
            <list>
              <bean parent="MaintainableFieldDefinition"
                    p:name="chartOfAccountsCode"
                    p:required="true" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="organizationCode"
                    p:required="true" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="organization.organizationName"
                    p:readOnly="true" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="proposalPrimaryOrganizationIndicator" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="newCollectionRecord" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="versionNumber" />
            </list>
          </property>
          <property name="attributeToHighlightOnDuplicateKey" value="organizationCode" />
        </bean>
      </list>
    </property>
  </bean>
  <bean id="ProposalAutoGeneratedMaintenanceDocument-ProjectDirectors" parent="ProposalAutoGeneratedMaintenanceDocument-ProjectDirectors-parentBean" />

  <bean id="ProposalAutoGeneratedMaintenanceDocument-ProjectDirectors-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Project Directors" />
    <property name="title" value="Project Directors" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableCollectionDefinition">
          <property name="name" value="proposalProjectDirectors" />
          <property name="businessObjectClass" value="org.kuali.module.cg.bo.ProposalProjectDirector" />
          <property name="maintainableFields" >
            <list>
              <bean parent="MaintainableFieldDefinition"
                    p:name="projectDirector.personUserIdentifier"
                    p:required="true" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="projectDirector.personName"
                    p:readOnly="true" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="personUniversalIdentifier"
                    p:readOnly="true" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="proposalPrimaryProjectDirectorIndicator" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="proposalProjectDirectorProjectTitle" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="newCollectionRecord" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="versionNumber" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>
  <bean id="ProposalAutoGeneratedMaintenanceDocument-Subcontractors" parent="ProposalAutoGeneratedMaintenanceDocument-Subcontractors-parentBean" />

  <bean id="ProposalAutoGeneratedMaintenanceDocument-Subcontractors-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Subcontractors" />
    <property name="title" value="Subcontractors" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableCollectionDefinition">
          <property name="name" value="proposalSubcontractors" />
          <property name="businessObjectClass" value="org.kuali.module.cg.bo.ProposalSubcontractor" />
          <property name="maintainableFields" >
            <list>
              <bean parent="MaintainableFieldDefinition"
                    p:name="subcontractorNumber" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="subcontractor.subcontractorName"
                    p:readOnly="true" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="proposalSubcontractorDescription" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="proposalSubcontractorAmount" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="newCollectionRecord" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>
  <bean id="ProposalAutoGeneratedMaintenanceDocument-ResearchRisks" parent="ProposalAutoGeneratedMaintenanceDocument-ResearchRisks-parentBean" />

  <bean id="ProposalAutoGeneratedMaintenanceDocument-ResearchRisks-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Research Risks" />
    <property name="title" value="Research Risks" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableCollectionDefinition">
          <property name="name" value="proposalResearchRisks" />
          <property name="businessObjectClass" value="org.kuali.module.cg.bo.ProposalResearchRisk" />
          <property name="includeAddLine" value="false" />
          <property name="maintainableFields" >
            <list>
              <bean parent="MaintainableFieldDefinition"
                    p:name="researchRiskType.researchRiskTypeDescription"
                    p:readOnly="true" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="researchRiskTypeIndicator" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="newCollectionRecord" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="versionNumber" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="researchRiskTypeCode" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="ProposalAutoGeneratedMaintenanceDocument-workflowProperties" parent="ProposalAutoGeneratedMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="ProposalAutoGeneratedMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
