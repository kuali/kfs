<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="AssignContractManagerDocument" parent="AssignContractManagerDocument-parentBean" />

  <bean id="AssignContractManagerDocument-parentBean" abstract="true" parent="TransactionalDocumentEntry">
    <property name="label" value="Contract Manager Assignment" />
    <property name="shortLabel" value="Contract Manager Assignment" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-PA"
            p:parameterName="ASSIGN_CONTRACT_MANAGER"
            p:parameterDetailType="AssignContractManager" />
    </property>
    <property name="documentTypeName" value="AssignContractManagerDocument" />
    <property name="documentClass" value="org.kuali.module.purap.document.AssignContractManagerDocument" />
    <property name="businessRulesClass" value="org.kuali.module.purap.rules.AssignContractManagerDocumentRule" />
    <property name="documentAuthorizerClass" value="org.kuali.module.purap.document.authorization.AssignContractManagerDocumentAuthorizer" />
    <property name="documentTypeCode" value="ACM" />
    <property name="workflowProperties" >
      <ref bean="AssignContractManagerDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>PA_PUR_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
    <property name="attributes" >
      <list>
        <ref bean="AssignContractManagerDocument-requisitionNumber" />
        <ref bean="AssignContractManagerDocument-deliveryCampusCode" />
        <ref bean="AssignContractManagerDocument-vendorName" />
        <ref bean="AssignContractManagerDocument-generalDescription" />
        <ref bean="AssignContractManagerDocument-requisitionTotalAmount" />
        <ref bean="AssignContractManagerDocument-requisitionCreateDate" />
        <ref bean="AssignContractManagerDocument-firstItemDescription" />
        <ref bean="AssignContractManagerDocument-firstItemCommodityCode" />
        <ref bean="AssignContractManagerDocument-firstObjectCode" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="AssignContractManagerDocument-requisitionNumber" parent="AssignContractManagerDocument-requisitionNumber-parentBean" />

  <bean id="AssignContractManagerDocument-requisitionNumber-parentBean" abstract="true" parent="PurApGenericAttributes-purapDocumentIdentifier">
    <property name="name" value="requisitionNumber" />
    <property name="label" value="Requisition Number" />
    <property name="shortLabel" value="Requisition Number" />
  </bean>
  <bean id="AssignContractManagerDocument-deliveryCampusCode" parent="AssignContractManagerDocument-deliveryCampusCode-parentBean" />

  <bean id="AssignContractManagerDocument-deliveryCampusCode-parentBean" abstract="true" parent="VendorGenericAttributes-campusCode">
    <property name="name" value="deliveryCampusCode" />
    <property name="label" value="Delivery Campus" />
    <property name="shortLabel" value="Delivery Campus" />
  </bean>
  <bean id="AssignContractManagerDocument-vendorName" parent="AssignContractManagerDocument-vendorName-parentBean" />

  <bean id="AssignContractManagerDocument-vendorName-parentBean" abstract="true" parent="VendorDetail-vendorName">
    <property name="label" value="Suggested Vendor" />
  </bean>
  <bean id="AssignContractManagerDocument-generalDescription" parent="AssignContractManagerDocument-generalDescription-parentBean" />

  <bean id="AssignContractManagerDocument-generalDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="generalDescription" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="General Description" />
    <property name="shortLabel" value="General Description" />
    <property name="maxLength" value="20" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="22" />
    </property>
  </bean>
  <bean id="AssignContractManagerDocument-requisitionTotalAmount" parent="AssignContractManagerDocument-requisitionTotalAmount-parentBean" />

  <bean id="AssignContractManagerDocument-requisitionTotalAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="requisitionTotalAmount" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Requisition Total Amount" />
    <property name="shortLabel" value="Total" />
    <property name="maxLength" value="20" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="22" />
    </property>
  </bean>
  <bean id="AssignContractManagerDocument-requisitionCreateDate" parent="AssignContractManagerDocument-requisitionCreateDate-parentBean" />

  <bean id="AssignContractManagerDocument-requisitionCreateDate-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="requisitionCreateDate" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Requisition Create Date" />
    <property name="shortLabel" value="Create Date" />
    <property name="maxLength" value="20" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="22" />
    </property>
  </bean>
  <bean id="AssignContractManagerDocument-firstItemDescription" parent="AssignContractManagerDocument-firstItemDescription-parentBean" />

  <bean id="AssignContractManagerDocument-firstItemDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="firstItemDescription" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="First Item Description" />
    <property name="shortLabel" value="First Item Description" />
    <property name="maxLength" value="20" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="22" />
    </property>
  </bean>
  <bean id="AssignContractManagerDocument-firstItemCommodityCode" parent="AssignContractManagerDocument-firstItemCommodityCode-parentBean" />

  <bean id="AssignContractManagerDocument-firstItemCommodityCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="firstItemCommodityCode" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="First Item Commodity Code" />
    <property name="shortLabel" value="First Item Commodity Code" />
    <property name="maxLength" value="20" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="22" />
    </property>
  </bean>
  <bean id="AssignContractManagerDocument-firstObjectCode" parent="AssignContractManagerDocument-firstObjectCode-parentBean" />

  <bean id="AssignContractManagerDocument-firstObjectCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="firstObjectCode" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="First Object Code" />
    <property name="shortLabel" value="First Object Code" />
    <property name="maxLength" value="20" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="22" />
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="AssignContractManagerDocument-workflowProperties" parent="AssignContractManagerDocument-workflowProperties-parentBean" />

  <bean id="AssignContractManagerDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
    <property name="workflowPropertyGroups" >
      <list>
        <bean parent="WorkflowPropertyGroup">
          <property name="workflowProperties" >
            <list>
              <bean parent="WorkflowProperty"
                    p:path="documentHeader" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>
</beans>
