<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../core/datadictionary/dataDictionary-1_0.dtd">
<dictionaryEntry>

    <businessObject businessObjectClass="org.kuali.module.kra.budget.bo.IndirectCostLookup">
        <customAttributeEnabled>true</customAttributeEnabled>
        <inquiry>
            <title>Indirect Cost Lookup Inquiry</title>

            <inquiryFields>
                <field attributeName="budgetOnCampusIndicator" />
                <field attributeName="budgetPurposeCode" />
                <field attributeName="budgetIndirectCostRate" />
            </inquiryFields>
        </inquiry>

        <lookup>
            <title>Indirect Cost Lookup Lookup</title>

            <menubar>
                <![CDATA[<a href="maintenance.do?methodToCall=start&businessObjectClassName=org.kuali.module.kra.budget.bo.IndirectCostLookup">Create New</a>]]>
            </menubar>

            <instructions>Lookup a Indirect Cost Lookup</instructions>

            <defaultSort attributeName="budgetPurposeCode" sortAscending="true" />

            <lookupFields>
                <lookupField attributeName="budgetOnCampusIndicator" required="false" />
                <lookupField attributeName="budgetPurposeCode" required="false" />
                <lookupField attributeName="budgetIndirectCostRate" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="budgetOnCampusIndicator" />
                <field attributeName="budgetPurposeCode" />
                <field attributeName="budgetIndirectCostRate" />
            </resultFields>
        </lookup>

        <attributes>
            <attribute name="budgetOnCampusIndicator" forceUppercase="false">
                <label>Budget On Campus Indicator</label>
                <shortLabel>on camput indicator</shortLabel>

                <maxLength>1</maxLength>
                <required>true</required>
                <control>
                    <checkbox />
                </control>

                <summary>Budget On Campus Indicator.</summary>
                <description>Budget On Campus Indicator.</description>
            </attribute>

            <attributeReference name="budgetPurposeCode" sourceClassName="org.kuali.module.kra.budget.bo.BudgetPurposeCode" sourceAttributeName="budgetPurposeCode">
                <label>Budget Purpose Code</label>
                <shortLabel>BdgtPrpsCd</shortLabel>
                <summary>Budget Purpose Code</summary>
                <description>Budget Purpose Code</description>
            </attributeReference>

            <attribute name="budgetIndirectCostRate">
                <label>Budget Indirect Cost Rate</label>
                <shortLabel>BdgtIdcCstRt</shortLabel>

                <maxLength>5</maxLength>
                <validationPattern>
                    <numeric />
                </validationPattern>

                <control>
                    <text size="5" />
                </control>

                <summary>Budget Indirect Cost Rate.</summary>
                <description>Budget Indirect Cost Rate.</description>
            </attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />
        </attributes>
    </businessObject>
</dictionaryEntry>
