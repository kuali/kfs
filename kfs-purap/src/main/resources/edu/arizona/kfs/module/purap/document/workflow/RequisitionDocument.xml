<?xml version='1.0' encoding='UTF-8'?> 
<data xmlns="ns:workflow" xmlns:fo="http://www.w3.org/1999/XSL/Format" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
  <documentTypes xmlns="ns:workflow/DocumentType" xsi:schemaLocation="ns:workflow/DocumentType resource:DocumentType">
    <documentType>
      <name>REQS</name>
      <parent>PUR</parent>
      <docHandler>${application.url}/purapRequisition.do?methodToCall=docHandler</docHandler>
      <routePaths>
        <routePath>
          <start name="AdHoc" nextNode="Organization"/>
          <requests name="Organization" nextNode="HasAccountingLines"/>
          <split name="HasAccountingLines" nextNode="SubAccount">
            <branch name="True">
              <simple name="NoOpHasAccountingLines" nextNode="JoinHasAccountingLines"/>
            </branch>
            <branch name="False">
              <requests name="Initiator" nextNode="JoinHasAccountingLines"/>
            </branch>
            <join name="JoinHasAccountingLines"/>
          </split>
          <requests name="SubAccount" nextNode="Account"/>
          <requests name="Account" nextNode="AccountingOrganizationHierarchy"/>
          <requests name="AccountingOrganizationHierarchy" nextNode="ObjectSubTypeCode"/>
          <requests name="ObjectSubTypeCode" nextNode="Commodity" />          
          <requests name="Commodity" nextNode="AmountRequiresSeparationOfDutiesReview"/>
          <split name="AmountRequiresSeparationOfDutiesReview">
            <branch name="True">
              <requests name="SeparationOfDuties" nextNode="Join"/>
            </branch>
            <branch name="False">
              <simple name="NoOp" nextNode="Join"/>
            </branch>
            <join name="Join"/>
          </split>
        </routePath>
      </routePaths>
      <routeNodes>
        <start name="AdHoc"/>
        <role name="Organization">
          <qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
          <activationType>P</activationType>
        </role>
        <split name="HasAccountingLines">
          <type>org.kuali.kfs.sys.document.workflow.SimpleBooleanSplitNode</type>
        </split>
        <role name="SubAccount">
          <qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
          <activationType>P</activationType>
        </role>
        <role name="Account">
          <qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
          <activationType>P</activationType>
        </role>
        <role name="AccountingOrganizationHierarchy">
          <qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
          <activationType>P</activationType>
        </role>
        <role name="ObjectSubTypeCode">
          <qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
          <activationType>P</activationType>
        </role>
        <role name="Commodity">
          <qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
          <activationType>P</activationType>
        </role>
        <role name="SeparationOfDuties">
          <qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
          <activationType>P</activationType>
        </role>
        <split name="AmountRequiresSeparationOfDutiesReview">
          <type>org.kuali.kfs.sys.document.workflow.SimpleBooleanSplitNode</type>
        </split>
        <simple name="NoOp">
          <type>org.kuali.rice.kew.engine.node.NoOpNode</type>
        </simple>
        <join name="Join"/>
        <role name="Initiator">
          <qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
          <activationType>P</activationType>
        </role>
        <simple name="NoOpHasAccountingLines">
          <type>org.kuali.rice.kew.engine.node.NoOpNode</type>
        </simple>
        <join name="JoinHasAccountingLines"/>
      </routeNodes>
    </documentType>
  </documentTypes>
</data>
