		            <maintainableField name="accountGuideline.accountExpenseGuidelineText" required="false" />
		            <maintainableField name="accountGuideline.accountIncomeGuidelineText" required="false" />
		            <maintainableField name="accountGuideline.accountPurposeText" required="false" />
