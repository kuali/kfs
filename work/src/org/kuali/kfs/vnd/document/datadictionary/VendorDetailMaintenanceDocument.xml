  <bean id="VendorDetailMaintenanceDocument" parent="VendorDetailMaintenanceDocument-parentBean" />

  <bean id="VendorDetailMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.vendor.bo.VendorDetail" />
    <property name="maintainableClass" value="org.kuali.module.vendor.maintenance.VendorMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="VendorDetailMaintenanceDocument-Vendor" />
        <ref bean="VendorDetailMaintenanceDocument-Address" />
        <ref bean="VendorDetailMaintenanceDocument-Contact" />
        <ref bean="VendorDetailMaintenanceDocument-SupplierDiversity" />
        <ref bean="VendorDetailMaintenanceDocument-ShippingSpecialConditions" />
        <ref bean="VendorDetailMaintenanceDocument-VendorCommodityCodes" />
        <ref bean="VendorDetailMaintenanceDocument-SearchAlias" />
        <ref bean="VendorDetailMaintenanceDocument-VendorPhoneNumber" />
        <ref bean="VendorDetailMaintenanceDocument-CustomerNumber" />
        <ref bean="VendorDetailMaintenanceDocument-Contracts" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>vendorHeaderGeneratedIdentifier</value>
        <value>vendorDetailAssignedIdentifier</value>
      </list>
    </property>
    <property name="label" value="Vendor" />
    <property name="shortLabel" value="Vendor" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-VN"
            p:parameterName="VENDOR"
            p:parameterDetailType="VendorDetail" />
    </property>
    <property name="documentTypeName" value="VendorDetailMaintenanceDocument" />
    <property name="businessRulesClass" value="org.kuali.module.vendor.rules.VendorRule" />
    <property name="documentAuthorizerClass" value="org.kuali.module.vendor.document.authorization.VendorDocumentAuthorizer" />
    <property name="preRulesCheckClass" value="org.kuali.module.vendor.rules.VendorPreRules" />
    <property name="documentTypeCode" value="PVEN" />
    <property name="workflowProperties" >
      <ref bean="VendorDetailMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>kualiUniversalGroup</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

