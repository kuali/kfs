                <shortLabel>Calc Amt</shortLabel>

                <maxLength>22</maxLength>

                <required>false</required>

                <control>
                    <text size="9" />
                </control>

                <summary>
                    <![CDATA[&nbsp;]]>
                </summary>
                <description>Use the "calculate" button to calculate the dollar amount for mileage involved in the Non-Employee Travel.</description>
            </attribute>

            <attribute name="disbVchrPersonalCarAmount">
