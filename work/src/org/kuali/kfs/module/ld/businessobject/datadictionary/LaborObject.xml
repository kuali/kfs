<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="LaborObject" parent="LaborObject-parentBean" />

  <bean id="LaborObject-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.labor.bo.LaborObject" />
    <property name="inquiryDefinition" >
      <ref bean="LaborObject-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="LaborObject-lookupDefinition" />
    </property>
    <property name="objectLabel" value="Labor Object Code" />
    <property name="attributes" >
      <list>
        <ref bean="LaborObject-chartOfAccountsCode" />
        <ref bean="LaborObject-detailPositionRequiredIndicator" />
        <ref bean="LaborObject-financialObjectCode" />
        <ref bean="LaborObject-financialObjectFringeOrSalaryCode" />
        <ref bean="LaborObject-financialObjectHoursRequiredIndicator" />
        <ref bean="LaborObject-financialObjectPayTypeCode" />
        <ref bean="LaborObject-positionObjectGroupCode" />
        <ref bean="LaborObject-universityFiscalYear" />
        <ref bean="LaborObject-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="LaborObject-chartOfAccountsCode" parent="LaborObject-chartOfAccountsCode-parentBean" />

  <bean id="LaborObject-chartOfAccountsCode-parentBean" abstract="true" parent="Chart-chartOfAccountsCode">
    <property name="label" value="Chart Of Accounts Code" />
  </bean>
  <bean id="LaborObject-detailPositionRequiredIndicator" parent="LaborObject-detailPositionRequiredIndicator-parentBean" />

  <bean id="LaborObject-detailPositionRequiredIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="detailPositionRequiredIndicator" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Detail Position Required Indicator" />
    <property name="shortLabel" value="Indicator" />
    <property name="maxLength" value="1" />
    <property name="control" >
      <bean parent="CheckboxControlDefinition" />
    </property>
  </bean>
  <bean id="LaborObject-financialObjectCode" parent="LaborObject-financialObjectCode-parentBean" />

  <bean id="LaborObject-financialObjectCode-parentBean" abstract="true" parent="ObjectCode-financialObjectCode">
    <property name="label" value="Financial Object Code" />
    <property name="shortLabel" value="Code" />
  </bean>
  <bean id="LaborObject-financialObjectFringeOrSalaryCode" parent="LaborObject-financialObjectFringeOrSalaryCode-parentBean" />

  <bean id="LaborObject-financialObjectFringeOrSalaryCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialObjectFringeOrSalaryCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Financial Object Fringe Or Salary Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="1" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="3" />
    </property>
  </bean>
  <bean id="LaborObject-financialObjectHoursRequiredIndicator" parent="LaborObject-financialObjectHoursRequiredIndicator-parentBean" />

  <bean id="LaborObject-financialObjectHoursRequiredIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialObjectHoursRequiredIndicator" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Financial Object Hours Required Indicator" />
    <property name="shortLabel" value="Indicator" />
    <property name="maxLength" value="1" />
    <property name="control" >
      <bean parent="CheckboxControlDefinition" />
    </property>
  </bean>
  <bean id="LaborObject-financialObjectPayTypeCode" parent="LaborObject-financialObjectPayTypeCode-parentBean" />

  <bean id="LaborObject-financialObjectPayTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialObjectPayTypeCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Financial Object Pay Type Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="1" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="3" />
    </property>
  </bean>
  <bean id="LaborObject-positionObjectGroupCode" parent="LaborObject-positionObjectGroupCode-parentBean" />

  <bean id="LaborObject-positionObjectGroupCode-parentBean" abstract="true" parent="PositionObjectGroup-positionObjectGroupCode">
  </bean>
  <bean id="LaborObject-universityFiscalYear" parent="LaborObject-universityFiscalYear-parentBean" />

  <bean id="LaborObject-universityFiscalYear-parentBean" abstract="true" parent="AttributeReferenceDummy-genericFiscalYear">
    <property name="name" value="universityFiscalYear" />
  </bean>
  <bean id="LaborObject-versionNumber" parent="LaborObject-versionNumber-parentBean" />

  <bean id="LaborObject-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="LaborObject-inquiryDefinition" parent="LaborObject-inquiryDefinition-parentBean" />

  <bean id="LaborObject-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Labor Object Code Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="chartOfAccountsCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="detailPositionRequiredIndicator" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialObjectCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialObjectFringeOrSalaryCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialObjectHoursRequiredIndicator" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialObjectPayTypeCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="positionObjectGroupCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="universityFiscalYear" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="LaborObject-lookupDefinition" parent="LaborObject-lookupDefinition-parentBean" />

  <bean id="LaborObject-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Labor Object Code Lookup" />
    <property name="instructions" value="Look up a Labor Object" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalYear" />
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectFringeOrSalaryCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectPayTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="positionObjectGroupCode" />
        <bean parent="FieldDefinition"
              p:attributeName="detailPositionRequiredIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectHoursRequiredIndicator" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="detailPositionRequiredIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectFringeOrSalaryCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectHoursRequiredIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectPayTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="positionObjectGroupCode" />
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalYear" />
      </list>
    </property>
  </bean>
</beans>
