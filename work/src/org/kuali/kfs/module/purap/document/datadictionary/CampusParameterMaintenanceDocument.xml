<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="CampusParameterMaintenanceDocument" parent="CampusParameterMaintenanceDocument-parentBean" />

  <bean id="CampusParameterMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.purap.bo.CampusParameter" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="CampusParameterMaintenanceDocument-EditCampusParameter" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>campusCode</value>
      </list>
    </property>
    <property name="label" value="Campus Parameter" />
    <property name="shortLabel" value="Campus Parameter" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-PA"
            p:parameterName="CAMPUS_PARAMETER"
            p:parameterDetailType="CampusParameter" />
    </property>
    <property name="documentTypeName" value="CampusParameterMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="PMCP" />
    <property name="workflowProperties" >
      <ref bean="CampusParameterMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>PA_PUR_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="CampusParameterMaintenanceDocument-EditCampusParameter" parent="CampusParameterMaintenanceDocument-EditCampusParameter-parentBean" />

  <bean id="CampusParameterMaintenanceDocument-EditCampusParameter-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Campus Parameter" />
    <property name="title" value="Edit Campus Parameter" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="campusCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="campusPurchasingDirectorName" />
        <bean parent="MaintainableFieldDefinition"
              p:name="campusPurchasingDirectorTitle" />
        <bean parent="MaintainableFieldDefinition"
              p:name="campusAccountsPayableEmailAddress" />
        <bean parent="MaintainableFieldDefinition"
              p:name="purchasingInstitutionName" />
        <bean parent="MaintainableFieldDefinition"
              p:name="purchasingDepartmentName" />
        <bean parent="MaintainableFieldDefinition"
              p:name="purchasingDepartmentLine1Address" />
        <bean parent="MaintainableFieldDefinition"
              p:name="purchasingDepartmentLine2Address" />
        <bean parent="MaintainableFieldDefinition"
              p:name="purchasingDepartmentCityName" />
        <bean parent="MaintainableFieldDefinition"
              p:name="purchasingDepartmentStateCode" />
        <bean parent="MaintainableFieldDefinition"
              p:name="purchasingDepartmentZipCode" />
        <bean parent="MaintainableFieldDefinition"
              p:name="purchasingDepartmentCountryCode" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="CampusParameterMaintenanceDocument-workflowProperties" parent="CampusParameterMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="CampusParameterMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
