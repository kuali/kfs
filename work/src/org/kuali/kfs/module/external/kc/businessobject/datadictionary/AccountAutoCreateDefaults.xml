 <bean id="AccountAutoCreateDefaults-inquirySectionDefinition-cgAccountDefaultsGuidelinesandPurpose" parent="AccountAutoCreateDefaults-inquirySectionDefinition-cgAccountDefaultsGuidelinesandPurpose-parentBean"/>
 <bean id="AccountAutoCreateDefaults-inquirySectionDefinition-cgAccountDefaultsGuidelinesandPurpose-parentBean" parent="InquirySectionDefinition" abstract="true" p:title="CG Account Defaults Guidelines and Purpose">        
