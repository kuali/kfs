<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="NonpersonnelCategoryMaintenanceDocument" parent="NonpersonnelCategoryMaintenanceDocument-parentBean" />

  <bean id="NonpersonnelCategoryMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.kra.budget.bo.NonpersonnelCategory" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="NonpersonnelCategoryMaintenanceDocument-EditNonpersonnelCode" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>code</value>
      </list>
    </property>
    <property name="label" value="Non-Personnel Category" />
    <property name="shortLabel" value="Non-Personnel Category" />
    <property name="summary" value="Nonpersonnel Category maintenance document" />
    <property name="description" value="Document used to create or update Nonpersonnel Category objects" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-RA"
            p:parameterName="NONPERSONNEL_CATEGORY"
            p:parameterDetailType="NonpersonnelCategory" />
    </property>
    <property name="documentTypeName" value="NonpersonnelCategoryMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="NPC" />
    <property name="workflowProperties" >
      <ref bean="NonpersonnelCategoryMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>RA_PRE_AWARD_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="NonpersonnelCategoryMaintenanceDocument-EditNonpersonnelCode" parent="NonpersonnelCategoryMaintenanceDocument-EditNonpersonnelCode-parentBean" />

  <bean id="NonpersonnelCategoryMaintenanceDocument-EditNonpersonnelCode-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Nonpersonnel Code" />
    <property name="title" value="Edit Nonpersonnel Code" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="code"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="name"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="sortNumber" />
        <bean parent="MaintainableFieldDefinition"
              p:name="active"
              p:defaultValue="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="NonpersonnelCategoryMaintenanceDocument-workflowProperties" parent="NonpersonnelCategoryMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="NonpersonnelCategoryMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
