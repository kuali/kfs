<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="NonResidentAlienTaxPercentMaintenanceDocument" parent="NonResidentAlienTaxPercentMaintenanceDocument-parentBean" />

  <bean id="NonResidentAlienTaxPercentMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.financial.bo.NonResidentAlienTaxPercent" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="NonResidentAlienTaxPercentMaintenanceDocument-EditNonResidentAlienTaxPercent" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>incomeClassCode</value>
        <value>incomeTaxTypeCode</value>
      </list>
    </property>
    <property name="label" value="Non-Resident Alien Tax Percent" />
    <property name="shortLabel" value="Non-Resident Alien Tax Percent" />
    <property name="summary" value="Non resident alien tax percent" />
    <property name="description" value="Document used to create or update Tax Percent objects" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-FP"
            p:parameterName="NON_RESIDENT_ALIEN_TAX_PERCENT"
            p:parameterDetailType="NonResidentAlienTaxPercent" />
    </property>
    <property name="documentTypeName" value="NonResidentAlienTaxPercentMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="DVTX" />
    <property name="workflowProperties" >
      <ref bean="NonResidentAlienTaxPercentMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>SY_MAINTAINENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="NonResidentAlienTaxPercentMaintenanceDocument-EditNonResidentAlienTaxPercent" parent="NonResidentAlienTaxPercentMaintenanceDocument-EditNonResidentAlienTaxPercent-parentBean" />

  <bean id="NonResidentAlienTaxPercentMaintenanceDocument-EditNonResidentAlienTaxPercent-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Non Resident Alien Tax Percent:" />
    <property name="title" value="Edit Non Resident Alien Tax Percent:" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="incomeClassCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="incomeTaxTypeCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="incomeTaxPercent"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="NonResidentAlienTaxPercentMaintenanceDocument-workflowProperties" parent="NonResidentAlienTaxPercentMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="NonResidentAlienTaxPercentMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
