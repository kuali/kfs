<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.cg.bo.ProposalOrganization">
        <customAttributeEnabled>false</customAttributeEnabled>

        <inquiry>
            <title>Proposal Organization Inquiry</title>

            <inquiryFields>
                <field attributeName="chartOfAccountsCode" />
                <field attributeName="organizationCode" />
                <field attributeName="proposalNumber" />
                <field attributeName="proposalPrimaryOrganizationIndicator" />
            </inquiryFields>
        </inquiry>

        <lookup>
            <title>Proposal Organization Lookup</title>

			<menubar><![CDATA[<a href="maintenance.do?methodToCall=start&businessObjectClassName=org.kuali.module.cg.bo.ProposalOrganization">Create New</a>]]></menubar>

            <instructions>Look up a Proposal Organization</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="chartOfAccountsCode" />
                    <sortAttribute attributeName="organizationCode" />
                    <sortAttribute attributeName="proposalNumber" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="chartOfAccountsCode" required="false" />
                <lookupField attributeName="organizationCode" required="false" />
                <lookupField attributeName="proposalNumber" required="false" />
                <lookupField attributeName="proposalPrimaryOrganizationIndicator" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="chartOfAccountsCode" />
                <field attributeName="organizationCode" />
                <field attributeName="proposalNumber" />
                <field attributeName="proposalPrimaryOrganizationIndicator" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="chartOfAccountsCode" forceUppercase="true">
                <label>Chart Of Accounts Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="4" />
                </control>

                <summary>chartOfAccountsCode</summary>
                <description>chartOfAccountsCode description...</description>
			</attribute>

            <attribute name="organizationCode" forceUppercase="true">
                <label>Organization Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="6" />
                </control>

                <summary>organizationCode</summary>
                <description>organizationCode description...</description>
			</attribute>

            <attribute name="proposalNumber" forceUppercase="true">
                <label>Proposal Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>12</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="14" />
                </control>

                <summary>proposalNumber</summary>
                <description>proposalNumber description...</description>
			</attribute>

            <attribute name="proposalPrimaryOrganizationIndicator" forceUppercase="true">
                <label>Proposal Primary Organization Indicator</label>
                <shortLabel>Indicator</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <checkbox />
                </control>

                <summary>proposalPrimaryOrganizationIndicator</summary>
                <description>proposalPrimaryOrganizationIndicator description...</description>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
