<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="ns:workflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
  <documentTypes xmlns="ns:workflow/DocumentType" xsi:schemaLocation="ns:workflow/DocumentType resource:DocumentType">
    <documentType>
      <name>YearEndBudgetAdjustmentDocument</name>
      <parent>KualiFinancialYearEndDocument</parent>
      <description>YearEndBudgetAdjustmentDocument</description>
      <label>YearEndBudgetAdjustmentDocument</label>
      <postProcessorName>org.kuali.workflow.postprocessor.KualiPostProcessor</postProcessorName>
      <superUserWorkgroupName>KUALI_ROLE_SUPERVISOR</superUserWorkgroupName>
      <blanketApproveWorkgroupName>KUALI_ROLE_ADMINISTRATOR</blanketApproveWorkgroupName>
      <defaultExceptionWorkgroupName>KUALI_ROLE_SUPERVISOR</defaultExceptionWorkgroupName>
      <docHandler>${application.url}/financialYearEndBudgetAdjustment.do?methodToCall=docHandler</docHandler>
      <active>true</active>
      <policies>
        <policy>
          <name>DEFAULT_APPROVE</name>
          <value>true</value>
        </policy>
      </policies>
      <routingVersion>1</routingVersion>
      <routePaths>
        <routePath>
          <start name="Adhoc Routing" nextNode="Org Review" />
          <requests name="Org Review" nextNode="Award Workgroup" />
          <requests name="Award Workgroup" />
        </routePath>
      </routePaths>
      <routeNodes>
        <start name="Adhoc Routing">
          <activationType>S</activationType>
          <mandatoryRoute>false</mandatoryRoute>
          <finalApproval>false</finalApproval>
        </start>
        <requests name="Org Review">
          <activationType>S</activationType>
          <ruleTemplate>KualiOrgReviewTemplate</ruleTemplate>
          <mandatoryRoute>false</mandatoryRoute>
          <finalApproval>false</finalApproval>
        </requests>
        <requests name="Award Workgroup">
          <activationType>S</activationType>
          <ruleTemplate>KualiAwardWorkgroupTemplate</ruleTemplate>
          <mandatoryRoute>false</mandatoryRoute>
          <finalApproval>false</finalApproval>
        </requests>
      </routeNodes>
    </documentType>
  </documentTypes>
</data>
