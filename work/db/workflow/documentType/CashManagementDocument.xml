<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="ns:workflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
  <documentTypes xmlns="ns:workflow/DocumentType" xsi:schemaLocation="ns:workflow/DocumentType resource:DocumentType">
    <documentType>
      <name>CashManagementDocument</name>
      <parent>KualiBaseDocument</parent>
      <label>Cash Management</label>
      <postProcessorName>org.kuali.workflow.postprocessor.KualiPostProcessor</postProcessorName>
      <superUserWorkgroupName>KUALI_ROLE_SUPERVISOR</superUserWorkgroupName>
      <blanketApproveWorkgroupName>KUALI_ROLE_ADMINISTRATOR</blanketApproveWorkgroupName>
      <defaultExceptionWorkgroupName>KUALI_ROLE_SUPERVISOR</defaultExceptionWorkgroupName>
      <docHandler>${application.url}/financialCashManagement.do?methodToCall=docHandler</docHandler>
      <active>true</active>
      <policies>
        <policy>
          <name>DEFAULT_APPROVE</name>
          <value>true</value>
        </policy>
        <policy>
          <name>INITIATOR_MUST_BLANKET_APPROVE</name>
          <value>false</value>
        </policy>
        <policy>
          <name>INITIATOR_MUST_CANCEL</name>
          <value>false</value>
        </policy>
        <policy>
          <name>INITIATOR_MUST_ROUTE</name>
          <value>false</value>
        </policy>
        <policy>
          <name>INITIATOR_MUST_SAVE</name>
          <value>false</value>
        </policy>
      </policies>
      <routingVersion>1</routingVersion>
      <routePaths>
        <routePath>
          <start name="Adhoc Routing" nextNode="Initiator Campus" />
          <requests name="Initiator Campus" />
        </routePath>
      </routePaths>
      <routeNodes>
        <start name="Adhoc Routing">
          <activationType>S</activationType>
          <mandatoryRoute>false</mandatoryRoute>
          <finalApproval>false</finalApproval>
        </start>
        <requests name="Initiator Campus">
          <activationType>S</activationType>
          <ruleTemplate>KualiInitiatorCampusRuleTemplate</ruleTemplate>
          <mandatoryRoute>false</mandatoryRoute>
          <finalApproval>false</finalApproval>
        </requests>
      </routeNodes>
    </documentType>
  </documentTypes>
</data>
