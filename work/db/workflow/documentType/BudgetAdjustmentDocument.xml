<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="ns:workflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
  <documentTypes xmlns="ns:workflow/DocumentType" xsi:schemaLocation="ns:workflow/DocumentType resource:DocumentType">
    <documentType>
      <name>BudgetAdjustmentDocument</name>
      <parent>KualiFinancialDocument</parent>
      <label>Budget Adjustment</label>
      <postProcessorName>org.kuali.workflow.postprocessor.KualiPostProcessor</postProcessorName>
      <superUserWorkgroupName>SY_FUNCTIONAL_SUPER_USERS</superUserWorkgroupName>
      <blanketApproveWorkgroupName>SY_BLANKET_APPROVERS</blanketApproveWorkgroupName>
      <defaultExceptionWorkgroupName>SY_FUNCTIONAL_SUPER_USERS</defaultExceptionWorkgroupName>
      <docHandler>${application.url}/financialBudgetAdjustment.do?methodToCall=docHandler</docHandler>
      <active>true</active>
      <policies>
        <policy>
          <name>DEFAULT_APPROVE</name>
          <value>true</value>
        </policy>
      </policies>
      <routingVersion>2</routingVersion>
      <routePaths>
        <routePath>
          <start name="Adhoc Routing" nextNode="Approval No Approval Split" />
          <split name="Approval No Approval Split" nextNode="Do More Nothing">
            <branch name="ApprovalBranch">
              <requests name="Account Review" nextNode="Org Review" />
              <requests name="Org Review" nextNode="Subfund" />
              <requests name="Subfund" nextNode="Award Workgroup" />
              <requests name="Award Workgroup" nextNode="Join" />
            </branch>
            <branch name="NoApprovalBranch">
              <simple name="Do Nothing" nextNode="Join" />
            </branch>
            <join name="Join" />
          </split>
          <simple name="Do More Nothing" />
        </routePath>
      </routePaths>
      <routeNodes>
        <start name="Adhoc Routing">
          <activationType>S</activationType>
          <mandatoryRoute>false</mandatoryRoute>
          <finalApproval>false</finalApproval>
        </start>
        <split name="Approval No Approval Split">
          <activationType>S</activationType>
          <mandatoryRoute>false</mandatoryRoute>
          <finalApproval>false</finalApproval>
          <type>org.kuali.workflow.node.BudgetAdjustmentDocumentApprovalNoApprovalSplitNode</type>
        </split>
        <requests name="Account Review">
          <activationType>S</activationType>
          <ruleTemplate>KualiAccountTemplate</ruleTemplate>
          <mandatoryRoute>false</mandatoryRoute>
          <finalApproval>false</finalApproval>
        </requests>
        <requests name="Org Review">
          <activationType>S</activationType>
          <ruleTemplate>KualiOrgReviewTemplate</ruleTemplate>
          <mandatoryRoute>false</mandatoryRoute>
          <finalApproval>false</finalApproval>
        </requests>
        <requests name="Subfund">
          <activationType>S</activationType>
          <ruleTemplate>KualiSubFundTemplate</ruleTemplate>
          <mandatoryRoute>false</mandatoryRoute>
          <finalApproval>false</finalApproval>
        </requests>
        <requests name="Award Workgroup">
          <activationType>S</activationType>
          <ruleTemplate>KualiAwardWorkgroupTemplate</ruleTemplate>
          <mandatoryRoute>false</mandatoryRoute>
          <finalApproval>false</finalApproval>
        </requests>
        <join name="Join">
          <activationType>S</activationType>
          <mandatoryRoute>false</mandatoryRoute>
          <finalApproval>false</finalApproval>
        </join>
        <simple name="Do Nothing">
          <activationType>S</activationType>
          <mandatoryRoute>false</mandatoryRoute>
          <finalApproval>false</finalApproval>
          <type>edu.iu.uis.eden.engine.node.NoOpNode</type>
        </simple>
        <simple name="Do More Nothing">
          <activationType>S</activationType>
          <mandatoryRoute>false</mandatoryRoute>
          <finalApproval>false</finalApproval>
          <type>edu.iu.uis.eden.engine.node.NoOpNode</type>
        </simple>
      </routeNodes>
    </documentType>
  </documentTypes>
</data>
