<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xmlns:dd="http://rice.kuali.org/dd"
	xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
http://rice.kuali.org/dd http://rice.kuali.org/dd/dd.xsd">

	<bean id="ArizonaKfsKimAttributes" parent="ArizonaKfsKimAttributes-parentBean" />
	
	<bean id="ArizonaKfsKimAttributes-parentBean" abstract="true" parent="KimAttributes-parentBean"
		p:businessObjectClass="edu.arizona.kfs.sys.identity.ArizonaKfsKimAttributes"
		p:objectLabel="KFS Core KIM Attributes">
		<property name="attributes">
			<list merge="true">
				<ref bean="ArizonaKfsKimAttributes-objectSubTypeCode" />
			</list>
		</property>
		<property name="relationships">
			<list merge="true">
 				<dd:relationship objectAttribute="objectSubType">
					<dd:primitiveAttribute source="financialObjectSubTypeCode"
						target="code" />
				</dd:relationship>
			</list>
		</property>
	</bean>

	<bean id="ArizonaKfsKimAttributes-objectSubTypeCode" parent="ObjectCode-financialObjectSubTypeCode-parentBean" />	

</beans>
