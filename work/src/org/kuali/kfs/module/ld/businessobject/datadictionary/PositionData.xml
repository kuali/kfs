<!-- Business Object Inquiry Definition -->


  <bean id="PositionData-inquiryDefinition" parent="PositionData-inquiryDefinition-parentBean" />

  <bean id="PositionData-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Position Data Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="budgetedPosition" />
              <bean parent="FieldDefinition"
                    p:attributeName="businessUnit" />
              <bean parent="FieldDefinition"
                    p:attributeName="departmentId" />
              <bean parent="FieldDefinition"
                    p:attributeName="description" />
              <bean parent="FieldDefinition"
                    p:attributeName="effectiveDate" />
              <bean parent="FieldDefinition"
                    p:attributeName="jobCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="positionEffectiveStatus" />
              <bean parent="FieldDefinition"
                    p:attributeName="positionFullTimeEquivalency" />
              <bean parent="FieldDefinition"
                    p:attributeName="positionGradeDefault" />
              <bean parent="FieldDefinition"
                    p:attributeName="positionNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="positionRegularTemporary" />
              <bean parent="FieldDefinition"
                    p:attributeName="positionSalaryPlanDefault" />
              <bean parent="FieldDefinition"
                    p:attributeName="positionStatus" />
              <bean parent="FieldDefinition"
                    p:attributeName="shortDescription" />
              <bean parent="FieldDefinition"
                    p:attributeName="standardHoursDefault" />
              <bean parent="FieldDefinition"
                    p:attributeName="standardHoursFrequency" />
              <bean parent="FieldDefinition"
                    p:attributeName="statusDate" />
            </list>
          </property>
        </bean>
      </list>
    </property>
    <property name="inquirableClass" value="org.kuali.module.labor.web.inquirable.PositionDataDetailsInquirableImpl" />
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="PositionData-lookupDefinition" parent="PositionData-lookupDefinition-parentBean" />

