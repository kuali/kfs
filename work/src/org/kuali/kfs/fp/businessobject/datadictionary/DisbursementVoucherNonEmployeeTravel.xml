                <label>City</label>
                <shortLabel>City</shortLabel>

                <maxLength>40</maxLength>

                <required>true</required>

                <control>
                    <text size="40" />
                </control>

                <summary>
                    <![CDATA[&nbsp;]]>
                </summary>
                <description>A free-form text field that identifies the City of the Non-Employee Travel.</description>
            </attribute>

            <attribute name="disbVchrTravelFromStateCode">
                <label>State</label>
                <shortLabel>St</shortLabel>

                <maxLength>2</maxLength>

                <required>false</required>

                <control>
                    <text size="2" />
