<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="PurchaseOrderQuoteLanguageMaintenanceDocument" parent="PurchaseOrderQuoteLanguageMaintenanceDocument-parentBean" />

  <bean id="PurchaseOrderQuoteLanguageMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.purap.bo.PurchaseOrderQuoteLanguage" />
    <property name="maintainableClass" value="org.kuali.module.purap.maintenance.PurchaseOrderQuoteLanguageMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="PurchaseOrderQuoteLanguageMaintenanceDocument-EditPurchaseOrderQuoteLanguage" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>purchaseOrderQuoteLanguageIdentifier</value>
      </list>
    </property>
    <property name="label" value="Purchase Order Quote Language" />
    <property name="shortLabel" value="Purchase Order Quote Language" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-PA"
            p:parameterName="PURCHASE_ORDER_QUOTE_LANGUAGE"
            p:parameterDetailType="PurchaseOrderQuoteLanguage" />
    </property>
    <property name="documentTypeName" value="PurchaseOrderQuoteLanguageMaintenanceDocument" />
    <property name="businessRulesClass" value="org.kuali.module.purap.rules.PurchaseOrderQuoteLanguageRule" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="PMQL" />
    <property name="workflowProperties" >
      <ref bean="PurchaseOrderQuoteLanguageMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>PA_PUR_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="PurchaseOrderQuoteLanguageMaintenanceDocument-EditPurchaseOrderQuoteLanguage" parent="PurchaseOrderQuoteLanguageMaintenanceDocument-EditPurchaseOrderQuoteLanguage-parentBean" />

  <bean id="PurchaseOrderQuoteLanguageMaintenanceDocument-EditPurchaseOrderQuoteLanguage-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Purchase Order Quote Language" />
    <property name="title" value="Edit Purchase Order Quote Language" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
        <bean parent="MaintainableFieldDefinition"
              p:name="purchaseOrderQuoteLanguageIdentifier" />
        <bean parent="MaintainableFieldDefinition"
              p:name="purchaseOrderQuoteLanguageDescription"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="active"
              p:defaultValue="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="purchaseOrderQuoteLanguageCreateDate"
              p:readOnly="true"
              p:defaultValueFinderClass="org.kuali.kfs.lookup.valuefinder.CurrentDateMMDDYYYYFinder" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="PurchaseOrderQuoteLanguageMaintenanceDocument-workflowProperties" parent="PurchaseOrderQuoteLanguageMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="PurchaseOrderQuoteLanguageMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
