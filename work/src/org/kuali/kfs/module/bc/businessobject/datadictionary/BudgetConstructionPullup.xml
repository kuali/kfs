<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="BudgetConstructionPullup" parent="BudgetConstructionPullup-parentBean" />

  <bean id="BudgetConstructionPullup-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.budget.bo.BudgetConstructionPullup" />
    <property name="inquiryDefinition" >
      <ref bean="BudgetConstructionPullup-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="BudgetConstructionPullup-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="BudgetConstructionPullup-chartOfAccountsCode" />
        <ref bean="BudgetConstructionPullup-organizationCode" />
        <ref bean="BudgetConstructionPullup-personUniversalIdentifier" />
        <ref bean="BudgetConstructionPullup-pullFlag" />
        <ref bean="BudgetConstructionPullup-reportsToChartOfAccountsCode" />
        <ref bean="BudgetConstructionPullup-reportsToOrganizationCode" />
        <ref bean="BudgetConstructionPullup-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="BudgetConstructionPullup-chartOfAccountsCode" parent="BudgetConstructionPullup-chartOfAccountsCode-parentBean" />

  <bean id="BudgetConstructionPullup-chartOfAccountsCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="chartOfAccountsCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Chart Of Accounts Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    <property name="summary" value="chartOfAccountsCode" />
    <property name="description" value="chartOfAccountsCode description..." />
  </bean>
  <bean id="BudgetConstructionPullup-organizationCode" parent="BudgetConstructionPullup-organizationCode-parentBean" />

  <bean id="BudgetConstructionPullup-organizationCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="organizationCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Organization Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="6" />
    </property>
    <property name="summary" value="organizationCode" />
    <property name="description" value="organizationCode description..." />
  </bean>
  <bean id="BudgetConstructionPullup-personUniversalIdentifier" parent="BudgetConstructionPullup-personUniversalIdentifier-parentBean" />

  <bean id="BudgetConstructionPullup-personUniversalIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="personUniversalIdentifier" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Person Universal Identifier" />
    <property name="shortLabel" value="Identifier" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
    <property name="summary" value="personUniversalIdentifier" />
    <property name="description" value="personUniversalIdentifier description..." />
  </bean>
  <bean id="BudgetConstructionPullup-pullFlag" parent="BudgetConstructionPullup-pullFlag-parentBean" />

  <bean id="BudgetConstructionPullup-pullFlag-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="pullFlag" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Pull Flag" />
    <property name="shortLabel" value="Flag" />
    <property name="maxLength" value="5" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="7" />
    </property>
    <property name="summary" value="pullFlag" />
    <property name="description" value="pullFlag description..." />
  </bean>
  <bean id="BudgetConstructionPullup-reportsToChartOfAccountsCode" parent="BudgetConstructionPullup-reportsToChartOfAccountsCode-parentBean" />

  <bean id="BudgetConstructionPullup-reportsToChartOfAccountsCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="reportsToChartOfAccountsCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Reports To Chart Of Accounts Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    <property name="summary" value="reportsToChartOfAccountsCode" />
    <property name="description" value="reportsToChartOfAccountsCode description..." />
  </bean>
  <bean id="BudgetConstructionPullup-reportsToOrganizationCode" parent="BudgetConstructionPullup-reportsToOrganizationCode-parentBean" />

  <bean id="BudgetConstructionPullup-reportsToOrganizationCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="reportsToOrganizationCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Reports To Organization Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="6" />
    </property>
    <property name="summary" value="reportsToOrganizationCode" />
    <property name="description" value="reportsToOrganizationCode description..." />
  </bean>
  <bean id="BudgetConstructionPullup-versionNumber" parent="BudgetConstructionPullup-versionNumber-parentBean" />

  <bean id="BudgetConstructionPullup-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="BudgetConstructionPullup-inquiryDefinition" parent="BudgetConstructionPullup-inquiryDefinition-parentBean" />

  <bean id="BudgetConstructionPullup-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Budget Construction Pullup Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="chartOfAccountsCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="organizationCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="personUniversalIdentifier" />
              <bean parent="FieldDefinition"
                    p:attributeName="pullFlag" />
              <bean parent="FieldDefinition"
                    p:attributeName="reportsToChartOfAccountsCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="reportsToOrganizationCode" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="BudgetConstructionPullup-lookupDefinition" parent="BudgetConstructionPullup-lookupDefinition-parentBean" />

  <bean id="BudgetConstructionPullup-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Budget Construction Pullup Lookup" />
    <property name="instructions" value="Look up a Budget Construction Pullup" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationCode" />
        <bean parent="FieldDefinition"
              p:attributeName="personUniversalIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="pullFlag" />
        <bean parent="FieldDefinition"
              p:attributeName="reportsToChartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="reportsToOrganizationCode" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationCode" />
        <bean parent="FieldDefinition"
              p:attributeName="personUniversalIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="pullFlag" />
        <bean parent="FieldDefinition"
              p:attributeName="reportsToChartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="reportsToOrganizationCode" />
      </list>
    </property>
  </bean>
</beans>
