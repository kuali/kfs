<!-- Business Object Inquiry Definition -->


  <bean id="BudgetConstructionAccountSelect-inquiryDefinition" parent="BudgetConstructionAccountSelect-inquiryDefinition-parentBean" />

  <bean id="BudgetConstructionAccountSelect-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Budget Construction Account Select Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="chartOfAccountsCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="accountNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="subAccountNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="documentNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialDocumentCreateDate" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialDocumentInitiatorIdentifier" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialDocumentStatusCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="organizationLevelCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="organizationChartOfAccountsCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="organizationCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="personUniversalIdentifier" />
              <bean parent="FieldDefinition"
                    p:attributeName="universityFiscalYear" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="BudgetConstructionAccountSelect-lookupDefinition" parent="BudgetConstructionAccountSelect-lookupDefinition-parentBean" />

  <bean id="BudgetConstructionAccountSelect-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Budget Construction Account Select Lookup" />
    <property name="lookupableID" value="accountSelectLookupable" />
    <property name="instructions" value="Look up a Budget Construction Account Select" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="accountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="subAccountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentStatusCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentCreateDate" />
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentInitiatorIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationLevelCode" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationChartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationCode" />
        <bean parent="FieldDefinition"
              p:attributeName="personUniversalIdentifier" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode"
              p:noInquiry="true" />
        <bean parent="FieldDefinition"
              p:attributeName="accountNumber"
              p:noInquiry="true" />
        <bean parent="FieldDefinition"
              p:attributeName="subAccountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentStatusCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentCreateDate" />
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentInitiatorIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationLevelCode" />
        <bean parent="FieldDefinition"
