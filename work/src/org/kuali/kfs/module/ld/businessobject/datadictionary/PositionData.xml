                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="positionNumber" required="false" />
                <lookupField attributeName="effectiveDate" required="false" />
                <lookupField attributeName="businessUnit" required="false" />
                <lookupField attributeName="departmentId" required="false" />
                
                <lookupField attributeName="jobCode" required="false" />
                <lookupField attributeName="positionStatus" required="false" />
                <lookupField attributeName="description" required="false" />
                
                <lookupField attributeName="positionSalaryPlanDefault" required="false" />
                <lookupField attributeName="positionGradeDefault" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="positionNumber"/>
                <field attributeName="effectiveDate"/>
                <field attributeName="description"/>
                
                <field attributeName="businessUnit"/>
                <field attributeName="departmentId"/>             
                <field attributeName="positionSalaryPlanDefault"/>
                <field attributeName="positionGradeDefault"/>
                <field attributeName="positionFullTimeEquivalency" />
                
                <field attributeName="jobCode"/>
                <field attributeName="positionStatus"/>
                <field attributeName="dummyBusinessObject.linkButtonOption" />
                
                <!-- 
                <field attributeName="positionEffectiveStatus" />
                <field attributeName="positionRegularTemporary" />

                <field attributeName="shortDescription" />
