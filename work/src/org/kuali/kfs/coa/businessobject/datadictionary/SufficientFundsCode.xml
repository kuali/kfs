<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="SufficientFundsCode" parent="SufficientFundsCode-parentBean" />

  <bean id="SufficientFundsCode-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.chart.bo.codes.SufficientFundsCode" />
    <property name="inquiryDefinition" >
      <ref bean="SufficientFundsCode-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="SufficientFundsCode-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="SufficientFundsCode-code" />
        <ref bean="SufficientFundsCode-name" />
        <ref bean="SufficientFundsCode-active" />
        <ref bean="SufficientFundsCode-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="SufficientFundsCode-code" parent="SufficientFundsCode-code-parentBean" />

  <bean id="SufficientFundsCode-code-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="code" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Account Sufficient Funds Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="1" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="3" />
    </property>
  </bean>
  <bean id="SufficientFundsCode-name" parent="SufficientFundsCode-name-parentBean" />

  <bean id="SufficientFundsCode-name-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="name" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Account Sufficient Funds Name" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="40" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="42" />
    </property>
  </bean>
  <bean id="SufficientFundsCode-active" parent="SufficientFundsCode-active-parentBean" />

  <bean id="SufficientFundsCode-active-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="active" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Active" />
    <property name="shortLabel" value="Active" />
    <property name="maxLength" value="1" />
    <property name="control" >
      <bean parent="CheckboxControlDefinition" />
    </property>
  </bean>
  <bean id="SufficientFundsCode-versionNumber" parent="SufficientFundsCode-versionNumber-parentBean" />

  <bean id="SufficientFundsCode-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="SufficientFundsCode-inquiryDefinition" parent="SufficientFundsCode-inquiryDefinition-parentBean" />

  <bean id="SufficientFundsCode-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Sufficient Funds Code Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Sufficient Funds Code Details" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="code" />
              <bean parent="FieldDefinition"
                    p:attributeName="name" />
              <bean parent="FieldDefinition"
                    p:attributeName="active" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="SufficientFundsCode-lookupDefinition" parent="SufficientFundsCode-lookupDefinition-parentBean" />

  <bean id="SufficientFundsCode-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Sufficient Funds Code Lookup" />
    <property name="instructions" value="Look up an Sufficient Funds Code" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="code" />
        <bean parent="FieldDefinition"
              p:attributeName="name" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="code" />
        <bean parent="FieldDefinition"
              p:attributeName="name" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
  </bean>
</beans>
