                <shortLabel>Purpose</shortLabel>
                <control>
                    <select valuesFinderClass="org.kuali.module.cg.lookup.keyvalues.ProposalPurposeValuesFinder" />
                </control>

                <summary>
                    Proposal Purpose Code
                </summary>
                <description>A proposal purpose code</description>
