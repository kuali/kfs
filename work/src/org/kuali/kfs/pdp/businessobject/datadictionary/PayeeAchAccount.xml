<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="PayeeAchAccount" parent="PayeeAchAccount-parentBean" />

  <bean id="PayeeAchAccount-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.pdp.bo.PayeeAchAccount" />
    <property name="inquiryDefinition" >
      <ref bean="PayeeAchAccount-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="PayeeAchAccount-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="PayeeAchAccount-achAccountGeneratedIdentifier" />
        <ref bean="PayeeAchAccount-bankAccountNumber" />
        <ref bean="PayeeAchAccount-bankAccountTypeCode" />
        <ref bean="PayeeAchAccount-payeeName" />
        <ref bean="PayeeAchAccount-payeeEmailAddress" />
        <ref bean="PayeeAchAccount-payeeIdentifierTypeCode" />
        <ref bean="PayeeAchAccount-payeeSocialSecurityNumber" />
        <ref bean="PayeeAchAccount-payeeFederalEmployerIdentificationNumber" />
        <ref bean="PayeeAchAccount-psdTransactionCode" />
        <ref bean="PayeeAchAccount-bankRoutingNumber" />
        <ref bean="PayeeAchAccount-vendorHeaderGeneratedIdentifier" />
        <ref bean="PayeeAchAccount-vendorDetailAssignedIdentifier" />
        <ref bean="PayeeAchAccount-vendorNumber" />
        <ref bean="PayeeAchAccount-disbVchrPayeeIdNumber" />
        <ref bean="PayeeAchAccount-personUniversalIdentifier" />
        <ref bean="PayeeAchAccount-active" />
        <ref bean="PayeeAchAccount-versionNumber" />
      </list>
    </property>
    <property name="relationships" >
      <list>
        <bean parent="RelationshipDefinition">
          <property name="objectAttributeName" value="vendorDetail" />
          <property name="primitiveAttributes" >
            <list>
              <bean parent="PrimitiveAttributeDefinition"
                    p:sourceName="vendorNumber"
                    p:targetName="vendorNumber" />
            </list>
          </property>
          <property name="supportAttributes" >
            <list>
              <bean parent="SupportAttributeDefinition"
                    p:sourceName="vendorHeaderGeneratedIdentifier"
                    p:targetName="vendorHeaderGeneratedIdentifier" />
              <bean parent="SupportAttributeDefinition"
                    p:sourceName="vendorDetailAssignedIdentifier"
                    p:targetName="vendorDetailAssignedIdentifier" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="PayeeAchAccount-achAccountGeneratedIdentifier" parent="PayeeAchAccount-achAccountGeneratedIdentifier-parentBean" />

  <bean id="PayeeAchAccount-achAccountGeneratedIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="achAccountGeneratedIdentifier" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="ACH Account Generated Identifier" />
    <property name="shortLabel" value="ACH Account ID" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
    <property name="summary" value="ACH Account Generated Identifier" />
    <property name="description" value="ACH Account Generated ID." />
  </bean>
  <bean id="PayeeAchAccount-bankAccountNumber" parent="PayeeAchAccount-bankAccountNumber-parentBean" />

  <bean id="PayeeAchAccount-bankAccountNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="bankAccountNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Bank Account Number" />
    <property name="shortLabel" value="Bank Account Number" />
    <property name="maxLength" value="17" />
    <property name="validationPattern" >
      <bean parent="AlphaNumericValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="17" />
    </property>
    <property name="displayMask" >
      <bean parent="Mask">
        <property name="maskFormatter" >
          <bean parent="MaskFormatterLiteral"
                p:literal="*****************" />
        </property>
      </bean>
    </property>
    <property name="displayWorkgroup" value="PD_VIEW_ACH" />
    <property name="summary" value="Bank Account Number" />
    <property name="description" value="Bank Account Number." />
  </bean>
  <bean id="PayeeAchAccount-bankAccountTypeCode" parent="PayeeAchAccount-bankAccountTypeCode-parentBean" />

  <bean id="PayeeAchAccount-bankAccountTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="bankAccountTypeCode" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Account Type" />
    <property name="shortLabel" value="Account Type" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="CharsetValidationPattern"
            p:validChars="23" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.module.chart.lookup.keyvalues.CheckingSavingsValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
    <property name="summary" value="Bank Account Type Code summary..." />
    <property name="description" value="22=Checking 32=Savings." />
  </bean>
  <bean id="PayeeAchAccount-payeeName" parent="PayeeAchAccount-payeeName-parentBean" />

  <bean id="PayeeAchAccount-payeeName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="payeeName" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Name" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="50" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="51" />
    </property>
    <property name="summary" value="Name" />
    <property name="description" value="Payee Name." />
  </bean>
  <bean id="PayeeAchAccount-payeeEmailAddress" parent="PayeeAchAccount-payeeEmailAddress-parentBean" />

  <bean id="PayeeAchAccount-payeeEmailAddress-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="payeeEmailAddress" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Email Address" />
    <property name="shortLabel" value="Email Address" />
    <property name="maxLength" value="43" />
    <property name="validationPattern" >
      <bean parent="EmailAddressValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="43" />
    </property>
    <property name="summary" value="Payee Email Address" />
    <property name="description" value="Payee Email Address." />
  </bean>
  <bean id="PayeeAchAccount-payeeIdentifierTypeCode" parent="PayeeAchAccount-payeeIdentifierTypeCode-parentBean" />

  <bean id="PayeeAchAccount-payeeIdentifierTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="payeeIdentifierTypeCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Payee Type Code" />
    <property name="shortLabel" value="Payee Type Code" />
    <property name="maxLength" value="1" />
    <property name="validationPattern" >
      <bean parent="CharsetValidationPattern"
            p:validChars="EFPSV" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.module.chart.lookup.keyvalues.PayeeTypeCodeValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
    <property name="summary" value="Payee Type Code" />
    <property name="description" value="E=Employee V=Vendor F=FEIN S=SSN P=DV Payee" />
  </bean>
  <bean id="PayeeAchAccount-payeeSocialSecurityNumber" parent="PayeeAchAccount-payeeSocialSecurityNumber-parentBean" />

  <bean id="PayeeAchAccount-payeeSocialSecurityNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="payeeSocialSecurityNumber" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Social Security Number" />
    <property name="shortLabel" value="SSN" />
    <property name="maxLength" value="9" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="9" />
    </property>
    <property name="displayMask" >
      <bean parent="Mask">
        <property name="maskFormatter" >
          <bean parent="MaskFormatterLiteral"
                p:literal="*********" />
        </property>
      </bean>
    </property>
    <property name="displayWorkgroup" value="KAULI_PDP_ACH" />
    <property name="summary" value="Payee SSN" />
    <property name="description" value="Payee SSN." />
  </bean>
  <bean id="PayeeAchAccount-payeeFederalEmployerIdentificationNumber" parent="PayeeAchAccount-payeeFederalEmployerIdentificationNumber-parentBean" />

  <bean id="PayeeAchAccount-payeeFederalEmployerIdentificationNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="payeeFederalEmployerIdentificationNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="FEIN Number" />
    <property name="shortLabel" value="FEIN Number" />
    <property name="maxLength" value="9" />
    <property name="validationPattern" >
      <bean parent="AlphaNumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="9" />
    </property>
    <property name="summary" value="Federal Employee Identification Number" />
    <property name="description" value="Federal Employee Identification Number." />
  </bean>
  <bean id="PayeeAchAccount-psdTransactionCode" parent="PayeeAchAccount-psdTransactionCode-parentBean" />

  <bean id="PayeeAchAccount-psdTransactionCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="psdTransactionCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="PSD Transaction Code" />
    <property name="shortLabel" value="PSD Transaction Code" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="5" />
    </property>
    <property name="summary" value="PSD Transaction Code" />
    <property name="description" value="PSD Transaction Code." />
  </bean>
  <bean id="PayeeAchAccount-bankRoutingNumber" parent="PayeeAchAccount-bankRoutingNumber-parentBean" />

  <bean id="PayeeAchAccount-bankRoutingNumber-parentBean" abstract="true" parent="AchBank-bankRoutingNumber">
  </bean>
  <bean id="PayeeAchAccount-vendorHeaderGeneratedIdentifier" parent="PayeeAchAccount-vendorHeaderGeneratedIdentifier-parentBean" />

  <bean id="PayeeAchAccount-vendorHeaderGeneratedIdentifier-parentBean" abstract="true" parent="VendorDetail-vendorHeaderGeneratedIdentifier">
    <property name="label" value="Vendor" />
    <property name="shortLabel" value="Vendor" />
  </bean>
  <bean id="PayeeAchAccount-vendorDetailAssignedIdentifier" parent="PayeeAchAccount-vendorDetailAssignedIdentifier-parentBean" />

  <bean id="PayeeAchAccount-vendorDetailAssignedIdentifier-parentBean" abstract="true" parent="VendorDetail-vendorDetailAssignedIdentifier">
    <property name="label" value="Vendor" />
    <property name="shortLabel" value="Vendor" />
  </bean>
  <bean id="PayeeAchAccount-vendorNumber" parent="PayeeAchAccount-vendorNumber-parentBean" />

  <bean id="PayeeAchAccount-vendorNumber-parentBean" abstract="true" parent="VendorDetail-vendorNumber">
    <property name="shortLabel" value="Vendor Number" />
    <property name="control" >
      <bean parent="LookupReadonlyControlDefinition" />
    </property>
  </bean>
  <bean id="PayeeAchAccount-disbVchrPayeeIdNumber" parent="PayeeAchAccount-disbVchrPayeeIdNumber-parentBean" />

  <bean id="PayeeAchAccount-disbVchrPayeeIdNumber-parentBean" abstract="true" parent="Payee-payeeIdNumber">
    <property name="name" value="disbVchrPayeeIdNumber" />
    <property name="label" value="Disbursement Voucher Payee ID" />
  </bean>
  <bean id="PayeeAchAccount-personUniversalIdentifier" parent="PayeeAchAccount-personUniversalIdentifier-parentBean" />

  <bean id="PayeeAchAccount-personUniversalIdentifier-parentBean" abstract="true" parent="UniversalUser-personUniversalIdentifier">
    <property name="label" value="Payee User ID" />
    <property name="shortLabel" value="Payee User ID" />
  </bean>
  <bean id="PayeeAchAccount-active" parent="PayeeAchAccount-active-parentBean" />

  <bean id="PayeeAchAccount-active-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
    <property name="name" value="active" />
    <property name="required" value="true" />
  </bean>
  <bean id="PayeeAchAccount-versionNumber" parent="PayeeAchAccount-versionNumber-parentBean" />

  <bean id="PayeeAchAccount-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="PayeeAchAccount-inquiryDefinition" parent="PayeeAchAccount-inquiryDefinition-parentBean" />

  <bean id="PayeeAchAccount-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Payee ACH Account" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="achAccountGeneratedIdentifier" />
              <bean parent="FieldDefinition"
                    p:attributeName="bankRoutingNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="bankAccountNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="payeeName" />
              <bean parent="FieldDefinition"
                    p:attributeName="payeeEmailAddress" />
              <bean parent="FieldDefinition"
                    p:attributeName="payeeIdentifierTypeCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="vendorHeaderGeneratedIdentifier" />
              <bean parent="FieldDefinition"
                    p:attributeName="vendorDetailAssignedIdentifier" />
              <bean parent="FieldDefinition"
                    p:attributeName="disbVchrPayeeIdNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="personUniversalIdentifier" />
              <bean parent="FieldDefinition"
                    p:attributeName="payeeSocialSecurityNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="payeeFederalEmployerIdentificationNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="psdTransactionCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="active" />
              <bean parent="FieldDefinition"
                    p:attributeName="bankAccountTypeCode" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="PayeeAchAccount-lookupDefinition" parent="PayeeAchAccount-lookupDefinition-parentBean" />

  <bean id="PayeeAchAccount-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Payee ACH Account Lookup" />
    <property name="lookupableID" value="pdpPayeeAchAccountLookupable" />
    <property name="instructions" value="Lookup a Payee ACH Account" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>achAccountGeneratedIdentifier</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="bankRoutingNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="bankAccountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="bankAccountTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="payeeName" />
        <bean parent="FieldDefinition"
              p:attributeName="payeeIdentifierTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="vendorNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="disbVchrPayeeIdNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="personUniversalIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="payeeSocialSecurityNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="payeeFederalEmployerIdentificationNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="psdTransactionCode" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="payeeName" />
        <bean parent="FieldDefinition"
              p:attributeName="payeeEmailAddress" />
        <bean parent="FieldDefinition"
              p:attributeName="payeeIdentifierTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
  </bean>
</beans>
