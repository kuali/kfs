<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="BudgetConstructionIntendedIncumbentSelect" parent="BudgetConstructionIntendedIncumbentSelect-parentBean" />

  <bean id="BudgetConstructionIntendedIncumbentSelect-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.budget.bo.BudgetConstructionIntendedIncumbentSelect" />
    <property name="inquiryDefinition" >
      <ref bean="BudgetConstructionIntendedIncumbentSelect-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="BudgetConstructionIntendedIncumbentSelect-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="BudgetConstructionIntendedIncumbentSelect-emplid" />
        <ref bean="BudgetConstructionIntendedIncumbentSelect-financialObjectCode" />
        <ref bean="BudgetConstructionIntendedIncumbentSelect-personName" />
        <ref bean="BudgetConstructionIntendedIncumbentSelect-personUniversalIdentifier" />
        <ref bean="BudgetConstructionIntendedIncumbentSelect-dummyBusinessObject.linkButtonOption" />
        <ref bean="BudgetConstructionIntendedIncumbentSelect-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="BudgetConstructionIntendedIncumbentSelect-emplid" parent="BudgetConstructionIntendedIncumbentSelect-emplid-parentBean" />

  <bean id="BudgetConstructionIntendedIncumbentSelect-emplid-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="emplid" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Emplid" />
    <property name="shortLabel" value="Emplid" />
    <property name="maxLength" value="11" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="13" />
    </property>
    <property name="summary" value="emplid" />
    <property name="description" value="emplid description..." />
  </bean>
  <bean id="BudgetConstructionIntendedIncumbentSelect-financialObjectCode" parent="BudgetConstructionIntendedIncumbentSelect-financialObjectCode-parentBean" />

  <bean id="BudgetConstructionIntendedIncumbentSelect-financialObjectCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialObjectCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Financial Object Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="6" />
    </property>
    <property name="summary" value="financialObjectCode" />
    <property name="description" value="financialObjectCode description..." />
  </bean>
  <bean id="BudgetConstructionIntendedIncumbentSelect-personName" parent="BudgetConstructionIntendedIncumbentSelect-personName-parentBean" />

  <bean id="BudgetConstructionIntendedIncumbentSelect-personName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="personName" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Person Name" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="50" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="52" />
    </property>
    <property name="summary" value="personName" />
    <property name="description" value="personName description..." />
  </bean>
  <bean id="BudgetConstructionIntendedIncumbentSelect-personUniversalIdentifier" parent="BudgetConstructionIntendedIncumbentSelect-personUniversalIdentifier-parentBean" />

  <bean id="BudgetConstructionIntendedIncumbentSelect-personUniversalIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="personUniversalIdentifier" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Person Universal Identifier" />
    <property name="shortLabel" value="Identifier" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
    <property name="summary" value="personUniversalIdentifier" />
    <property name="description" value="personUniversalIdentifier description..." />
  </bean>
  <bean id="BudgetConstructionIntendedIncumbentSelect-dummyBusinessObject.linkButtonOption" parent="BudgetConstructionIntendedIncumbentSelect-dummyBusinessObject.linkButtonOption-parentBean" />

  <bean id="BudgetConstructionIntendedIncumbentSelect-dummyBusinessObject.linkButtonOption-parentBean" abstract="true" parent="TransientBalanceInquiryAttributes-linkButtonOption">
    <property name="name" value="dummyBusinessObject.linkButtonOption" />
    <property name="label" value="Incumbent Salary Setting" />
    <property name="shortLabel" value="Incumbent SalSet" />
    <property name="summary" value="Incumbent Salary Setting" />
    <property name="description" value="Incumbent Salary Setting ..." />
  </bean>
  <bean id="BudgetConstructionIntendedIncumbentSelect-versionNumber" parent="BudgetConstructionIntendedIncumbentSelect-versionNumber-parentBean" />

  <bean id="BudgetConstructionIntendedIncumbentSelect-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="BudgetConstructionIntendedIncumbentSelect-inquiryDefinition" parent="BudgetConstructionIntendedIncumbentSelect-inquiryDefinition-parentBean" />

  <bean id="BudgetConstructionIntendedIncumbentSelect-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Budget Construction Intended Incumbent Select Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="emplid" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialObjectCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="personName" />
              <bean parent="FieldDefinition"
                    p:attributeName="personUniversalIdentifier" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="BudgetConstructionIntendedIncumbentSelect-lookupDefinition" parent="BudgetConstructionIntendedIncumbentSelect-lookupDefinition-parentBean" />

  <bean id="BudgetConstructionIntendedIncumbentSelect-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Budget Construction Intended Incumbent Select Lookup" />
    <property name="lookupableID" value="intendedIncumbentSelectLookupable" />
    <property name="instructions" value="Look up a Budget Construction Intended Incumbent Select" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="emplid" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="personName" />
        <bean parent="FieldDefinition"
              p:attributeName="personUniversalIdentifier" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="emplid" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="personName" />
        <bean parent="FieldDefinition"
              p:attributeName="dummyBusinessObject.linkButtonOption" />
      </list>
    </property>
  </bean>
</beans>
