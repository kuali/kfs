    <property name="summary" value="offCampusStateCode" />
    <property name="description" value="offCampusStateCode description..." />
  </bean>
  <bean id="AssetTransferDocument-offCampusZipCode" parent="AssetTransferDocument-offCampusZipCode-parentBean" />

  <bean id="AssetTransferDocument-offCampusZipCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="offCampusZipCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Zip" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="9" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
